|SI mesp = "10"; mespl = "Octubre   "; |FINSI;
|SI mesp = "11"; mespl = "Noviembre "; |FINSI;
|SI mesp = "12"; mespl = "Diciembre "; |FINSI;
|FPRC;

|PROCESO fechadev;
fechavo = #1#34 + 0.004000;

diaf    = fechavo % 102;
anyof   = fechavo % -102;
mesf    = fechavo % 402;

diav    = #1#34 % 102;
