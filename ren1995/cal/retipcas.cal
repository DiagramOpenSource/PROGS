|FICHEROS;
    retipcas #0;
    recasill #1;
|FIN;

|VARIABLES;
    modo = 0;
|FIN;

|PROGRAMA;
    |CLS;
    |CABEZA "Datos de Renta.";
    |ABRE #1;
    |LEE 000#1p;
    |SI FSalida ! 0;
        |MENAV "    Debera instalar Ficheros Patrones ...";
        |PTEC 807;
    |FINSI;
    |CIERRA #1;
    |ENTLINEAL #1#0,1,1,10,1,min,max;
|FPRO;

|PROCESO min;
         #0#0 = 1;
|FPRC;

|PROCESO max;
         #0#0 = 999;
|FPRC;

|PROCESO casilla_ini; |TIPO 7;
         |ENTLINEAL #1#1,1,4,20,0,inf,sup;
|FPRC;

|PROCESO entra_casillas; |TIPO 2;
         modo = 0 +. 1;
         |SI modo = 1;
             |ATRI R;
             |PINDA #0#0;
             |ATRI r;
             |ENTLINEAL #1#1,1,1,20,0,inf,sup;
             |PINDA #0#0;
         |FINSI;
|FPRC;

|PROCESO inf;
         #1#0 = #0#0;
|FPRC;

|PROCESO sup;
         #1#0 = #0#1;
|FPRC;
