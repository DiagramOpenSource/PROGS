|FICHEROS;
   rerendtr #0;
   reparame #1;
|FIN;

|VARIABLES;
   &indivi    = "";
   &errorlin  = 0;
   &empresa   = 0;
   &nombre    = "";
   &nif       = "";
   &tipo      = "";
   &perceptor = "";
   &entidad   = "";
   &modo      = 0;
   &modelito  = 0;
   &numerica1 = 0;
   &numerica2 = 0;
   &numerica3 = 0;
   &numerica4 = 0;
   &numerica5 = 0;
   &numerica6 = 0;
   &numerica7 = 0;
   &numerica8 = 0;
   &numerica9 = 0;
   &numerica10 = 0;
   &numerica11 = 0;
   &numerica12 = 0;
   &numerica13 = 0;
   &numerica14 = 0;
   &numerica15 = 0;
   &numerica16 = 0;
   &numerica17 = 0;
   &numerica18 = 0;
   &numerica19 = 0;
   &numerica20 = 0;
   &numerica21 = 0;
   &numerica22 = 0;
   &alfanume1 = "";
   &alfanume2 = "";
   pantalla   = 0;
|FIN;

|INCLUYE i_activo;

|RUTINA inferior;
  #0#0 = empresa;
  #0#1 = 1;
|FRUT;

|RUTINA superior;
  #0#0 = empresa;
  #0#1 = 99;
|FRUT;

|PROGRAMA;
npro = 1;
|ABRE #0;
|PINPA #0#0;
|SI modo = 1;
    |ENTLINEAL #1#0,2,1,15,0,inferior,superior;
|FINSI;
|SI modo = 2;
    |ENTLINEAL #1#0,2,2,15,0,inferior,superior;
|FINSI;
|SI modo = 4;
    |ENTLINEAL #1#0,2,4,15,0,inferior,superior;
|FINSI;
|FPRO;

|PROCESO ayuda; |TIPO 2;
pantalla = 0 +. 1;
|SI pantalla = 1; |Y #0#28 ! "X"; |HAZ cargadat; |FINSI;
|FPRC;

|PROCESO consulta; |TIPO 0;
modelito = (FEntrada / 100) ! 0;
|SI modelito = 4; |HAZ cargadat; |FINSI;
|SI modelito = 1; |HAZ parametro; |FINSI;
|FPRC;

|PROCESO parametro;
|ABRE #1;
|LEE 040#1=;
|SI FSalida = 0;
   |SI #1#6 ! "V"; |Y #1#6 ! "G";
       #0#2 = #1#6;
       |PINTA #0#2;
   |FINSI;
|FINSI;
|CIERRA #1;
|FPRC;

|PROCESO cargadat;
tipo       = #0#2;
perceptor  = #0#3;
entidad    = #0#4;
numerica1  = #0#5;
numerica2  = #0#6;
numerica3  = #0#7;
numerica4  = #0#8;
numerica5  = #0#9;
numerica6  = #0#11;
numerica7  = #0#12;
numerica8  = #0#13;
numerica9  = #0#15;
numerica10 = #0#16;
numerica11 = #0#17;
numerica12 = #0#18;
numerica13 = #0#19;
numerica14 = #0#20;
numerica15 = #0#21;
numerica16 = #0#22;
numerica17 = #0#23;
numerica18 = #0#24;
numerica19 = #0#25;
numerica20 = #0#26;
numerica21 = #0#27;
numerica22 = #0#29;
alfanume1  = #0#10;
alfanume2  = #0#14;

|SUB_EJECUTA "repant01.run";
|SI errorlin = 0; |ERROR; |SINO; |HAZ carga; |FINSI;
|FPRC;

|PROCESO carga;
|SI modelito ! 4;
   #0#2  = tipo;
   #0#3  = perceptor;
   #0#4  = entidad;
   #0#6  = numerica2;
   #0#7  = numerica3;
   #0#8  = numerica4;
   #0#9  = numerica5;
   #0#11 = numerica6;
   #0#12 = numerica7;
   #0#13 = numerica8;
   #0#15 = numerica9;
   #0#16 = numerica10;
   #0#17 = numerica11;
   #0#18 = numerica12;
   #0#19 = numerica13;
   #0#20 = numerica14;
   #0#21 = numerica15;
   #0#22 = numerica16;
   #0#23 = numerica17;
   #0#24 = numerica18;
   #0#25 = numerica19;
   #0#26 = numerica20;
   #0#27 = numerica21;
   #0#29 = numerica22;
   #0#10 = alfanume1;
   #0#14 = alfanume2;
   #0#5  = #0#7 + #0#15 + #0#16 + #0#12 + #0#27;
   #0#5  = #0#5 - #0#17 - #0#18 - #0#19 - #0#20;
   |HAZ sigrabo;

   |PINTA #0#2;
   |PINTA #0#3;
   |PINTA #0#4;
   |PINTA #0#5;
|FINSI;
|FPRC;
