|FICHEROS;
   rez00000 #0;
|FIN;

|VARIABLES;
   cont  = 0;
   tecla = 0;
   &DEmpre = 0;
   &HEmpre = 0;
|FIN;

|PROCESO funcion; |TIPO 11;
tecla = FSalida;

|SI tecla > 10; |ACABA; |FINSI;
|SI tecla < 9;  |ACABA; |FINSI;

|SI #0#2 = "P"; |Y tecla = 10; |ACABA; |FINSI;

|HAZ cuadro1;

|SI #0#2 = "P"; |Y tecla = 9; |Y DEmpre = 1; |Y HEmpre = 99999;
    tecla = 0;
    |MENAV "     El fichero ya ha sido pasado";
    |CONFI "2400NDesea pasarlo de nuevo : ";
    |SI FSalida = 0; tecla = 9; |FINSI;
|FINSI;

|LEE 140#0=;
|SI tecla = 9;  #0#2 = "S"; |FINSI;
|SI tecla = 10; #0#2 = "N"; |FINSI;
|GRABA 140#0a;
|LIBERA #0;

|PTEC 809;
|FPRC;

|PROCESO linea; |TIPO 2;
cont = (FEntrada / 100) ! 0;
|SI cont = 3;
    |MENAV "    No se puede dar de baja";
    |ERROR;
|FINSI;
|FPRC;

|PROCESO cuadro1; |TIPO 7;
|SI #0#0 = 9; |O #0#0 = 10; |O #0#0 = 11; |O #0#0 = 12; |O #0#0 = 13;
    |PUSHV 0501 2380;
    |CUADRO 1229 1671;
    |ATRI R;
    |PINTA 1330, "  Si los ficheros de actividades se van  ";
    |PINTA 1430, "a obtener mediante la Captura de R.E.O.  ";
    |PINTA 1530, "introducir en PASAR S/N una <N>          ";
    |ATRI 0;
    |PAUSA;
    |POPV;
|FINSI;
|FPRC;
