|FICHEROS;
    retotpat #06;
    zetotpat #07;

    retotrea #08;
    zetotrea #09;

    retotren #10;
    zetotren #11;

    reliquid #12;
    zeliquid #13;

    repagret #14;
    zepagret #15;

    reempagr #16;
    zeempagr #17;
|FIN;

|VARIABLES;
    campos_1 =  47;  || nro. de campos de cada uno
    campos_2 = 250;
    campos_3 = 228;
    campos_4 =  43;
    campos_5 =  9;
    campos_6 =  50;
    puntero  = 0;
|FIN;

|INCLUYE i_expor1;
|INCLUYE i_expor2;

|PROGRAMA;
|HAZ imp_1;
|HAZ imp_2;
|HAZ imp_3;
|HAZ imp_4;
|HAZ imp_6;
|HAZ imp_7;
|CORRE "reexpor1.run";
|FPRO;

|PROCESO imp_6;
    nume1 = 51;
|SI #1nume1 = 1;
    |ABRE #15;
    |PATH_DAT #14 path;
        error = -1;
    |PARA; |SI error ! 0; |HACIENDO;
        |ABRE #14;
            error = FSalida;
    |SIGUE;
        #15#0 = codi;
        #15#1 = 1;             || bucle de lectura de lineas
    |LEE 001#15=;
    |SI FSalida ! 0;  |LEE 001#15];  |FINSI;
    |PARA; |SI FSalida = 0;|Y #15#0 = codi; |HACIENDO;
            #14#0 = #15#0;
            #14#1 = #15#1;
        |LEE 001#14=;
        |SI FSalida = 0;   |BORRA 020#14a;   |FINSI;
        |PARA nume1 = 0; |SI nume1 [ campos_1; |HACIENDO nume1 = nume1 + 1;
                #14nume1 = #15nume1;
        |SIGUE;
        |GRABA 020#14n;
        |BORRA 020#15a;
        |LEE 001#15s;
    |SIGUE;
    |CIERRA #14;
    |CIERRA #15;
|FINSI;
|FPRC;

|PROCESO imp_7;
    nume1 = 52;
|SI #1nume1 = 1;
    |ABRE #17;
    |PATH_DAT #16 path;
        error = -1;
    |PARA; |SI error ! 0; |HACIENDO;
        |ABRE #16;
            error = FSalida;
    |SIGUE;
        #17#0 = codi;
        #17#1 = 1;             || bucle de lectura de lineas
    |LEE 001#17=;
    |SI FSalida ! 0;  |LEE 001#17];  |FINSI;
    |PARA; |SI FSalida = 0;|Y #17#0 = codi; |HACIENDO;
            #16#0 = #17#0;
            #16#1 = #17#1;
        |LEE 001#16=;
        |SI FSalida = 0;   |BORRA 020#16a;   |FINSI;
        |PARA nume1 = 0; |SI nume1 [ campos_1; |HACIENDO nume1 = nume1 + 1;
                #16nume1 = #17nume1;
        |SIGUE;
        |GRABA 020#16n;
        |BORRA 020#17a;
        |LEE 001#17s;
    |SIGUE;
    |CIERRA #16;
    |CIERRA #17;
|FINSI;
|FPRC;
