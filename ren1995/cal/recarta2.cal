|FICHEROS;
   retmpcar #0;
   m2proedi #1;
   m2profco #2;
|FIN;

|VARIABLES;
    &swdef = 0;
    &empresa   = 0;
    &fichero = "";
    &lin_tmp = 0;
    &path = "";
    relleno = "                    ";
|FIN;

|PROCESO graba_tmp;
         |DDEFECTO #0;
         lin_tmp = lin_tmp + 1;
         #0#0 = lin_tmp;
         |SI swdef = 7; #0#1 = #1#15;  |FINSI;
         |SI swdef = 8; #0#1 = #2#15;  |FINSI;
         #0#1 = relleno + #0#1;
         |GRABA 020#0n;
|FPRC;

|DEFBUCLE reprofco;
          #2#1;
          ;
          empresa, 01;
          empresa, 99;
          e;
          NULL,graba_tmp;
|FIN;

|DEFBUCLE reproedi;
          #1#1;
          ;
          empresa, 01;
          empresa, 99;
          e;
          NULL,graba_tmp;
|FIN;

|PROGRAMA;
     |PATH_DAT #1 path;
     |PATH_DAT #2 path;
     |NOME_DAT #1 "reproedi";
     |NOME_DAT #2 "reprofco";
     |NOME_DAT #0 fichero;
     |ABRE #0;
     |SI swdef = 7; |BUCLE reproedi; |FINSI;
     |SI swdef = 8; |BUCLE reprofco; |FINSI;
     |CIERRA #0;
|FPRO;
