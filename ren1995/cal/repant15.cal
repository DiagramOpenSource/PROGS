|SI #0Campo = Contenido; |ACABA; |FINSI;
  |SI #0#11 > #0#12;
      |SI #0#19 > #0#17; #0#19 = #0#17; |FINSI;
      #0#20 = #0#17 - #0#19;
      |SI #0#21 > #0#20; |O modelito = 1;
          #0#21 = #0#20;
          |PINTA #0#21;
      |FINSI;
      #0#14 = 0;
      #0#16 = 0;
      #0#18 = 0;
      #0#22 = 0;
  |FINSI;

  |SI #0#11 [ #0#12;
      |SI #0#22 > #0#18; |O modelito = 1;
          #0#22 = #0#18;
          |PINTA #0#22;
      |FINSI;
      #0#13 = 0;
      #0#15 = 0;
      #0#17 = 0;
      #0#19 = 0;
      #0#20 = 0;
      #0#21 = 0;
  |FINSI;
  |PARA x = 13; |SI x [ 22; |HACIENDO x = x + 1;
      |PINTA #0x;
  |SIGUE;
|FPRC;

|PROCESO increme;
  #0#13 = #0#11 - #0#12;
  coef = 0;
  anyo = #0#15;
  |SI anyo ] 3;
      |SI #0#23 = "I"; coef = (anyo - 2) * 5.26;  |FINSI;
      |SI #0#23 = "A"; coef = (anyo - 2) * 11.11; |FINSI;
      |SI #0#23 = "R"; coef = (anyo - 2) * 7.14;  |FINSI;
  |FINSI;
  |SI coef > 99; coef = 100; |FINSI;
  |SI coef [ 100;
      #0#17 = #0#13 % (100 - coef);
      resul = #0#17;
  |SINO;
      #0#17 = 0;
      #0#19 = 0;
      #0#20 = 0;
      #0#21 = 0;
      |CAMPO_MODIFICA #0#19, 1, "N";
      |CAMPO_MODIFICA #0#21, 1, "N";
  |FINSI;
|FPRC;

|PROCESO decreme;
  #0#14 = #0#12 - #0#11;
  coef = 0;
  anyo = #0#16;
||    anyo = (anyo + 0.49) ! 0;
  |SI anyo ] 3;
      |SI #0#23 = "I"; coef = (anyo - 2) * 5.26;  |FINSI;
      |SI #0#23 = "A"; coef = (anyo - 2) * 11.11; |FINSI;
      |SI #0#23 = "R"; coef = (anyo - 2) * 7.14;  |FINSI;
  |FINSI;
