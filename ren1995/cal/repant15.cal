|PROCESO variacion; |TIPO 0;
  |SI resul ! #0#17;
      coef = 100 - ((#0#17 * 100) / #0#13);

      |SI #0#23 = "I"; anyo = (coef /  5.26) + 2; |FINSI;
      |SI #0#23 = "A"; anyo = (coef / 11.11) + 2; |FINSI;
      |SI #0#23 = "R"; anyo = (coef / 7.14)  + 2; |FINSI;

      #0#15 = anyo;
      |PINTA #0#15;
  |FINSI;
|FPRC;

|PROCESO variacio1; |TIPO 0;
  |SI resul1 ! #0#18;
      coef = 100 - ((#0#18 * 100) / #0#14);

      |SI #0#23 = "I"; anyo = (coef /  5.26) + 2; |FINSI;
      |SI #0#23 = "A"; anyo = (coef / 11.11) + 2; |FINSI;
      |SI #0#23 = "R"; anyo = (coef / 7.14)  + 2; |FINSI;

      #0#16 = anyo;
      |PINTA #0#16;
  |FINSI;
