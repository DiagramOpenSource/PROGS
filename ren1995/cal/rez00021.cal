|PROCESO famili;   || Solo para el ejercicio 95 en el 96 quitar.
#3#202 = 0;
#3#203 = 0;
#3#204 = 0;
#3#205 = 0;
#3#206 = 0;
#3#207 = 0;
#3#181 = 0;
#3#182 = 0;

|PARA x = 1; |SI x [ 10; |HACIENDO x = x + 1;
      esde = 153 + x;
      naci = 67 + x;
      tpar = 163 + x;
      minu = 87 + x;
      |SI #3naci ! "    ";
         anyonum  = #3naci;
         anyofam  = 1995 - #3naci;
         |SI #3esde = "A";  coefi1 = 0.5;  coefi2 = 0.5;  |FINSI;
         |SI #3esde = "T";  coefi1 = 1;    coefi2 = 0;    |FINSI;
         |SI #3esde = "C";  coefi1 = 0;    coefi2 = 1;    |FINSI;
         |SI #3tpar = "H"; |O #3tpar = "D";      || Si son Descendiente o Hijos.
             |SI #3esde = "A";
                 #3#181 = #3#181 + 1;
                 #3#182 = #3#182 + 1;
             |FINSI;
             |SI #3esde = "T";
                 #3#181 = #3#181 + 1;
             |FINSI;
             |SI #3esde = "C";
                 #3#182 = #3#182 + 1;
             |FINSI;
             |SI anyofam < 30;                   || Si son Menores de 30 a¤os.
                 |SI #3#181 [ 2; #3#202 = #3#202 + (1 * coefi1); |FINSI;
                 |SI #3#182 [ 2; #3#203 = #3#203 + (1 * coefi2); |FINSI;
                 |SI #3#181 = 3; #3#204 = #3#204 + (1 * coefi1); |FINSI;
                 |SI #3#182 = 3; #3#205 = #3#205 + (1 * coefi2); |FINSI;
                 |SI #3#181 ] 4; #3#206 = #3#206 + (1 * coefi1); |FINSI;
                 |SI #3#182 ] 4; #3#207 = #3#207 + (1 * coefi2); |FINSI;
             |FINSI;

             |SI anyofam > 30; |Y #3minu = "S";  || Si son Mayores a 30 y Minus.
                 |SI #3#181 [ 2; #3#202 = #3#202 + (1 * coefi1); |FINSI;
                 |SI #3#182 [ 2; #3#203 = #3#203 + (1 * coefi2); |FINSI;
                 |SI #3#181 = 3; #3#204 = #3#204 + (1 * coefi1); |FINSI;
                 |SI #3#182 = 3; #3#205 = #3#205 + (1 * coefi2); |FINSI;
                 |SI #3#181 ] 4; #3#206 = #3#206 + (1 * coefi1); |FINSI;
                 |SI #3#182 ] 4; #3#207 = #3#207 + (1 * coefi2); |FINSI;
             |FINSI;
         |FINSI;
      |FINSI;
|SIGUE;

#3#181 = #3#202 + #3#204 + #3#206;
#3#182 = #3#203 + #3#205 + #3#207;
