|FICHEROS;
    rein210a #0;
    recapinm #1;
    redatcon #2;
    rerenpre #4;
    rehacien #5;
|FIN;

|VARIABLES;
    informe  = "";
    x        = 0;
    hay      = 0;
    conta    = 0;
    empresa  = 0;
    sw_print = 0;

    &nombre  = "";
    &nif     = "";
    &retri   = 0;
    &tretri  = 0;
    &tcuota  = 0;
    &impor   = 0;
    &diap    = "";
    &mesp    = "";
    &mespl   = "";
    &anyop   = "";
|FIN;

|PROCESO hoja;
|INFOR informe;
|SI FSalida = 0;
    |PRINT; |PIEINF; |FININF;
|FINSI;
|FPRC;

|PROCESO capital;
|SI hay = 0;
    hay = 1;
|FINSI;

|SI #1#2 = "G";
    nombre = #2#1;
    nif    = #2#2;
    retri  = #1#47 % 50;
    tcuota = retri % 25;
    conta = conta + 1;
    |SI conta > 6; |ACABA; |FINSI;

    |SI sw_print = 1; |PRINT; |FINSI;
    tretri = tretri + retri;

    nombre = #2#28;
    nif    = #2#29;
    retri  = #1#47 % 50;
    tcuota = retri % 25;
    conta = conta + 1;
    |SI conta > 6; |ACABA; |FINSI;

    |SI sw_print = 1; |PRINT; |FINSI;
    tretri = tretri + retri;
|SINO;
    nombre = #2#1;
    nif    = #2#2;
    retri  = #1#47;
    tcuota = retri % 25;
    conta = conta + 1;
    |SI conta > 6; |ACABA; |FINSI;

    |SI sw_print = 1; |PRINT; |FINSI;
    tretri = tretri + retri;
|FINSI;
impor = tretri % 25;
|FPRC;

|DEFBUCLE recapinm;
#1#1;
;
empresa, 00;
empresa, 99;
e;
NULL, capital;
|FIN;

|PROCESO impresio;
|PARA x = 0; |SI x [ 17; |HACIENDO x = x + 1;
    |SI #0x ! 0;
        #2#0 = #0x;
        |LEE 040#2=;
        |SI FSalida ! 0; |ACABA; |FINSI;
        |SI #2#4 = "S";  |ACABA; |FINSI;

        #5#0 = #2#40;
        #5#1 = #2#41;
        |LEE 040#5=;
        |SI FSalida ! 0; |DDEFECTO #5; |FINSI;

        empresa = #0x;

        sw_print = 0;
        hay = 0; |BUCLE recapinm;
        |SI hay = 0; |ACABA; |FINSI;

        #4#0 = #2#44;
        |LEE 040#4=;
        |SI FSalida ! 0; |DDEFECTO #4; |FINSI;

        informe = "rei210a7"; |HAZ hoja;

        informe = "rei210a9";
        |INFOR informe;
        |SI FSalida = 0;
            sw_print = 1; hay = 1; conta = 0; tretri = 0;
            |BUCLE recapinm;
            |PIEINF;
            |FININF;
        |FINSI;

        |SI #0#20 = 1;
            informe = "rehoblan"; |HAZ hoja;
            informe = "rehoblan"; |HAZ hoja;
            informe = "rehoblan"; |HAZ hoja;
            informe = "rehoblan"; |HAZ hoja;
            informe = "rehoblan"; |HAZ hoja;
        |FINSI;
        informe = "rei210a8"; |HAZ hoja;
    |FINSI;
|SIGUE;
|FPRC;

|PROCESO impre; |TIPO 3;
|HAZ fechapre;

|IMPRE 0;
|SI FSalida = 0;
   |ABRE #2;
   |ABRE #4;
   |ABRE #5;

   |HAZ impresio;

   |CIERRA #2;
   |CIERRA #4;
   |CIERRA #5;
   |FINIMP;
|SINO;
   |MENAV "      Proceso Interrumpido";
|FINSI;
|FPRC;

|PROCESO fechapre;
diap  = #0#18 % 102;
anyop = #0#18 % -102;
mesp  = #0#18 % 402;
|SI mesp = "01"; mespl = "Enero     "; |FINSI;
|SI mesp = "02"; mespl = "Febrero   "; |FINSI;
|SI mesp = "03"; mespl = "Marzo     "; |FINSI;
|SI mesp = "04"; mespl = "Abril     "; |FINSI;
|SI mesp = "05"; mespl = "Mayo      "; |FINSI;
|SI mesp = "06"; mespl = "Junio     "; |FINSI;
|SI mesp = "07"; mespl = "Julio     "; |FINSI;
|SI mesp = "08"; mespl = "Agosto    "; |FINSI;
|SI mesp = "09"; mespl = "Septiembre"; |FINSI;
|SI mesp = "10"; mespl = "Octubre   "; |FINSI;
|SI mesp = "11"; mespl = "Noviembre "; |FINSI;
|SI mesp = "12"; mespl = "Diciembre "; |FINSI;
|FPRC;
