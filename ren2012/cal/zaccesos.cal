     |ABRE #3;
     |SI nRegraba = 0;  |BUCLE dsam0004;  |FINSI;
     |CIERRA #3;
|FPRC;

|PROCESO MiraIntegrada;
     |ABRE #715;
     #715#0 = #0#0;

     |LIBERA #715;
     |LEE 000#715=;
     |SI FSalida ! 0;
         |MENAV "     No existe datos personales en el cliente ";
         |CONFI "0000SDesea introducirlos ";
         |SI FSalida = 0;
             aLanza = "*:ren2012/zacceso9;" + (("00000" + #0#0) % -105) + "ALTACLIENTE";
             |SUB_EJECUTA aLanza;

             #715#0 = #0#0;
             |LIBERA #715;
             |LEE 040#715=;
             |SI FSalida ! 0;
                 |MENAV "     No existe datos personales en el cliente ";
                 |CIERRA #715;
             |FINSI;
         |SINO;
             |CIERRA #715;
         |FINSI;
     |FINSI;
     |CIERRA #715;

     |ABRE #791;
     #791#0 = #715#0;
     |LEE 101#791=;
     |SI FSalida = 0;
         #791#68 = "S";
         |GRABA 020#791a;
         |LIBERA #791;
     |FINSI;
     |CIERRA #791;

     nCompara = 1;
     nRegraba = 0;
     |ABRE #2;
     #2#0 = #0#0;
     |LEE 101#2=;
     |SI FSalida ! 0;
         |DDEFECTO #2;
         nCompara = 0;
         nRegraba = 0;

         #2#0     = #0#0;
         #2#1     = #0#2;
         #2#5     = #0#1;

         |HAZ Separa;

         |GRABA 020#2b;

         |HAZ CargaCampos;

         nRegraba = 0;
     |SINO;
         |HAZ CargaCampos;
     |FINSI;

