         |CONFI "0000NDatos personales por actualizar, desea actualizarlos : ";
         |SI FSalida = 0;
             nCompara = 0;
             nRegraba = 0;
             |HAZ CargaCampos;

             |GRABA 020#2a;

             #0#1   = #715#5;
             #0#2   = #715#1;
             #0#21  = #2#35;
             #0#22  = #2#39;
         |FINSI;
     |SINO;
         |GRABA 020#2a;

         #0#1   = #715#5;
         #0#2   = #715#1;
         #0#21  = #2#35;
         #0#22  = #2#39;
     |FINSI;

     |LIBERA #2;

