     |SI FSalida = 0;
         |LEE 000#20a;
     |SINO;
         |LEE 000#20u;
     |FINSI;

     |DDEFECTO #641;
     #641#0 = enEmpresa;
     #641#1 = 1;
     |SI FSalida = 0;  |Y  #20#0 = enEmpresa;
         #641#1 = #20#1 + 1;
