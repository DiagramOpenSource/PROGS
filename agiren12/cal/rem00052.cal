|PROCESO Tipo12Fw652;  |TIPO 12;
     |SI FSalida = 7;  |ACABA;  |FINSI;

     aAlfa = #652#2;  |QBF aAlfa;
     |SI aAlfa = "";
         |MENAV "0000No se puede validar la ficha, no hay contribuyente introducido";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Tipo0C1Fw652;  |TIPO 0;
     |C_M #652#1, 0, aAlfa;
     |SI aAlfa = "N";  |ACABA;  |FINSI;

     #20#0 = #652#0;
     #20#1 = #652#1;
     |LEE 000#20=;
     |SI FSalida = 0;
         |MENAV "0000El registro ya existe.";
         |ERROR;
         |ACABA;
     |FINSI;
|FPRC;

|PROCESO Totalm020;
     aAlfa1      = #652#17 % -104;
     aAlfa2      = #652#17 % 0105;
     nAnyoCompra = aAlfa1;
     |SI aAlfa2 = "31.12";  nAnyoCompra = nAnyoCompra + 1;  |FINSI;

     #652#21 = #652#19 - #652#20;

     |C_M #652#22, 1, "S";
     |C_M #652#24, 1, "S";
     |SI #652#17 < "31.12.1996";
         |SI #652#21 < 0;
             #652#22 = -#652#21;
             #652#23 = 0;
             #652#24 = 0;
             |C_M #652#24, 1, "N";
         |SINO;
             #652#23 = (1996 - nAnyoCompra) + 1;
             #652#24 = #652#21;
             #652#22 = 0;
             |C_M #652#22, 1, "N";
         |FINSI;
     |SINO;
         |SI #652#21 < 0;
             #652#22 = -#652#21;
             #652#23 = 0;
             #652#24 = 0;
         |SINO;
             #652#24 = #652#21;
             #652#22 = 0;
             #652#23 = 0;
         |FINSI;
     |FINSI;

     |PINTA #652#21;
     |PINTA #652#22;
     |PINTA #652#23;
     |PINTA #652#24;
|FPRC;

|PROCESO Usufructuario;
     |C_M #652#14, 1, "S";
     |SI #652#13 ! "N";
         |C_M #652#14, 1, "N";  #652#14 = 0;  |PINTA #652#14;
     |FINSI;
|FPRC;

|PROCESO CargaFantasma;
     |C_M #652#11, 1, "S";
     |C_M #652#13, 1, "S";
     |C_M #652#14, 1, "S";
     |C_M #652#15, 1, "S";
     |C_M #652#16, 1, "S";
     |C_M #652#17, 1, "S";
     |C_M #652#18, 1, "S";
     |C_M #652#19, 1, "S";
     |C_M #652#20, 1, "S";
     |C_M #652#22, 1, "S";
     |C_M #652#24, 1, "S";

     |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
           #652nCampo = #20nCampo;

           |C_V #652nCampo, 0, aAlfa;
           |SI aAlfa = "S";  |PINTA #652nCampo;  |FINSI;
     |SIGUE;

     eaDeDonde  = "C4";
     eaLinDonde = #20#1;
     eaLetra    = #20#11;
