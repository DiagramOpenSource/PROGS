|PROCESO BotonCalc;
     |SI enModoReg = 4;  |ACABA;  |FINSI;

     |DDEFECTO #rew00005;

     |SI FSalida = nBotonCalc1; nCampo = 15;  |FINSI;

     #rew00005#1  = #652nCampo;
     #rew00005#20 = #652nCampo;

     |HAZ PantaREW00005;

     #652nCampo = #rew00005#20;  |PINTA #652nCampo;

     aAlfa = "|C" + (("000" + nCampo) % -103) + "WID";
