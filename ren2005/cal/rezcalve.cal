|PROCESO rem00101;
     aAlfa = #101#18 % -104;
     |SI #101#5 = "2005";  |O aAlfa = "2005";
         nHijos = 1;
         |ERROR10;
     |FINSI;
|FPRC;

|DEFBUCLE rem00101;
     #101#1;
     4;
     #100#0, 01, "DESCENDIENTE";
     #100#0, 99, "DESCENDIENTE";
     ;
     NULL, rem00101;
|FIN;

|PROCESO rem00044;
     nHijos = 1;
     |ERROR10;
|FPRC;

|DEFBUCLE rem00044;
     #44#1;
     11, 13;
     #100#0, 01, 17, 882;
     #100#0, 99, 17, 882;
     ;
     NULL, rem00044;
|FIN;

|PROCESO rem00100;
     #10#0 = #100#0;
     |LEE 000#10=;
     |SI FSalida ! 0;  |DDEFECTO #10;  |FINSI;

     |SI #10#12 ! "*";  |ACABA;  |FINSI;

     nHijos = 0;
     |BUCLE rem00101;
     |SI nHijos = 0;  |ACABA;  |FINSI;

     nHijos = 0;
     |BUCLE rem00044;
     |SI nHijos = 1;  |ACABA;  |FINSI;

     #1001#0 = #100#0;
     #1001#2 = "";
     #1001#5 = "100";
     #1001#7 = #10#1;
     #1002#4 = "882";
     #1002#5 = "Revisar deduccion autonomica Valencia casilla 882.";
     #1002#6 = 0;
     enAnt   = 0;
     |PRINT;
     nSwPrint   = 1;
|FPRC;

|DEFBUCLE rem00100;
     #100#1;
     11;
     00001, 17;
     99999, 17;
     ;
     NULL, rem00100;
|FIN;

|PROCESO RevisaAutonomicas;
     |ABRE #10;
     |ABRE #44;
     |BUCLE rem00100;
     |CIERRA #10;
     |CIERRA #44;
|FPRC;

|PROCESO Repasa;
     |SI enEmpresa = #1001#0;  |ACABA;  |FINSI;

     eaModoRenta = "";
     |SI #1001#2 = "CONJUNTA  ";
         eaModoRenta = "C";
     |FINSI;

     enEmpresa = #1001#0;
     |SUB_EJECUTA_NP "rezcalcu";
|FPRC;

|DEFBUCLE rem01001P;
     #1001#1;
     5;
     00001, 00, "          ", "100";
     99999, 99, "zzzzzzzzzz", "714";
     ;
     NULL, Repasa;
|FIN;

|PROCESO BuscaIncidencias;
     #2#0 = #4#0;
     #2#1 = #4#1;
     #2#2 = #4#2;
     #2#3 = #4#3;
     #2#4 = #4#4;
     |LEE 040#2=;
     |SI FSalida ! 0;   |DDEFECTO #2;  |FINSI;

     |SI #4#4 = "761";  |ACABA;  |FINSI;
     |SI #4#4 = "762";  |ACABA;  |FINSI;
     |SI #4#4 = "765";  |ACABA;  |FINSI;
     |SI #4#4 = "a  ";  |ACABA;  |FINSI;
     |SI #4#4 = "b  ";  |ACABA;  |FINSI;
     |SI #4#4 = "768";  |ACABA;  |FINSI;
     |SI #4#4 = "769";  |ACABA;  |FINSI;
     |SI #4#4 = "770";  |ACABA;  |FINSI;

     |SI #4#4 = "TM1";  |O #4#4 = "TM2";
         |SI #3#5 ! "714";
             |ACABA;
         |FINSI;
     |FINSI;

     nImporte1 = #2#6 + #2#9;
     nImporte2 = #4#6 + #4#9;
     |SI nImporte1 ! nImporte2;
         #1001#0 = #3#0;
         #1001#1 = #3#1;
         #1001#2 = #3#2;
         #1001#5 = #3#5;
         #1001#7 = #3#7;
         #1002#4 = #4#4;
         #1002#5 = #4#5;
         #1002#6 = #4#6;
         enAnt   = #2#6;
         |PRINT;
         nSwPrint = 1;
     |FINSI;
|FPRC;

