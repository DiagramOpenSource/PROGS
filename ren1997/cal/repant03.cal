|PROCESO retenci; |TIPO 0;
  |ABRE #1;
  #1#0 = #0#10;
  |SI  #0#10 = "    ";
       rete = 25;
       |CIERRA #1;
       |ACABA;
  |FINSI;
  |LEE 040#1=;
  |SI FSalida ! 0;
      rete = 25;
  |SINO;
      ||valor = #1#15;
      ||SI #1#16 > #1#15; valor = #1#16; |FINSI;
      ||#0#31 = valor * #0#13; |PINTA #0#31;

      #0#16 = #1#5;  || Cotizacion Ultimo Trimestre

      #0#31 = #0#16 * #0#13; |PINTA #0#31;

      #0#17 = 0;
      Ptas  = 0;

      |SI #0#15 [ #1#11; Ptas = #1#7;  |FINSI;
      |SI #0#15 [ #1#12; Ptas = Ptas + #1#8;  |FINSI;
      |SI #0#15 [ #1#13; Ptas = Ptas + #1#9;  |FINSI;
      |SI #0#15 [ #1#14; Ptas = Ptas + #1#10; |FINSI;

      #0#17 = Ptas * #0#13;
      |SI #1#4 = 0;
          rete = 25;
      |SINO;
          rete = #1#4;
      |FINSI;
      #0#18 = #0#17 % rete;
      |PINTA #0#16;
      |PINTA #0#17;
      |PINTA #0#18;
  |FINSI;
  |CIERRA #1;
|FPRC;

