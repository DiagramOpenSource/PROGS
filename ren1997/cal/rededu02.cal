|PROCESO Actualiza; |TIPO 2;
  pantalla   = 0 +. 1;
  QueTipo    = (FEntrada / 100) ! 0;
  HContribu  = #0#0;
  HNombre    = nombre;
  HPrograma  = "(rededu02) DEDUCCIONES EJERCICIO A¥O ACTUAL / INCENTIVOS FISCALES";
  HLinea     = #0#1;
  HPerceptor = #0#3;
  HRdto      = #0#9;

