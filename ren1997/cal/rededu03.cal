|FICHEROS;
   rededu03 #0;
   reparame #1;
|FIN;

|VARIABLES;
   pepe       = 0;
   modelito   = 0;
   pantalla   = 0;

   &empresa   = 0;
   &nombre    = "";
   &modo      = 0;
   &indivi    = "";
|FIN;

|INCLUYE i_husuar;
|INCLUYE i_activo;

|RUTINA inferior;
  #0#0 = empresa;
  #0#1 = 1;
|FRUT;

|RUTINA superior;
  #0#0 = empresa;
  #0#1 = 99;
|FRUT;

|PROGRAMA;
|PINPA #0#0;
|SI modo = 1;  |ENTLINEAL #1#0, 2, 1, 13, 0, inferior, superior;  |FINSI;
|SI modo = 2;  |ENTLINEAL #1#0, 2, 2, 13, 0, inferior, superior;  |FINSI;
|SI modo = 4;  |ENTLINEAL #1#0, 2, 4, 13, 0, inferior, superior;  |FINSI;
|FPRO;

|PROCESO Actualiza; |TIPO 2;
pantalla   = 0 +. 1;
QueTipo    = (FEntrada / 100) ! 0;
HContribu  = #0#0;
HNombre    = nombre;
HPrograma  = "(rededu03) INVERSIONES EN CANARIAS ";
HLinea     = #0#1;
HPerceptor = #0#3;
HRdto      = #0#9;

|SI QueTipo = 1; HTipo = "ALTA FICHA";         |FINSI;
|SI QueTipo = 2; HTipo = "MODIFICACION FICHA"; |FINSI;
|SI QueTipo = 3; HTipo = "BAJA FICHA";         |FINSI;

|SI QueTipo = 3; |Y pantalla ! 1; |HAZ AltaHistorico;  |FINSI;
|SI QueTipo < 3; |Y pantalla = 1; |HAZ AltaHistorico;  |FINSI;
|FPRC;

|PROCESO parametro; |TIPO 0;
modelito = (FEntrada / 100) ! 0;
|SI modelito = 1;
   |ABRE #1;
   |LEE 040#1=;
   |SI FSalida = 0;
      #0#2  = #1#6;
      #0#11 = #1#7;
      |PINTA #0#2;
      |PINTA #0#11;
   |FINSI;
   |CIERRA #1;
|FINSI;
|FPRC;

|PROCESO TipoIndi; |TIPO 7;
|SI indivi = "S"; |Y #0Campo ! "V";
    #0Campo = "T"; |PINTA #0Campo;
|FINSI;
|FPRC;

|PROCESO perceptor; |TIPO 0;
|CAMPO_MODIFICA #0#12, 1, "N";
|CAMPO_MODIFICA #0#13, 1, "N";
|CAMPO_MODIFICA #0#14, 1, "N";
|CAMPO_MODIFICA #0#15, 1, "N";
|CAMPO_MODIFICA #0#16, 1, "N";
|CAMPO_MODIFICA #0#17, 1, "N";

|SI indivi = "S"; |Y #0#2 ! "T";
    |MENAV "      La renta es individual";
    |ERROR; |ACABA;
|FINSI;

npro = 22; |HAZ sigrabo;
|SI #0#2 = "T"; #0#3 = "TITULAR  "; |FINSI;
|SI #0#2 = "C"; #0#3 = "CONYUGE  "; |FINSI;
|SI #0#2 = "G"; #0#3 = "GANANCIAL"; |FINSI;
|SI #0#2 = "1"; #0#3 = "FAMILIAR "; |FINSI;
|SI #0#2 = "2"; #0#3 = "FAMILIAR "; |FINSI;
|SI #0#2 = "3"; #0#3 = "FAMILIAR "; |FINSI;
|SI #0#2 = "4"; #0#3 = "FAMILIAR "; |FINSI;
|SI #0#2 = "V"; |Y #0#11 ! "X";
    #0#3 = "VARIOS   ";
    |CAMPO_MODIFICA #0#12, 1, "S";
    |CAMPO_MODIFICA #0#13, 1, "S";
    |CAMPO_MODIFICA #0#14, 1, "S";
    |CAMPO_MODIFICA #0#15, 1, "S";
    |CAMPO_MODIFICA #0#16, 1, "S";
    |CAMPO_MODIFICA #0#17, 1, "S";
|FINSI;
|PINTA #0#3;
|FPRC;

|PROCESO deducci; |TIPO 0;
  |CAMPO_MODIFICA #0#5, 1, "S";
  |CAMPO_MODIFICA #0#7, 1, "S";
  |CAMPO_MODIFICA #0#8, 1, "S";

  #0#5 = 70;
  |SI #0#4 = "MA";             #0#7 = 30;  |FINSI;
  |SI #0#4 = "EL";             #0#7 = 25;  |FINSI;
  |SI #0#4 = "PC";             #0#7 = 30;  |FINSI;
  |SI #0#4 = "BI";             #0#7 = 30;  |FINSI;
  |SI #0#4 = "AE";             #0#7 = 45;  |FINSI;
  |SI #0#4 = "AF"; #0#5 = 50;  #0#7 = 25;  |FINSI;
  |SI #0#4 = "GD";             #0#7 = 40;  |FINSI;
  |SI #0#4 = "FP";             #0#7 = 25;  |FINSI;

  |SI #0#4 = "RI"; |O #0#4 = "RE";
      |SI #0#20 ! "96";
          |MENAV "      El a¤o debe ser el actual.";
          |ERROR;
          |ACABA;
      |FINSI;

      #0#7 = 0;
      #0#5 = 0;

      |CAMPO_MODIFICA #0#5, 1, "N";
      |CAMPO_MODIFICA #0#7, 1, "N";
      |CAMPO_MODIFICA #0#8, 1, "N";
  |FINSI;

  |PINTA #0#5;
  |PINTA #0#7;
|FPRC;

|PROCESO aplicado; |TIPO 0;
  |SI #0#11 ! "X";
     #0#9 = #0#8 % #0#7;
     #0#10 = 0;
     |PINTA #0#9;
     |PINTA #0#10;
     pepe = #0#9;
  |FINSI;
|FPRC;

|PROCESO error; |TIPO 0;
  |SI #0#4 = "RI"; |O #0#4 = "RE"; |ACABA; |FINSI;

  |SI #0#11 ! "X";
      |SI pepe < #0#9;
         |MENAV "    Exceso sobre el Limite de Deduccion";
         |ERROR;
         |ACABA;
      |SINO;
         |SI pepe ! #0#9;
             #0#10 = pepe - #0#9;
             |PINTA #0#10;
         |FINSI;
     |FINSI;
  |FINSI;
|FPRC;

|PROCESO nomodi; |TIPO 0;
|SI #0#11 = "X";
    |CAMPO_MODIFICA #0#2, 1, "N";
    |CAMPO_MODIFICA #0#3, 1, "N";
    |CAMPO_MODIFICA #0#4, 1, "N";
