  #900#0  = #901#0;
  #900#1  = #901#1;
  #900#2  = #901#48;
  #900#3  = #901#2;
  #900#4  = #901#3;
  #900#5  = #901#5;
  #900#8  = #901#49;
  #900#9  = #901#50;
  #900#10 = #901#51;
  #900#11 = #901#52;
  #900#12 = #901#53;
  #900#13 = #901#54;
  |GRABA 140#900n;
|FPRC;

|DEFBUCLE recapinm;
  #901#1;
  ;
  #2#0, 00;
  #2#0, 99;
  ;
  NULL, Graba900
|FIN;

|PROCESO Inmueble;
