|PROCESO Recoge;
  |BUCLE redatcon;
|FPRC;

|PROCESO Tipo8; |TIPO 8;
  |ABRE #900;
  |LEE 040#900p;
  |SI FSalida ! 0;  |HAZ Recoge;  |FINSI;
  |CIERRA #900;


  |DBASS DirControl;
  |PATH_DAT #98 DirControl;

  |ABRE #98;
  #98#0 = 1;
  |LEE 040#98=;
  |SI FSalida ! 0; VArterisco = "*"; |CIERRA #98; |ACABA; |FINSI;
  |CIERRA #98;

  VArterisco = #98#6;   |QBF VArterisco;
  |SI VArterisco  = "*"; |ACABA; |FINSI;

  PathFich = #98#8; |QBF PathFich;
  PathPan  = (PathFich - "fich/") + "pan/";

  |PATH_DAT #5 PathFich;
  |PATH_PAN #5 PathPan;
|FPRC;

|PROCESO Separa;
  Cadena = #2#1;
  |SI Cadena ! "";
      |HAZ SeparaNombre;
      #2#209 = Cadena1;
      #2#210 = Cadena2;
      #2#211 = Cadena3;
  |FINSI;
|FPRC;

|PROCESO MiraAfigen;
  |ABRE #5;
  |ABRE #2;
  #5#0 = #0#0;
  |LIBERA #5;
  |LEE 110#5=;
  |SI FSalida = 0;                || Recoge variables del agicli
      #5#22 = "S";
      |GRABA 020#5a;                 || Graba el agicli

      |DDEFECTO #2;
      #2#0  = #0#0;
      #2#1  = #5#2;  |HAZ Separa;
      #2#2  = #5#13;
      #2#4  = "N";
      #2#6  = #5#3;
      #2#7  = #5#4;
      #2#8  = #5#5;
      #2#9  = #5#6;
      #2#10 = #5#7;
      #2#11 = #5#8;
      #2#12 = #5#9;
      #2#13 = #5#10;
      #2#14 = #5#11;
      #2#15 = #5#14;
      #2#16 = #5#16;
      #2#40 = #5#63;
      #2#41 = #5#64;
      |GRABA 040#2n;

      #0#1  = #5#2;     |PINTA #0#1;
      #0#2  = #5#13;    |PINTA #0#2;
  |SINO;
      |CONFI "2400S Confirma el alta en Clientes General : ";
      |SI FSalida = 0;
          |PUSHV 0501 2380;
          |PINPA #0#5;
          |DDEFECTO #5;
          |PINDA #0#5;
          #5#0 = #0#0; |PINTA #5#0;
          |ENDATOS #1#5;
          |SI FSalida = 0;
              #5#22 = "S";
              |GRABA 121#5n;                 || Graba el agicli

              |DDEFECTO #2;
              #2#0  = #0#0;
              #2#1  = #5#2;    |HAZ Separa;
              #2#2  = #5#13;
              #2#6  = #5#3;
              #2#7  = #5#4;
              #2#8  = #5#5;
              #2#9  = #5#6;
              #2#10 = #5#7;
              #2#11 = #5#8;
              #2#12 = #5#9;
              #2#13 = #5#10;
              #2#16 = #5#16;
              #2#15 = #5#14;
              |GRABA 040#2n;

              #0#1  = #5#2;     |PINTA #0#1;
              #0#2  = #5#13;    |PINTA #0#2;
          |FINSI;
          |POPV;
      |FINSI;
  |FINSI;
  |CIERRA #2;
  |CIERRA #5;
|FPRC;

|PROCESO AltaCliente;
  |SI VArterisco  = "";  |HAZ Tipo8;  |FINSI;
  |SI VArterisco  = "*"; |ACABA;      |FINSI;

   |HAZ MiraAfigen;
|FPRC;

|PROCESO NoCalcu; |TIPO 0;
  modo = (FEntrada / 100) ! 0;

  |ABRE #2;
  #2#0 = #0#0;
  |LEE 040#2=;
  |SI FSalida ! 0;  |DDEFECTO #2;  #2#4 = "N";  |FINSI;
  |CIERRA #2;

  |SI #2#4 = "S";
      |MENAV "       El Contribuyente es Nacional";
      |ERROR;
      |ACABA;
  |FINSI;

  |SI modo ! 1;  |ACABA;  |FINSI;

  |HAZ AltaCliente;
|FPRC;

|PROCESO Contador; |TIPO 7;
  modo = (FEntrada / 100) ! 0;
  |SI modo ! 1;  |ACABA;  |FINSI;

  |ABRE #1;
  |LEE 101#1p;
  |SI FSalida ! 0; |DDEFECTO #1; #1#9 = 1; |GRABA 020#1b; |FINSI;
  |LIBERA #1;
  |CIERRA #1;
  |SI #1#8 = "A"; #0#0 = #1#9; |PINTA #0#0; |FINSI;
|FPRC;

|PROCESO ModiCliente;
  |SI VArterisco  = "*"; |ACABA;     |FINSI;
  |SI VArterisco  = "";  |HAZ Tipo8; |FINSI;

  |ABRE #5;
  #5#0 = #0#0;
  |LEE 040#5=;
  |SI FSalida = 0;                || Recoge variables del agicli
      |SI #0#2 ! #5#13;
          |MENAV "     D.N.I. del Cliente es distinto al D.N.I. del Cliente General";
      |FINSI;
  |SINO;
      |MENAV "      No existe Cliente General";
  |FINSI;
  |CIERRA #5;
|FPRC;

|PROCESO Tipo30;  |TIPO 30;
  modo = (FEntrada / 100) ! 0;
  |SI modo = 2; |HAZ ModiCliente; |FINSI;
  |SI modo = 1; |O modo = 2; |HAZ Menus; |FINSI;
  |SI modo = 1;
      |ABRE #1;
      |LEE 101#1p;
      |SI #1#8 = "A"; #1#9 = #0#0 + 1; |GRABA 020#1a; |FINSI;
      |LIBERA #1;
      |CIERRA #1;
  |FINSI;
|FPRC;

|PROCESO Tipo13; |TIPO 13;
  |ABRE #2;
  #2#0 = #0#0;
  |LEE 040#2=;
  |SI FSalida ! 0;  |DDEFECTO #2;  |FINSI;
  |CIERRA #2;

  |SI #2#4 = "S"; |Y #2#0 ! 1;
      |PTEC 807;
      |ACABA;
  |FINSI;

  modo = FEntrada / 100;
  modo = modo ! 0;
  |SI modo = 4;
      |PINTA #0#0;
      |PINTA #0#1;
      |PINTA #0#2;
      |HAZ Menus;
  |FINSI;
|FPRC;

|RUTINA ClaveBaja900;
  #900#0 = #0#0;
  #900#1 = 1;
|FRUT;

|RUTINA ClaveAlta900;
  #900#0 = #0#0;
  #900#1 = 99;
|FRUT;

|PROCESO LineasInmuebles;
  |PUSHV 0501 2380;
  |ENTLINEAL #1#900, 2, 2, 22, 1, ClaveBaja900, ClaveAlta900;
  |POPV;
|FPRC;

|PROCESO Menus;
  MGeneral2  = 1;
  empresa    = #0#0;
  nombre     = #0#1;
  nif        = #0#2;
  Salir      = 0;
  |PARA;  |SI Salir = 0;  |HACIENDO;
      Salir = 1;
      |MENUG MGeneral;
      MGeneral2 = FSalida;
      |SI MGeneral2 ] 1; |Y MGeneral2 [ 3;
          |SI MGeneral2 = 1; Salir = 0;  |SUB_EJECUTA "redatcon"; |FINSI;
          |SI MGeneral2 = 2; Salir = 0;  |HAZ LineasInmuebles;    |FINSI;
          |SI MGeneral2 = 3; Salir = 0;  |SUB_EJECUTA "rehusuar"; |FINSI;
      |FINSI;
  |SIGUE;

  |PINPA #0#0;
|FPRC;

