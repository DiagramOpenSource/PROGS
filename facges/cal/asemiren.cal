|FICHEROS;
    asemirem #0;
    ascabrem #1;
    aslinrem #2;
    asempres #3;
    asbancos #4;
|FIN;

|VARIABLES;
    &suma = 0;
    &trec = 0;
|FIN;

|PROCESO imprerec;
    trec = trec + 1;
|FPRC;

|PROCESO imprelin;
    suma = suma + #2#7;
    |PRINT;
|FPRC;

|PROCESO remesa;
suma = 0;
trec = 0;
#4#0 = #1#1;
|LEE 040#4=;
|SI FSalida ! 0; #4#1 = ABANCO INEXISTENTE; |FINSI;

|BUCLELIN 0imprerec#1;
|SI trec = 0; |ACABA; |FINSI;

|BUCLELIN 0imprelin#1;
|PIEINF;
|FPRC;

|DEFBUCLE ascabrem;
#1#1;
;
#0#3;
#0#4;
e;
NULL, remesa;
|FIN;

|PROCESO impre; |TIPO 3;
|IMPRE 0;
|SI FSalida ! 0; |ACABA; |FINSI;

|INFOR "asemirem";
|SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;

|ABRE #2;
|ABRE #3;
|ABRE #4;
#3#0 = #0#1;
|LEE 040#3=;
|SI FSalida ! 0; |DDEFECTO #3; |FINSI;

|BUCLE ascabrem;

|CIERRA #2;
|CIERRA #3;
|CIERRA #4;
|FININF;
|FINIMP;
|FPRC;
