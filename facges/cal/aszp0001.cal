alfa1     = fecha_num;
alfa1     = ("0000" + alfa1) % -104;
alfa2     = (alfa1 % 102) + "." + (alfa1 % 302) + "." + anyo;
#0#17     = alfa2;

|SI #0#17 < #1#3;
    anyo      = #1#3 % -104;
    fecha_num = anyo;
    fecha_num = fecha_num + 1;
    anyo      = fecha_num;
    fecha_num = #0#3;
    |HAZ fecha_ent;
    #0#17 = alfa2;
|FINSI;

|GRABA 140#0a;
|LIBERA #0;
|FPRC;

|PROCESO fecha_ent;
fecha  = ~;
alfa1  = fecha_num;
alfa1  = ("0000" + alfa1) % -104;
alfa2  = (alfa1 % 102) + "." + (alfa1 % 302) + "." + anyo;
|FPRC;

|DEFBUCLE asexpeli;
#0#1;
;
00, 000000, 000;
99, 999999, 999;
be;
NULL, pon_bien;
|FIN;

