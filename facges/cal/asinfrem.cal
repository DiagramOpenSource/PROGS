     sw2 = 0;
     &swlin = 1;
     &sumcal = 0;
     fFech = @;
     qFech = "";
     dFech = "";
     hFech = "";
|FIN;
|CALCULO imprelin;
   sumcal = sumcal + #1#7;
   |PRINT;
|FCAL;

|CALCULO fechafac;
  swlin = 1;
|BUCLELIN 0imprelin#0;
|FCAL;

|DEFBUCLE cainfrem0;
#0#1;
3,1;
#2#0 , #2#2 , #2#4;
#2#1 , #2#3 , #2#5;
e;
NULL,fechafac;
|FIN;

|CALCULO impre;|TIPO 3;
|IMPRE 0;
|SI FSalida = 0;
    |INFOR informe;
    |SI FSalida = 0;
        |BUCLE cainfrem0;
     |SINO;
        |MENAV men1;
     |FINSI;
  |PIEINF;
  |FININF;
  |FINIMP;
|SINO;
    |MENAV men2;
|FINSI;
|FCAL;
|PROCESO sacaano; |TIPO 7;
     fFech = ~;
     qFech = fFech % -104;
     dFech = "01.01." + qFech;
     hFech = "31.12." + qFech;
     #2#2  = dFech;
     #2#3  = hFech;
     |PINTA #2#2;
     |PINTA #2#3;
|FPRC;
