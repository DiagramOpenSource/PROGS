|FICHEROS;
     ascabrem #0;                         ||CALCULO DESDE-HASTA PARA INFORMES;
     asinfrem #2;
     aslinrem #1;
|FIN;

|VARIABLES;
     informe = "asinfrem";
     men1 = "    NO EXISTE INFORME.";
     men2 = "    NO EXISTE IMPRESORA.";
     &vacio = "                                        ";
