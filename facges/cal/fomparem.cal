|FICHEROS;
    fomparem #0;
    fomparca #1;
    fomparli #2;
    facemp   #3;
|FIN;

|VARIABLES;
   &vacio = "                                        ";
   traba = "";
   trab1 = "";
   directo = "";
   dir_fich = "";
|FIN;

|PROCESO fomparli;
|PRINT;
|FPRC;

|PROCESO cabecera;
|BUCLELIN 0fomparli#1;
|PIEINF;
|FPRC;

|DEFBUCLE fomparca;
#1#3;
6, 7;
#0#0, #0#2, #0#4, #0#7, #0#9;
#0#1, #0#3, #0#5, #0#8, #0#10;
e;
NULL, cabecera;
|FIN;

|PROCESO tipo_3; |TIPO 3;
  |IMPRE 0;
  |SI FSalida ! 0; |ACABA; |FINSI;

  |INFOR "fomparem";
  |SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;
  |DBASE dir_fich;
  dir_fich = dir_fich + "fich/";
  |PATH_DAT #3 dir_fich;
  |ABRE #3;
  |DFICE directo;
  traba = directo - dir_fich;
  trab1 = traba % 102;
  #3#0 = trab1;
  |LEE 011#3=;
  |CIERRA #3;
  |BUCLE fomparca;
  |FININF;
  |FINIMP;
|FPRC;
