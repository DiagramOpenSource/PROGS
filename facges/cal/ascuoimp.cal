|FICHEROS;
     ascuoimp #0;
     ascuocab #1;
     ascuolin #2;
|FIN;

|VARIABLES;
     nSwPrint = 0;
|FIN;

|PROCESO impresion;
     |PRINT;
|FPRC;

|PROCESO imprelin;
     nSwPrint = 1;
     |SI #0#2 = "R";
         |PRINT;
     |SINO;
         |BUCLELIN 0impresion#1;
         |PIEINF;
     |FINSI;
|FPRC;

|DEFBUCLE ascuocab;
     #1#1;
     ;
     #0#0;
     #0#1;
     ;
     NULL, imprelin;
|FIN;

|PROCESO impre;|TIPO 3;
     |IMPRE 0;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |SI #0#2 = "R";
         |INFOR "ascuoim1";
     |SINO;
         |INFOR "ascuoim2";
     |FINSI;

     |SI FSalida ! 0;  |FINIMP;  |ACABA;  |FINSI;

     |BUCLE ascuocab
     |SI nSwPrint = 1;  |Y #0#2 = "R";  |PIEINF;  |FINSI;

     |FININF;
     |FINIMP;
|FPRC;
