|FICHEROS;
     ashisfac #0;
|FIN;

|VARIABLES;
 {-1}menu = "";
     menu1 = "2400";
     menu2 = "1";
     menu3 = "";
     menu4 = "CSAPU";
     menu5 = "   Clave   ";
     menu6 = " Siguiente ";
     menu7 = " Anterior ";
     menu8 = " Primera ";
     menu9 = "  Ultima  ";
|FIN;

|PROGRAMA;
     |HAZ Tipo8Version;

     |CLS;
     |PINPA #0#0;
     |CABEZA "Consulta H.Facturas";
     |ABRE #0;
     FSalida = 999;
     |PARA ; |SI FSalida > 0; |HACIENDO;
          |MENU menu;
          menu2 = FSalida;
          |SI FSalida = 0; |O FSalida = -1; |SAL; |FINSI;
          |SI FSalida = 1;    ||Clave
               |HAZ clave;
               |LEE 001#0];
          |FINSI;
          |SI FSalida = 2;    ||Siguiente
               |LEE 001#0s;
               |SI FSalida ! 0;
                    |MENAV "0000Ultima Ficha";
                    |LEE 001#0=;
               |FINSI;
          |FINSI;
          |SI FSalida = 3;    ||Anterior
               |LEE 001#0a;
               |SI FSalida ! 0;
                    |MENAV "0000Primera Ficha";
                    |LEE 001#0=;
               |FINSI;
          |FINSI;
          |SI FSalida = 4;    ||Primera
               |LEE 001#0p;
          |FINSI;
          |SI FSalida = 5;    ||Ultima
               |LEE 001#0u;
          |FINSI;
          |PINDA #0#0;
          FSalida = 999;
     |SIGUE;
     |CIERRA #0;
|FPRO;

|PROCESO clave;
|CAMPO_MODIFICA #0#0 , 1 , "S";
|CAMPO_MODIFICA #0#1 , 1 , "S";
|CAMPO_MODIFICA #0#3 , 1 , "S";

|ET campo0;
    |ENCAMPO #0#0;

|ET campo1;
    |ENCAMPO #1#0;
    |SI FSalida = 2; |VETE campo0; |FINSI;
    |SI FSalida = 0; |SELECT #1#0; |C_M #0#3, 1, "N"; |VETE campo99; |FINSI;

|ET campo2;
    |ENCAMPO #3#0;
    |SI FSalida = 2; |VETE campo1; |FINSI;
    |SI FSalida = 0; |SELECT #2#0; |FINSI;

|ET campo99;
    |CAMPO_MODIFICA #0#0 , 1 , "N";
    |CAMPO_MODIFICA #0#1 , 1 , "N";
    |CAMPO_MODIFICA #0#3 , 1 , "N";
|FPRC;
