|FICHEROS;
     ashisfac #0;                         ||CALCULO DESDE-HASTA PARA INFORMES;
     asinfirp #1;
     asclient #2;
|FIN;

|VARIABLES;
     &cod     = "";
     &swtotal = 0;
     &ordena  = "Clientes.";

     client   = 0;
     sw1      = 0;
     sw2      = 0;
     cliente  = 0;
     varalfa  = "";
     sw       = 0;
     fFech = @;
     qFech = "";
     dFech = "";
     hFech = "";
|FIN;

|CALCULO imprimir;
|SI sw = 0; cliente = #0#3; sw = 1; |FINSI;

|SI cliente ! #0#3;
    client  = cliente + 100000;
    varalfa =  client;
    cod     = varalfa % A205;
    swtotal = 1;
    |PRINT;
    cliente = #0#3;
    swtotal = 0;
|FINSI;

#2#0 = #0#3;
|LEE 040#2=;
|SI FSalida ! 0; |DDEFECTO #2; |FINSI;
|PRINT;
|FCAL;

|DEFBUCLE cainfirp;
#0#2;
2;
#1#4, #1#0, #1#2, #1#6;
#1#5, #1#1, #1#3, #1#7;
e;
NULL,imprimir;
|FIN;

|CALCULO impre;|TIPO 3;
|ABRE #2;
|IMPRE 0;
|SI FSalida ! 0; |ACABA; |FINSI;

|INFOR "asinfirp";
|SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;

sw = 0;
|BUCLE cainfirp;
|SI sw = 1; |PIEINF; |FINSI;
|FININF;
|FINIMP;

|CIERRA #2;
|FCAL;

|PROCESO sacaano; |TIPO 7;
     fFech = ~;
     qFech = fFech % -104;
     dFech = "01.01." + qFech;
     hFech = "31.12." + qFech;
     #1#6  = dFech;
     #1#7  = hFech;
     |PINTA #1#6;
     |PINTA #1#7;
|FPRC;
