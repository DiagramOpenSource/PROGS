|FICHEROS;
   ascamlod #0;
   asclient #1;
|FIN;


|VARIABLES;
 codigo = 0;
 linea = 0;
 codac = 0;
   nombre = "";
|FIN;


|CALCULO codact;|TIPO 0;
|SELECT #2#0;
codigo = #0#0;
linea = #0#1;
|LEE 040#0=;
|SI FSalida = 0;|Y linea ! #0#1;
   |MENAV "    El codigo actual ya ha sido seleccionado.";
   |ERROR;
   #0#0 = codigo;
   #0#1 = linea;
|FINSI;
|SELECT #1#0;
|FCAL;


|CALCULO codnue;|TIPO 0;
|SELECT #3#0;
codigo = #0#0;
linea = #0#1;
codac = #0#2;
nombre = #0#3;
|LEE 040#0=;
|SI FSalida = 0;|Y linea ! #0#1;
      |MENAV "    El codigo nuevo ya ha sido seleccionado.";
      |ERROR;
      |DDEFECTO #0;
      #0#0 = codigo;
      #0#1 = linea;
      #0#2 = codac;
      #0#3 = nombre;
      |VETE abajo;
|FINSI;

|ABRE #1;
#1#0 = #0#4;
|LEE 040#1=;
|SI FSalida = 0;
    |MENAV "     El codigo nuevo ya existe";
    |ERROR;
|FINSI;
|CIERRA #1;

|ET abajo;
|FCAL;
