|FICHEROS;
    asfachis #0;
|FIN;

|VARIABLES;
     &d_serie = "";
     &h_serie = "";
     &d_factu = 0;
     &h_factu = 0;
     &d_fecha = @;
     &h_fecha = @;
     &empresa = 0;
     &d_viene = 2;
     &cuotas  = "N";
     fFech = @;
     qFech = "";
     dFech = "";
     hFech = "";
|FIN;

|PROCESO tipo_3; |TIPO 3;
d_serie = #0#0;
h_serie = #0#1;
d_factu = #0#2;
h_factu = #0#3;
d_fecha = #0#4;
h_fecha = #0#5;
empresa = #0#6;
cuotas  = #0#7;

|SUB_EJECUTA "asfacges.run";
|FPRC;

|PROCESO sacaano; |TIPO 7;
     fFech = ~;
     qFech = fFech % -104;
     dFech = "01.01." + qFech;
     hFech = "31.12." + qFech;
     #0#4  = dFech;
     #0#5  = hFech;
     |PINTA #0#4;
     |PINTA #0#5;
|FPRC;
