|PROCESO LeeMoneda; |TIPO 9;
     |VARIABLES;
          nEmpresa = 0;
          aEmpresa = "";
          aAlfa    = "";
          aTrab    = "";
          aDire    = "";
          aDire2   = "";
          nNume    = 0;
          nTrab    = 0;
          nPara    = 0;
          nBarra   = 0;
     |FIN;

     nEmpresa = #48#0;
     aEmpresa = ("00" + nEmpresa) % -102;

     |DBASE aDire;
     aAlfa = aDire % 0;
     nNume = aAlfa;
     |PARA nPara = nNume; |SI nPara ] 1; |HACIENDO nPara = nPara - 1;
          nTrab = (nPara * 100) + 1;
          aTrab = aDire % nTrab;
          |SI aTrab = "/";
               |SI nBarra = 1;
                    |SAL;
               |FINSI;
               nBarra = 1;
          |SINO;
               aDire2 = aTrab + aDire2;
          |FINSI;
     |SIGUE;
     aDire = aDire2;

     aAlfa =  ":agitool/zmonedas;*" + aDire + "*" + aEmpresa + "*";
     |SUB_EJECUTA_NP aAlfa;
|FPRC;
