|FICHEROS;
   asinfpre #0;
   ascabmin #1;
   asinfpr1 #2;
|FIN;

|VARIABLES;
 sw_pant = 0;
 lin = 0;
   informe = "asinfpre";
   &descri = "";
   {-1}menu = "";
   menu1 = "2400";
   menu2 = "1";
   menu3 = "  Elija opcion : ";
   menu4 = "PI";
   menu5 = "  Pantalla  ";
   menu6 = "  Impresora  ";
|FIN;

|CALCULO carga;
sw_pant = 1;
|SI menu2 = 1;
    #2#6 = descri;
    |PINTA #2#6;
    #2#0 = #1#0;
    #2#1 = #1#1;
    #2#2 = #1#3;
    #2#3 = #1#27;
    #2#4 = #1#28;
    #2#5 = #1#29;
    Pos = lin;
    |PINDA #0#2;
    Pos = -1;
    lin = lin + 100;
    |SI lin > 1400;
        |HAZ cambiapa;
    |FINSI;
|SINO;
    |PRINT;
|FINSI;
|FCAL;

|CALCULO imprimir;
|SI #0#2 = "S";
    descri = "* INFORME DE FACTURAS PREPARADAS *";
    |SI #1#28 ! 0;
        |HAZ carga;
    |FINSI;
|FINSI;
|SI #0#2 = "N";
    descri = "* INFORME DE FACTURAS NO PREPARADAS *";
    |SI #1#28 = 0;
        |HAZ carga;
    |FINSI;
|FINSI;
|FCAL;

|DEFBUCLE asinfpre0;
#1#1;
;
#0#0, 00001;
#0#1, 99999;
e;
NULL,imprimir;
|FIN;

|CALCULO pantalla;
lin = 100;
|PINPA #0#2;
|BUCLE asinfpre0;
|SI sw_pant = 1; |PAUSA; |FINSI;
|FCAL;

|CALCULO impresora;
|IMPRE 0;
|SI FSalida = 0;
   |INFOR informe;
   |SI FSalida = 0;
      |BUCLE asinfpre0;
      |SI sw_pant = 1; |PIEINF; |FINSI;
      |FININF;
   |SINO;
      |MENAV "     Error en Informe.";
   |FINSI;
   |FINIMP;
|SINO;
   |MENAV "     Impresion Cancelada.";
|FINSI;
|FCAL;

|CALCULO impre; |TIPO 3;
sw_pant = 0;
|MENU menu;
menu2 = FSalida;
|SI menu2 = 1; |HAZ pantalla;  |FINSI;
|SI menu2 = 2; |HAZ impresora; |FINSI;
|FCAL;

|CALCULO cambiapa;
|PAUSA;
lin = 100;
|PINPA #0#2;
|FCAL;
