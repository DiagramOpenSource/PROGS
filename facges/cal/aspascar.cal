  aFecha = #0#8; aFecha = (aFecha % 102) + (aFecha % 402) + (aFecha % 704);

  |SI #0#26 = "S";
      aAlfa = Usuario; aAlfa = "r1" + aAlfa;
      |NOME_DAT #901 aAlfa; |DELINDEX #901;
      |ABRE #901;
  |FINSI;

  nConta = 0;
  nTotal = 0;
  nError = 0;
  |ABRE #2;
  |ABRE #3;

  |SI #0#15 = "N";
      |ABRE #90;
      nSwOp = 1; |BUCLE ashisrec;
      |SI nError = 1;
          |PIEINF;
          |FININF;
          |FINIMP;
          |SI nConta ! 0;
              |CONFI "0000SHay errores en los Datos de los Recibos. Continuar con la Remesa";
              |SI FSalida = 0; nError = 0; |FINSI;
          |SINO;
              |MENAV "0000Hay errores en los Datos de los Recibos y no pasan a la Remesa";
          |FINSI;
      |FINSI;
      |SI nError = 0;
          nConta = 0;
          nTotal = 0;
          nSwOp  = 0;
          |BUCLE ashisrec;
      |FINSI;
      |CIERRA #90;
  |SINO;
      |ABRE #1;
      nSwOp = 1; |BUCLE aslinrem;
      |SI nError = 1;
          |PIEINF;
          |FININF;
          |FINIMP;
          |MENAV "0000Hay Errores en los Datos de los Recibos. Proceso interrumpdio";
      |SINO;
          nConta = 0;
          nTotal = 0;
          nSwOp  = 0;
          |BUCLE aslinrem;
      |FINSI;
      |CIERRA #1;
  |FINSI;

  |CIERRA #3;
  |CIERRA #2;
  |SI #0#26 = "S";
      |CIERRA #901;
  |FINSI;

  |SI nError = 0; |Y nConta = 0; |Y #0#15 = "N";
      |MENAV "    No existen Recibos para remesar";
      nError = 1;
  |FINSI;

  |SI nError = 0;

      |ABRE #5;
      #5#0 = #0#9;
      |LEE 040#5=;

      |SI #0#25 = "T";
          |ABRE #3;
          |ABRE #2;
          |ABRE #6;
          |DFICE empresa;
          fiche = "wb  " + empresa + "assecrec.seq";
          |QBF fiche;
          |FABRE fiche;
          handle = FSalida;
          |SI FSalida ] 0;
              |HAZ cabecera_p;
              |HAZ cab_ordenante;
              |HAZ regindiv;
              |HAZ campo_totales;
          |FINSI;
          FSalida = handle;
          |FCIERRA FSalida;
          |CIERRA #6;
          |CIERRA #2;
          |CIERRA #3;
      |SINO;
          |HAZ NuevoXML;
      |FINSI;

      |CIERRA #5;
  |FINSI;

  |DELINDEX #3;
  |CIERRA #asclient;

  |SI nError = 0;
      E_sup = 1;
      E_inf = "SNsn";
      sino  = "S";
      sino  = 1769 ? 1;
      sino  = sino > " ";
      |SI sino = "S"; |SUB_EJECUTA_NP "ascopdis"; |FINSI;
  |FINSI;
|FCAL;

||***********************************************************;
