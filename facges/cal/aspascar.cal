        #8#14 = "G";
        #8#15 = "00.00.0000";
        |GRABA 040#8n;
        |CIERRA #8;
        |HAZ graba_con;

        #7#19 = #7#19 + #8#13;
        #7#26 = #7#24 - #7#19 - #7#25;
    |FINSI;

    #12#0 = #9#4;
