|FICHEROS;
   ascuolin #0;
   ascuocab #1;
|FIN;

|VARIABLES;
   cod  = 0;
   cta  =  " ";
|FIN;

|PROCESO importe;|TIPO 2;
#0#7 = #0#5 * #0#6;
#1#6 = #1#6 +. #0#7;
|SI #1#2 = "M"; #1#7 = #1#6 / 12; |FINSI;
|SI #1#2 = "T"; #1#7 = #1#6 / 4;  |FINSI;
|SI #1#2 = "S"; #1#7 = #1#6 / 2;  |FINSI;
|SI #1#2 = "A"; #1#7 = #1#6 / 1;  |FINSI;
|PINTA #0#7;
|PINTA #1#6;
|PINTA #1#7;
|FPRC;

|PROCESO concep;|TIPO 0;
cod = #0Campo;
cta = "00" + cod;
cta = cta % -102;
#0Campo = cta;
|PINTA #0Campo;
|FPRC;
