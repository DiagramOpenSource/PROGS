|FICHEROS;
    eximport #0;         || importacion
    exintrod #1;         || expedientes cabs.
    extramit #2;         || expedientes lins.
    asexpcab #3;         || expedientes simples cabs.
    asexplin #4;         || expedientes simples lins.
    exasunto #5;         || asuntos
|FIN;

|VARIABLES;
    alfa1 = "";
    alfa2 = "";
    alfa3 = "";
    alfa4 = "";
    nume1 = 0;
    nume2 = 0;
    nume3 = 0;
    nume4 = 0;
    para1 = 0;
    para2 = 0;
    para3 = 0;
    para4 = 0;
    fech1 = @;
    fech2 = @;
    fech3 = @;
    fech4 = @;
    elsw1 = 0;
    elsw2 = 0;
    elsw3 = 0;
    elsw4 = 0;
    FEstado = 0;
    seleccio = 0;
    modo = 0;
    sw_rup = 0;

    exped = 0;
    ant_exped = -1;
    sw_pasa = 0;
    nexpe = 0;
    ntram = 0;
|FIN;
____________________________________/
|PROCESO mayus; |TIPO 0;
    #0Campo = #0Campo > #0Campo;
|PINTA #0Campo;
|FPRC;
____________________________________/
|PROCESO pasa_asunto;
     #5#0 = #4#5;
|LEE 000#5=;
|SI FSalida ! 0;
        #5#1 = #4#4;
    |GRABA 020#5n;
|FINSI;
|FPRC;

|PROCESO pasa_lin;
|DDEFECTO #2;
    #2#0 = exped;        || expediente
    #2#1 = #4#1 $ 100;   || linea de tramite
    #2#2 = #4#2;         || texto (contrario)
    #2#3 = "->";         || (flecha)
    #2#4 = #4#3;         || fecha
    #2#5 = 0;            || hora
    #2#6 = 99999;        || letrado
    #2#7 = "";           || nombre letrado
|GRABA 020#2n;
|SI FSalida = 0;
        ntram = ntram + 1;
|FINSI;
|FPRC;

|PROCESO pasa_cab;
|DDEFECTO #1;
    #1#0 = exped;        || expediente
    #1#1 = #4#3;         || fecha (del primer tramite)
    #1#2 = 99999;        || letrado
    #1#3 = #4#0;         || cliente
    #1#4 = 99999;        || procurador
    #1#5 = 99999;        || contrario
    #1#6 = 999;          || tribunal
    #1#7 = "";           || autos
    #1#8 = "";           || procedimiento
    #1#9 = "";           || cuantia
    #1#10 = "";          || seccion
    #1#11 = "";          || concepto
    #1#12 = "S";         || cerrado
    #1#13 = "";          || resumen 1
    #1#14 = "";          || resumen 2
    #1#15 = "";          || resumen 3
    #1#16 = "";          || resumen 4
    #1#17 = "";          || resumen 5
    #1#18 = "";          || resumen 6
    #1#19 = "";          || resumen 7
    #1#20 = "";          || resumen 8
    #1#21 = "";          || resumen 9
    #1#22 = "";          || resumen 10
    #1#23 = "";          || ref. cliente
    #1#24 = "";          || ref. procurador
    #1#25 = "";          || ref. contrario
    #1#26 = "S";         || minutado
    #1#27 = 9;           || tipo facturacion
    #1#28 = 1;           || tarifa conceptos
    #1#29 = 99999;       || letrado contrario
    #1#30 = 99999;       || procurador contrario
    #1#31 = #4#5;        || asunto (del primer tramite)
    #1#32 = #4#4;        || descrip. asunto (del primer tramite)
|GRABA 020#1n;
|SI FSalida = 0;
        nexpe = nexpe + 1;
|FINSI;
|FPRC;

|PROCESO asexplin;
    exped = (#4#1 - (#4#1 $ 100)) / 100;
|SI exped ] #0#3;|Y exped [ #0#4;
    |SI ant_exped ! exped;
            #0#6 = exped;
        |PINTA #0#6;

            sw_pasa = 0;
            #1#0 = exped;
        |LEE 000#1=;
        |SI FSalida = 0;
            |SI #0#5 = "S";
                |BUCLELIN 1NULL#1;
                |BORRA 020#1a;
                |HAZ pasa_cab;
            |SINO;
                    sw_pasa = 1;
            |FINSI;
        |SINO;
            |HAZ pasa_cab;
        |FINSI;
            ant_exped = exped;
    |FINSI;
    |SI sw_pasa = 0;
        |HAZ pasa_lin;
        |HAZ pasa_asunto;
    |FINSI;
|FINSI;
|FPRC;

|DEFBUCLE asexpcab;
 #3#1;
 ;
 #0#1;
 #0#2;
 ;
 NULL, NULL, asexplin;
|FIN;

|PROCESO tipo3; |TIPO 3;
|SI #0#0 = "SI";
    |ABRET #1;
    |ABRET #2;
    |ABRET #5;
    |BUCLE asexpcab;
    |CIERRAT #1;
    |CIERRAT #2;
    |CIERRAT #5;
        alfa1 = "    Grabados " + nexpe + " expedientes y " + ntram + " tramites ...";
    |MENAV alfa1;
|FINSI;
|FPRC;
