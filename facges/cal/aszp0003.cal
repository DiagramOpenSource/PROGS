|FICHEROS;
    aszp0003 #0;
    asconcep #1;
    asconcli #2;
    agitab03 #3;
    agim0001 #4;
    asclient #5;
    agicontr #99;
|FIN;

|VARIABLES;
   Path        = "";
   MiraCliente = 0;
   &DCliente   = 0;
   &HCliente   = 0;
|FIN;

|PROCESO Pasando;
|SI #4#12 ! "01.01.0000"; |ACABA; |FINSI;

|SI MiraCliente = 0;
    #5#0 = #4#0;
    |LEE 040#5=;
    |SI FSalida ! 0; |ACABA; |FINSI;
|FINSI;

#3#0 = #4#2;
|LEE 040#3=;
|SI FSalida ! 0;                   |ACABA; |FINSI;
|SI #3#2 = "    "; |Y #3#3 = "  "; |ACABA; |FINSI;
|SI #3#5 ! #48#0; |Y #3#5 ! 0;     |ACABA; |FINSI;

#1#0 = #3#2;
#1#1 = #3#3;
|LIBERA #1;
|LEE 140#1=;
|SI FSalida ! 0;
    |DDEFECTO #1;
    #1#0  = #3#2;
    #1#1  = #3#3;
    #1#2  = #3#1;
    #1#3  = #3#4;
    #1#4  = #4#5;
    #1#5  = "G";
    #1#16 = #1#2;
    |GRABA 140#1n;
|FINSI;

#2#0 = #4#0
#2#1 = #3#2;
#2#2 = #3#3;
|LIBERA #2;
|LEE 140#2=;
|SI FSalida ! 0;
    |DDEFECTO #2;
    #2#0  = #4#0
    #2#1  = #3#2;
    #2#2  = #3#3;
    #2#3  = #3#4;
    #2#4  = #3#6;
    #2#6  = #3#1;
    #2#7  = #3#1;
    #2#10 = #4#5;
    |GRABA 140#2n;
|SINO;
    |SI #0#2 = "S";
        #2#10 = #4#5;
        |GRABA 140#2a;
    |FINSI;
|FINSI;
|FPRC;

|DEFBUCLE agim0001;
#4#1;
;
#0#0, 00;
#0#1, 99;
;
NULL, Pasando;
|FIN;

|PROCESO Tipo3;
|DBASS Path;
|PATH_DAT #99 Path;

|ABRE #99;
|LEE 040#99p;
|SI FSalida ! 0; Path = "*"; |CIERRA #99; |ACABA; |FINSI;
|CIERRA #99;

Path = #99#1;  |QBF Path;
|SI Path  = "*"; |ACABA; |FINSI;

Path = #99#8; |QBF Path;

|PATH_DAT #3 Path;
|PATH_DAT #4 Path;

|ABRE #1;
|ABRE #2;
|ABRE #3;
|ABRE #5;
|BUCLE agim0001;
|CIERRA #1;
|CIERRA #2;
|CIERRA #3;
|CIERRA #5;
|FPRC;

|PROGRAMA;
|SI DCliente = 0;
    MiraCliente = 0;
    |CLS;
    |CABEZA "Pase Conceptos Contratados";
    |PINPA #0#0;
    |DDEFECTO #0;
    |PINDA #0#0;
    |ENDATOS #1#0;
    |SI FSalida = 0; |HAZ Tipo3; |FINSI;
|SINO;
    MiraCliente = 1;
    #0#0        = DCliente;
    #0#1        = HCliente;
    #0#2        = "N";
    |HAZ Tipo3;
|FINSI;
|FPRO;
