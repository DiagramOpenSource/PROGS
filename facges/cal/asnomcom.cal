|FICHEROS;
   asnomcom #0;
   asnomdat #1;
|FIN;

|VARIABLES;
  &aster  = "";
  fecha  = @;
  varalf = "";
|FIN;

|PROCESO impresio;
aster = "";
|SI #1#05 ! #1#06; aster = "*"; |FINSI;
|SI #1#07 ! #1#08; aster = "*"; |FINSI;
|SI #1#09 ! #1#10; aster = "*"; |FINSI;
|SI #1#11 ! #1#12; aster = "*"; |FINSI;
|SI #1#13 ! #1#14; aster = "*"; |FINSI;
|SI #1#15 ! #1#16; aster = "*"; |FINSI;
|SI #1#17 ! #1#18; aster = "*"; |FINSI;
|PRINT;
|FPRC;

|DEFBUCLE asnomdat;
#1#1;
;
#0#0, #0#2, #0#3;
#0#1, #0#2, #0#3;
e;
NULL, impresio;
|FIN;

|PROCESO pinta; |TIPO 7;
fecha = ~;
varalf = fecha % 402;
#0#2 = varalf;
varalf = fecha % 902;
#0#3 = varalf;
|PINTA #0#2;
|PINTA #0#3;
|FPRC;

|PROCESO impre; |TIPO 3;
|IMPRE 0;
|SI FSalida = 0;
    |INFOR "asnomcom";
    |SI FSalida = 0;
        |BUCLE asnomdat;
        |PIEINF;
        |FININF;
    |FINSI;
    |FINIMP;
|FINSI;
|FPRC;
