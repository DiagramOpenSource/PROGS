|PROCESO imp_1;
    nume1 = puntero + 1;
|SI #20nume1 = 1;
    |ABRE #7;
    |PATH_DAT #6 path;
        error = -1;
    |PARA; |SI error ! 0; |HACIENDO;
        |ABRE #6;
            error = FSalida;
    |SIGUE;
        #6#0 = codi;
        #6#1 = 1;             || bucle de lectura de lineas
    |LEE 001#6=;
    |SI FSalida ! 0;  |LEE 001#6];  |FINSI;
    |PARA; |SI FSalida = 0;|Y #6#0 = codi; |HACIENDO;
        |SI borra = "S";  |BORRA 020#6a; |FINSI;
        |PARA nume1 = 0; |SI nume1 [ campos_1; |HACIENDO nume1 = nume1 + 1;
            #7nume1 = #6nume1;
        |SIGUE;
        |GRABA 020#7n;
        |LEE 001#6s;
    |SIGUE;
    |CIERRA #6;
    |CIERRA #7;
|FINSI;
|FPRC;

