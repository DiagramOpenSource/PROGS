            |SI mGeneral2 = "5";  |HAZ mPuntosD;               |FINSI;
            |SI mGeneral2 = "6";  |HAZ mPuntosE;               |FINSI;
            |SI mGeneral2 = "7";  |HAZ mPuntosF;               |FINSI;
            |SI mGeneral2 = "8";  |HAZ mPuntosM;               |FINSI;
            |SI mGeneral2 = "9";  |HAZ mMenuPat;               |FINSI;
            |SI mGeneral2 = "10";
                |SI enModo ! 4;
                    |SUB_EJECUTA_NP "rezcalcu";
                |FINSI;
                |SUB_EJECUTA_NP "rem01001";
                |SI enModo ! 4;  |SAL;  |FINSI;
            |FINSI;
     |SIGUE;
     |PINPA #0#0;
|FPRC;

|PROCESO LetraNif;  |TIPO 6;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     eaVarDni = #0Campo;
     |HAZ CalculoDNI;
     #0Campo = eaVarDni;
     |PINTA #0Campo;
     |ERROR;
|FPRC;

|PROCESO Contador; |TIPO 7;
     enModo = (FEntrada / 100) ! 0;
     |SI enModo ! 1;  |ACABA;  |FINSI;

     |ABRE #1;
     |LEE 101#1p;
     |SI FSalida ! 0; |DDEFECTO #1; #1#9 = 1; |GRABA 020#1b; |FINSI;
     |LIBERA #1;
     |CIERRA #1;

     |SI #1#8 = "A"; #0#0 = #1#9; |PINTA #0#0; |FINSI;
|FPRC;

|PROCESO Acceso;  |TIPO 3;
     enEmpresa = #0#0;
     enModo    = (FEntrada / 100) ! 0;

     |HAZ Astericos;

     aAlfa = (aGeneral13 % 1160) + " [*]";
     |SI aAlfa = mGeneral13;  |Y enModo ! 4;
         |CONFI "     Renta ya calculada, desea anular calculo ";
         |SI FSalida ! 0;  |PINPA #0#0;  |ACABA;  |FINSI;

         |HAZ BorraLiquidacion;
         eaPrograma   = "rem01001";
         |HAZ ProgramaActivo;
     |FINSI;

     |HAZ CogeReo;

     |SI enModo = 2; |HAZ Modicli; |FINSI;
     |SI enModo = 1; |O enModo = 2;
         |HAZ Pantalla;
     |FINSI;

     |SI enModo = 1;
         |ABRE #1;
         |LEE 101#1p;
         |SI #1#8 = "A";
             #1#9 = #0#0 + 1;
             |GRABA 020#1a;
         |FINSI;
         |LIBERA #1;
         |CIERRA #1;
     |FINSI;
|FPRC;

|PROCESO Consulta; |TIPO 13;
     |SI #0#0 = 0;  |ACABA;  |FINSI;

     enModo = (FEntrada / 100) ! 0;
     |SI enModo ! 4;  |ACABA;  |FINSI;
     |PINTA #0#0;
     |PINTA #0#1;
     |PINTA #0#2;
     |HAZ Pantalla;
|FPRC;

|PROCESO Borrado; |TIPO 2;
     enEmpresa = #0#0;
     eaNombre  = #0#1;
     enModo    = (FEntrada / 100) ! 0;
     |SI enModo ! 3;  |ACABA;  |FINSI;

     |PINTA 2430,"Borrando Registros ....";

     |HAZ BorraRenta;

     |PINTA 2430,"                       ";
|FPRC;
