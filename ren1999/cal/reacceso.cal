|PROCESO RecogeReo;
     |SALVA_FICHA #0;

     |CIERRAT #0;
     |BUCLE agiconpa;
     |ABRET #0;

     |REPON_FICHA #0;
|FPRC;

|PROCESO CogeActiv;
     aAlfa = (aGeneral8 % 1160) + " [*]";
     |SI aAlfa = mGeneral8;  |ACABA;  |FINSI;

     nSwActiv  = 0;
     aTipo     = "R1";
     nSwRecoge = 1;
     |HAZ RecogeReo;
     nSwRecoge = 0;
     |SI nSwActiv = 0;  |ACABA;  |FINSI;

     |PUSHV 0501 2380;
     |CUADRO 1221 1664;
     |ATRI R;
     |PINTA 1322, "    DETECTADO ENLACE RENTA CON R.E.O.     ";
     |PINTA 1422, "------------------------------------------";
     |PINTA 1522, " Desea hacer el pase en este momento :    ";

     E_sup = 1;
     E_inf = "SN";
     aSiNo = "S";
     aSiNo = 1561 ? 1;

     |ATRI 0;
     |SI aSiNo = "S"; |HAZ RecogeReo; |FINSI;
     |POPV;
|FPRC;

|PROCESO CogeArrend;
     nSwActiv   = 0;
     aTipo      = "R2";
     nSwRecoge = 1;  |HAZ RecogeReo;
     nSwRecoge = 0;
     |SI nSwActiv = 0; |ACABA; |FINSI;

     |PUSHV 0501 2380;
     |CUADRO 1221 1664;
     |ATRI R;
     |PINTA 1322, "DETECTADO ENLACE CON R.E.O.ARRENDAMIENTOS ";
     |PINTA 1422, "------------------------------------------";
     |PINTA 1522, " Desea hacer el pase en este momento :    ";

     E_sup = 1;
     E_inf = "SN";
     aSiNo  = "S";
     aSiNo  = 1561 ? 1;

     |ATRI 0;
     |SI aSiNo = "S"; |HAZ RecogeReo; |FINSI;
     |POPV;
|FPRC;

|PROCESO CogeReo;
     |HAZ CogeActiv;
     |HAZ CogeArrend;
|FPRC;

