     #1#15 = (N\ #0#22 % nCoefi);
     #1#16 = (N\ #0#24 % nCoefi);
     #1#17 = (N\ #0#25 % nCoefi);
     #1#18 = #0#23;

     |GRABA 020#1n;
     |LIBERA #1;

     #2#0 = #1001#0;
     #2#1 = #1001#1;
     #2#2 = #1001#2;
     |LEE 101#2=;
     |SI FSalida ! 0;
         |DDEFECTO #2;
         #2#0 = #1001#0;
         #2#1 = #1001#1;
         #2#2 = #1001#2;
         |GRABA 020#2b;
     |FINSI;

     |SI #0#13 = "C"; #2#14 = "X";  |FINSI;
     |SI #0#13 = "F"; #2#15 = "X";  |FINSI;
     |SI #2#14 = "X"; #2#15 = " ";  |FINSI;

     #2#4  = (N\ #2#4 + (#0#14 % nCoefi));
     #2#5  = (N\ #2#5 + (#0#22 % nCoefi));
     |GRABA 020#2a;
     |LIBERA #2;

