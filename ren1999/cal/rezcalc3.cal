     #1#3  = nLinea;
     #1#6  = #0#11;
     #1#5  = #0#12;
     #1#4  = eaContri;
     |SI #1#2 = "CONJUNTA  ";
         #1#4 = #0#2;
         |SI #1#4 = "VARIOS    ";  #1#4 = "COMUNES";  |FINSI;
         |SI #1#4 = "GANANCIAL ";  #1#4 = "COMUNES";  |FINSI;
     |FINSI;
