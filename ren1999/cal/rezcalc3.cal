         |LEE 040#1a;
     |SINO;
         |LEE 040#1u;
     |FINSI;

     |SI FSalida = 0; |Y #1#0 = #1001#0; |Y #1#1 = #1001#1; |Y #1#2 = #1001#2;
         nLinea = #1#3 + 1;
     |SINO;
         nLinea = 1;
