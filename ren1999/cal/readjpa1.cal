|| *********************** De afectos a patrimonio inmuebles urbana *******

|PROCESO inmueblea;
enCodProv = #rem00099#8;
enCodPobl = #rem00099#9;
|HAZ LeePoblacion;

coefi = 100;
alfa1 = #rem00099#16;
alfa4 = #rem00099#19;
alfa2 = #rem00099#17;
alfa3 = #rem00099#6 + "  ";
canti = #rem00099#10;

Comodin = #rem00099#3; |QBF Comodin;
|SI Comodin = "GANANCIAL";
    |SI #99#1 = "T"; coefi = 50; |HAZ carga_01_1; |ACABA; |FINSI;
    |SI #99#1 = "C"; coefi = 50; |HAZ carga_01_1; |ACABA; |FINSI;
|SINO;
    |SI Comodin = "VARIOS";
       |SI #99#1 = "T"; coefi = #rem00099#20; |HAZ carga_01_1; |ACABA; |FINSI;
       |SI #99#1 = "C"; coefi = #rem00099#21; |HAZ carga_01_1; |ACABA; |FINSI;
    |SINO;
       Comodin = #rem00099#3 % 0101;
       |SI #99#1 = Comodin; |HAZ carga_01_1; |FINSI;
    |FINSI;
|FINSI;
|FPRC;

|DEFBUCLE repatria;
#rem00099#1;
;
#99#0, "A1", 001;
#99#0, "A1", 999;
e;
NULL, inmueblea;
