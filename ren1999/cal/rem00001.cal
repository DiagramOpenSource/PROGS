|PROCESO Menus;
     MGeneral2  = 1;
     empresa    = #0#0;
     nombre     = #0#1;
     nif        = #0#2;
     Salir      = 0;
     enEmpresa  = #0#0;
     eaNombre   = #0#1;
     eaNif      = #0#2;

     |PARA;  |SI Salir = 0;  |HACIENDO;
         Salir = 1;
         |MENUG MGeneral;
         MGeneral2 = FSalida;
         |SI MGeneral2 ] 1; |Y MGeneral2 [ 2;
             |SI MGeneral2 = 1; Salir = 0;  |SUB_EJECUTA_NP "rem00100"; |FINSI;
             |SI MGeneral2 = 2; Salir = 0;  |HAZ LineasInmuebles;    |FINSI;
         |FINSI;
     |SIGUE;

     |PINPA #0#0;
|FPRC;

