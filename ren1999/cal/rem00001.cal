|PROCESO AltaCliente;  |TIPO 0;
     enModo = (FEntrada / 100) ! 0;
     |SI enModo ! 1;  |ACABA;  |FINSI;

     enCodCli = #0#0;
     |HAZ LeePersona;
     |SI enSwDeDonde = 1;
         |ABRE #3;
         |HAZ MiraIntegrada;
         |CIERRA #3;
     |SINO;
         |HAZ MiraAgifigen;
     |FINSI;
|FPRC;

|PROCESO ModiCliente;
     enCodCli = #0#0;
     |HAZ LeePersona;

     |SI enSwDeDonde = 1;
         |SI #0#2 ! #715#1;
            |MENAV "     D.N.I. del Cliente es distinto al D.N.I. del Cliente General";
         |FINSI;
     |SINO;
         |SI #0#2 ! #715#13;
            |MENAV "     D.N.I. del Cliente es distinto al D.N.I. del Cliente General";
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO NoCalcu; |TIPO 0;
     modo = (FEntrada / 100) ! 0;

     |ABRE #2;
     #2#0 = #0#0;
     |LEE 040#2=;
     |SI FSalida ! 0;  |DDEFECTO #2;  #2#10 = "N";  |FINSI;
     |CIERRA #2;

     enEmpresa = #rem00100#0;
     eaNombre  = #0#1;
     eaNif     = #0#2;

     |SI #2#10 = "S";
         |MENAV "       El Contribuyente es Nacional";
         |ERROR;
         |ACABA;
     |FINSI;

     |SI modo ! 1;  |ACABA;  |FINSI;

     |HAZ AltaCliente;
|FPRC;

|PROCESO Contador; |TIPO 7;
     modo = (FEntrada / 100) ! 0;
     |SI modo ! 1;  |ACABA;  |FINSI;

     |ABRE #1;
     |LEE 101#1p;
     |SI FSalida ! 0; |DDEFECTO #1; #1#9 = 1; |GRABA 020#1b; |FINSI;
     |LIBERA #1;
     |CIERRA #1;
     |SI #1#8 = "A"; #0#0 = #1#9; |PINTA #0#0; |FINSI;
|FPRC;

|PROCESO Tipo30;  |TIPO 30;
     modo = (FEntrada / 100) ! 0;
     enModo = modo;
     |SI modo = 2; |HAZ ModiCliente; |FINSI;
     |SI modo = 1; |O modo = 2; |HAZ Menus; |FINSI;
     |SI modo = 1;
         |ABRE #1;
         |LEE 101#1p;
         |SI #1#8 = "A"; #1#9 = #0#0 + 1; |GRABA 020#1a; |FINSI;
         |LIBERA #1;
         |CIERRA #1;
     |FINSI;
|FPRC;

|PROCESO Tipo13; |TIPO 13;
     |ABRE #2;
     #2#0 = #0#0;
     |LEE 040#2=;
     |SI FSalida ! 0;  |DDEFECTO #2;  |FINSI;
     |CIERRA #2;

     |SI #2#10 = "S"; |Y #2#0 ! 1;
         |PTEC 807;
         |ACABA;
     |FINSI;

     modo = FEntrada / 100;
     modo = modo ! 0;
     |SI modo = 4;
         |PINTA #0#0;
         |PINTA #0#1;
         |PINTA #0#2;
         |HAZ Menus;
     |FINSI;
|FPRC;

