|FICHEROS;
     rem01000 #1000;
|FIN;

|VARIABLES;
     nCampo = 0;
     nNume  = 0;
     aAlfa  = "";

     {50}paAlfa = "";
|FIN;

|RUTINA ClaveBaja1000;
     #1000#0 = 0;
     #1000#1 = "   ";
|FRUT;

|RUTINA ClaveAlta1000;
     #1000#0 = 99;
     #1000#1 = "zzz";
|FRUT;

|PROCESO Tipo5;  |TIPO 5;
       |LEE 040#1000p;
       |LINEAS_BI_ATRI #1000#3, S, D, -1;
|FPRC;

|PROCESO Tipo2;  |TIPO 2;
     #1000#3 = "D";
     |SI #1000#1 = "   ";  #1000#3 = "T";  |FINSI;
|FPRC;

|PROCESO Tipo11;  |TIPO 11;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     |PUSHV 0501 2380;
     |SALVA_DATOS #1000;
     E_inf  = 00;
     E_sup  = 99;
     nNume  = 1750 ? 1;

     E_inf  = ""
     E_sup  = "3";
     aAlfa  = 1753 ? 1;
     |POPV;

     #1000#0 = nNume;
     #1000#1 = aAlfa;
     |LEE 040#1000=;
     |SI FSalida ! 0;  |REPON_DATOS #1000;  |ACABA;  |FINSI;

     IpaAlfa = paAlfa1 <-;
     |PARA nCampo = 4;  |SI nCampo [ 52;  |HACIENDO nCampo = nCampo + 1;
           paAlfa = #1000nCampo;
           IpaAlfa = IpaAlfa + 1;
     |SIGUE;

     |REPON_DATOS #1000;
     |LEE 101#1000=;
     IpaAlfa = paAlfa1 <-;
     |PARA nCampo = 4;  |SI nCampo [ 52;  |HACIENDO nCampo = nCampo + 1;
           #1000nCampo = paAlfa;
           IpaAlfa = IpaAlfa + 1;
     |SIGUE;
     |GRABA 020#1000a;
     |LIBERA #1000;
     |ERROR;
|FPRC;

|PROGRAMA;
     |ENTLINEAL #1#1000, 1, 2, 22, 1, ClaveBaja1000, ClaveAlta1000;
|FPRO;
