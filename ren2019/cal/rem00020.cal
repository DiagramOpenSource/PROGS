     21, 22;
     #rem00020#0, 01, "S", #rem00020#1;
     #rem00020#0, 99, "S", #rem00020#1;
     be;
     NULL, TraePensiones;
|FIN;

|PROCESO Pensiones;
     |SI FSalida = 10;
         |VENTANA 0501, 3499, -1, 16, "Pensiones";
         nVent = FSalida;

         aVent1 = -1;
         aVent2 = -1;
         aVent3 = 0;
         aVent4 = nVent;
         aVent5 = "MODAL";
         |ESPECIFICA "ESTADO_VENTANA", aVent;

         |PTEC 802;  |PAUSA;

         enLinTrabajo = #rew00620#1;
         eaDeclaTraba = #rew00620#2;
         |SUB_EJECUTA_NP "rem00053";

         aVent1 = -1;
         aVent2 = -1;
         aVent3 = 0;
         aVent4 = nVent;
         aVent5 = "MODELESS";
         |ESPECIFICA "ESTADO_VENTANA", aVent;

         |FINVENTANA nVent;
     |FINSI;

     #rew00620#16 = 0;
     nSw      = 0;   |BUCLE rem00053;

     |PINTA #rew00620#13;
     |PINTA #rew00620#16;
|FPRC;

|PROCESO Irregular;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |C_M #rew00620#17, 1, "S";
