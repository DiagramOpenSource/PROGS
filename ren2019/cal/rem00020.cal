     |SI aAlfa = "N";  |ACABA;  |FINSI;

     |SI #rew00620#6 = 0;
         |MENAV "0000Revise si se tiene que marcar la casilla <Pagador no obligado a retener>";
     |FINSI;
|FPRC;

