     aFicRespTxt = "Resp" + #reemidir#14 + ".txt";
     aAbre      = eaPathEmision + aFicPortal;
     |XABRE "WB", aAbre, nHandle;

     aAlfa = "<!DOCTYPE HTML PUBLIC " + &34 + "-//W3C//DTD HTML 4.01 ";
