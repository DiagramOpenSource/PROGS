     aAlfa = aAlfa + " content=" + &34 + "text/html; charset=iso-8859-1";
     aAlfa = aAlfa + &34 + ">";
     |HAZ GrabaPortal;

     aAlfa = &9 + "</head>";
     |HAZ GrabaPortal;

     aAlfa = "";
     |HAZ GrabaPortal;

     aAlfa = &9 + "<script language=" + &34 + "javascript" + &34 + ">";
     |HAZ GrabaPortal;

     aAlfa = (&9 * 2) + "function carga()";
     |HAZ GrabaPortal;

     aAlfa = (&9 * 2) + "{";
     |HAZ GrabaPortal;

     aAlfa = (&9 * 3) + "document.form1.submit();";
     |HAZ GrabaPortal;

     aAlfa = (&9 * 2) + "}";
     |HAZ GrabaPortal;

     aAlfa = &9 + "</script>";
     |HAZ GrabaPortal;

     aAlfa = "";
     |HAZ GrabaPortal;

     aAlfa = &9 + "<body onload=" + &34 + "javascript:carga();" + &34 + ">";
