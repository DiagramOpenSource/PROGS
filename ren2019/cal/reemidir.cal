     aAlfa = eaPathEmision + aFicPortal;      |FBORRA aAlfa;
     aAlfa = eaPathEmision + eaFicheroPlano;  |FBORRA aAlfa;

     |SI enErrorPortal = 0;
         aAbre   = eaPathEmision + Usuario + ".ok";
         |XABRE "WB", aAbre, nHandle;
         |XGRABA nHandle, "OK";
         |XCIERRA nHandle;
     |FINSI;
