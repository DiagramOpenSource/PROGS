|FICHEROS;
     dsnetm11 #0;
     dsnetm12 #1;
|FIN;

|VARIABLES;
     nCampo = 0;
     nCampoAct = 0;
     nPara = 0;
     aAlfa = "";
|FIN;

|PROCESO PonMensa; |TIPO 7;
     |ABRE #1;
     |LEE 000#1p;
     nCampoAct = Campo + 20;
     nCampo = Campo;
     aAlfa = "0000" + #1Campo; |QBF aAlfa;

     |MENSA aAlfa;
     |CIERRA #1;
|FPRC;

|PROGRAMA;
     |CLS;
     |CABEZA "Parametros Varios";
     |ABRE #0;
     |LEE 101#0p;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     |PINPA #0#0;
     |ABRE #1;
     |LEE 000#1p;
     |PARA nPara = 1; |SI nPara [ 20; |HACIENDO nPara = nPara + 1;
          nCampo = nPara + 20;
          |SI #1nCampo = "X";
               |C_M #0nPara, 1, "N";
               |C_V #0nPara, 1, "N";
          |FINSI;
          |SI #1nCampo = "N";
               |C_M #0nPara, 1, "N";
               |C_V #0nPara, 1, "S";
          |FINSI;
          |SI #1nCampo = "S";
               |C_M #0nPara, 1, "S";
               |C_V #0nPara, 1, "S";
          |FINSI;
     |SIGUE;
     |CIERRA #1;

     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |GRABA 020#0a;
     |FINSI;
     |CIERRA #0;
|FPRO;
