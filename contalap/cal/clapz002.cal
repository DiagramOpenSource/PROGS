|PROCESO PrimeroAna;
 |SI nConta = 0;
     aDef = eaPathDef + "anm00001";
 |SINO;
     |SI enSwAna = 1;
         aDef = eaPathDef + "cactaana";
     |SINO;
         aDef = eaPathDef + "camandep";
     |FINSI;
 |FINSI;

 |SI enSwAna = 1;
     |CARGA_DEF aDef, anm0001@;
     |SI FSalida ! 0;
         aDef321 = "";
         |ACABA;
     |FINSI;
     aDef321 = aDef;
     |PATH_DAT #321 eaPathConta;

     |BUCLE LeeCtaAna;
     |DESCARGA_DEF #anm0001@;
     aDef321 = "";

 |SINO;

     |SI nConta = 1;
         |CARGA_DEF aDef, camande@;
         |SI FSalida ! 0;
             aDef322 = "";
             |ACABA;
         |FINSI;
         aDef322 = aDef;
         |PATH_DAT #322 aPathEmp0;

         |BUCLE LeeDepar;
         |DESCARGA_DEF #camande@;
         aDef322 = "";
     |FINSI;

 |FINSI;
|FPRC;

|PROCESO LeeCliPro;
 aCif  = "";
 aCaja = " " * 12;

 |ABRE #311;
 |DDEFECTO #311;
 #311#23 = "C";
 #311#10 = #310#0;
 #311#11 = #310#1;
 |LEE 040#311=;
 |SI FSalida = 0;
     aCif = #311#9;
 |SINO;
     #311#23 = "P";
     |LEE 040#311=;
     |SI FSalida = 0;
         aCif = #311#9;
     |FINSI;
 |FINSI;
 |CIERRA #311;
 aCif = (aCif + "               ") % 115;

 |ABRE #3;
 #3#0 = enEmpresa;
 #3#1 = enPeriodo;
 #3#2 = #310#0;
 |LEE 040#3=;
 |SI FSalida = 0;
     aCaja = #3#7;
 |FINSI;
 |CIERRA #3;
|FPRC;

|PROCESO LeeCtas;
 aBlanc = " " * 50;

 aAlfa1 = #310#0; |QBF aAlfa1;
 |SI aAlfa1 = ""; |ACABA; |FINSI;

 aAlfa1 = #310#0;
 aVarAlfa1 = "[C]" + aAlfa1;     ||codigo de cuenta

 aAlfa1 = #310#2;
 eaAlfa = aAlfa1; |HAZ eQuitaRaros; aAlfa1 = eaAlfa;
 aAlfa1 = (aAlfa1 + aBlanc) % 150;
 aVarAlfa1 = aVarAlfa1 + aAlfa1;   || descripcion

 |HAZ LeeCliPro;
 aVarAlfa1 = aVarAlfa1 + aCif;
 aVarAlfa1 = aVarAlfa1 + aCaja;

 |SI enVer = 1;
     aVarAlfa1 = aVarAlfa1 + "   "; || 7,15 o vacio, y A/P o vacio. Retencion
 |FINSI;

 |HAZ GraboReg;
|FPRC;

