 aAlfa1 = #321#0; |QBF aAlfa1;
 |SI aAlfa1 = ""; |ACABA; |FINSI;

 aAlfa1 = (aAlfa1 + aBlanc) % 110;
 aVarAlfa1 = "[A]" + aAlfa1;     ||codigo de cuenta

 aAlfa1 = #321#1;
 eaAlfa = aAlfa1; |HAZ eQuitaRaros; aAlfa1 = eaAlfa;
 aAlfa1 = (aAlfa1 + aBlanc) % 150;
 aVarAlfa1 = aVarAlfa1 + aAlfa1;   || descripcion
 |HAZ GraboReg;
|FPRC;

