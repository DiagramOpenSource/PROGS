 aAlfa1 = #3#2; |QBF aAlfa1; |SI aAlfa1 = ""; |ACABA; |FINSI;
 aAlfa1 = #3#5; |QBF aAlfa1; |SI aAlfa1 = ""; |ACABA; |FINSI;

 aVarAlfa1 = "[R]" + #3#2;     ||codigo de cuenta
 aVarAlfa1 = aVarAlfa1 + #3#5; ||Cuenta Contrapartida
 |HAZ GraboReg;
|FPRC;

|DEFBUCLE LeeRelacion;
 #3#1;
 ;
 enEmpresa, enPeriodo, aDCuenta;
 enEmpresa, enPeriodo, aHCuenta;
 ;
 NULL, LeeRelacion;
|FIN;

|PROCESO LeeCtaAna;
