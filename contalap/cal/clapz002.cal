|PROCESO HazTodo;

 enEmpresa = #330#2;
 enPeriodo = #330#3;

 |HAZ eVarEmpresa;
 aAlfa1 = #330#2; |QBF aAlfa1; aAlfa1 = ("00000" + aAlfa1) % -105;
 aAlfa2 = #330#3; |QBF aAlfa2; aAlfa2 = ("0"     + aAlfa2) % -101;
 aEmpresa = aAlfa1 + aAlfa2;


 aDef     = eaPathDef + "cacuenta";
 |CARGA_DEF aDef, cacuent@;
 |SI FSalida ! 0;
     |MENAV "    Error Cargado Fichero de Cuentas. Proceso Interrumpido";
     aDef310 = "";
     |ACABA;
 |FINSI;
 aDef310 = aDef;
 aDef     = eaPathDef + "caclipro";
 |CARGA_DEF aDef, caclipr@;
 |SI FSalida ! 0;
     |MENAV "    Error Cargado Fichero de Clientes. Proceso Interrumpido";
     |DESCARGA_DEF #cacuent@; aDef310 = "";
     aDef311 = "";
     |ACABA;
 |FINSI;
 aDef311 = aDef;

 |PATH_DAT #310 eaPathConta;
 |PATH_DAT #311 eaPathConta;

 |HAZ eAnalitica;

 |SI #0#51 = "S";
     |HAZ eCargaRelacion;
 |FINSI;

 nAlgun = 0;
 aFiche = ("ct" + Usuario) % 108 + ".dat";
 aFiche = aBase + aFiche;
 eaOrigen = aFiche;

 |XABRE "WB",aFiche,nHadle;
 |SI FSalida ] 0;
     |SI enSwAna ! 0;
         |HAZ PrimeroAna;
     |FINSI;
     |SI nConta = 0;
         |BUCLE LeeCtas_01;
     |SINO;
         |BUCLE LeeCtas_02;
     |FINSI;
     |SI #0#52 = "S";
         |BUCLE LeeRelacion; ||  Tipo [R]
     |FINSI;
 |FINSI;

 |XCIERRA nHadle;

 |SI aDef311 ! ""; |DESCARGA_DEF #caclipr@; aDef311 = ""; |FINSI;
 |SI aDef310 ! ""; |DESCARGA_DEF #cacuent@; aDef310 = ""; |FINSI;

 |SI nAlgun ! 0;
     eaDestino = eaPathExImp + aEmpresa + "/" + eaFichCtas;
     |HAZ eEnvioFich;
 |FINSI;
|FPRC;


