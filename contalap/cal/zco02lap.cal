     |SI FSalida ! 0;
         |MENAV "    Error Cargado Fichero de Cuentas. Proceso Interrumpido";
         |ACABA;
     |FINSI;

     aDef     = eaPathDef + "cacuetmp";
     |CARGA_DEF aDef, cacuetp@;
     |SI FSalida ! 0;
         |MENAV "    Error Cargado Fichero de Cuentas. Proceso Interrumpido";
         |DESCARGA_DEF #cacuetl@;
         |ACABA;
     |FINSI;

     aAlfa1 = enEmpresa; |QBF aAlfa1; aAlfa1 = ("00000" + aAlfa1) % -105;
     aAlfa2 = enPeriodo; |QBF aAlfa2; aAlfa2 = ("0"     + aAlfa2) % -101;
     aEmpresa = aAlfa1 + aAlfa2;
     aPath = eaPathDatos + aEmpresa + "/";

     |PATH_DAT #369 aPath;
     |PATH_DAT #400 aPath;


     aBuscar1 = "";
     aBuscar2 = "";
     aAste    = (eaCuenta % 102) > " ";
     aAsteN   = eaCuenta % 301;               ||permite Alta
     |SI eaCodNif = ""; aAsteN   = ""; |FINSI;
     aAsteN   = ""; || en modelos por ahora no puedo dar alta
     |SI aAste = "N*";
         nCampo   = Campo;
         |HAZ ConsultaPorNombre;
         Campo    = nCampo;
