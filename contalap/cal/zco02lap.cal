     |SI aAste ! "F*"; |O aAsteN = "+";
         |SI aAsteN = "+";
             |C_V #400#4, 1, "S";
             |C_A #400#1, 1, 34;
         |SINO;
             |C_V #400#4, 1, "N";
             |C_A #400#1, 1, 80;
         |FINSI;
         |ENTLINEAL #2#400, -1, 4, 22, 1, ClaveBaja400N, ClaveAlta400N;
     |SINO;
         |ENTLINEAL #3#400, -1, 4, 22, 1, ClaveBaja400F, ClaveAlta400F;
     |FINSI;
