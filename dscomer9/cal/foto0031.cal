     #0#10="";
     #0#11="";
     #0#12="";
     #0#13="";
     #0#14="";
     #0#15="";
     #0#16="";
     #0#17="";
|FPRC;

|PROCESO imprecab;
     #0#8=#2#0;
     |BUCLE lee_art_alm;
     |PRINT;
     |HAZ actualiza;
|FPRC;

|DEFBUCLE lee_art;
     #2#1;
     125, 2, 3, 126;     || tan solo se pone el n£mero del campo
     #0#6, #0#0, #0#2, #0#4,#0#18;
     #0#7, #0#1, #0#3, #0#5,#0#19;
     ;
     NULL, imprecab;
|FIN;

|PROCESO Tipo3; |TIPO 3;
          |IMPRE 1;
          |SI FSalida = 0;
               |INFOR infor;
               |BUCLE lee_art;
               |PIEINF;
               |FININF;
               |FINIMP;
          |FINSI;
|FPRC;
