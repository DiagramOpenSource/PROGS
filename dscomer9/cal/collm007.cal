|FICHEROS;
     collm007 #0;
     collm008 #1;
     collm006 #6;
     collw001 #10,MANTE;
|FIN;

|VARIABLES;
     nSw = 0;
     aAlfa = "";
|FIN;

|PROCESO Tecla; |TIPO 0;
     |SI FSalida = 0; |Y #10Campo = Contenido;
          |ABRE #6;
          #6#0 = #10#0;
          #6#1 = #10#1;
          #6#2 = #10#2;
          |LEE 121#6=;
          |SI FSalida = 0;
               |SI nSw = 1;
                    #6#12 = "";
               |SINO;
                    #6#12 = #1#3;
               |FINSI;
               |GRABA 020#6a;
          |FINSI;
          |CIERRA #6;

          |SI nSw = 1;
               #10#12 = "";
          |SINO;
               #10#12 = #1#3;
          |FINSI;
          |GRABA 020#10a;
          |PINTA #10#12;
     |FINSI;
|FPRC;

|PROCESO Terrenos;
     |SI nSw = 1;
          |SI #6#12 ! #1#3; |ACABA; |FINSI;
     |FINSI;
     #10#0 = #6#0;
     #10#1 = #6#1;
     #10#2 = #6#2;
     #10#3 = #6#3;
     #10#4 = #6#4;
     #10#5 = #6#5;
     #10#6 = #6#6;
     #10#7 = #6#7;
     #10#8 = #6#8;
     #10#9 = #6#9;
     #10#10 = #6#10;
     #10#11 = #6#11;
     #10#12 = #6#12;
     |GRABA 020#10n;
|FPRC;

|DEFBUCLE Terrenos;
     #6#1;
     ;
     #0#0, #0#1, 001;
     #0#0, #0#1, 999;
     ;
     NULL, Terrenos;
|FIN;

|PROCESO Min;
     #10#0 = #0#0;
     #10#1 = #0#1;
     #10#2 = 1;
|FPRC;

|PROCESO Max;
     #10#0 = #0#0;
     #10#1 = #0#1;
     #10#2 = 999;
|FPRC;

|PROCESO Tipo11; |TIPO 11;
     |SI FSalida = 10; || Ver
          |PUSHV 0101 2380;
          aAlfa = Usuario;
          |NOME_DAT #10 aAlfa; |DELINDEX #10;
          nSw = 1;
          |ABRE #10;
          |BUCLE Terrenos;
          |CIERRA #10;
          |MENSA "0000<ENTER> Desasigna contador del terreno";
          |ENTLINEAL #1#10, 2, 4, 22, 1, Min, Max;
          |DELINDEX #10;
          |POPV;
          |ERROR;
     |FINSI;
     |SI FSalida = 12; ||Asignar
          |PUSHV 0101 2380;
          aAlfa = Usuario;
          |NOME_DAT #10 aAlfa; |DELINDEX #10;
          nSw = 2;
          |ABRE #10;
          |BUCLE Terrenos;
          |CIERRA #10;
          aAlfa = "0000<ENTER> Asigna contador '"+#1#3+"' al terreno";
          |MENSA aAlfa;
          |ENTLINEAL #1#10, 2, 4, 22, 1, Min, Max;
          |DELINDEX #10;
          |POPV;
          |ERROR;
     |FINSI;
|FPRC;
