|PROCESO Exporta;
     |DDEF aAlfa;
     aAlfa = aAlfa + aMas;
     |CARGA_DEF aAlfa, referen@;
     |SI FSalida = 0;
          |ABRE #referen@;
          aCsv = aPath + aFic + ".csv";
          |XABRE "BW", aCsv, nHandle;
          |SI FSalida ] 0;
               |HAZ Campos;
               |HAZ Datos;
               |XCIERRA nHandle;

               aAlfa = aPathDes + aFic + ".csv";
               |SI aIP = "";
                    |COPIA_FICHERO aCsv, aAlfa;
               |SINO;
                    |ENVIA_FICHERO aCsv, aAlfa;
               |FINSI;
          |FINSI;
          |CIERRA #referen@;
          |DESCARGA_DEF #referen@;
     |SINO;
          aAlfa = "0000Error al cargar:" + aMas;
          |MENAV aAlfa;
     |FINSI;
|FPRC;

|PROCESO FormaPago;
     |DELINDEX #8;
     |ABRE #8;

     |ABRE #91;
     |LEE 000#91p;
     |PARA; |SI FSalida = 0; |HACIENDO;
          |HAZ RodacalDesFpago;
          |PARA i = 0; |SI i [ 74; |HACIENDO i = i + 1;
               #8i = #91i;
          |SIGUE;
          #8#76 = aNameFP;
          #8#77 = aNoteFP;
          |SI nVtoFP = 0;
               #8#75 = nVtoFP;
               #8#78 = "balance";
               #8#79 = #91#6;
               #8#80 = 0;
               |GRABA 000#8n; || sin errores para no duplicar
          |SINO;
               |SI nVtoFP = 1;
                    #8#75 = nVtoFP;
                    #8#78 = "balance";
                    #8#79 = #91#6;
                    #8#80 = 0;
                    |GRABA 000#8n; || sin errores para no duplicar
               |SINO;
                    |PARA i = 6; |SI i [ 59; |HACIENDO i = i + 1;
                         |SI #91i = 0; |SAL; |FINSI;
                         nContaVto = i - 5;
                         nAntes = i - 1;
                         |SI i = 6;               ||Primero
                              #8#75 = nContaVto;
                              #8#78 = "procent";
                              #8#79 = #91#6;
                              #8#80 = 100 / #91#3;
                         |SINO;
                              |SI nContaVto ! nVtoFP;   ||Siguientes
                                   #8#75 = nContaVto;
                                   #8#78 = "procent";
                                   #8#79 = #91nAntes + #91#4;
                                   #8#80 = 100 / #91#3;
                              |SINO;            ||Ultimo
                                   #8#75 = 999;
                                   #8#78 = "balance";
                                   #8#79 = #91nAntes + #91#4;
                                   #8#80 = 0;
                              |FINSI;
                         |FINSI;
                         |GRABA 000#8n; || sin errores para no duplicar
                    |SIGUE;
               |FINSI;
          |FINSI;
          |LEE 000#91s;
     |SIGUE;

     |CIERRA #91;
     |CIERRA #8;

     aMas = "odoo0008";
     |HAZ Exporta;
|FPRC;

|PROCESO T3; |TIPO 3;
     |SI #0#0 ! "SI"; |ACABA; |FINSI;

     |HAZ EsInforpor; nSwInforpor = FSalida;
     |HAZ EsRodacal; nSwRodacal = FSalida;

     aIP = ""; |IP_REMOTO aIP;

     |ABRE #1; |LEE 000#1p; |CIERRA #1;

     aDobleComi = &34;
     aSepara = ";";
     aFin = &13 + &10;

     |DFICE aAlfa;
     aPath = aAlfa + Usuario + "/odoo/";
     |MKDIR aPath;

     aPathDes = #1#1; |QBF aPathDes;
     |SI aPathDes = "";
          |MENAV "0000Parámetros no configurados...";
          |ACABA;
     |FINSI;

     aAlfa = aPathDes % -101;
     |SI aAlfa ! "/"; |Y aAlfa ! "\"; aPathDes = aPathDes + "/"; |FINSI;
     |RMKDIR aPathDes;

     |ABRE #4;
     |LEE 000#4p;
     |PARA; |SI FSalida = 0; |HACIENDO;
          |SI #4#2 = "*";
               aMas = #4#0; |QBF aMas;
               aFic = aMas;
               |SI aFic = "agifa091";
                    |SI nSwRodacal = 0;
                         |HAZ RodacalFormaPago;
                    |SINO;
                         |HAZ FormaPago;
                    |FINSI;
               |SINO;
                    |HAZ Exporta;
               |FINSI;
               |SI aFic = "agifa019";
                    |HAZ Categorias;
               |FINSI;
          |FINSI;
          |LEE 000#4s;
     |SIGUE;

     |SI nSwInforpor = 0;
          |HAZ InforporDirUsu;
     |FINSI;
     |SI nSwRodacal = 0;
          |HAZ RodacalCategoria;
     |FINSI;

     |HAZ RmDir;
|FPRC;
