                    |SI FSalida = 0; |SAL; |FINSI;
                    nConta = nConta + 1;
               |SIGUE;
               aNom = aAlfa;
          |SINO;
               |GRABA 020#3n;
          |FINSI;

          |SI nCampo ! nUltimo;
               aNom = aNom + aSepara;
          |SINO;
               |SI aMas = "agifa024"; |O aMas = "agifa112";
                    aNom = aNom + aSepara + "modo_pago";
