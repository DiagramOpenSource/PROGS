|PROCESO RmDir;
     aAlfa = ""; |DIRECTORIO aAlfa; |QBF aAlfa;
     aAlfa = aAlfa + "bin/agirm -r " + aPath;
     |SYSTEM aAlfa;
|FPRC;

|PROCESO CorrigePuntoComa;
     aDato = "";
     aLon = aAlfa % A0;
     |PARA i = 0; |SI i [ aLon; |HACIENDO i = i + 1;
          nPos = (i + 1) * 100 + 1;
          aCar = aAlfa % nPos;
          |SI aCar = ";"; aCar = ","; |FINSI;
          aDato = aDato + aCar;
     |SIGUE;
     aAlfa = aDato;
|FPRC;

|PROCESO CorrigeBarra;
     aDato = "";
     aLon = aAlfa % A0;
     |PARA i = 0; |SI i [ aLon; |HACIENDO i = i + 1;
          nPos = (i + 1) * 100 + 1;
          aCar = aAlfa % nPos;
          |SI aCar = "/"; aCar = "|"; |FINSI;
          aDato = aDato + aCar;
     |SIGUE;
     aAlfa = aDato;
|FPRC;

|PROCESO CorrigeDobleComillas;
     aDato = "";
     aLon = aAlfa % A0;
     |PARA i = 0; |SI i [ aLon; |HACIENDO i = i + 1;
          nPos = (i + 1) * 100 + 1;
          aCar = aAlfa % nPos;
          |SI aCar = aDobleComi; aCar = aDobleComi + aDobleComi; |FINSI;
          aDato = aDato + aCar;
     |SIGUE;
     aAlfa = aDato;
|FPRC;

|PROCESO Nom;
     aCarAnt = "";
     aAlfa = "";
     aNom = aNom < "";
     aLon = aNom % A0;
     |PARA i = 1; |SI i [ aLon; |HACIENDO i = i + 1;
          nPos = i * 100 + 1;
          aCar = aNom % nPos;
          |SI aCar = "¤"; aCar = "ny"; |FINSI;
          |SI aCar < "0";
               aCar = "_";
          |SINO;
               |SI aCar > "9"; |Y aCar < "a";
                    aCar = "_";
               |SINO;
                    |SI aCar > "z"; aCar = "_"; |FINSI;
               |FINSI;
          |FINSI;
          |SI aCarAnt ! "_"; |O aCar ! "_";
               aAlfa = aAlfa + aCar;
          |FINSI;
          aCarAnt = aCar;
     |SIGUE;
     aNom = aAlfa;

     ||quita subguion final
     aLon = aAlfa % A0;
     nPos = aLon * 100 + 1;
     aCar = aAlfa % nPos;
     |SI aCar = "_";
          nPos = aLon + 99;
          aNom = aAlfa % nPos;
     |SINO;
          aNom = aAlfa;
     |FINSI;

     || quita el subguion inicial
     aLon = aNom % A0;
     |PARA i = 1; |SI i [ aLon; |HACIENDO i = i + 1;
          nPos = i * 100 + 1;
          aAlfa = aNom % nPos;
          |SI aAlfa = "_";
               aNom = aNom - "_";
          |SINO;
               |SAL;
          |FINSI;
     |SIGUE;

     || Si comiemsa por un numero le a¤ado una c
     aAlfa = aNom % 101;
     |SI aAlfa ] "0"; |Y aAlfa [ "9";
          aNom = "c" + aNom;
     |FINSI;

     ||SI aNom = ""; aNom = "x"; |FINSI;
|FPRC;

|PROCESO Campos;
