               |XCIERRA nHandle;

               aAlfa = aPathDes + aFic + ".csv";
               |SI aIP = "";
                    |COPIA_FICHERO aCsv, aAlfa;
               |SINO;
                    |ENVIA_FICHERO aCsv, aAlfa;
               |FINSI;
          |FINSI;
     |SIGUE;


     aFic = "categorias";
     aCsv = aPath + aFic + ".csv";
     |XABRE "BW", aCsv, nHandle;
     |SI FSalida ] 0;
          aAlfa = "categoria" + aFin;
          |XGRABA nHandle, aAlfa;
          |LEE 000#7p;
          |PARA; |SI FSalida = 0; |HACIENDO;
               aAlfa = #7#0; |QBF aAlfa;
               aDato = aAlfa + aFin;
               |XGRABA nHandle, aDato;
               |LEE 000#7s;
          |SIGUE;

          |XCIERRA nHandle;

          aAlfa = aPathDes + aFic + ".csv";
          |SI aIP = "";
               |COPIA_FICHERO aCsv, aAlfa;
          |SINO;
               |ENVIA_FICHERO aCsv, aAlfa;
          |FINSI;
     |FINSI;

     |CIERRA #7;
|FPRC;

|PROCESO InforporDirUsu;
     |DDEF aDef;
     aAlfa = aDef + "infom003";
     |CARGA_DEF aAlfa, refere1@;
     |SI FSalida = 0;
          aAlfa = aDef + "infom008";
          |CARGA_DEF aAlfa, refere2@;
          |SI FSalida = 0;
               |DELINDEX #refere2@;
               |ABRE #30;
               |ABRE #refere1@;
               |ABRE #refere2@;
               |LEE 000#refere1@.p;
               |PARA; |SI FSalida = 0; |HACIENDO;
                    #30#0 = #refere1@#2;
                    #30#1 = 1;
                    |LEE 000#30];
                    |PARA; |SI FSalida = 0; |Y #30#0 = #refere1@#2; |HACIENDO;
                         #refere2@#0 = #refere1@#4;
                         #refere2@#1 = #refere1@#2;
                         #refere2@#2 = #30#1;
                         |GRABA 000#refere2@.n;   || sin errores, poque se repiten y no deberia
                         |LEE 000#30s;
                    |SIGUE;
                    |LEE 000#refere1@.s;
               |SIGUE;
               |CIERRA #refere1@;
               |CIERRA #refere2@;
               |CIERRA #30;
               |DESCARGA_DEF #refere2@;
          |FINSI;
          |DESCARGA_DEF #refere1@;
     |FINSI;

     aMas = "infom008"; |QBF aMas;
     aFic = "usu_dir";
     |HAZ Exporta;
|FPRC;

