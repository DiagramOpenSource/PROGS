|PROCESO RodacalDesFpago;
     aNoteFP = (("000"+#91#3)%-103)+"-"+(("000"+#91#2)%-103)+"-"+ (("000"+#91#4)%-103);
     aNoteFP = (("00"+#91#0)%-102) + "-" + aNoteFP;
     |SI #91#2 = 0; |Y #91#4 = 0;
          nVtoFP = 0;
          aNameFP = #91#1;
     |SINO;
          nVtoFP = 0;
          |PARA i = 6; |SI i [ 59; |HACIENDO i = i + 1;
               |SI #91i = 0; |SAL; |FINSI;
               nVtoFP = nVtoFP + 1;
          |SIGUE;

          aNameFP = "";
          aDias = "";
          |PARA i = 6; |SI i [ 59; |HACIENDO i = i + 1;
               |SI #91i = 0; |SAL; |FINSI;
               |SI i = 6;                    || Primero
                    nDias = #91#6;
               |SINO;
                    nAntes = i - 1;
                    nDias = #91nAntes + #91#4;
               |FINSI;

               |SI aDias = "";
                    aDias = nDias;
               |SINO;
                    aDias = aDias + ", " + nDias;
               |FINSI;
          |SIGUE;
          aNameFP = aDias + " Dias";
     |FINSI;
     |QBF aNameFP;
|FPRC;

|PROCESO RodacalFormaPago;
     |DELINDEX #8;
     |ABRE #8;
     || Unifica las 2 empresas
     |PARA nEmpresa = 1; |SI nEmpresa [ 2; |HACIENDO nEmpresa = nEmpresa + 1;
          aEmp = ("00000" + nEmpresa) % -105;
          |DBASE aAlfa;
          aAlfa = aAlfa + "fich/" + aEmp + "/";
          |PATH_DAT #91 aAlfa;
          |ABRE #91;
          |LEE 000#91p;
          |PARA; |SI FSalida = 0; |HACIENDO;
               |HAZ RodacalDesFpago;
               |PARA i = 0; |SI i [ 74; |HACIENDO i = i + 1;
                    #8i = #91i;
               |SIGUE;
               #8#76 = aNameFP;
               #8#77 = aNoteFP;
               |SI nVtoFP = 0;
                    #8#75 = nVtoFP;
                    #8#78 = "balance";
                    #8#79 = #91#6;
                    #8#80 = 0;
                    |GRABA 000#8n; || sin errores para no duplicar
               |SINO;
                    |SI nVtoFP = 1;
                         #8#75 = nVtoFP;
                         #8#78 = "balance";
                         #8#79 = #91#6;
                         #8#80 = 0;
                         |GRABA 000#8n; || sin errores para no duplicar
                    |SINO;
                         |PARA i = 6; |SI i [ 59; |HACIENDO i = i + 1;
                              |SI #91i = 0; |SAL; |FINSI;
                              nContaVto = i - 5;
                              nAntes = i - 1;
                              |SI i = 6;               ||Primero
                                   #8#75 = nContaVto;
                                   #8#78 = "procent";
                                   #8#79 = #91#6;
                                   #8#80 = 100 / #91#3;
                              |SINO;
                                   |SI nContaVto ! nVtoFP;   ||Siguientes
                                        #8#75 = nContaVto;
                                        #8#78 = "procent";
                                        #8#79 = #91nAntes + #91#4;
                                        #8#80 = 100 / #91#3;
                                   |SINO;            ||Ultimo
                                        #8#75 = 999;
                                        #8#78 = "balance";
                                        #8#79 = #91nAntes + #91#4;
                                        #8#80 = 0;
                                   |FINSI;
                              |FINSI;
                              |GRABA 000#8n; || sin errores para no duplicar
                         |SIGUE;
                    |FINSI;
               |FINSI;
               |LEE 000#91s;
          |SIGUE;
          |CIERRA #91;
     |SIGUE;
     |CIERRA #8;

     aMas = "odoo0008";
     |HAZ Exporta;


     |DBASE aAlfa;
     aEmp = ("00000" + #48#0) % -105;
     aAlfa = aAlfa + "fich/" + aEmp + "/";
