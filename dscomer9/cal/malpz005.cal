|FICHEROS;
     malpz005 #0;
     malpe141 #141;
|FIN;

|VARIABLES;
     aFic = "";
     aTab = "";
     aAlfa = "";
     nHandle = 0;
     CRLF = "";
|FIN;

|PROCESO malpe141;
     aAlfa = #141#40
     |XGRABA nHandle, aAlfa;
     aAlfa = aTab + #141#1 + aTab + #141#0 + CRLF;
     |XGRABA nHandle, aAlfa;
|FPRC;

|DEFBUCLE malpe141;
     #141#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, malpe141;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     CRLF = &13; CRLF = CRLF + &10;
     aTab = &9;
     aFic = #0#0;
     |QBF aFic;
     |XABRE "WB", aFic, nHandle;
     |SI FSalida ] 0;
          |BUCLE malpe141;
          |XCIERRA nHandle;
     |SINO;
          aAlfa = "0000Error al crear " + aFic;
          |MENAV aAlfa;
     |FINSI;
|FPRC;
