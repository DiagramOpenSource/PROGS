|FICHEROS;
     coima022 #0;
|FIN;

|VARIABLES;
     aAlfa = "";
|FIN;

|PROCESO DSCorreo; |TIPO 0;
     |QUE_SISTEMA aAlfa;
     |SI aAlfa = "ESBSD";|Y #0#8 = "S";
          |MENAV "    Configuracion no permitida, servidor UNIX. Recomendado: [F]";
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO SoloFija; |TIPO 7;
     |SI #0#8 = "F";
          aAlfa = #0#7; |QBF aAlfa;
          |SI aAlfa = "Automatico";
               #0#7 = "";
               |PINTA #0#7;
          |FINSI;
          |C_M #0#7, 1, "S";
     |SINO;
          |C_M #0#7, 1, "N";
          #0#7 = "Automatico";
          |PINTA #0#7;
     |FINSI;
|FPRC;

|PROCESO Asterisco; |TIPO 0;
     aAlfa = "*" * 30;
     |PINTA 1042, aAlfa;
|FPRC;

|PROCESO PathLocal; |TIPO 7;
     aAlfa = #0#4;
     |QBF aAlfa;
     |SI aAlfa = "";
          |DBASE aAlfa;
          |QBF aAlfa;
          aAlfa = aAlfa + "correo/";
          #0#4 = aAlfa;
          |PINTA #0#4;
     |FINSI;
|FPRC;

|PROGRAMA;
     |CLS;
     |CABEZA "Parametros de DSCorreo";
     |ABRE #0;
     |LEE 101#0=;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     |PINPA #0#0;
     |PINDA #0#0;
     |HAZ Asterisco;
     |ENDATOS #1#0;
     |SI FSalida = 0; |GRABA 020#0a; |FINSI;
     |CIERRA #0;
|FPRO;
