|FICHEROS;
     tagm0080 #0;
     tagm0085 #85;
     referen@;
|FIN;

|VARIABLES;
     &Tempo = "";
     aAlfa = "";
     &ser_ped = "";
     &num_ped = 0;
 {-1}aMenu  = "";
     aMenu1 = "2400";
     aMenu2 = "1";
     aMenu3 = "Opciones:  ";
     aMenu4 = "ST";
     aMenu5 = " Salir ";
     aMenu6 = " Tiempos ";
|FIN;

|PROCESO Min;
     #85#0 = #0#0;
     #85#1 = #0#1;
     #85#2 = 1;
|FPRC;

|PROCESO Max;
     #85#0 = #0#0;
     #85#1 = #0#1;
     #85#2 = 5;
|FPRC;

|PROGRAMA;
     |ABRE #0;
     #0#0 = ser_ped;
     #0#1 = num_ped;
     |LEE 000#0=;
     |SI FSalida = 0;
          |PINPA #0#0;
          |PINDA #0#0;
          |PARA; |SI; |HACIENDO;
               |MENU aMenu;
               |SI FSalida = 2;
                    |DDEF aAlfa;
                    aAlfa = aAlfa + "tagm0085";
                    |CARGA_DEF aAlfa, referen@;
                    |SI FSalida = 0;
                         |HAZ CreaTempo;
                         |NOME_DAT #referen@#Tempo#;
                         |DELINDEX #referen@;
                         |ABRE #referen@;
                         |ABRE #85;
                         #85#0 = #0#0;
                         #85#1 = #0#1;
                         #85#2 = 1;
                         |LEE 000#85];
                         |PARA; |SI FSalida = 0; |Y #85#0 = #0#0; |Y #85#1 = #0#1; |HACIENDO;
                              |SALVA_DATOS #85;
                              |REPON_DATOS #referen@;
                              |GRABA 020#referen@.n:
                              |LEE 000#85s;
                         |SIGUE;
                         |LEE 000#referen@.p;
                         |CONSULTA_CLAVE #1#referen@;

                         |CIERRA #referen@;
                         |DELINDEX #referen@;
                         |HAZ BoraTempo;
                         |DESCARGA_DEF #referen@;
                    |FINSI;
/*

                    |ABRE #85;
                    #85#0 = #0#0;
                    #85#1 = #0#1;
                    #85#2 = 1;
                    |LEE 000#85];
                    |CONSULTA_F_CLAVE #1#85, Min, Max;
                    |CIERRA #85;
*/
               |SINO;
                    |SAL;
               |FINSI;
          |SIGUE;
     |SINO;
          |MENAV "0000No existen procesos para este pedido";
     |FINSI;
     |CIERRA #0;
|FPRO;
