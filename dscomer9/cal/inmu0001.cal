|FICHEROS;
     inmu0001 #0;
|FIN;

|VARIABLES;
     &enError = 0;
     &eaAlfa = "";
|FIN;

|PROGRAMA;
     |SI PARAMETRO = "alta";
          |ABRE #0;
          #0#0 = eaAlfa;
          |LEE 000#0=;
          |SI FSalida ! 0;
               |GRABA 020#0b;
               |PINPA #0#0;
               |PINDA #0#0;
               |CONFI "0000SConfirma Alta (S/N):";
               |SI FSalida = 0;
                    |ENDATOS #1#0;
               |FINSI;
               |SI FSalida = 0;
                    enError = 0;
                    |GRABA 020#0a;
               |SINO;
                    |BORRA 020#0c;
                    enError = 1;
               |FINSI;
          |FINSI;
          |CIERRA #0;
     |SINO;
          |MANTE #0;
     |FINSI;
|FPRO;
