     #agis0003#10 = AS;
     |GRABA 020#agis0003.a;
     |PIEINF;
     |FININF;
|FINSI;
|LIBERA #agis0003;
|FCAL;

|DEFBUCLE agifa523m0;
     #agis0003#1;
     2,7,9,10;
     " ", ser , doc , 01 , "      " , "01.01.1900" , -99999999 , "N" ;
     "z", ser , doc , 99 , "zzzzzz" , "31.12.2999" , 999999999 , "N" ;
     be;
     NULL, recimpr1;
|FIN;

|PROCESO ImpreACta;
/*
     |SI eaDivisa = "EUR"; |O eaDivisa = "";
          EURODB = 2;
          EURO = 166.386;
          EURODV = 0;
     |SINO;
          EURODB = 0;
          EURO = 1 / 166.386;
          EURODV = 2;
     |FINSI;
*/

 |ABRE #agifa024;
 |IMPRE 0;
 |SI 0 = FSalida;
      |BUCLE agifa523m0;
      |FINIMP;
 |FINSI;
 |CIERRA #agifa024;
|FPRC;

|PROCESO mirainf021;
     |ABRE #agifa007;
     #agifa007#26 = #agis0003#19;
     |LEE 001#agifa007.=;
     |SI FSalida = 0;
          informe = #agifa007#19; |QBF informe;
     |SINO;
          informe = informe1;
     |FINSI;
     |CIERRA #agifa007;
|FPRC;
