|FICHEROS;
     dsm00032 #0;
     agift555 #30;
|FIN;

|VARIABLES;
     aParam    = "";
     &enCaja   = 0;
     path      = "";
     &qFichero  = "";
|FIN;

|PROCESO Tecla; |TIPO 11;
     |SI FSalida = 10; |Y #0#0 = "I";
          |PUSHV 0501 2380;
          |BLANCO 1118 1549;
          |CUADRO 1118 1549;
          |PINTA 1322, "N§ CAJA:";
          E_sup = 99;
          E_inf = 00;
          enCaja = 1331 ? 1;
          |SI FSalida = 0;
               aParam = "agift550;" + #0#5;
               |SUB_EJECUTA_NP aParam;
          |FINSI;
          |ERROR;
          |POPV;
     |FINSI;
     |SI FSalida = 13; |Y #0#0 = "E";
          |HAZ reexpor;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO Min;
     #0#0 = "E";
     #0#1 = "01.01.0000";
     #0#2 = 001;
|FPRC;

|PROCESO Max;
     #0#0 = "I";
     #0#1 = "31.12.9999";
     #0#2 = 999;
|FPRC;

|PROGRAMA;
     |CLS;
     |CABEZA "Historico Impor/Expor TPV";
     |ENTLINEAL #1#0, 1, 2, 22, 1, Min, Max;
|FPRO;

|PROCESO reexpor;
     |ABRE #30;
     |LEE 001#30p;
          |SI FSalida ! 0;
               |CIERRA #30;
               |MENAV "0000No se encuentra el Control Path";
               |ERROR;
               |ACABA;
          |FINSI;
          |CIERRA #30;
          path = #30#2;  |QBF path;
          qFichero = path + #0#5;
          |QBF qFichero;
          |PUSHV 0501 2380;
          |CONFI "0000SEnviar Exportacion a central ahora: ";
          |SI FSalida = 0;
               |MENAV "0000Atencion!! Debe estar conectado a internet";
               |SUB_EJECUTA "dsm00205.tab";
          |FINSI;
          |POPV;
|FPRC;
