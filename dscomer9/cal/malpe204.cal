|FICHEROS;
     malpe204 #0;
     malpe004 #4;
     malpe005 #5;
     malpe205 #6;   ||Tmp
|FIN;

|VARIABLES;
     nLin = 0;
     aAlfa = "";
|FIN;

|PROCESO Lineas;
     |SI #5#3 ] #0#0; |Y #5#3 [ #0#1;
          nLin = nLin + 1;
          #6#0 = nLin;
          #6#1 = #4#9;
          #6#2 = #4#0;
          #6#3 = #4#1;
          #6#4 = #5#2;
          #6#5 = #5#30;
          #6#6 = #5#5;
          #6#7 = #5#31;
          |GRABA 020#6n;
     |FINSI;
|FPRC;

|PROCESO Cabecera;
     |SI #4#15 = "S";
          |ERROR;
     |FINSI;
|FPRC;

|DEFBUCLE malpe004;
     #4#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, Cabecera, Lineas;
|FIN;

|PROCESO Min;
     #6#0 = 1;
|FPRC;

|PROCESO Max;
     #6#0 = 999999;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     aAlfa = Usuario;
     |NOME_DAT #6 aAlfa; |DELINDEX #6;
     |ABRE #6;
     |BUCLE malpe004;
     |CIERRA #6;

     |SI nLin = 0;
          |MENAV "0000No hay registros...";
     |SINO;
          |ENTLINEAL #1#6, 1, 4, 22, 1, Min, Max;
     |FINSI;

     |DELINDEX #6;
|FPRC;

|PROGRAMA;
     |MANTE #0;
|FPRO;
