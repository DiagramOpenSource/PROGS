|FICHEROS;
     golis006 #0;
     guerw024 #1; ||TMP
     guerm018 #18;
     guerm019 #19;
|FIN;

|VARIABLES;
     aInforme  = "golis006";
     aAlfa     = "";
     nHay      = 0;
|FIN;

|PROCESO Tmp;
     |DDEFECTO #19;
     #19#0 = #1#0;
     |LEE 000#19=;

     nHay = 1;
     |PRINT;
|FPRC;

|DEFBUCLE Tmp;
     #1#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, Tmp;
|FIN;

|PROCESO guerm018;
     |DDEFECTO #1;
     #1#0 = #18#0;
     #1#1 = #18#1;
     |LEE 101#1=;
     |SI FSalida ! 0; |GRABA 020#1b; |FINSI;

     |SI #18#20 = 0;
          #1#2 = #1#2 + #18#5;              ||Horas
          #1#3 = #1#3 + (#18#5 * #18#16);   ||T.Horas
     |FINSI;
     #1#4 = #1#3/#1#2;                      ||P.Medio
     #1#5 = #1#5 + #18#20;                  ||Toneladas
     #1#6 = #1#6 + (#18#20*#18#21);         ||T.Toneladas
     #1#7 = #1#6/#1#5;                      ||P.Medio

     |GRABA 020#1a;
     |LIBERA #1;
|FPRC;

|DEFBUCLE guerm018;
     #18#1;
     10, 11, 12, 13;
     #0#0, #0#2, 0001, 001, #0#4, #0#6, #0#8, #0#10
     #0#1, #0#3, 9999, 999, #0#5, #0#7, #0#9, #0#11;
     ;
     NULL, guerm018;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |HAZ MiraLogos; || Copia Logos para el Crystal
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR aInforme;
          |SI FSalida = 0;
               aAlfa = Usuario; |QBF aAlfa;
               |NOME_DAT #1 aAlfa;
               |DELINDEX #1;
               |ABRE #1;
               |BUCLE guerm018;
               |CIERRA #1;

               |ABRE #19;
               |BUCLE Tmp;
               |CIERRA #19;

               |SI nHay ! 0;
                    |PIEINF;
               |FINSI;

               |FININF;
               |DELINDEX #1;
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;
