|FICHEROS;
     guerm019 #0;        ||Mant. Maq/Art. (CAB)
     guerm020 #20;       ||Mant. Maq/Art. (LIN)
     goparame #100;      ||Parametros Construccion
|FIN;

|VARIABLES;
     &eaElemento = "";
     aMensaje = "";
     aTipoArt = "";
     nModo = 0;
     nForma = 0;
|FIN;

|PROCESO TipoLin;
     |ABRE #goparame;
     |LEE 000#goparame.p;
     |SI FSalida ! 0;
          |DDEFECTO #goparame;
     |FINSI;
     |SI #guerm020#3 = #goparame#5; |O #guerm020#3 = #goparame#7; |O #guerm020#3 = #goparame#47;
          |SI #guerm020#3 = #goparame#5;
               ||Mano obra
               aTipoArt = "O";
          |SINO;
               |SI #guerm020#3 = #goparame#7;
                    ||Maquinaria
                    aTipoArt = "M";
               |SINO;
                    ||Sin Naturaleza
                    aTipoArt = "S";
               |FINSI;
          |FINSI;
     |SINO;
          aMensaje = "0000El tipo de linea No es Mano de Obra [" + #goparame#5 + "], Maquinaria [" + #goparame#7 + "] o Sin Naturaleza [" + #goparame#47 + "]";
          |MENAV aMensaje;
          |ERROR;
     |FINSI;
     |CIERRA #goparame;
|FPRC;

|PROCESO Tipo8; |TIPO 8;
     |HAZ DecimalesBase;
|FPRC;

|PROCESO Tipo2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     nForma = 0 +. 1;
     |SI nModo = 3;
          |HAZ EsMarcosJi;
          |SI FSalida = 0;
               eaElemento = #0#0;
               |SUB_EJECUTA_NP "guerm061;BAJA";
          |FINSI;
     |FINSI;

     |SI nForma = 1;
          |HAZ EsMarcosJi;
          |SI FSalida = 0;
               eaElemento = #0#0;
               |SUB_EJECUTA_NP "guerm061";
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
|FPRC;

|PROCESO Tipo20; |TIPO 20;
     |SI FSalida = "OPCION";
          |HAZ EsMarcosJi;
          |SI FSalida = 0;
               FSalida = "Tarjeta";
          |SINO;
               FSalida = "";
          |FINSI;
     |SINO;
          |HAZ EsMarcosJi;
          |SI FSalida = 0;
               eaElemento = #0#0;
               |SUB_EJECUTA_NP "guerm061";
          |FINSI;
     |FINSI;
|FPRC;
