     nBase = nBase + #referen@#30;
     nToIva = nToIva + #referen@#31;
     nTota = nTota + #referen@#32;
|FPRC;

|DEFBUCLE SumaLineas;
     #referen@#1003;
     ;
     #1#66, #1#0, 001;
