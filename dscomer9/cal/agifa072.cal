|PROCESO apendc; |TIPO 6;
     nModo = (FEntrada / 100) ! 0;
     nGuarda = FEntrada;
     prov  = #1#2;
     fpago = #1#11;
     dto   = #1#4;
     dtoa  = #1#5;
     dtop  = #1#6;
     |SI FSalida = 12;          || <F4> Albaranes sin facturar
          |DDEF aDef;
          aDef = aDef + "agifa077";
          |CARGA_DEF aDef, agifa077@;
          |SI FSalida = 0;
               |HAZ CreaTempo; aTempo100 = Tempo;
               |NOME_DAT #100 aTempo100; |DELINDEX #100;
               |ABRE #100;
               |SI Campo = 17; ||Llamado desde campo serie
                    aDSerie = "     ";
                    aHSerie = "zzzzz";
               |SINO; ||Llamado desde pedido
                    aDSerie = #0#17;
                    aHSerie = #0#17;
               |FINSI;
               nDeci_TotVis = nDeci_TotVisF;
               |BUCLE leealbs;
               |LEE 000#100p;
               |SI Campo = 17; ||Llamado desde campo serie
                    |C_V #100#6, 1, "S";
                    |CONSULTA_CLAVE #1#100;
                    |SI FSalida = 0; ||Intro
                         #0#17 = #100#50; ||Pongo Serie seleccionada
                         #0#2 = #100#0;   ||Pongo Npedido
                         |PINTA #0#17;
                         |PINTA #0#2;
                         |PTEC 802;
                    |SINO;
                         |ERROR;
                    |FINSI;
               |SINO;
                    |CONSULTA_CLAVE #1#100; ||Llamado desde pedido
                    |SI FSalida = 0; ||Intro
                         #0#2  = #100#0; ||Pongo Npedido
                         |PINTA #0#2;
                    |SINO;
                         |ERROR;
                    |FINSI;
               |FINSI;
               |CIERRA #100;
               |DELINDEX #100; Tempo = aTempo100; |HAZ BoraTempo;
               |DESCARGA_DEF #agifa077@;

               |ABRE #2;
               #2#50 = #0#17;
               #2#0 = #0#2;
               |LEE 000#2=;
               |CIERRA #2;
          |FINSI;
          |ACABA;
     |FINSI;
     |SI FSalida = 13;           || <F5> Nuevo Alba
          |SI nModo = 1;
               |ABRE #2;
               enSwMenu = 1;
               #2#50 = #0#17;         ||Serie
               |HAZ ContaAC7;
               |CIERRA #2;
               #0#2 = #2#0;           ||Conta
          |FINSI;
          aAlfa = "dsz99957;N" + #0#17 + #0#2;
          eaProve = #1#2;
          eaFPago = #1#11;
          enDto1 = #1#4;
          enDto2 = #1#5;
          enDto3 = #1#6;
          eaSerFacCom = #0#16;
          eaNumFacCom = #0#0;

          |SUB_EJECUTA aAlfa;
          |SI enSalida ! 0; |ERROR; |FINSI;
          FEntrada = nGuarda;
          |ACABA;
     |FINSI;
     |SI Campo = 2;
          |ABRE #2;
          #2#50 = #0#17;
          #2#0 = #0#2;
          |LEE 000#2=;
          |SI FSalida ! 0; |Y #0#2 ! 0;
               aAlfa = "dsz99957;N" + #0#17 + #0#2;
               eaProve = #1#2;
               eaFPago = #1#11;
               enDto1 = #1#4;
               enDto2 = #1#5;
               enDto3 = #1#6;
               eaSerFacCom = #0#16;
               eaNumFacCom = #0#0;
               |SUB_EJECUTA aAlfa;
               |SI enSalida ! 0; |ERROR; |FINSI;
          |FINSI;
          |CIERRA #2;
     |FINSI;

     FEntrada = nGuarda;
|FPRC;

|PROCESO CompDiv; |TIPO 0;
     |ABRE #142;
     |LEE 000#142p;
     |CIERRA #142;
     |SI #142#128 = "S"; |Y #0#17 ! #1#66;
          |MENAV "0000Las series de Factura/Albaran no se corresponden";
          |ERROR;
          |ACABA;
     |FINSI;

     |ABRE #agifa007;
     #agifa007#26 = #0#16;
     |LEE 010#agifa007.=;
     serie_fac = #agifa007#39;
     #agifa007#26 = #0#17;
     |LEE 010#agifa007.=;
     serie_alb = #agifa007#39;
     |CIERRA #agifa007;
     |SI serie_alb ! serie_fac;
          |SI serie_alb = "S";
               |MENAV "0000La serie de la factura no es de divisas";
          |SINO;
               |MENAV "0000La serie de la factura es de divisas";
          |FINSI;
          |ERROR;
     |SINO;
          eaSerAlbUlt = #0#17;
     |FINSI;
|FPRC;

