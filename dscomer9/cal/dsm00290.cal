|FICHEROS;
     dsm00290 #0;
|FIN;

|VARIABLES;
     &eInfClave = "";
     &eInfNombre = "";
     nTecla = 0;
|FIN;

|PROCESO Min290;
     #0#0 = eInfClave;
     #0#1 = "        ";
|FPRC;

|PROCESO Max290;
     #0#0 = eInfClave;
     #0#1 = "zzzzzzzz";
|FPRC;

|PROCESO NomInf290_0;
     nTecla = FSalida;
     |ABRE #0;
     #0#0 = eInfClave;
     #0#1 = eInfNombre;
     |LEE 000#0=;
     |SI nTecla = 13; || F5
          |SI FSalida = 0;
               |CONFI "0000NBorrar nombre informe S/N:";
               |SI FSalida = 0;
                    eInfNombre = "";
                    |BORRA 000#0a;
               |FINSI;
          |FINSI;
          |ERROR;
     |SINO;
          |SI FSalida ! 0;
               |CONFI "0000NGrabar nuevo nombre informe S/N:";
               |SI FSalida = 0;
                    |GRABA 020#0n;
               |FINSI;
          |FINSI;
     |FINSI;
     |CIERRA #0;
|FPRC;

|PROCESO NomInf290_66;
     |ABRE #0;
     #0#0 = eInfClave;
     #0#1 = eInfNombre;
     |LEE 000#0];
     |SI FSalida ! 0; |O #0#0 ! eInfClave;
          #0#0 = eInfClave;
          #0#1 = eInfNombre;
          |GRABA 000#0n;
     |FINSI;
     |CONSULTA_F_CLAVE #1#0, Min290, Max290;
     |SI FSalida = 0;
          eInfNombre = #0#1;
     |SINO;
          |ERROR;
     |FINSI;
     |CIERRA #0;
|FPRC;
