|FICHEROS;
   agifa703 #0;
|FIN;

|VARIABLES;
   Cont = 0;
|FIN;

|PROCESO MiraEnlace;
   |SI #agifa703#Campo# = "X";
      |PARA Cont = 0; |SI Cont [ 5; |HACIENDO Cont = Cont + 1;
         |SI Cont = 5; Cont = 9; |FINSI;
         |SI Campo ! Cont; #agifa703#Cont# = " "; |PINTA #agifa703#Cont#; |FINSI;
         |SI Cont = 9; Cont = 5; |FINSI;
      |SIGUE;
   |FINSI;
|FPRC;

|PROCESO MiraCampo;
   |SI #agifa703#Campo# = "X";
      |PARA Cont = 5; |SI Cont [ 8; |HACIENDO Cont = Cont + 1;
         |SI Campo ! Cont; #agifa703#Cont# = " "; |PINTA #agifa703#Cont#; |FINSI;
      |SIGUE;
   |FINSI;
|FPRC;
