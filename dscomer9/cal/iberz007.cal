     nTota = nTota + #502#7;
|FPRC;

|PROCESO Final;
     #501#9 = nTota;
     |GRABA 020#501a;
|FPRC;

|DEFBUCLE agifa502;
     #agifa502#1;
     ;
     #agifa501#36, #agifa501#0, 001;
     #agifa501#36, #agifa501#0, 999;
     ;
     NULL, agifa502;
|FIN;

