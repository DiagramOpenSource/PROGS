         |C_M #0#12, 1, "S";
     |SINO;
         |PARA nCampo = 13;  |SI nCampo [ 34;  |HACIENDO nCampo = nCampo + 1;
               |C_M #0nCampo, 1, "S";
         |SIGUE;
         |C_M #0#11, 1, "N";  #0#11 = "  ";  |PINTA #0#11;
         |C_M #0#12, 1, "N";  #0#12 = "zz";  |PINTA #0#12;
     |FINSI;
|FPRC;

|PROCESO NoPasa;
     |SI #0Campo = "  ";
         |PARA nCampo = Campo + 1;  |SI nCampo [ 34;  |HACIENDO nCampo = nCampo + 1;
               #0nCampo = "  ";  |C_M #0nCampo, 1, "N";  |PINTA #0nCampo;
