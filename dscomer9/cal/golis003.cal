|FICHEROS;
     golis003 #0;
     goartive #1;
     goartdes #2;
|FIN;

|VARIABLES;
     &enDes    = 0;
     aInf      = "golis003";
|FIN;

|PROCESO goartdes;
     |SI enDes = 0;
          enDes = 1;
          |PRINT;
     |FINSI;
     enDes = 2;
     |PRINT;
|FPRC;

|DEFBUCLE goartdes;
     #2#1;
     ;
     #1#0, 001;
     #1#0, 999;
     ;
     NULL, goartdes;
|FIN;

|PROCESO goartive;
     enDes = 0;
     |PRINT;
     |BUCLE goartdes;
|FPRC;

|DEFBUCLE goartive;
     #1#3;
     7, 11, 13;
     #0#1, #0#0, #0#2, #0#3;
     #0#5, #0#4, #0#6, #0#7;
     ;
     NULL, NULL, NULL, NULL, NULL, goartive;
     #1#0;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR aInf;
          |SI FSalida = 0;
               |BUCLE goartive;
               |PIEINF;
               |FININF;
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;
