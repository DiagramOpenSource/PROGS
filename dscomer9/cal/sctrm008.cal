|FICHEROS;
     sctrm008 #0;
     sctrm001 #100;
|FIN;

|VARIABLES;
     &enPantaLogo = 0;
     &eaNomLogo = "";
     nPanta = 0;
|FIN;

|PROCESO T8_13; |TIPO 13;
     |ABRE #100; |LEE 000#100p; |CIERRA #100;
     |PINPA #0#0; nPanta = FSalida;
     eaNomLogo = #100#1;
     |HAZ TraeLogoPan;
     enPantaLogo = nPanta;
     |HAZ PintaLogoPan;
|FPRC;

|PROCESO Fhasta; |TIPO 0;
     |SI FSalida = 2; |ACABA; |FINSI;
     |SI #0#2 > #0#3;
          |MENAV "0000Fecha incorrecta...";
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO T8_2; |TIPO 2;
     |SI #0#2 > #0#3;
          |MENAV "0000Fecha incorrecta...";
          |ERROR;
     |FINSI;
|FPRC;
