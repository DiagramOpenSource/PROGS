|FICHEROS;
     iber0055 #0;
|FIN;

|VARIABLES;
     nModo  = 0;
     nForma = 0;

     &eaCliente = "";
|FIN;

||INC-LUYE llena_c;

|RUTINA ClaveBaja0;
     #0#0 = 1;
     #0#1 = "      ";
     #0#2 = "01.01.1900";
|FRUT;

|RUTINA ClaveAlta0;
     #0#0 = 999;
     #0#1 = "zzzzzz";
     #0#2 = "31.12.2999";
|FRUT;

|PROCESO Tipo2m55;  |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
         #0#5 = 0;
         #0#6 = 0;
         |GRABA 020#0a;

         eaCliente = #0#1;
         |SUB_EJECUTA_NP "iberz001";
     |SINO;
         nForma = 0 +. 1;
         |SI nForma = 1;
             |GRABA 020#0a;
             eaCliente = #0#1;
             |SUB_EJECUTA_NP "iberz001";
         |FINSI;
     |FINSI;
|FPRC;

|PROGRAMA;
      |ENTLINEAL #1#0, -1, 2, 22, 1, ClaveBaja0, ClaveAlta0;
|FPRO;
