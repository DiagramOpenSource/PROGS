|FICHEROS;
agifa329 #0;
agifa328 #1;
|FIN;

|VARIABLES;
tecla = "";
cli = "";
linea = 0;
|FIN;

|PROCESO DivDefecto;
tecla = FSalida;
|SI FSalida = "14";
  #agifa328#5 = #agifa329#2;
  |PINTA 1072,#agifa328#5;
|FINSI;
|FPRC;

|PROCESO Fin_o_no; |TIPO 0;
  |SI #agifa329#7 = "S";
    |MENAV "0000Este periodo para esta divisa ya ha finalizado";
    |C_M #agifa329#2,1,"N";
    |C_M #agifa329#3,1,"N";
    |C_M #agifa329#4,1,"N";
    |C_M #agifa329#5,1,"N";
    |C_M #agifa329#6,1,"N";
    |C_M #agifa329#7,1,"N";
  |SINO;
    |C_M #agifa329#2,1,"S";
    |C_M #agifa329#3,1,"S";
    |C_M #agifa329#5,1,"S";
    |C_M #agifa329#6,1,"S";
    |C_M #agifa329#7,1,"S";
   |FINSI;
|FPRC;

|PROCESO Comprueba; |TIPO 0;

  cli = #agifa329#0;
  linea = #agifa329#1;
  |SELECT #2#agifa329;
  #agifa329#7 = "N";
  |LEE 001#agifa329.=;
  |SI FSalida = 0; |Y linea ! #agifa329#1;
   |MENAV "0000 Ya existe un periodo vigente para esta divisa."
   |ERROR;
  |FINSI;
  |SELECT #1#agifa329;
  #agifa329#0 = cli;
  #agifa329#1 = linea;
  |LEE 001#agifa329.=;

|FPRC;
