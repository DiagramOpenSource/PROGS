|FICHEROS;
     inmoz013 #0;
     inmom001 #1;
     inmom005 #5;
|FIN;

|VARIABLES;
     &enSali = 0;
     nSwCheq = 0;
     nPrime = 0;
     nImpre = 1;
     nInfor = 1;
|FIN;

|PROCESO Imprime;
     |SI nPrime = 0;
          nPrime = 1;
          |IMPRE 0;
          nImpre = FSalida;
          |SI FSalida ! 0; |ACABA; |FINSI;
          |INFOR "inmoz013";
          nInfor = FSalida;
          |SI FSalida ! 0; |ACABA; |FINSI;
     |FINSI;
     |SI nInfor = 0; |PRINT; |FINSI;
|FPRC;

|PROCESO inmom005;
     |SI nSwCheq = 1;
          #1#0 = #5#2;
          |LEE 000#1=;
          enSali = FSalida;
          |SI FSalida ! 0;
               |HAZ Imprime;
          |SINO;
               |SI #1#9 ! #5#8; |O #1#11 ! #5#3;
                    |HAZ Imprime;
               |FINSI;
          |FINSI;
     |SINO;
          #1#0 = #5#2;
          |LEE 121#1=;
          |SI FSalida = 0;
               #1#25 = #5#5;
               #1#26 = #5#0;
               #1#27 = #5#7;
               |GRABA 020#1a;
               |LIBERA #1;

               #5#6 = "S";
               |GRABA 020#5a;
          |FINSI;
     |FINSI;
     |LIBERA #5;
|FPRC;

|DEFBUCLE inmom005;
     #5#2;
     6;
     #0#0, " ";
     #0#0, " ";
     be;
     NULL, inmom005;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     nSwCheq = 1;
     |BUCLE inmom005;
     |SI nPrime = 0;
          nSwCheq = 0;
          |BUCLE inmom005;
     |SINO;
          |SI nInfor = 0; |PIEINF; |FININF; |FINSI;
          |SI nImpre = 0; |FINIMP; |FINSI;
     |FINSI;
|FPRC;
