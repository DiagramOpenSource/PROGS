|PROCESO ModoConsulta;
     |PINPA #1#1;
     |PINPA #2#1;
     |PINPA #3#1;
     |PINPA #4#1;

     |PINDA #1#1;
     |PINDA #2#1;
     |PINDA #3#1;
     |PINDA #4#1;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
         FSalida = "::P.Anterior";
         |LEETECLA aTecla;
         |SI aTecla = aEsc1; |SAL;  |FINSI;
         |SI aTecla = aEsc2; |SAL;  |FINSI;
         |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;
     aTecla = "";

     |PULSATECLA;
|FPRC;

|PROCESO ModoModifica;
     |SELECT #1#0;
     #0#0  = aSerie;
     #0#1  = nAlbaran;
     |LEE 101#0=;
     |SI FSalida ! 0;
         |REFRESCACONTROL nGrid;
         |ACABA;
     |FINSI;

     |VENTANA 0501, 2899, -1, -1, "";
     nVentana = FSalida;

     |ABRET #1;
     #1#52 = #0#0;

     #1#0  = #0#1;
     |LEE 101#1=;
     |SI FSalida = 0;
         |HAZ Control;

         |PINPA #0#1;  |PINDA #0#1;
         |PINPA #0#2;  |PINDA #0#2;

         |SI nModoDatos = 2;
             |ENDATOS #2#1;
             |SI FSalida = 0;
                 |GRABA 020#1a;
                 |HAZ dsm01001;
             |SINO;
                 |ABRE #2;
                 #2#29 = #1#52;
                 #2#0  = #1#0;
                 #2#1  = 1;
                 |LEE 040#2];
                 |SI FSalida = 0;  |Y #2#29 = #1#52;  |Y  #2#0  = #1#0;
                     |HAZ dsm01001;
                 |SINO;
                     |BORRA 020#1a;
                     |SI enEsAlbaran = 1;
                         nDestruir = 1;
                         |HAZ ConstruyeLinealAlb;
                         nDestruir = 0;
                     |FINSI;
                     |REFRESCACONTROL nGrid;
                 |FINSI;
                 |CIERRA #2;
             |FINSI;
         |FINSI;

         |SI nModoDatos = 4;  |HAZ ModoConsulta;  |FINSI;

         FSalida = "SKIPDEFAULT";
    |FINSI;

    |LIBERA #1;
    |LIBERA #0;

    |HAZ FinSubBotones;
    |FINVENTANA nVentana;

    #0#0  = aSerie;
    #0#1  = nAlbaran;
    |LEE 040#0=;
    |REFRESCACONTROL nGrid;

    |PULSATECLA;
    |CIERRAT #1;
|FPRC;

