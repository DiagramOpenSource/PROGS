|PROCESO BorLinAlb;
     enForma = -1;
     |SI #71#12 ! 0;     || De pedido
          #104#25 = #71#31;
          #104#0 = #71#12;
          |LEE 121#104=;
          |SI FSalida = 0;
               #73#28 = #71#31;
               #73#0 = #71#12;
               #73#1 = #71#21;
               |LEE 121#73=;
               |SI FSalida = 0;
                    #73#43 = #73#43 - #71#5;
                    #73#49 = #73#49 - #71#48;
                    |GRABA 020#73a;

                    enUnidPend = #71#5;
                    eaReservado = #73#22;
                    |HAZ AcumulaAV;

                    #104#24 = #104#24 - #71#5;

                    aDivisa = #104#35;
                    aMoneda = #104#37;
                    |HAZ Divisas104;
                    |HAZ CalCabAgifa104;
                    |GRABA 020#104a;
                    |LIBERA #73;
               |FINSI;
               |LIBERA #104;
          |FINSI;
     |SINO;
          |HAZ AcumulaAV;
     |FINSI;
     |BORRA 020#71a;
|FPRC;

|DEFBUCLE BorLinAlb;
     #71#1;
     ;
     #1#52, #1#0, 001;
     #1#52, #1#0, 999;
     ;
     NULL, BorLinAlb;
|FIN;

|PROCESO ActCli;
     nImpo = ((#1#15 < #1#5) < #1#32) < #1#7;
     |ABRE #24;
     #24#0 = #1#3;
     |LEE 121#24=;
     |SI FSalida = 0;
          #24#50 = #24#50 - nImpo;
          |GRABA 020#24a;
     |FINSI;
     |CIERRA #24;

     enForma = -1;
     enImpCliPen = -nImpo;
     eaCliente = #1#3;
     efFecha = #1#1;
     |HAZ AcumulaCli;
     eaTag = "AV";  |HAZ Riesgos;
|FPRC;

|PROCESO BorraAlbaran;
     |ABRE #1;
     #1#52 = #0#0;
     #1#0 = #0#1;
     |LEE 121#1=
     |SI FSalida = 0;
          |SI #1#38 = "S";
               |MENAV "0000Albaran Facturado, no se permite la baja...";
          |SINO;
               aAlfa = "0000NBorrar Albaran: [" + #1#52 + "/" + (("000000"+#1#0)%-106)+"]";
               |CONFI aAlfa;
               |SI FSalida = 0;
                    |ABRE #104;
                    |ABRE #73;
                    |BUCLE BorLinAlb;
                    |CIERRA #73;
                    |CIERRA #104;
                    |HAZ ActCli;
                    |BORRA 020#1a;
               |FINSI;
          |FINSI;
     |FINSI;
     |CIERRA #1;
|FPRC;
