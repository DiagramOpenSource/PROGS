|PROCESO Control;
     |SI #1#42 > 0;
         |MENAV "0000Este albaran contiene lineas provenientes de Depositos";
         nModoDatos = 4;
         |ACABA;
     |FINSI;

     |SI #1#39 = 999999;
         |MENAV  "0000Este albaran esta en modo Exportacion";
         nModoDatos = 4;
         |ACABA;
     |FINSI;

     |SI #1#38 = "S";
         |MENAV "0000Este albaran esta FACTURADO";
         nModoDatos = 4;
         |ACABA;
     |FINSI;
