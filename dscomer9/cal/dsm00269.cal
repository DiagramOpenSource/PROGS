|FICHEROS;
     dsm00269 #0;
     dsm00270 #270,MANTE;
     dsm00271 #271,MANTE;
     dsm00272 #272,MANTE;
|FIN;

|VARIABLES;
     nModo = 0;
     nForma = 0;
     {-1}aMenu = "";
     aMenu1 = "2400";
     aMenu2 = "1";
     aMenu3 = "Opciones: ";
     aMenu4 = "FSA";
     aMenu5 = " Familias ";
     aMenu6 = " Subfamilias ";
     aMenu7 = " Articulos ";
|FIN;

|PROCESO Min;
     #270#0 = #0#0;
     #270#1 = "   ";
     #271#0 = #0#0;
     #271#1 = "     ";
     #272#0 = #0#0;
     #272#1 = " " * 20;
|FPRC;

|PROCESO Max;
     #270#0 = #0#0;
     #270#1 = "zzz";
     #271#0 = #0#0;
     #271#1 = "zzzzz";
     #272#0 = #0#0;
     #272#1 = "z" * 20;
|FPRC;

|PROCESO Tipo13; |TIPO 13;
     |ENTLINEAL #1#270, 1, 7, 22, 0, Min, Max;
     |ENTLINEAL #1#271, 1, 7, 22, 0, Min, Max;
     |ENTLINEAL #1#272, 1, 7, 22, 0, Min, Max;
|FPRC;

|PROCESO Tipo20; |TIPO 20;
     |SI FSalida = "OPCION";
          FSalida = "Opciones";
     |SINO;
          |LEE 000#0=;
          |SI FSalida ! 0; |ACABA; |FINSI;
          |MENU aMenu;
          aMenu2 = FSalida;
          |SI aMenu2 = 1; |ENTLINEAL #1#270, 1, 2, 22, 0, Min, Max; |FINSI;
          |SI aMenu2 = 2; |ENTLINEAL #1#271, 1, 2, 22, 0, Min, Max; |FINSI;
          |SI aMenu2 = 3; |ENTLINEAL #1#272, 1, 2, 22, 0, Min, Max; |FINSI;
     |FINSI;
|FPRC;

|PROCESO Borra270;
     |BORRA 020#270a;
|FPRC;
|PROCESO Borra271;
     |BORRA 020#271a;
|FPRC;
|PROCESO Borra272;
     |BORRA 020#272a;
|FPRC;

|DEFBUCLE Borra270;
     #270#1;
     ;
     #0#0, "   ";
     #0#0, "zzz";
     ;
     NULL, Borra270;
|FIN;

|DEFBUCLE Borra271;
     #271#1;
     ;
     #0#0, "     ";
     #0#0, "zzzzz";
     ;
     NULL, Borra271;
|FIN;

|DEFBUCLE Borra272;
     #272#1;
     ;
     #0#0, "                    ";
     #0#0, "zzzzzzzzzzzzzzzzzzzz";
     ;
     NULL, Borra272;
|FIN;

|PROCESO BajaImp; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     nForma = 0 +. 1;
     |SI nModo = 3;
          |BUCLE Borra270;
          |BUCLE Borra271;
          |BUCLE Borra272;
     |FINSI;
     |SI nForma = 1;
          |PARA aMenu2 = 1; |SI aMenu2 ] 1; |HACIENDO;
               |MENU aMenu;
               aMenu2 = FSalida;
               |SI aMenu2 = 1; |ENTLINEAL #1#270, 1, 2, 22, 0, Min, Max; |FINSI;
               |SI aMenu2 = 2; |ENTLINEAL #1#271, 1, 2, 22, 0, Min, Max; |FINSI;
               |SI aMenu2 = 3; |ENTLINEAL #1#272, 1, 2, 22, 0, Min, Max; |FINSI;
          |SIGUE;
     |FINSI;
|FPRC;
