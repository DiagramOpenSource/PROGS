     aAlfa = "0000NCerrar aviso:" + #referc1@#0+"/"+(("000000"+#referc1@#1)%-106);
     |CONFI aAlfa;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |ABRE #17;
     #17#0 = #referc1@#0;
     #17#1 = #referc1@#1;
     |LEE 121#17=;
     enError = 0;
     |SI #referc1@#46 = 0;        || Sin pedido crea albaran
