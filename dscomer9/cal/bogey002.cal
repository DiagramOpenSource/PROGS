|FICHEROS;
     bogey002 #0;
     bogem012 #12;
     bogem015 #15;
     bogem016 #16;
|FIN;

|VARIABLES;
     nOK = 0;
     nHay = 0;
     &enTipo = 0;
|FIN;

|PROCESO Bogem012;
     enTipo = 1;
     |PRINT;
|FPRC;

|DEFBUCLE Bogem012;
     #12#2;
     ;
     #16#3, #16#17;
     #16#3, #16#17;
     ;
     NULL, Bogem012;
|FIN;

|PROCESO Bogem016;
     nOK = 0;

     |SI #0#6 = "T";
          nOK = 1;
     |FINSI;
     |SI #0#6 = "S";
          |SI #16#6 > #16#15; nOK = 1; |FINSI;
     |FINSI;
     |SI #0#6 = "N";
          |SI #16#6 [ #16#15; nOK = 1; |FINSI;
     |FINSI;

     |SI nOK = 1;
          |SI #0#7 = "S";
               enTipo = 0;
               |PRINT;
               |BUCLE Bogem012;
          |SINO;
               enTipo = 0;
               |PRINT;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE Bogem016;
     #16#1;
     ;
     #15#0, #15#24, 001;
     #15#0, #15#24, 999;
     ;
     NULL, Bogem016;
|FIN;

|PROCESO Bogem015;
     |BUCLE Bogem016;
|FPRC;

|DEFBUCLE Bogem015;
     #15#1;
     26, 14;
     #0#2, 000, #0#0, #0#4;
     #0#3, 999, #0#1, #0#5;
     ;
     NULL, Bogem015;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     Impresora = "Crystal Reports";
     |IMPRE 1;
     |SI FSalida = 0;
          |INFOR "bogey002";
          |SI FSalida = 0;
               |BUCLE Bogem015;
               |PIEINF;
               |FININF;
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;
