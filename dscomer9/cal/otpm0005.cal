|FICHEROS;
   otpm0005 #0;

   otpm0005@ #10;
|FIN;

|VARIABLES;
   aAlfa   = "";

   nRango  = 0;
   nPos1   = 0;
   nPos2   = 0;

   nSw     = 0;
|FIN;

|PROCESO MiraSPAs;
   |DDEF aAlfa; |QBF aAlfa;
   aAlfa = aAlfa + "otpm0005.mas";
   |CARGA_DEF aAlfa, otpm0005@;
   |SI FSalida = 0;
      aAlfa = #otpm0005#1;

      |ABRE #otpm0005@;
      |SELECT #2#otpm0005@;
      #otpm0005@#1 = aAlfa;
      |LEE 000#otpm0005@.=;
      |SI FSalida = 0;
         aAlfa = ("0000" + #otpm0005@#0) % -104;
         nSw = -1;
      |SINO;
         nSw = 0;
      |FINSI;
      |SELECT #1#otpm0005@;
      |CIERRA #otpm0005@; |DESCARGA_DEF #otpm0005@;
   |FINSI;

   |SI nSw < 0;
      aAlfa = "    Esa descripcion del SPA ya existe [" + aAlfa + "]";
      |MENAV aAlfa; |ERROR;
   |FINSI;
|FPRC;

|RUTINA Inf;
   #otpm0005#0 = 0000;
|FRUT;

|RUTINA Sup;
   #otpm0005#0 = 9999;
|FRUT;

|PROGRAMA;
   |CLS;
   |CABEZA "SPAs de CEBI";
   |PINPA #0#0;

   |ABRE #otpm0005;

   |ENTLINEAL #1#otpm0005, 1, -2, 21, 1, Inf, Sup;

   |CIERRA #otpm0005;
|FPRO;
