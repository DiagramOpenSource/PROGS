|FICHEROS;
   agifw798 #0;
|FIN;

|VARIABLES;
   nCont = 0;
|FIN;

|PROCESO MiraCampo;
   |SI #agifw798#Campo# = "S";
      |PARA nCont = 0; |SI nCont [ 3; |HACIENDO nCont = nCont + 1;
         |SI nCont ! Campo;
            #agifw798#nCont# = "N"; |PINTA #agifw798#nCont#;
         |FINSI;
      |SIGUE;
   |FINSI;
|FPRC;
