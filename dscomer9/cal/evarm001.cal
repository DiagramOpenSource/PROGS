|FICHEROS;
     evarm001 #0;
     evarm002 #1,MANTE;
     dsm00071 #71;
     dsm00072 #72;
     dsm00073 #73;
     agifa321 #321;
     agifa324 #324;
     dsm00010 #910;
     dsm00015 #915;
|FIN;

|VARIABLES;
     &nDeci_PreBase = 0;
     &nDeci_PreDiv = 0;
     nModo = 0;
     aAlfa = "";
     nNume = 0;
|FIN;

|INCLUYE i_varart;

|PROCESO Tipo7; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |ABRE #321; |LEE 000#321p; |CIERRA #321;
          #0#8 = #321#9; |PINTA #0#8;
     |FINSI;
|FPRC;

|PROCESO Tipo13; |TIPO 13;
     |ABRE #910; |LEE 000#910p; |CIERRA #910;
     |ATRI 0;
     aAlfa = #910#3; |PINTA 1003, aAlfa;
     aAlfa = #910#7; |PINTA 1103, aAlfa;
     aAlfa = #910#11; |PINTA 1203, aAlfa;

     nNume = #910#4; |CAMPO_ANCHO #0#2, 1, nNume;
     aAlfa = "\" + #910#6 + "+#"; |CAMPO_MAXIMO #0#2, 1, aAlfa;
     |C_P #0#2, 0, nNume; nNume = nNume + #910#4 + 2;
     |C_P #0#3, 1, nNume;

     nNume = #910#8; |CAMPO_ANCHO #0#4, 1, nNume;
     aAlfa = "\" + #910#10 + "+#"; |CAMPO_MAXIMO #0#4, 1, aAlfa;
     |C_P #0#4, 0, nNume; nNume = nNume + #910#8 + 2;
     |C_P #0#5, 1, nNume;

     nNume = #910#12; |CAMPO_ANCHO #0#6, 1, nNume;
     aAlfa = "\" + #910#14 + "+#"; |CAMPO_MAXIMO #0#6, 1, aAlfa;
     |C_P #0#6, 0, nNume; nNume = nNume + #910#12 + 2;
     |C_P #0#7, 1, nNume;

     |ATRI S;
     aAlfa = #910#20;
     |PINTA 1503, aAlfa;
     |ATRI 0;
     nNume = #910#21; |CAMPO_ANCHO #1#1, 1, nNume;
     aAlfa = "\" + #910#23 + "+#"; |CAMPO_MAXIMO #1#1, 1, aAlfa;

     |SI nModo < 1; |O nModo ] 4;
          nModo = 7;
          |HAZ Columna;
     |FINSI;
|FPRC;

|PROCESO Tipo12; |TIPO 12;
     |SI FSalida ! 7;
          nModo = (FEntrada / 100) ! 0;
          |HAZ Columna;
     |FINSI;
|FPRC;

|PROCESO Columna;
     |ABRE #321; |LEE 000#321p; |CIERRA #321;

     |ABRE #324;
     #324#0 = #321#9;
     |LEE 000#324=;
     |SI FSalida ! 0;  |DDEFECTO #324;  |FINSI;
     nDeci_PreBase = #324#9;
     |CIERRA #324;

     |ENTCOLUMNA #1#1, 2, nModo, 5, 0, Min, Max;
|FPRC;

|PROCESO Min;
     #1#0 = #0#0;
     #1#1 = "      ";
|FPRC;

|PROCESO Max;
     #1#0 = #0#0;
     #1#1 = "zzzzzz";
|FPRC;

|PROCESO Borra;
     |BORRA 020#1a;
|FPRC;

|DEFBUCLE Borra;
     #1#1;
     ;
     #0#0, "      ";
     #0#0, "zzzzzz";
     be;
     NULL, Borra;
|FIN;

|PROCESO Tipo2;  |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3; |BUCLE Borra; |FINSI;
|FPRC;

|PROCESO Arti6; |TIPO 6;
     enAlta = 0;
     eaArticulo = #0Campo;
     |HAZ LlenaArticulo;
     |SI FSalida = 0;
          #0Campo = eaArticulo;
          |PINTA #0Campo;
     |SINO;
          |ERROR; |ACABA;
     |FINSI;

     eAlfa = #0#0;
     |SUB_EJECUTA_NP "dsz99990;V";
     #0#2 = eAlfa1; |PINTA #0#2;
     |ABRE #71;
     #71#0 = #0#2;
     |LEE 000#71=;
     |SI FSalida ! 0; |DDEFECTO #71; |FINSI;
     |CIERRA #71;
     #0#3 = #71#1; |PINTA #0#3;

     #0#4 = eAlfa2;
     |ABRE #72;
     #72#0 = #0#4; |PINTA #0#4;
     |LEE 000#72=;
     |SI FSalida ! 0; |DDEFECTO #72; |FINSI;
     |CIERRA #72;
     #0#5 = #72#1; |PINTA #0#5;

     #0#6 = eAlfa3;
     |ABRE #73;
     #73#0 = #0#6; |PINTA #0#6;
     |LEE 000#73=;
     |SI FSalida ! 0; |DDEFECTO #73; |FINSI;
     |CIERRA #73;
     #0#7 = #73#1; |PINTA #0#7;
|FPRC;
