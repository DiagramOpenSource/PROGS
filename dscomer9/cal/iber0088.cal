|FICHEROS;
     iber0088 #0;
|FIN;

|VARIABLES;
     nSw  = 0;
     nCod = 1;
     nMin = 0;
     nMax = 0;
     i    = 0;
     aAlfa= "";
|FIN;

|PROCESO Tecla; |TIPO 11;
     |SI FSalida = 10;
          nCod = #0#0;
          nSw = 1;
          |PTEC 807;
     |FINSI;
|FPRC;

|PROCESO Min;
     |SI FSalida = "POSICION";
          #0#0 = nCod;
     |SINO;
          #0#0 = 1;
     |FINSI;
|FPRC;

|PROCESO Max;
     #0#0 = 999999;
|FPRC;

|PROCESO Carga;
     |ABRE #0;
     |PARA i = nMin; |SI i [ nMax; |HACIENDO i = i + 1;
          #0#0 = i;
          aAlfa = ("000000" + i) % -106;
          #0#1 = "TARJETA NUMERO:" + aAlfa;
          |GRABA 040#0n;
     |SIGUE;
     |CIERRA #0;
|FPRC;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PARA nSw = 1; |SI nSw = 1; |HACIENDO;
          nSw = 0;
          |ENTLINEAL #1#0, -1, 2, 22, 0, Min, Max;
          |SI nSw = 1;
               |PUSHV 0501 2380;
               |BLANCO 1230 1560;
               |CUADRO 1230 1560;
               |ATRI R; |PINTA 1131, "    Alta Automatica       "; |ATRI 0;
               |ATRI I; |PINTA 1331, " Desde:        Hasta:     "; |ATRI 0;
               E_inf = 000001;
               E_sup = 999999;
               nMin = 1;
               nMin = 1339 ? 1;
               |SI FSalida = 0;
                    E_inf = 000001;
                    E_sup = 999999;
                    nMax = 999999;
                    nMax = 1353 ? 1;
                    |SI FSalida = 0;
                         |HAZ Carga;
                    |FINSI;
               |FINSI;
               |POPV;
          |FINSI;
     |SIGUE;
|FPRO;
