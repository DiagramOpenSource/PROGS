|FICHEROS;
     dsm00108 #0;
     dsm00109 #1;
     dsm00109@ #10;
     dsm00010 #910;
|FIN;

|VARIABLES;
     nModo     = 0;
     aAlfa     = "";
     nForma    = 0;
|FIN;

|INCLUYE i_varart;

|PROCESO Calc6_108; |TIPO 6;
     enAlta = 1;
     eaArticulo = #1Campo;
     |HAZ LlenaArticulo;
     |SI FSalida = 0;
          #1Campo = eaArticulo;
          |PINTA #1Campo;
     |SINO;
          |ERROR; |ACABA;
     |FINSI;
|FPRC;

|PROCESO Tipo2; |TIPO 2;
     nForma = 0 +. 1;
     |SI nForma ! 1; |ACABA; |FINSI;

     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1; |O nModo = 2;
          |DBASE aAlfa;
          aAlfa = aAlfa + "def/dsm00109";
          |CARGA_DEF aAlfa , dsm00109@;
          |SI FSalida ! 0;
               aAlfa = "000Error al cargar " + aAlfa;
               |MENAV aAlfa;
               |ACABA;
          |FINSI;

          aAlfa = "";
          |ABRE #10;
          |SELECT #2#10;
          #10#0 = #1#0;
          #10#2 = #1#2;
          |LEE 000#10=;
          |SI FSalida = 0; |Y #10#1 ! #1#1;
               aAlfa = "0000Codigo Articulo existente en Linea: [" + #10#1 + "]";
               |MENAV aAlfa;
          |FINSI;
          |SELECT #3#10;
          #10#0 = #1#0;
          #10#3 = #1#3;
          |LEE 000#10=;
          |SI FSalida = 0; |Y #10#1 ! #1#1;
               aAlfa = "0000Codigo Cliente existente en Linea: [" + #10#1 + "]";
               |MENAV aAlfa;
          |FINSI;
          |CIERRA #10;
          |DESCARGA_DEF #dsm00109@;

          |SI aAlfa ! ""; |ERROR; |FINSI;
     |FINSI;
|FPRC;
