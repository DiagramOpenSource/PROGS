|FICHEROS;
     desan057 #0;
     desan058 #1,MANTE;
     desan059 #2,MANTE;
|FIN;

|VARIABLES;
     nModo = 0;
     {-1}aMenu = "";
     aMenu1 = "2400";
     aMenu2 = "1";
     aMenu3 = "Opciones:  ";
     aMenu4 = "LA";
     aMenu5 = " Localización ";
     aMenu6 = " Artículo ";
|FIN;

|PROCESO Min2;
     #2#0 = #0#0;
     #2#1 = #0#1;
     #2#2 = #0#2;
     #2#3 = #0#3;
     #2#4 = " " * 20;
|FPRC;

|PROCESO Max2;
     #2#0 = #0#0;
     #2#1 = #0#1;
     #2#2 = #0#2;
     #2#3 = #0#3;
     #2#4 = "z" * 20;
|FPRC;


|PROCESO Min1;
     #1#0 = #0#0;
     #1#1 = #0#1;
     #1#2 = #0#2;
     #1#3 = #0#3;
     #1#4 = " " * 30;
|FPRC;

|PROCESO Max1;
     #1#0 = #0#0;
     #1#1 = #0#1;
     #1#2 = #0#2;
     #1#3 = #0#3;
     #1#4 = "z" * 30;
|FPRC;

|PROCESO T13; |TIPO 13;
     |ENTLINEAL #1#1, 5, 7, 22, 0, Min1, Max1;
     |ENTLINEAL #1#2, 5, 7, 22, 0, Min2, Max2;
|FPRC;

|PROCESO T12; |TIPO 12;
     nModo = (FEntrada / 100) ! 0;
     |ENTLINEAL #1#1, 5, nModo, 22, 0, Min1, Max1;
     |ENTLINEAL #1#2, 5, nModo, 22, 0, Min2, Max2;
|FPRC;

|PROCESO Bdesan059;
     |BORRA 020#2a;
|FPRC;

|DEFBUCLE Bdesan059;
     #2#1;
     ;
     #0#0, #0#1, #0#2, #0#3, "                    ";
     #0#0, #0#1, #0#2, #0#3, "zzzzzzzzzzzzzzzzzzzz";
     ;
     NULL, Bdesan059;
|FIN;

|PROCESO Bdesan058;
     |BORRA 020#1a;
|FPRC;

|DEFBUCLE Bdesan058;
     #1#1;
     ;
     #0#0, #0#1, #0#2, #0#3, "                    ";
     #0#0, #0#1, #0#2, #0#3, "zzzzzzzzzzzzzzzzzzzz";
     ;
     NULL, Bdesan058;
|FIN;

|PROCESO T2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |BUCLE Bdesan059;
          |BUCLE Bdesan058;
     |FINSI;
|FPRC;

|PROCESO T20; |TIPO 20;
     |SI FSalida = "OPCION";
          FSalida = "Opciones";
     |SINO;
          |MENU aMenu;
          |SI FSalida = 1;
               |ENTLINEAL #1#1, 5, 1, 22, 0, Min1, Max1;
          |FINSI;
          |SI FSalida = 2;
               |ENTLINEAL #1#2, 5, 1, 22, 0, Min2, Max2;
          |FINSI;
     |FINSI;
|FPRC;
