|FICHEROS;
     albz0007 #0;
     agifa073 #73;
     albm0026 #26; ||Articulos
     albm0028 #28; ||Aux.Ped Vta
|FIN;

|VARIABLES;
     nDOrden = 0;
     nHOrden = "";
     aAlfa = "";
     nHay = 0;
 {-1}Menu = "";
     Menu1 = "2400";
     Menu2 = "1";
     Menu3 = "  ";
     Menu4 = "SCT";
     Menu5 = " Sin Orden de Fab. ";
     Menu6 = " Con Orden de Fab. ";
     Menu7 = " Todas ";
|FIN;

|PROCESO Min;
     #0#0 = "     ";
     #0#1 = 1;
     #0#2 = 1;
|FPRC;

|PROCESO Max;
     #0#0 = "zzzzz";
     #0#1 = 999999;
     #0#2 = 999;
|FPRC;

|PROCESO Tecla;
     |SI #0Campo = Contenido; |Y FSalida = 0;
          |SI #0#6 = "*";
               #0#6 = "";
          |SINO;
               #0#6 = "*";
          |FINSI;
          |GRABA 020#0a;
          |PINTA #0#6;
     |FINSI;
|FPRC;

|PROCESO Tmp;
     #28#0 = #0#0;
     #28#1 = #0#1;
     #28#2 = #0#2;
     |LEE 121#28=;
     |SI FSalida = 0;
          |SI #0#6 ! " ";
               #28#3 = "S";
               |GRABA 020#28a;
          |FINSI;
          |LIBERA #28;
     |FINSI;
|FPRC;

|DEFBUCLE Tmp;
     #0#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, Tmp;
|FIN;

|PROCESO albm0028;
     #73#28 = #28#0;
     #73#0 = #28#1;
     #73#1 = #28#2;
     |LEE 000#73=;
     |SI FSalida = 0; |Y #73#43 = 0;
          |DDEFECTO #26;
          #26#0 = #73#2;
          |LEE 000#26=;
          |SI #26#12 ] nDOrden; |Y #26#12 [ nHOrden;
               #0#0 = #73#28;
               #0#1 = #73#0;
               #0#2 = #73#1;
               #0#3 = #73#2;
               #0#4 = #73#4;
               #0#5 = #73#5;
               |GRABA 020#0n;
               nHay = 1;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE albm0028;
     #28#1;
     3;
     "      ", 000001, 001, "N";
     "zzzzzz", 999999, 999, "N";
     ;
     NULL, albm0028;
|FIN;

|PROGRAMA;
     |CLS;
     |ENTLINEAL #1#0, 1, 7, 22, 1, Min, Max;
     |MENU Menu;
     Menu2 = FSalida;
     |SI FSalida < 0; |ACABA; |FINSI;

     |SI Menu2 = 1;
          nDOrden = 0;
          nHOrden = 0;
     |FINSI;
     |SI Menu2 = 2;
          nDOrden = 1;
          nHOrden = 99999;
     |FINSI;
     |SI Menu2 = 3;
          nDOrden = 0;
          nHOrden = 99999;
     |FINSI;

     |MENSA "0000Cargando Temporal....";
     aAlfa = Usuario;
     |QBF aAlfa;
     |NOME_DAT #0 aAlfa; |DELINDEX #0;
     |ABRE #0;
     |ABRE #73;
     |ABRE #26;
     |BUCLE albm0028;
     |CIERRA #26;
     |CIERRA #73;
     |CIERRA #0;
     |SI nHay = 0;
          |MENAV "0000No hay registros...";
     |SINO;
          |ENTLINEAL #1#0, 1, 4, 22, 0, Min, Max;
          |CONFI "0000NActualizar Pedido...";
          |SI FSalida = 0;
               |ABRE #28;
               |BUCLE Tmp;
               |CIERRA #28;
          |FINSI;
     |FINSI;
     |DELINDEX #0;
|FPRO;
