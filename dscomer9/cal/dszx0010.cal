|FICHEROS;
     agifa029 #29,MANTE;
|FIN;

|VARIABLES;
     aAlfa = "";
     aParam = "";
     aAlma = "";
     aTipo = "";
|FIN;

|PROCESO AltaAlma;
     |ABRE #29;
     #29#0 = aAlma;
     |GRABA 020#29b;
     |SI FSalida ! 0;
          |MENAV "0000Almacen existente...";
          |ACABA;
     |FINSI;

     |PINPA #0#29;
     |PINDA #0#29;
     |ENDATOS #1#29;
     |SI FSalida = 0;
          |GRABA 020#29a;
     |SINO;
         |BORRA 020#29c;
     |FINSI;
     |CIERRA #29;
|FPRC;

|PROCESO ModiAlma;
     |ABRE #29;
     #29#0 = aAlma;
     |LEE 101#29=;
     |SI FSalida ! 0;
          |MENAV "0000Alamcen inexistente...";
          |ACABA;
     |FINSI;
     |PINPA #0#29;
     |PINDA #0#29;
     |ENDATOS #2#29;
     |SI FSalida = 0;
          |GRABA 020#29a;
     |FINSI;
     |CIERRA #29;
|FPRC;

|PROGRAMA;
     |CLS;
     aTipo = PARAMETRO % 101;
     aAlma = PARAMETRO % 205;
     |SI aTipo = "A";
          |HAZ AltaAlma;
     |SINO;
          |SI aTipo = "M";
               |HAZ ModiAlma;
          |FINSI;
     |FINSI;
|FPRO;
