     || Por problemas con proceso duplicados al compilar
     || al hacer los proceso Acumula??_?? lo hago con un sub_ejecuta
     || contempla los procesos:
     ||   |HAZ AcumulaES_SS;  |HAZ AcumulaME_MS
     || ...(a¤adir los que hagan falta)

|FICHEROS;
     dsm00043 #43;
     dsm00044 #44;
     dsm00054 #54;
     dsm00055 #55;
|FIN;

|VARIABLES;
     &eaMovimi = "";
     &enCodigo = 0;
     &eaSerie = "";
     &enLinea = 0;

     &enForma = 0;
|FINSI;

|PROCESO _AcumulaME_MS;
     |ABRE #54;
     |ABRE #55;

     #54#2 = eaSerie;
     #54#0 = enCodigo;
     |LEE 020#54=;
     |SI FSalida = 0;
          #55#28 = eaSerie;
          #55#27 = enCodigo;
          #55#11 = enLinea;
          |LEE 020#55=;
          |SI FSalida = 0;
               |HAZ AcumulaME_MS;
          |FINSI;
     |FINSI;
     |CIERRA #54;
     |CIERRA #55;
|FPRC;

|PROCESO _AcumulaES_SS;
     |ABRE #43;
     |ABRE #44;

     #43#4 = eaSerie;
     #43#0 = enCodigo;
     |LEE 020#43=;
     |SI FSalida = 0;
          #44#21 = eaSerie;
          #44#18 = enCodigo;
          #44#8 = enLinea;
          |LEE 020#44=;
          |SI FSalida = 0;
               |HAZ AcumulaES_SS;
          |FINSI;
     |FINSI;
     |CIERRA #43;
     |CIERRA #44;
|FPRC;

|PROGRAMA;
     |SI eaMovimi = "ES_SS"; |HAZ _AcumulaES_SS; |FINSI;
     |SI eaMovimi = "ME_MS"; |HAZ _AcumulaME_MS; |FINSI;
|FPRO;
