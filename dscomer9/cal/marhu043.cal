|PROCESO ProcesaCampo;
     nCampo = -1;
     aCampo = "";
     |PARA nNume2=1;|SI nNume2 [ 100;|HACIENDO nNume2 = nNume2 + 1;
          nNume3    = (nNume2 * 100) + 1;
          aPos      = nNume3;
          aCaracter = aField % aPos;
          nNume1 = &aCaracter;
          |SI aCaracter ! "";
               |SI aCaracter = ";";
                    nCampo = nCampo + 1;
                    #1#nCampo# = aCampo;
                    aCampo = "";
               |SINO;
                    aCampo = aCampo + aCaracter;
               |FINSI;
          |SINO;
               nCampo = nCampo + 1;
               #1#nCampo# = aCampo;
               |GRABA 020#1n;
               nCampo = 0;
               aCampo = "";
               |PINTA 1310, #1#0;
               |PINTA 1312, #1#1;
               |PINTA 1317, #1#2;
               |PINTA 1319, #1#3;
               |PINTA 1321, #1#4;
               |PINTA 1323, #1#5;
               |PINTA 1325, #1#6;
               nCuantos = nCuantos + 1;
               |DDEFECTO #1;
               |SAL;
          |FINSI;
     |SIGUE;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     |DELINDEX #1;
     |ABRE #1;
     aAlfa1 = #0#0; |QBF aAlfa1;
     |XABRE "U",aAlfa1,nNume1;
     |SI FSalida ] 0;
         |XLEE nNume1,250,aField;
         |PARA; |SI aField > ""; |HACIENDO;
               |HAZ ProcesaCampo;
               |XLEE nNume1,250,aField;
         |SIGUE;
     |SINO;
