|FICHEROS;
     inmoz018 #0;
     inmom001 #1;
     inmom007 #7;
|FIN;

|VARIABLES;
     &enPlacTot = 0;
     &enPlacOk = 0;
     &enPlacErr = 0;
     &enPlacSin = 0;
|FIN;

|PROCESO inmom007E;
     |DDEFECTO #1;
     #1#0 = #7#2;
     |LEE 000#1=;

     |PRINT;
|FPRC;

|DEFBUCLE inmom007E;
     #7#2;
     8;
     #0#0, "E";
     #0#0, "E";
     ;
     NULL, inmom007E;
|FIN;

|PROCESO inmom007;
     enPlacTot = enPlacTot + 1;
     |SI #7#8 = "S";
          enPlacOk = enPlacOk + 1;
     |FINSI;
     |SI #7#8 = "E";
          enPlacErr = enPlacErr + 1;
     |FINSI;
     |SI #7#8 = " "; |O #7#8 = "N";
          enPlacSin = enPlacSin + 1;
     |FINSI;
|FPRC;

|DEFBUCLE inmom007;
     #7#2;
     ;
     #0#0;
     #0#0;
     ;
     NULL, inmom007;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR "inmoza18";
          |SI FSalida = 0;
               |BUCLE inmom007;
               |PRINT;
               |PIEINF;
               |FININF;
          |FINSI;

          |SI #0#1 = "S";
               |SI enPlacErr = 0;
                    |MENAV "0000No hay placas erroneas...";
               |SINO;
                    |INFOR "inmozb18";
                    |SI FSalida = 0;
                         |ABRE #1;
                         |BUCLE inmom007E;
                         |CIERRA #1;
                         |PIEINF;
                         |FININF;
                    |FINSI;
               |FINSI;
          |FINSI;

          |FINIMP;
     |FINSI;
|FPRC;
