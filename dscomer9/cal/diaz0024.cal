|FICHEROS;
     diaz0024 #0;
     agifa010 #10;
     agifa059 #59;
     agifa134 #134;
|FIN;

|VARIABLES;
     aAlfa       = "";
     aAlfa1      = "";
     aAlfa2      = "";
     nSwErr      = 0;
     &eaProg     = "";
     &enErrCarte = 0;
     &nSwFlag    = 0;
|FIN;

|PROCESO agifa059;
     #10#52 = #59#15;
     #10#0 = #59#2;
     |LEE 121#10=;
     |SI FSalida = 0;
          #10#53 = "";
          #10#39 = 0;
          #10#38 = "N";
          |GRABA 020#10a;
          |LIBERA #10;
     |FINSI;
     |BORRA 020#59a;
|FPRC;

|PROCESO agifa134Fin;
     |BORRA 020#134a;
|FPRC;

|PROCESO borrarec;
     eaProg = "agifa134";
     |HAZ MiraRecVenta;
     |SI enErrCarte ! 0;
          aAlfa = "0000Recibos con movimientos, NO SE DARA DE BAJA... [" + enErrCarte + "]";
||        |MENAV aAlfa;
          nSwErr = 1;
          |ACABA;
     |FINSI;
     eaProg = "agifa134";
     |HAZ BorraRecVenta;
|FPRC;

|PROCESO agifa134;
     |HAZ borrarec;
     |SI nSwErr ! 0;
        nSwFlag = 1; |ERROR; |ACABA;
     |FINSI;
     |HAZ BorraVenciVta;
|FPRC;

|DEFBUCLE agifa134;
     #134#1;
     1, 48;
     #0#0, #0#1, #0#2, "N";
     #0#3, #0#4, #0#5, "N";
     be;
     NULL, agifa134, agifa059, agifa134Fin;
|FIN;

|PROCESO T3; |TIPO 3;
     |ABRE #10;
     |BUCLE agifa134;
     |CIERRA #10;
|FPRC;

|PROGRAMA;
     |SI PARAMETRO = "";
         |MANTE #diaz0024;
     |SINO;
         aAlfa1 = PARAMETRO % 105;
         aAlfa2 = PARAMETRO % 606;

         #diaz0024#0  = aAlfa1;
         #diaz0024#1  = aAlfa2;
         #diaz0024#2  = "01.01.0000";
         #diaz0024#3  = aAlfa1;
         #diaz0024#4  = aAlfa2;
         #diaz0024#5  = "31.12.9999";
         |HAZ T3;
     |FINSI;
|FPRO;
