|FICHEROS;
     agiw0007 #0; ||Este Def.
     agifa019 #1; ||Articulos.
     agifa219 #2; ||Descripcion Ampliada.
     agiw0008 #3; ||Auxiliar Etiquetas.
     agifa220 #4; ||Descrip.Ampliada.
|FIN;

|VARIABLES;
     &Tempo = "";
     aTempo3= "";
     swpar = 0;
     nImp = 0;
     aFich = "";
     nCon = 0;
     nLi  = 0;
     nEt  = 0;
|FIN;

|PROCESO agifa219;
     #1#0 = #2#0;
     |LEE 000#1=;
     |SI FSalida = 0;
          #3#0 = #1#0;
          #3#1 = #1#1;
          #3#2 = #0#0;
          |GRABA 020#3n;
          nImp = 1;
     |FINSI;
|FPRC;


|DEFBUCLE agifa219;
     #2#1;
     2;
#0#1,"S";
#0#2,"S";
;
NULL,agifa219;
|FIN;

|PROGRAMA;
     |CLS;
     |ABRE #0;
     |PINPA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |HAZ CreaTempo; aTempo3 = Tempo;
          |NOME_DAT #3 aTempo3; |DELINDEX #3;

          |ABRE #1;
          |ABRE #3;
          |BUCLE agifa219;
          |CIERRA #1;
          |CIERRA #3;
          |SI nImp = 1;
               |ENTLINEAL #1#3 , 1 , 2 , 22 , 1 , inf, sup;
               |HAZ impresion;
          |FINSI;

          |DELINDEX #3; Tempo = aTempo3; |HAZ BoraTempo;
     |FINSI;
|FPRO;

|RUTINA inf;
     #3#0 = #0#1;
|FRUT;

|RUTINA sup;
     #3#0 = #0#2;
|FRUT;

|PROCESO agifa220;
     |PARA nLi = 1; |SI nLi [ 6; |HACIENDO nLi = nLi +1;
          #4#0 = #3#0;
          #4#1 = nLi;
          |LEE 000#4=;
          |SI FSalida ! 0;
               |DDEFECTO #4;
          |FINSI;
          |SI swpar = 1;
               nCon = nLi + 2;
          |SINO;
               nCon = nLi + 12;
          |FINSI;
          #0nCon = #4#2;
     |SIGUE;
     |SI swpar = 2;
          |PRINT;
          |PIEINF;
          swpar = 0;
               #0#9  = "";
               #0#10 = "";
               #0#3 = "";
               #0#4 = "";
               #0#5 = "";
               #0#6 = "";
               #0#7 = "";
               #0#8 = "";
               #0#11 = "";
               #0#12 = "";
               #0#13 = "";
               #0#14 = "";
               #0#15 = "";
               #0#16 = "";
               #0#17 = "";
               #0#18 = "";
     |FINSI;
|FPRC;

|PROCESO agiw0008;
     #1#0 = #3#0;
     |LEE 000#1=;
     |SI FSalida ! 0; |ACABA; |FINSI;
     |PARA nEt = 1; |SI nEt [ #3#2; |HACIENDO nEt = nEt + 1;
          swpar = swpar + 1;
          |SI swpar = 1;
               #0#9  = #1#0;
               #0#10 = #1#2;
          |FINSI;
          |SI swpar = 2;
               #0#11 = #1#0;
               #0#12 = #1#2;
          |FINSI;
          |HAZ agifa220;
     |SIGUE;
|FPRC;

|DEFBUCLE agiw0008;
     #3#1;
     2;
#0#1,   1;
#0#2,9999;
;
NULL,agiw0008;
|FIN;

|PROCESO impresion;
     |IMPRE 0;
     |SI FSalida ! 0; |ACABA; |FINSI;
     |INFOR "agiw0008";
     |SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;
     |ABRE #1;
     |ABRE #4;
     |BUCLE agiw0008;
     |SI swpar = 1;
          |PRINT;
          |PIEINF;
     |FINSI;
     |CIERRA #1;
     |CIERRA #4;
     |FININF;
     |FINIMP;
|FPRC;
