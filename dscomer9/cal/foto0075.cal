|FICHEROS;
     foto0075 #0;
|FIN;

|VARIABLES;
     modo = 0;
     base = "";
     dir = "";
     dele = "";
     dirant = "";
     fin = "";
     fin2 = "";
|FIN;

|CALCULO proponpath;|TIPO 7;
     modo = (FEntrada / 100) ! 0;
     dirant = #0#1; |QBF dirant;
     |SI modo = 1;|Y dirant = "";
       |DBASS base; |QBF base;
       dele = #0#0; |QBF dele;
       dir = base + "dscomer/tpv/impor/" + dele + "/";
       #0#1 = dir;
     |FINSI;
|FCAL;

|CALCULO finbarra;|TIPO 0;
   fin = #0#1;|QBF fin;
   fin2 = fin % -101;
   |SI fin2 ! "/";|Y fin2 ! "\";
       |MENAV "0000El path debe acabar en '/'";
       |ERROR;
   |FINSI;
|FCAL;

|PROCESO inf;
     #0#0 = 1;
|FPRC;

|PROCESO sup;
     #0#0 = 99;
|FPRC;

|PROGRAMA;
     |CLS;
     |CABEZA "Control Importacion";
     |ENTLINEAL #1#0, 1, 1, 20, 1, inf, sup;
     |MENAV "0000Recuerde que debe crear los nuevos directorios si no existen.";
|FPRO;
