|FICHEROS;
     gomed001 #1;
     gomed002 #2;
     gomed003 #3;
     gomed004 #4;
|FIN;
----------------------------------------------------------------------
|VARIABLES;
     nModo     = 0;
     nConta    = 0;
     nBotPisos = 0;
|FIN;
----------------------------------------------------------------------
|PROCESO Ini; |TIPO 13;
     |ENTLINEAL #1#3,-3,7,22,0,MinMedi,MaxMedi;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO MinMedi;
     #3#0 = #1#0;
     #3#1 = #2#1;
     #3#2 = 1;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO MaxMedi;
     #3#0 = #1#0;
     #3#1 = #2#1;
     #3#2 = 999;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO Pisos; |TIPO 11;
     |SI FSalida = 10;
          |SI #2#2 = "                              ";
               |MENAV "0000DEBE INTRODUCIR UNA DESCRIPCION DE PLANTA";
               |ERROR10;
               |ACABA;
          |FINSI;
          |ESTADO_CONTROL nBotPisos, "HIDE";
          |SI nModo = 1; |O nModo = 2;
               |ENTLINEAL #1#3,-3,2,22,0,MinMedi,MaxMedi;
               |ERROR10;
          |FINSI;
          |SI nModo = 4;
               |ENTLINEAL #1#3,-3,4,22,0,MinMedi,MaxMedi;
               |ERROR10;
          |FINSI;
          |ESTADO_CONTROL nBotPisos, "SHOW";
     |FINSI;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO Conta; |TIPO 7;
     nModo = (FEntrada/100)!0;
     |SI nModo = 1;
          |LEE 000#1u;
          |SI FSalida = 0;
               nConta = #1#0 + 1;
          |SINO;
               nConta = 1;
          |FINSI;
          |DDEFECTO #1;
          #1#0 = nConta;
          |PINTA #1#0;
     |FINSI;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO PintPiso; |TIPO 7;
     |HAZ Ini;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO BInici; |TIPO 80;
     |CLS;
     |CONTROL_SIMPLE 0, "BOTON,F2 - Mantenimiento PISOS", 2425, 2455, F22;
     nBotPisos = FSalida;
     |ESTADO_CONTROL nBotPisos, "HIDE";
|FPRC;
-------------------------------------------------------------------------------
|PROCESO BFinali; |TIPO 90;
     |PULSATECLA;
     |FIN_CONTROL_WINDOWS nBotPisos;
     |PULSATECLA;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO F22;
     |PTEC 824;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO BSho; |TIPO 7;
     |ESTADO_CONTROL nBotPisos, "SHOW";
|FPRC;
-------------------------------------------------------------------------------
|PROCESO BBor; |TIPO 0;
     |ESTADO_CONTROL nBotPisos, "HIDE";
|FPRC;
-------------------------------------------------------------------------------
|PROCESO BBorr; |TIPO 11;
     |ESTADO_CONTROL nBotPisos, "HIDE";
|FPRC;
-------------------------------------------------------------------------------
|PROCESO Medicion; |TIPO 7;
     nModo = (FEntrada/100)!0;
     |SI nModo = 2;
          |ABRE #4;
          #4#0 = #1#0;
          |LEE 000#4=;
          |SI FSalida = 0;
               |MENAV "0000­­ATENCION!! Proyecto incluido en una medicion. La modificacion no sera registrada";
          |FINSI;
          |CIERRA #4;
     |FINSI;
|FPRC;
-------------------------------------------------------------------------------
