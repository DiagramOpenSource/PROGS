|FICHEROS;
     baezam15 #0;
     baezam15@;
|FIN;

|VARIABLES;
     aAlfa = "";
|FIN;

|PROCESO Defecto; |TIPO 0;
     |SI #0#2 = "N"; |ACABA; |FINSI;
     |DDEF aAlfa;
     aAlfa = aAlfa + "baezam15";
     |CARGA_DEF aAlfa, baezam15@;
     |SI FSalida = 0;
          |ABRE #baezam15@;
          |SELECT #2#baezam15@;
          #baezam15@#2 = "S";
          |LEE 000#baezam15@.=;
          |SI FSalida = 0;
               aAlfa = "0000La serie por defecto es:" + #baezam15@#0;
               |MENAV aAlfa;
               |ERROR;
          |FINSI;
          |CIERRA #baezam15@;
          |DESCARGA_DEF #baezam15@;
     |FINSI;
|FPRC;
