|FICHEROS;
     gomedive #0;
     dsm00239 #1;
|FIN;

|VARIABLES;
   nModo = 0;
   aAlfa = "";
|FIN;


|PROCESO Mediciones; |TIPO 0;
     |SI #0#2 = "N";
          |C_M #0#3, 1, "N";
          |C_M #0#4, 1, "N";
          |C_M #0#5, 1, "N";
          #0#3="N";#0#4="N";#0#5="N";
          |C_M #0#6, 1, "S";
          |C_M #0#7, 1, "S";
          |C_M #0#8, 1, "S";
          |C_M #0#9, 1, "S";
          |C_M #0#10, 1, "S";
          |C_M #0#11, 1, "S";

          |C_M #0#12, 1, "S";
     |SINO;
          |C_M #0#3, 1, "S";
          |C_M #0#4, 1, "S";
          |C_M #0#5, 1, "S";
          |C_M #0#6, 1, "N";
          |C_M #0#7, 1, "N";
          |C_M #0#8, 1, "N";
          |C_M #0#9, 1, "N";
          |C_M #0#10, 1, "N";
          |C_M #0#11, 1, "N";
          #0#6="N";#0#7="N";#0#8="N";
          #0#9="N";#0#10="N";#0#11="N";

          |C_M #0#12, 1, "N";
     |FINSI;

     |PINTA #0#3;|PINTA #0#4;|PINTA #0#5;
     |PINTA #0#6;|PINTA #0#7;|PINTA #0#8;
     |PINTA #0#9;|PINTA #0#10;|PINTA #0#11;

|FPRC;

|PROCESO Porcentaje; |TIPO 0;
     |SI #0#6 = "N";
          |C_M #0#7,  1, "N";
          |C_M #0#8,  1, "N";
          |C_M #0#9,  1, "N";
          |C_M #0#10, 1, "N";
          |C_M #0#11, 1, "N";
          #0#7="N";#0#8="N";
          #0#9="N";#0#10="N";#0#11="N";
     |SINO;
          |C_M #0#7,  1, "S";
          |C_M #0#8,  1, "S";
          |C_M #0#9,  1, "S";
          |C_M #0#10, 1, "S";
          |C_M #0#11, 1, "S";
     |FINSI;

     |PINTA #0#7;|PINTA #0#8;
     |PINTA #0#9;|PINTA #0#10;|PINTA #0#11;

|FPRC;
------------------------------------------------------------------------------
|PROCESO AltaGen; |TIPO 3;
     nModo = (FEntrada / 100) ! 0;

     |SI nModo = 1; |O nModo = 2;
          |ABRE #dsm00239; |DDEFECTO #dsm00239;
          #dsm00239#0 = #gomedive#0;
          |LEE 000#dsm00239.=;

          |SI FSalida ! 0;
                #dsm00239#1 = #gomedive#1;
                aAlfa = #dsm00239#0; |QBF aAlfa; aAlfa = aAlfa % 0103;
                #dsm00239#2 = aAlfa;
               |GRABA 020#dsm00239.c;
          |SINO;
                #dsm00239#1 = #gomedive#1;
                aAlfa = #dsm00239#0; |QBF aAlfa; aAlfa = aAlfa % 0103;
                #dsm00239#2 = aAlfa;
               |GRABA 020#dsm00239.a;
          |FINSI;

          |CIERRA #dsm00239;
     |FINSI;

|FPRC;
------------------------------------------------------------------------------
|PROCESO BajaGen; |TIPO 1;
   nModo = (FEntrada / 100) ! 0;
   |SI nModo = 3;
      |ABRE #dsm00239;
      #dsm00239#0 = #gomedive#0;
      |LEE 101#dsm00239.=;
      |SI FSalida = 0;
         |BORRA 020#dsm00239.a;
         |LIBERA #dsm00239;
      |FINSI;
      |CIERRA #dsm00239;
   |FINSI;
|FPRC;
