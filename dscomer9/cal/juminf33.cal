|PROCESO juminf13;
     aAlfa = ("0000" + #13#3) % -104;
     a1 = aAlfa % 102; h1 = a1;
     a2 = aAlfa % 302; h2 = a2;
     aAlfa = ("0000" + #13#4) % -104;
     a1 = aAlfa % 102; h3 = a1;
     a2 = aAlfa % 302; h4 = a2;
     nHora = (h3 * 60 + h4) - (h1 * 60 + h2);

     nMinu = nHora $ 60;
     enHora = (nHora - nMinu) / 60;
     enHora = enHora + (nMinu / 100);

     |PRINT;
|FPRC;

|DEFBUCLE juminf13;
     #13#1;
     6;
     #0#0, #0#1, #0#2, "S";
     #0#0, #0#1, #0#3, "S";
     ;
     NULL, NULL, NULL, NULL, NULL, juminf13;
     #13#5, #13#3, #13#4;
|FIN;

