|FICHEROS;
     dsm00214 #0
|FIN;

|VARIABLES;
  aAlfa = "";
  aAlfa1 = "";
  pass = "";
  numpass = "";
  num = 0;
  aIPRemoto = "";
|FIN;

|CALCULO finbarra;|TIPO 0;
     aAlfa = #0Campo;
     |QBF aAlfa;
     aAlfa1 = aAlfa % -101;
     |SI aAlfa1 ! "/";
          aAlfa = aAlfa + "/";
          #0Campo = aAlfa;
          |PINTA #0Campo;
     |FINSI;
|FCAL;


|PROGRAMA;
  |CLS;
  |CABEZA "Parametros de DSCorreo";
  |PINPA #0#0;
  |ABRE #0;
  |DDEFECTO #0;
  #0#0 = "A";
  |LEE 101#0=;
  |SI FSalida ! 0;
      |DDEFECTO #0;
      |GRABA 020#0b;
  |FINSI;
  |PINDA #0#0;
  |ENDATOS #1#0;
  |SI FSalida = 0;
      |GRABA 020#0a;
  |FINSI;
  |LIBERA #0;
  |CIERRA #0;
|FPRO;

|PROCESO copia;
|SI #0Campo ! Contenido;
#0#13 = #0#16;
pass = #0#13;
|QBF pass;
num = 0;
numpass = pass % num;
pass = "**********************************************";
numpass = 10 + numpass;
pass = pass % numpass;
#0#16 = pass;
|PINTA #0#16;
|FINSI;
|FPRC;

|PROCESO DSCorreo; |TIPO 0;
     |QUE_SISTEMA aAlfa1;
     |SI aAlfa1 = "ESBSD";|Y #dsm00214#19 = "S";
          |MENAV "    Configuracion no permitida, servidor UNIX. Recomendado: [F]";
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO SoloFija; |TIPO 7;
     |SI #dsm00214#19 = "F";
          aAlfa1 = #dsm00214#18; |QBF aAlfa1;
          |SI aAlfa1 = "Automatico";
               #dsm00214#18 = "";
               |PINTA #dsm00214#18;
          |FINSI;
          |C_M #dsm00214#18, 1, "S";
     |SINO;
          |C_M #dsm00214#18, 1, "N";
          #dsm00214#18 = "Automatico";
          |PINTA #dsm00214#18;
     |FINSI;
|FPRC;

|PROCESO EsMultipuesto; |TIPO 0;
     |IP_REMOTO aIPRemoto;
     |QBT aIPRemoto;
     |SI aIPRemoto ! "";|Y #dsm00214#19 ! "F";
          |QUE_SISTEMA aAlfa1;
          |SI aAlfa1 = "ESBSD";
               #dsm00214#19 = "L";
          |SINO;
               #dsm00214#19 = "S";
          |FINSI;
          |PINTA #dsm00214#19;
     |FINSI;
|FPRC;
