|FICHEROS;
     goparam1 #0;
     goparam2 #1,MANTE;
     gopre003 #2;
     goparam3 #3;
     goparam4 #4;
     goartive #5;
|FIN;
------------------------------------------------------------------------------
|VARIABLES;
     &cTitulo1      = "";
     &cTitulo2      = "";
     &cTitulo3      = "";
     &cTitulo4      = "";
     &cCombina      = "";
     &cPart         = "";

     &nSalida       = 0;

     cPartida       = "";
     cDescrip       = "";
|FIN;
------------------------------------------------------------------------------
|PROCESO Pinta1; |TIPO 7;
     |ATRI R;
     |PINTA 0814, cTitulo1;
     |ATRI 0;
     |ATRI I;
     |ABRE #5;
     #5#0 = cPart;
     |LEE 000#5=;

     |SI FSalida = 0;
          cPartida = #5#0;
          cDescrip = #5#1;
          |QBF cPartida;
          |QBF cDescrip;
     |FINSI;

     |CIERRA #5;
     nSalida = 0;
     |PINTA 0515, cPartida;
     |PINTA 0615, cDescrip;
     |ATRI 0;

     |SI cTitulo2 ! "";
          |ENTLINEAL #1#1, 1, 7, 14, 0, MinPar1, MaxPar1;
     |FINSI;

     |ATRI R;
     |PINTA 0854, cTitulo2;
     |ATRI 0;

     |SI cTitulo3 ! "";
          |ENTLINEAL #1#3, 1, 7, 22, 0, MinPar1, MaxPar1;
     |FINSI;

     |ATRI R;
     |PINTA 1614, cTitulo3;
     |ATRI 0;

     |SI cTitulo4 ! "";
          |ENTLINEAL #1#4, 1, 7, 22, 0, MinPar1, MaxPar1;
     |FINSI;

     |ATRI R;
     |PINTA 1654, cTitulo4;
     |ATRI 0;
|FPRC;
------------------------------------------------------------------------------
|PROCESO Marca1; |TIPO 0;

     |SI FSalida = 0;
          #0#0 = "*";
          cCombina = #0#1;
          |PINTA #0#0;

          |SI cTitulo2 ! "";
               |ENTLINEAL #1#1, 1, 4, 14, 0, MinPar1, MaxPar1;
          |FINSI;

          |SI cTitulo2 = "";
               |PTEC 807;
          |FINSI;

     |FINSI;

|FPRC;
------------------------------------------------------------------------------
|PROCESO MinPar1;
     #1#1 = " ";
     #3#1 = " ";
     #4#1 = " ";
|FPRC;
------------------------------------------------------------------------------
|PROCESO MaxPar1;
     #1#1 = "z";
     #3#1 = "z";
     #4#1 = "z";
|FPRC;
