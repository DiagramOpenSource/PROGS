                  || Me guardo el puntero
                  serie   = #0#20;   || Serie y pedido actual
                  pedido  = #0#0;
                  nlinea  = #0#1;

                  |HAZ crealineasV;

                  || Recupero puntero
                  #0#20 = serie;
                  #0#0  = pedido;
                  #0#1  = nlinea;
                  |LEE 001#0=;

