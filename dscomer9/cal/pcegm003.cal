           |SIGUE;
      |SINO;
           |MENAV "0000Atencion !!! No se ha encontrado la F.Proforma indicada";
           |ERROR;
      |FINSI;

      pedido_o = 0;

      || Devuelvo el puntero de la linea
      #0#20= #1#48;       || Serie
      #0#0 = #1#0;        || pedido
      #0#1 = nLineaD;     || Linea
