     #referen@#0 = #0#0;
     #referen@#1 = #0#36;
     |LEE 000#referen@.=;
     |SI FSalida = 0;
          nLin = #0#36;
     |SINO;
          #referen@#1 = 999;
          |LEE 000#referen@.];
          |SI FSalida = 0;
               |LEE 000#referen@.a;
          |SINO;
               |LEE 000#referen@.u;
          |FINSI;
          |SI FSalida = 0; |Y #referen@#20 = #0#20;  |Y #referen@#0 = #0#0;
               nLin = #referen@#1 + 1;
          |SINO;
               nLin = 1;
          |FINSI;
          |SI nLin = #0#1; nLin = nLin + 1; |FINSI;
          #referen@#20 = #0#20;
          #referen@#0 = #0#0;
          #referen@#1 = nLin;
          |GRABA 020#referen@.b;
     |FINSI;
     |SI #0#39 > 0;
          |SALVA_DATOS #0;
          |REPON_DATOS #referen@;

