|FICHEROS;
  guerm008 #0;
  guerm007 #7;
|FIN;

|VARIABLES;
     aAlfa     = "";
     i         = 0;
     {-1}menu = "";
     menu1 = "2400";
     menu2 = "1";
     menu3 = "Contador:  ";
     menu4 = "AM";
     menu5 = " Automatico ";
     menu6 = " Manual ";
     nConta = 0;
     nModo = 0;
|FIN;

|PROCESO calcula;
  #0#12 = (#0#10 * #0#11) ! 2;
  |PINTA #0#12;
|FPRC;


|PROCESO Conta; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |MENU menu;
          |SI FSalida = 1;
               |LEE 000#0u;
               |SI FSalida = 0;
                    nConta = #0#0 + 1;
               |SINO;
                    nConta = 1;
               |FINSI;
               |DDEFECTO #0;
               #0#0 = nConta;
               |PTEC 802;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |PARA i = 1; |SI i [ 18; |HACIENDO i = i + 1;
               |C_M #0i, 1, "S";
          |SIGUE;
     |SINO;
          |PARA i = 1; |SI i [ 18; |HACIENDO i = i + 1;
                |C_M #0i, 1, "N";
          |SIGUE;
          |C_M #0#5, 1, "S";
          |SI nModo = 2;  |Y #0#5 = "999999";
              |PARA i = 1; |SI i [ 18; |HACIENDO i = i + 1;
                    |C_M #0i, 1, "S";
              |SIGUE;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Hora; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |HORA aAlfa;
          #0#2 = aAlfa;
     |FINSI;
|FPRC;

|PROCESO Tarjeta; |TIPO 0;
     |SI #0Campo ! Contenido;
          |ABRE #7;
          #7#0 = #0#5;
          |LEE 020#7=;
          |SI FSalida = 0;
               #0#6 = #7#1; |PINTA #0#6;
               #0#7 = #7#2; |PINTA #0#7;
               #0#9 = #7#2; |PINTA #0#9;
               #0#16 = #7#3;|PINTA #0#16;
               #0#18 = #7#5;|PINTA #0#18;
          |FINSI;
          |CIERRA #7;
     |FINSI;
|FPRC;
