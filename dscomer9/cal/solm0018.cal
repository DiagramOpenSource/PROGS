|FICHEROS;
     solm0018 #0;
|FIN;

|VARIABLES;
     nLin = 0;
     nModo = 0;
     aCli = "";
     aNom = "";
|FIN;

|PROCESO Conta; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          aCli = #0#0;
          aNom = #0#2;
          #0#1 = 999;
          |LEE 000#0];
          |SI FSalida = 0;
               |LEE 000#0a;
          |SINO;
               |LEE 000#0u;
          |FINSI;
          |SI FSalida = 0; |Y aCli = #0#0;
               nLin = #0#1 + 1;
          |SINO;
               nLin = 1;
          |FINSI;
          |DDEFECTO #0;
          #0#0 = aCli;
          #0#1 = nLin;
          #0#2 = aNom;
     |FINSI;
|FPRC;

|PROCESO Min;
     #0#0 = "      ";
     #0#1 = 1;
|FPRC;

|PROCESO Max;
     #0#0 = "zzzzzz";
     #0#1 = 999;
|FPRC;

|PROGRAMA;
     |ENTLINEAL #1#0, 1, 2, 22, 1, Min, Max;
|FPRO;
