|FICHEROS;
 tlfm0021 #0;
|FIN;

|VARIABLES;
     base      = "";
     fichero   = "";
     aAlfa = "";
|FIN;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ABRE #0;
     |DDEFECTO #0;
     |LEE 101#0p;
     |SI FSalida ! 0;
          |DBASS base; |QBF base;
          fichero = base + "agicont/";  #0#1 = fichero;
          |GRABA 020#0b;
     |FINSI;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          aAlfa = #0#1; |QBF aAlfa;
          aAlfa = aAlfa % -101;
          |SI aAlfa ! "/"; |Y aAlfa ! "\";
               aAlfa = #0#1; |QBF aAlfa;
               aAlfa = aAlfa + "/";
               #0#1 = aAlfa;
          |FINSI;
          |GRABA 020#0a;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;
