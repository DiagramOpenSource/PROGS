-----------------------------------------------------------------------------
|PROCESO MuLinPar; |TIPO 7;
     |PINTA 0547, #3#11;
     |PINTA 0567, #3#12;
     |ATRI P;
     |PINTA 1461, "CTD./TOT.";
     |PINTA 1471, "T.CAPITUL";
     |ATRI 0;
     |ENTLINEAL #2#4,-4,7,22,0,MinDesTot,MaxDesTot;
     |ENTLINEAL #2#1,-5,7,12,0,MinPar,MaxPar;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO EnLinPar; |TIPO 0;
     |ATRI R;
     |PINTA 1461, "CTD./MED.";
     |PINTA 1471, "T.PARTIDA";
     |ATRI 0;
     |ENTLINEAL #2#1,-5,4,12,0,MinPar,MaxPar;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO MinPar;
     #1#0 = #0#0;
     #1#1 = #0#1;
     #1#2 = #0#3;
     #1#4 = " "*20;
     #1#3 = 1;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO MaxPar;
     #1#0 = #0#0;
     #1#1 = #0#1;
     #1#2 = #0#3;
     #1#4 = "z"*20;
     #1#3 = 999;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO MinDesTot;
     #4#0 = #0#0;
     #4#1 = #0#1;
     #4#2 = #0#3;
     #4#4 = 1;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO MaxDesTot;
     #4#0 = #0#0;
     #4#1 = #0#1;
     #4#2 = #0#3;
     #4#4 = 9999;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO MuLinDesTot; |TIPO 11;
     |SI FSalida = 10;
          |ENTLINEAL #2#4,4,4,22,0,MinDesTot,MaxDesTot;
     |FINSI;
|FPRC;
