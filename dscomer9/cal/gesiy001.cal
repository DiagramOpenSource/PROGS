|FICHEROS;
     gesiy001 #0;
     gesim001 #1;
     gesim005 #5;
     gesim006 #6;
|FIN;

|VARIABLES;
     aInf = "";
     &enHay = 0;
|FIN;

|PROCESO Partes;
     enHay = 1;
     |PRINT;
|FPRC;

|DEFBUCLE Partes;
     #6#1;
     ;
     #5#0, #5#1, 001;
     #5#0, #5#1, 999;
     ;
     NULL, Partes;
|FIN;

|PROCESO Avisos;
     enHay = 0;
     |BUCLE Partes;
     |SI enHay = 0;
          |DDEFECTO #6;
          |PRINT;
     |FINSI;
|FPRC;

|DEFBUCLE Avisos;
     #5#2;
     64, 17, 18;
     #0#0, #0#2, #0#10, "     ", 000000, #0#4, #0#6, #0#8;
     #0#1, #0#3, #0#11, "zzzzz", 999999, #0#5, #0#7, #0#9;
     ;
     NULL, Avisos;
|FIN;

|PROCESO T3; |TIPO 3;
     |ABRE #1; |LEE 000#1p; |CIERRA #1;
     |SI #0#12 = "D";
          aInf = #1#27; |QBF aInf;
     |SINO;
          aInf = #1#28; |QBF aInf;
     |FINSI;

     Impresora = "CrystalReport";
     |IMPRE -1;
     |SI FSalida = 0;
          |INFOR aInf;
          |SI FSalida = 0;
               |BUCLE Avisos;
               |PIEINF;
               |FININF;
          |SINO;
               aInf = "0000Error en informe:" + aInf;
               |MENAV  aInf;
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;
