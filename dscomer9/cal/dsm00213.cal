|FICHEROS;
     dsm00213 #0;
|FIN;

|VARIABLES;
     aAlfa     = "";
     i         = 0;
     base      = "";
     fichero   = "";
     aAlfa1    = "";
     nMod      = 0;
|FIN;

|PROCESO inf;
     #0#0 = "  ";
|FPRC;

|PROCESO sup;
     #0#0 = "zz";
|FPRC;

|PROCESO McaDir;
     aAlfa = #0#1;
     |QBF aAlfa;
     |MKDIR aAlfa;
|FPRC;

|DEFBUCLE McaDir;
     #0#1;
     ;
     "  ";
     "zz";
     ;
     NULL,McaDir;
|FIN;

|PROGRAMA;
     |CLS;
     |CABEZA "Control Exportacion";
     |ENTLINEAL #1#0, -1, 1, 22, 1, inf, sup;
     |BUCLE McaDir;
|FPRO;

|PROCESO MiraDir; |TIPO 7;
     nMod = (FEntrada / 100 ) ! 0;
     |SI nMod = 1;
        ||  aAlfa1 = #0#0;
        ||  |QBF aAlfa1;
        ||  aAlfa1 = ("00" + aAlfa1) % -102;
          |DBASS base; |QBF base;
          fichero = base; || + "tpvimpor/" + aAlfa1 + "/";
          #0#1 = fichero;
          |PINTA #0#1;
     |FINSI;
|FPRC;

|PROCESO MiraBarra; |TIPO 0;
     aAlfa = #0Campo;
     |QBF aAlfa;
     aAlfa1 = aAlfa % -101;
     |SI aAlfa1 ! "/";
          aAlfa = aAlfa + "/";
          #0Campo = aAlfa;
          |PINTA #0Campo;
     |FINSI;
     |MKDIR aAlfa;
|FPRC;
