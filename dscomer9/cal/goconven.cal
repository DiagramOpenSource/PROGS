|FICHEROS;
     goconven #0;
|FIN;

|VARIABLES;
     nConta    = 0;
     nModo     = 0;
|FIN;

|PROCESO ContConv; |TIPO 7;
     nModo = (FEntrada/100)!0;
     |SI nModo ! 1;
          |ACABA;
     |FINSI;
     |ABRE #0;
     |LEE 000#0u;
     |SI FSalida = 0;
          nConta = #0#0;
          nConta = nConta + 1;
     |SINO;
          nConta = 1;
     |FINSI;
     |DDEFECTO #0;
     #0#0 = nConta;
     |PINTA #0#0;
|FPRC;
