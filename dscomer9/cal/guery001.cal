|FICHEROS;
     guery001 #0;
     guerm001 #1;
     guerm002 #2;
|FIN;

|VARIABLES;
     nLin      = 0;
     &enSwlin  = 0;
|FIN;

|PROCESO guerm002;
     |SI nLin = 0;
         enSwlin  = 0;
     |SINO;
         enSwlin  = 1;
     |FINSI;

     nLin = nLin + 1;
     |PRINT;
|FPRC;

|PROCESO guerm001;
     nLin = 0;
     |SI #1#15 = "A"; |Y #0#4 = "N";
          |ERROR;
     |FINSI;
     |SI #1#15 = "T"; |Y #0#5 = "N";
          |ERROR;
     |FINSI;
     |SI #1#15 = "A"; |Y #0#6 = "N";
          |ERROR;
     |FINSI;
|FPRC;

|DEFBUCLE guerm001;
     #1#6;
     ;
     #0#0, #0#2;
     #0#1, #0#3;
     ;
     NULL, NULL, NULL, NULL, NULL, guerm001, guerm002;
     #1#15, #0#0;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR "guery001";
          |SI FSalida = 0;
               |BUCLE guerm001;
               |PIEINF;
               |FININF;
          |SINO;
               |MENAV "0000No existe informe";
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;
