|FICHEROS;
     dsz00014 #0;
     agifa065 #65;
     dsm00006 #906;
|FIN;

|VARIABLES;
     aDArti    = " " * 20;
     aHArti    = "z" * 20;
     aAlfa     = "";
     nReg      = 0;
|FIN;

|PROCESO dsm00006;
     #65#0 = #906#0;            ||arti
     #65#1 = #906#2;            ||fecha
     #65#2 = #906#3;            ||ope
     #65#3 = #906#5;            ||doc
     #65#4 = #906#6;            ||line
     #65#11 = #906#4;           ||serie
     |LEE 000#65=;
     |SI FSalida ! 0;
          nReg = nReg + 1;
          |BORRA 020#906a;
     |FINSI;
|FPRC;

|DEFBUCLE dsm00006;
     #906#1;
     ;
     aDArti, 00001, "01.01.0000", "  ", "     ", 000000, 00000, "      ";
     aHArti, 99999, "31.12.9999", "zz", "zzzzz", 999999, 99999, "zzzzzz";
     b;
     NULL, dsm00006;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |SI #0#0 = "SI";
          |ABRE #65;
          |BUCLE dsm00006;
          |CIERRA #65;
          |SI nReg > 0;
               aAlfa = "0000Borrados " + nReg + " registros...";
          |SINO;
               aAlfa = "0000No hay diferencias...";
          |FINSI;
          |MENAV aAlfa;
     |FINSI;
|FPRC;
