|FICHEROS;
     agifa556 #0;
|FIN;

|VARIABLES;
     aAlfa     = "";
     i         = 0;
     base      = "";
     fichero   = "";
     aAlfa1    = "";
     nMod      = 0;
|FIN;

|PROCESO CompruSesion; |TIPO 0;
     |SI #0#2 = "S"; |Y #0#3 = "  ";
          |MENAV "0000La sesion de desglose no puede ser blanco...";
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO Cliente; |TIPO 7;
     |SI #0#10 = "T";
          #0#11 = ""; |PINTA #0#11;
          |C_M #0#11, 1, "N";
     |SINO;
          |SI #0#2 = "S";
               |C_M #0#11, 1, "S";
          |SINO;
               |C_M #0#11, 1, "N";
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Desglose; |TIPO 0;
     aAlfa = #0#2;
     |PARA i = 3; |SI i [ 15; |HACIENDO i = i + 1;
          |C_M #0i, 1, aAlfa;
     |SIGUE;
|FPRC;

|PROCESO inf;
     #0#0 = 1;
     #0#16 = "     ";
|FPRC;

|PROCESO sup;
     #0#0 = 99999;
     #0#16 = "zzzzz";
|FPRC;

|PROGRAMA;
     |CLS;
     |CABEZA "Control Importacion";
     |ENTLINEAL #1#0, -1, 1, 22, 1, inf, sup;
|FPRO;
