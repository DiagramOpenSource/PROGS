|FICHEROS;
     guerz046 #0;
     goalbpa4 #4;
     goalbpa5 #5;
     goalbpa6 #6;
     guerm045 #45;
|FIN;

|VARIABLES;
     nSwImpre = 0;
     nSwInfor = 0;
     nSwPrime = 0;
|FIN;

|PROCESO goalbpa6;
     |SI #0#1 = "S";
          #6#10 = -#6#10;
          |HAZ CalcuGopartia;
          |GRABA 020#6a;
     |FINSI;
     |LIBERA #6;
     |SI #0#0 = "S";
          |SI nSwPrime = 0;
               nSwPrime = 1;
               |IMPRE 0;
               nSwImpre = FSalida;
               |SI FSalida = 0;
                    |INFOR "guerz046";
                    nSwInfor = FSalida;
                    |SI FSalida ! 0;
                         |MENAV "0000No existe informe 'guerz046'";
                    |FINSI;
               |FINSI;
          |FINSI;
          |SI nSwImpre = 0; |Y nSwInfor = 0;
               |PRINT;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE goalbpa6;
     #6#1;
     ;
     "A", #5#0, #5#1, #5#2, 001;
     "A", #5#0, #5#1, #5#2, 999;
     be;
     NULL, goalbpa6;
|FIN;

|PROCESO goalbpa5;
     |BUCLE goalbpa6;
     |SI #0#1 = "S";
          #5#5 = -#5#5;
          #5#6 = -#5#6;
          #5#10 = #5#9 * #5#6;
          |GRABA 020#5a;
          |LIBERA #5;
     |FINSI;
|FPRC;

|DEFBUCLE goalbpa4;
     #4#1;
     ;
     #45#4, #45#5;
     #45#4, #45#5;
     be;
     NULL, NULL, goalbpa5;
|FIN;

|PROCESO guerm045;
     |BUCLE goalbpa4;
|FPRC;

|DEFBUCLE guerm045;
     #45#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, guerm045;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |SI #0#0 = "S"; |O #0#1 = "S";
          |BUCLE guerm045;
     |FINSI;
     |SI #0#0 = "S";
          |SI nSwPrime ! 0;
               |SI nSwInfor = 0; |PIEINF; |FININF; |FINSI;
               |SI nSwImpre = 0; |FINIMP; |FINSI;
          |SINO;
               |MENAV "0000No hay registros...";
          |FINSI;
     |FINSI;
|FPRC;
