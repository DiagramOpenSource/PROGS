     ialbm018 #18;

     &referc1@;
     &referc2@;
     &referc3@;
|FIN;

|VARIABLES;
     &enPestana = 0;
     &eaCliDoc = "";
     &enTipo = 0;
     &enModo = 0;
     &enError = 0;
     &eaMatricula = "";
     &eaSerAviso = "";
     &enNumAviso = 0;
     v0 = "000000";
     v1 = "   ";
     v2 = 0;
     aPath = "";
     nPanta = 0;
     aAlfa = "";
     nRango = 0;
     nPos1 = 0;
     nPos2 = 0;
     nHay = 0;
     nNume = 0;

     nBtnSalir3 = 0;
     nBtnReabre3 = 0;
     nBtnCerrar3 = 0;
     nBtnImpre3 = 0;
     nBtnModifi3 = 0;
     nBtnHisMaq3 = 0;
     nBtnAsgMaq3 = 0;
     nBtnModiPar3 = 0;
     nBtnImpPar3 = 0;
     nBtnHisMaqPar3 = 0;
     nBtnAsgMaqPar3 = 0;
     nEvtFoco = 0;
     &enGridc1 = 0;
     &enGridc2 = 0;
     &enGridc3 = 0;
|FIN;

|PROCESO CheqMatri;
     aAlfa = #18#41; |QBF aAlfa;
     |SI aAlfa = "";
          aAlfa = "0000No se ha indicado la matr¡cula en el parte:" + (("000"+#18#2)%-103);
          |MENAV aAlfa;
          nHay = 1;
     |FINSI;
|FPRC;

|DEFBUCLE CheqMatri;
     #18#1;
     ;
     #referc1@#0, nNume, 001;
     #referc1@#0, nNume, 999;
     ;
     NULL, CheqMatri;
|FIN;

|PROCESO EventoFoco3;
     enPestana = 3;
|FPRC;

|PROCESO BtnModiPar3;
     |LEE 000#referc2@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |SALVA_DATOS #referc1@;
     |REPON_DATOS #17;
     |SALVA_DATOS #referc2@;
     |REPON_DATOS #18;

     enModo = 2; |HAZ EditaParte;
     |SI enError = 0;
          #referc2@#1 = 0; |REFRESCACONTROL enGridc3;
          |SALVA_DATOS #18;
          |REPON_DATOS #referc2@;
          |LEE 000#referc2@.=;
          |REFRESCACONTROL enGridc2;
          |REFRESCACONTROL enGridc1;
     |FINSI;
|FPRC;

|PROCESO BtnImpPar3;
     |LEE 000#referc2@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |SALVA_DATOS #referc1@;
     |REPON_DATOS #17;
     |SALVA_DATOS #referc2@;
     |REPON_DATOS #18;
     |HAZ ImpreParte;
|FPRC;

|PROCESO BtnHisMaqPar3;
     |LEE 000#referc2@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     eaMatricula = #referc2@#41;
     eaSerAviso = #referc2@#0;
     enNumAviso = #referc2@#1;
     |HAZ HistoMaquina;
|FPRC;

|PROCESO BtnAsgMaqPar3;
     |LEE 000#referc2@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |SALVA_DATOS #referc1@;
     |REPON_DATOS #17;
     |SALVA_DATOS #referc2@;
     |REPON_DATOS #18;
     |HAZ AsignaMatriPar;
     |SI enError = 0;
          |REFRESCACONTROL enGridc2;
          |REFRESCACONTROL enGridc1;
     |FINSI;
|FPRC;

|PROCESO BtnReabre3;
     |LEE 000#referc1@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     aAlfa = "0000NReabrir aviso:" + #referc1@#0+"/"+(("000000"+#referc1@#1)%-106);
     |CONFI aAlfa;
     |SI FSalida = 0;
          |ABRE #17;
          #17#0 = #referc1@#0;
          #17#1 = #referc1@#1;
          |LEE 121#17=;
          |SI FSalida = 0;
               #17#24 = "T";
               |GRABA 020#17a;
               |ABRE #18;
               #18#0 = #17#0;
               #18#1 = #17#1;
               #18#2 = 999;
               |LEE 000#18];
               |SI FSalida = 0;
                    |LEE 000#18a;
               |SINO;
                    |LEE 000#18u;
               |FINSI;
               |SI FSalida = 0; |Y #18#0 = #17#0; |Y #18#1 = #17#1;
                    |LEE 121#18=;
                    #18#3 = "A";
                    |GRABA 020#18a;
               |FINSI;
               |CIERRA #18;
               #referc2@#1 = 0; |REFRESCACONTROL enGridc3;
               #referc1@#1 = 0; |REFRESCACONTROL enGridc2;
               |REFRESCACONTROL enGridc1;
               |HAZ Refresca2;
          |FINSI;
          |CIERRA #17;
     |FINSI;
|FPRC;

|PROCESO BtnCerrar3;
     |LEE 000#referc1@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     aAlfa = "0000NCerrar aviso:" + #referc1@#0+"/"+(("000000"+#referc1@#1)%-106);
     |CONFI aAlfa;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |ABRE #17;
     #17#0 = #referc1@#0;
     #17#1 = #referc1@#1;
     |LEE 121#17=;
     enError = 0;
     |SI #referc1@#46 = 0;        || Sin pedido crea albaran
          nHay = 0;
          nNume = #referc1@#1;
          |BUCLE CheqMatri;
          |SI nHay ! 0;
               |CONFI "0000NNo se ha indicado la matr¡cula en todos los partes. ¨Continuar?";
               enError = FSalida;
          |SINO;
               enError = 0;
          |FINSI;
          |SI enError = 0;
               enTipo = 1;
               |HAZ CreaAlba;
          |FINSI;
     |FINSI;
     |SI enError = 0;
          #17#24 = "V";
          |GRABA 020#17a;

          |HAZ Refresca3;
          |HAZ Refresca4;
     |FINSI;
     |CIERRA #17;
|FPRC;

|PROCESO BtnImpre3;
     |LEE 000#referc1@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |SALVA_DATOS #referc1@;
     |REPON_DATOS #17;
     |HAZ ImpreAviso;
|FPRC;

|PROCESO BtnModifi3;
     |LEE 000#referc1@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |SALVA_DATOS #referc1@;
     |REPON_DATOS #17;
     enModo = 2; |HAZ EditaAviso;
     |SI enError = 0; |HAZ Refresca3; |FINSI;
|FPRC;

|PROCESO BtnHisMaq3;
     |LEE 000#referc1@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |SALVA_DATOS #referc1@;
     |REPON_DATOS #17;
     enModo = 2; |HAZ EditaAviso;
     |SI enError = 0; |HAZ Refresca3; |FINSI;
|FPRC;

|PROCESO BtnAsgMaq3;
     |LEE 000#referc1@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |SALVA_DATOS #referc1@;
     |REPON_DATOS #17;
     |HAZ AsignaMatri;
     |SI enError = 0; |HAZ Refresca3; |FINSI;
|FPRC;

|PROCESO Evento31;
     |SI FSalida = 1; |O FSalida = 2;
          |LEE 000#referc1@.=;
          |SI FSalida = 0;
               eaCliDoc = #referc1@#5;
               #0#6 = #referc1@#7;
               #0#7 = #referc1@#8;
               #0#8 = #referc1@#9;
          |SINO;
               #0#6 = "";
               #0#7 = "";
               #0#8 = "";
          |FINSI;
          |PINPA #3#0;
          |PINTA 1609, #0#6;
          |PINTA 1709, #0#7;
          |PINTA 1809, #0#8;
          #referc2@#1 = 0; |REFRESCACONTROL enGridc3;
          |REFRESCACONTROL enGridc2;
     |FINSI;
|FPRC;

|PROCESO Evento32;
     |SI FSalida = 1; |O FSalida = 2;
          |LEE 000#referc2@.=;
          |REFRESCACONTROL enGridc3;
     |FINSI;
|FPRC;

|PROCESO Evento33;
     |SI FSalida = 1; |O FSalida = 2;
          |LEE 000#referc3@.=;
     |FINSI;
|FPRC;

|PROCESO Min31;
     #referc1@#0 = "     ";
     #referc1@#1 = 000000;
     #referc1@#5 = #0#0;
     #referc1@#18 = #0#1;
     #referc1@#19 = #0#2;
     #referc1@#24 = "C";
     |SI #0#0 = "000000"; #referc1@#5# = "      "; |FINSI;
     |SI #0#1 = "000"; #referc1@#18# = "   "; |FINSI;
|FPRC;

|PROCESO Max31;
     #referc1@#0 = "zzzzz";
     #referc1@#1 = 999999;
     #referc1@#5 = #0#0;
     #referc1@#18 = #0#1;
     #referc1@#19 = #0#2;
     #referc1@#24 = "C";
     |SI #0#0 = "000000"; #referc1@#5# = "zzzzzz"; |FINSI;
     |SI #0#1 = "   "; #referc1@#18# = "zzz"; |FINSI;
     |SI #0#1 = "000"; #referc1@#18# = "zzz"; |FINSI;
     |SI #0#2 = 0; #referc1@#19# = 99999; |FINSI;
|FPRC;

|PROCESO Min32;
     #referc2@#0 = #referc1@#0;
     #referc2@#1 = #referc1@#1;
     #referc2@#2 = 1;
|FPRC;

|PROCESO Max32;
     #referc2@#0 = #referc1@#0;
     #referc2@#1 = #referc1@#1;
     #referc2@#2 = 999;
|FPRC;

|PROCESO Min33;
     #referc3@#0 = #referc2@#0;
     #referc3@#1 = #referc2@#1;
     #referc3@#2 = #referc2@#2;
     #referc3@#3 = 1;
|FPRC;

|PROCESO Max33;
     #referc3@#0 = #referc2@#0;
     #referc3@#1 = #referc2@#1;
     #referc3@#2 = #referc2@#2;
     #referc3@#3 = 999;
|FPRC;

|PROCESO Actualiza3;
     |DESTRUYECONTROL enGridc1;

     |PINPA #3#0; nPanta = FSalida;
     nRango = 4 + 8 + 16 + 32;
     nPos1 = 0602;
     nPos2 = 1598;

     |SI #0#0 ! v0;
          |SI #0#1 ! v1;
               |LINEAL_SIMPLE #8#referc1@, nRango, nPos1, nPos2, Min31, Max31, Evento31;
          |SINO;
               |LINEAL_SIMPLE #9#referc1@, nRango, nPos1, nPos2, Min31, Max31, Evento31;
          |FINSI;
     |SINO;
          |SI #0#1 ! v1;
               |SI #0#0 ! v0;
                    |LINEAL_SIMPLE #10#referc1@, nRango, nPos1, nPos2, Min31, Max31, Evento31;
               |SINO;
                    |LINEAL_SIMPLE #11#referc1@, nRango, nPos1, nPos2, Min31, Max31, Evento31;
               |FINSI;
          |SINO;
               |SI #0#0 ! v0;
                    |LINEAL_SIMPLE #12#referc1@, nRango, nPos1, nPos2, Min31, Max31, Evento31;
               |SINO;
                    |LINEAL_SIMPLE #13#referc1@, nRango, nPos1, nPos2, Min31, Max31, Evento31;
               |FINSI;
          |FINSI;
     |FINSI;
     enGridc1 = FSalida;
     #referc2@#1 = 0; |REFRESCACONTROL enGridc3;
     #referc1@#1 = 0; |REFRESCACONTROL enGridc2;
     |REFRESCACONTROL enGridc1;
|FPRC;

|PROCESO PintaPan3;
     |DDEF aPath;
     aAlfa = aPath + "ialbm017"; |CARGA_DEF aAlfa, referc1@;
     aAlfa = aPath + "ialbm018"; |CARGA_DEF aAlfa, referc2@;
     aAlfa = aPath + "ialbm019"; |CARGA_DEF aAlfa, referc3@;
     |ABRET #referc1@;
     |ABRET #referc2@;
     |ABRET #referc3@;

     |PINPA #3#0; nPanta = FSalida;
     nRango = 4 + 8 + 16 + 32;
     nPos1 = 2802;
     nPos2 = 3398;
     |LINEAL_SIMPLE #1#referc3@, nRango, nPos1, nPos2, Min33, Max33, Evento33;
     enGridc3 = FSalida;

     nRango = 4 + 8 + 16 + 32;
     nPos1 = 2002;
     nPos2 = 2798;
     |LINEAL_SIMPLE #1#referc2@, nRango, nPos1, nPos2, Min32, Max32, Evento32;
     enGridc2 = FSalida;

     nRango = 4 + 8 + 16 + 32;
     nPos1 = 0602;
     nPos2 = 1598;
     |LINEAL_SIMPLE #1#referc1@, nRango, nPos1, nPos2, Max31, Min31, Evento31;
     enGridc1 = FSalida;

     |CONTROL_SIMPLE nPanta, "EVENTOPAN", 0000, 0000, EventoFoco3;
     nEvtFoco = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,Reabrir", 0501, 0513, BtnReabre3;
     nBtnReabre3 = FSalida;
     |CONTROL_SIMPLE nPanta, "BOTON,Cerrar", 0514, 0526, BtnCerrar3;
     nBtnCerrar3 = FSalida;
     |CONTROL_SIMPLE nPanta, "BOTON,Imprimir", 0527, 0539, BtnImpre3;
     nBtnImpre3 = FSalida;
     |CONTROL_SIMPLE nPanta, "BOTON,Modificar", 0540, 0552, BtnModifi3;
     nBtnModifi3 = FSalida;
     |CONTROL_SIMPLE nPanta, "BOTON,Histo. Maq.", 0553, 0565, BtnHisMaq3;
     nBtnHisMaq3 = FSalida;
     |CONTROL_SIMPLE nPanta, "BOTON,Asig. Maq.", 0566, 0578, BtnAsgMaq3;
     nBtnAsgMaq3 = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,Modificar", 1901, 1913, BtnModiPar3;
     nBtnModiPar3 = FSalida;
     |CONTROL_SIMPLE nPanta, "BOTON,Imprimir", 1914, 1926, BtnImpPar3;
     nBtnImpPar3 = FSalida;
     |CONTROL_SIMPLE nPanta, "BOTON,Histo. Maq.", 1927, 1939, BtnHisMaqPar3;
     nBtnHisMaqPar3 = FSalida;
     |CONTROL_SIMPLE nPanta, "BOTON,Asig. Mat.", 1940, 1952, BtnAsgMaqPar3;
     nBtnAsgMaqPar3 = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{207}} Salir", 3488, 3498, BtnSalir;
     nBtnSalir3 = FSalida;

     |PINTA 1609, #0#6;
     |PINTA 1709, #0#7;
     |PINTA 1809, #0#8;
|FPRC;

