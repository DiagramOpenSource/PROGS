|FICHEROS;
     gofase04 #0;
     gofase05 #1;
     gofase06 #2;
|FIN;
----------------------------------------------------------------------
|VARIABLES;
     cPartida   = "";
|FIN;
----------------------------------------------------------------------
|PROCESO MirPar; |TIPO 7;
     cPartida = #2#4;
     |QBF cPartida;

     |SI cPartida = ""; |Y #2#3 ! 1;
          |PTEC 808;
     |FINSI;

|FPRC;
-------------------------------------------------------------------------------
|PROCESO CamEst06; |TIPO 0;
     |C_M #2#12, 1, "N";
     |C_M #2#13, 1, "N";

     |SI #2Campo ! Contenido;

          |SI #2#15 = "E";

               |SI Contenido = "F";
                    |CONFI "0000N¡ATENCION! LA PARTIDA ESTABA FINALIZADA. ¿CONTINUAR?";

                    |SI FSalida ! 0;
                         |ERROR10;
                         |ACABA;
                    |FINSI;

               |FINSI;

               |C_M #2#12, 1, "S";
               #2#12 = ~;
               #2#13 = "01.01.0000";
               |PINTA #2#12; |PINTA #2#13;

          |FINSI;

          |SI #2#15 = "F";

               |SI Contenido ! "E";
                    |MENAV "0000EL CAPITULO NO ESTA EN EJECUCION";
                    |ERROR10;
                    |ACABA;
               |FINSI;

               |C_M #2#13, 1, "S";
               #2#13 = ~;

          |FINSI;

          |SI #2#15 = "P";

               |SI Contenido = "F";
                    |CONFI "0000N¡ATENCION! EL CAPITULO ESTABA FINALIZADO. ¿CONTINUAR?";

                    |SI FSalida ! 0;
                         |ERROR10;
                         |ACABA;
                    |FINSI;

               |FINSI;

               |SI Contenido = "E";
                    #2#13 = "01.01.0000";
               |SINO;
                    #2#12 = "01.01.0000";
                    #2#13 = "01.01.0000";
               |FINSI;

               |PINTA #2#12;
               |PINTA #2#13;

          |FINSI;
     |FINSI;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO SoloModi6; |TIPO 0;

     |SI #2#8 = 0;
          |ERROR10;
          |ACABA;
     |SINO;
          |PTEC 802;
     |FINSI;

|FPRC;
-------------------------------------------------------------------------------
|PROCESO MiraFin; |TIPO 0;

     |SI #2#13 ! "01.01.0000"; |Y #2#13 < #2#12;
          |MENAV "0000LA FECHA FIN NO PUEDE SER INFERIOR A LA DE INICIO";
          |ERROR10;
     |FINSI;

|FPRC;
-------------------------------------------------------------------------------
