|FICHEROS;
     gonomica #0;
     gonomili #1;
     gotrabaj #2;
     gotrabal #3;
|FIN;
------------------------------------------------------------------------------
|VARIABLES;
     &cCodMonBase   = "";
     nModo          = 0;
|FIN;
------------------------------------------------------------------------------
|PROCESO MiraFech; |TIPO 0;
     nModo = (FEntrada/100)!0;

     |SI #1#3 [ #1#2;
          |MENAV "0000FECHA INFERIOR A LA DE INICIO";
          |ERROR;
          |ACABA;
     |FINSI;

     |HAZ Dias;

     |SI nModo = 1;
          |ABRE #2;
          #2#0 = #0#0;
          |LEE 000#2=;

          |SI FSalida = 0;
               #1#14 = #2#23;
          |FINSI;

          |CIERRA #2;
     |FINSI;

|FPRC;
------------------------------------------------------------------------------
|PROCESO Dias; |TIPO 0;

     |SI #1#17 = "N";
          #1#4 = (#1#3 - #1#2) + 1;
     |SINO;
          #1#4 = 0;
     |FINSI;

     |SI #1#4 ] 32;
          |MENAV "0000LA DIFERENCIA DE FECHAS NO PUEDES SER SUPERIOR A 31 DIAS";
          |ERROR10;
          |ACABA;
     |FINSI;
     |PINTA #1#4;
|FPRC;
------------------------------------------------------------------------------
|PROCESO ValMonNom; |TIPO 7;
     |HAZ MonBas;
     #0#4 = cCodMonBase;
     |PINTA #0#4;
|FPRC;
------------------------------------------------------------------------------
|PROCESO TotLiq; |TIPO 0;
     #1#5  = #1#11+#1#8+#1#9+#1#10;     ||Total devengado.
     #1#6  = #1#12+#1#13;               ||Total Deducciones.
     #1#7  = #1#5 - #1#6;               ||Total Liquido.
     #1#15 = #1#14 - #1#7;              ||Diferencia Sueldo.
     |HAZ PinTot;
|FPRC;
------------------------------------------------------------------------------
|PROCESO PinTot; |TIPO 7;
     |PINTA #1#5;
     ||PINDA #0#0;
     |ATRI I;
     |PINTA 1968, #1#7;
     |ATRI 0;
|FPRC;
------------------------------------------------------------------------------
|PROCESO TotCab; |TIPO 2;

     #0#5 = #0#5 +. #1#7;
     #0#6 = #0#6 +. #1#4;

     |PINTA #0#5;
     |PINTA #0#6;
|FPRC;
------------------------------------------------------------------------------
|PROCESO PintPact; |TIPO 11;
     |SI FSalida = 13;
          |ATRI I;
          |C_V #1#14, 1, "S";
          |C_V #1#15, 1, "S";
          |C_M #1#14, 1, "S";
          |PINTA 2148, "SUELDO PACTADO:";
          |PINTA 2248, "DIFERENCIA....:";
          |PINTA #1#14;
          |PINTA #1#15;
          |ATRI 0;
          |ERROR;
     |SINO;
          |HAZ DesPact;
     |FINSI;
|FPRC;
------------------------------------------------------------------------------
|PROCESO DesPact; |TIPO 7;
     |ATRI 0;
     |C_V #1#14, 1, "N";
     |C_V #1#15, 1, "N";
     |C_M #1#14, 1, "N";
     |C_M #1#15, 1, "N";
     |PINTA 2147, "                   ";
     |PINTA 2247, "                   ";
     |PINTA 2167, "             ";
     |PINTA 2267, "             ";
|FPRC;
------------------------------------------------------------------------------
|PROCESO Anti; |TIPO 0;
     |ABRE #2;
     #2#0 = #0#0;
     |LEE 000#2=;
     |SI FSalida = 0;
          #0#10 = #2#23;                          ||Sueldo Pactado.
     |FINSI;
     |CIERRA #2;
     |ABRE #3;
     #3#0 = #0#0;
     #3#1 = 1;
     |LEE 000#3=;
     |SI FSalida = 0;
          #0#3 = #3#2;                            ||antiguedad (Fecha Inicio 1er contrato).
          |PINTA #0#3;
     |FINSI;
     |CIERRA #3;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO MiraObliga; |TIPO 0;
     |SI #1#11 = 0; |Y Campo = 11;
          |MENAV "0000EL SALARIO BASE ES OBLIGATORIO";
          |ERROR10;
     |FINSI;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO MiAn; |TIPO 0;
     |SI #1#2 < #0#3;
          |CONFI "0000SLA FECHA DE NOMINA ES ANTERIOR A LA ANTIGUEDAD.¿CONTINUAR?";
          |SI FSalida ! 0;
               |ERROR10;
          |FINSI;
     |FINSI;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO PinNom; |TIPO 20;
     FSalida = "";
     |SI FEntrada = 401;
          |PINPA #0#0;
          |PINDA #0#0;
          |PINDA #0#1;
     |FINSI;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO MiraPactado; |TIPO 3;
     |SI #1#14 = 0;
          #1#14 = #1#7;
     |FINSI;

     |HAZ TotLiq;
|FPRC;
-------------------------------------------------------------------------------
