|VARIABLES;
     nCer = 0;
     nFacDir = 0;
     aInforme  = "";
     aInformes = "goliss11"
     aInformec = "golisc11";
     aInformeo = "goliso11";
     nEje      = 0;
     &enEje    = 0;
     &enGGBI   = 0;
     &enSub    = 0;
     &enCer    = 0;
     nHay      = 0;
     nPor      = 0;
     nSw0      = 0;
     nSw1      = 0;
     nTotal    = 0;
     n31       = 0;
     n32       = 0;
     n33       = 0;
     nDescuen  = 0;
     nSwDupli = 0;
     nNumPre = 0;
     nDto = 0;
     nGG = 0;
     nBI = 0;
     nBaja = 0;
|FIN;

|PROCESO Facturas;
     #84#48 = #43#0;
     #84#0 = #43#1;
     |LEE 000#84=;
     |SI FSalida = 0;
          nFacDir = nFacDir + (#84#16+#84#19+#84#22+#84#28+#84#42+#84#45);
     |FINSI;
|FPRC;

|DEFBUCLE Facturas;
     #43#2;
     15;
     #10#0, #10#1, #0#18;
     #10#0, #10#1, #0#19;
     ;
