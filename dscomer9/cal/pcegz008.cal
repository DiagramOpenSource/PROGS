|FICHEROS;
     pcegm001 #1;
     pcegm004 #4,MANTE;
     agifa024 #24;
|FIN;

|VARIABLES;
     nBoton1 = -1;
     &enSwMenu = 0;
     fFecha = @;
     aAlfa = "";
     aModo = "";
     aParam = "";
     aCli = "";
     aDir = "";
     aSer = "";
     aNum = "";
|FIN;

|PROGRAMA;
     |CLS;
     aModo = PARAMETRO % 101;
     aSer = PARAMETRO % 205;
     aNum = PARAMETRO % 706;
     aCli = PARAMETRO % 1306;
     aDir = PARAMETRO % 1904;

     |ABRE #24;
     #24#0 = aCli;
     |LEE 000#24=;
     |CIERRA #24;

     |ABRE #1;
     #1#0 = #24#205;
     |LEE 000#1=;
     |SI FSalida ! 0;
          aAlfa = "0000No existe el Pais:" + #1#0 + " en el fichero de series facturacion por paises";
          |MENAV aAlfa;
          |ACABA;
     |FINSI;
     |CIERRA #1;

     |ABRE #4;
     |PINPA #0#4;
     |SI aModo = 1;
          fFecha = ~;
          aAlfa = fFecha % -102;
          #4#25 = #1#1 + aAlfa;
          enSwMenu = 1;
          |HAZ ContaPe7;
          |GRABA 020#4b;
          |SI FSalida ! 0; |ACABA; |FINSI;

          #4#4 = aCli;
          #4#57 = aDir;
          |SI #4#57 = 0; #4#57 = 1; |FINSI;

          |DEFECTO #4#24;
          FEntrada = 101; Campo = 4; || Para que entre en CambioDivisa
          |HAZ CambioDivisa;
          |HAZ Cliente6;
          |HAZ Cliente0;

          |PINDA #0#4;
          |CONTROL_SIMPLE 0, "BOTON,Datos Calibración", 0514, 0529, Calibracion;
          nBoton1 = FSalida;
          |ENDATOS #1#4;
          |SI FSalida = 0;
               |GRABA 020#4a;
          |SINO;
               |BORRA 020#4c;
          |FINSI;
     |FINSI;
     |SI aModo = 2;
          #4#25 = aSer;
          #4#0 = aNum;
          |LEE 121#4=;

          |PINDA #0#4;
          |CONTROL_SIMPLE 0, "BOTON,Datos Calibración", 0514, 0529, Calibracion;
          nBoton1 = FSalida;
          |ENDATOS #2#4;
          |SI FSalida = 0;
               |GRABA 020#4a;
          |FINSI;
     |FINSI;
     |SI aModo = 4;
          #4#25 = aSer;
          #4#0 = aNum;
          |LEE 020#4=;

          |PINDA #0#4;
          |ENDATOS #4#4;
     |FINSI;
     |CIERRA #4;

     |SI nBoton1 ! -1; |FIN_CONTROL_WINDOWS nBoton1; |FINSI;
     |PULSATECLA;
|FPRO;
