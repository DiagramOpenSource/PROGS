|FICHEROS;
     gomantca #1;
     godeexpe #6;
     godeexpf #7,MANTE;
|FIN;
------------------------------------------------------------------------------
|VARIABLES;
     aDirectorio    = "";
     aNFichero      = "";
     aVariableCarga = "";
     aFichero       = "";
     aAlfa          = "";

     nVariable      = 0;
     nBaffer        = 0;
     nTotalLinea    = 0;
     nTope          = 0;
     nVari1         = 0;
     nVari2         = 0;

     &aCodigo      = "";
     &jLinea       = 0;
     &jFecha       = @;
     &aSerie        = "";

     &eaNombreList  = "";
     &enCuadroIni   = 0;
     &enCuadroFin   = 0;
     &enCampoAncho  = 0;
     &enModoEdita   = 0;
     &enTotalVaria  = 0;

{100}&epPuntEdita   = "";

     cMensa         = "";
|FIN;
------------------------------------------------------------------------------
|PROCESO GrabaSecuencial2;
     IepPuntEdita  = epPuntEdita1 <-;
     |PARA nTotalLinea = 0;  |SI nTotalLinea [ enTotalVaria;  |HACIENDO nTotalLinea = nTotalLinea + 1;
          |AL_BUF nBaffer, nTotalLinea, epPuntEdita;
          IepPuntEdita = IepPuntEdita + 1;
     |SIGUE;
|FPRC;
------------------------------------------------------------------------------
|PROCESO Edicion2;
     nTope  = nTotalLinea - 1;
     nVari1 = 0;
     nVari2 = 0;
     enModoEdita = enModoEdita + 100;
     |EDITA nBaffer, 0, nTope, enCuadroIni, enCuadroFin, enModoEdita, eaNombreList, enCampoAncho, nVari1, nVari2;
     enModoEdita = enModoEdita $ 100;
|FPRC;
------------------------------------------------------------------------------
|PROCESO CogePuntero2;
     IepPuntEdita  = epPuntEdita1 <-;
     |PARA nTotalLinea = 0;  |SI nTotalLinea [ enTotalVaria;  |HACIENDO nTotalLinea = nTotalLinea + 1;
          |DEL_BUF nBaffer, nTotalLinea, epPuntEdita;
          IepPuntEdita = IepPuntEdita + 1;
     |SIGUE;
|FPRC;
------------------------------------------------------------------------------
|PROCESO Edita2;
     |PUSHV 0501 2380;
     |DIMENSIONA  200, nBaffer, 0;

     |HAZ GrabaSecuencial2;
     |HAZ Edicion2;
     |HAZ CogePuntero2;

     |FINDIM nBaffer;
     |POPV;
|FPRC;
------------------------------------------------------------------------------
|PROCESO seguimiento;
     enTotalVaria  = 100;
     eaNombreList  = "";
     enCuadroIni   = 0811;
     enCuadroFin   = 2071;
     enCampoAncho  = 60;
     enModoEdita   = 0;
     |ABRE #6;
     |PARA nVariable = 1;  |SI nVariable [ 100;  |HACIENDO nVariable = nVariable + 1;
           IepPuntEdita  = epPuntEdita1 <-;
           IepPuntEdita = IepPuntEdita + (nVariable - 1);
           #6#0 = aSerie;
           #6#1 = aCodigo;
           #6#2 = jLinea;
           #6#3 = jFecha;
           #6#4 = nVariable;
           |LIBERA #6;
           |LEE 101#6=;
           |SI FSalida ! 0;
               epPuntEdita  = "";
           |SINO;
               epPuntEdita  = #6#5;
               |BORRA 020#6a;
           |FINSI;
     |SIGUE;

     |PUSHV  0501 2380;
     |COLOR 0, 2; |ATRI 0;
     |BLANCO 0506 2176;
     |COLOR 0, 7;|ATRI N;
     |PINTA 0611, "Seguimiento: ";
     |COLOR 9, 7;|ATRI N;
     |PINTA 0626, #7#4;
     |PINTA 0630, #7#5;
     |PINTA 0641, #7#6;
     |ATRI 0;

     |HAZ Edita2;
     |POPV;

     |PARA nVariable = 1;  |SI nVariable [ 100;  |HACIENDO nVariable = nVariable + 1;
           IepPuntEdita = epPuntEdita1 <-;
           IepPuntEdita = IepPuntEdita + (nVariable - 1);

           |DDEFECTO #6;
           #6#0 = aSerie;
           #6#1 = aCodigo;
           #6#2 = jLinea;
           #6#3 = jFecha;
           #6#4 = nVariable;
           |LIBERA #6;
           |LEE 101#6=;
           |SI FSalida ! 0;  |GRABA 020#6b;  |FINSI;
           #6#5 = epPuntEdita;
           |GRABA 020#6a;
     |SIGUE;

     |PARA nVariable = 100;  |SI nVariable ] 1;  |HACIENDO nVariable = nVariable - 1;
           #6#0 = aSerie;
           #6#1 = aCodigo;
           #6#2 = jLinea;
           #6#3 = jFecha;
           #6#4 = nVariable;
           |LIBERA #6;
           |LEE 101#6=;
           aAlfa = #6#5;  |QBF aAlfa;

           |SI aAlfa ! "";  |SAL;  |FINSI;

           |BORRA 020#6a;
     |SIGUE;
     |CIERRA #6;
|FPRC;
------------------------------------------------------------------------------
