|PROCESO Relleno;
     |SI aTipo = "A";
          aRelleno = " " * nLon;
          aAlfa = aAlfa + aRelleno;
          nPos = nLon + 100;
          aAlfa = aAlfa % nPos;
     |SINO;
          nNume = aAlfa;
          |SI nNume < 0; nNume = nNume * - 1; |FINSI;
          nNu1 = nNume ! 0;
          |SI nNu1 > nNume; nNu1 = nNu1 - 1; |FINSI;
          nNu2 = nNume - nNu1;
          |SI nDec = 0;
               aAlfa = nNu1;
          |SINO;
               aAlfa = nNu2;
               aAlfa = aAlfa + "00000000000000000000000000";
               nPos = nDec + 300;
               aAlfa = aAlfa % nPos;
               aAlfa = nNu1 + "." + aAlfa;
          |FINSI;
          aAlfa = "000000000000000000000000000000000000000000000" + aAlfa;
          nPos = -1 * (nLon + 100);
          aAlfa = aAlfa % nPos;
     |FINSI;
     |XGRABA nHandle, aAlfa;
     aTipo = "";
     nDec = 0;
|FPRC;

