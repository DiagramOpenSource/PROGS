|FICHEROS;
     guerz055 #0;
     gopartia #1;
     goartdes #2;
     agifa019 #19;

     guerw043 #43; ||Tmp   (para el articulo del gopartia)
     guerw044 #44,MANTE; ||Tmp
|FIN;

|VARIABLES;
     aAlfa = "";
|FIN;

|PROCESO guerw044;
     |SI #44#2 = "*";
          #19#0 = #44#0;
          |BORRA 000#19c;
     |FINSI;
|FPRC;

|DEFBUCLE guerw044;
     #44#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, guerw044;
|FIN;

|PROCESO agifa019;
     #43#0 = #19#0;
     |LEE 000#43=;
     |SI FSalida = 0; |ACABA; |FINSI;

     #2#0 = #19#0;
     |LEE 000#2=;
     |SI FSalida = 0; |ACABA; |FINSI;

     #44#0 = #19#0;
     #44#1 = #19#1;
     |GRABA 020#44n;
|FPRC;

|DEFBUCLE agifa019;
     #19#1;
     125,2,3;
     "                    ", #0#0, #0#2, #0#4;
     "zzzzzzzzzzzzzzzzzzzz", #0#1, #0#3, #0#5;
     ;
     NULL, agifa019;
|FIN;

|PROCESO gopartia;
     #43#0 = #1#20;
     |LEE 000#43=;
     |SI FSalida ! 0; |GRABA 020#43n; |FINSI;
|FPRC;

|DEFBUCLE gopartia;
     #1#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, gopartia;
|FIN;

|PROCESO TeclaArti; |TIPO 0;
     |SI FSalida  = 0; |Y #44#0 = Contenido;
          |SI #44#2 = " ";
              #44#2 = "*";
          |SINO;
              #44#2 = " ";
          |FINSI;
          |GRABA 020#44a;
          |PINTA #44#2;
     |FINSI;
|FPRC;

|PROCESO Min;
     #44#0 = " " * 20;
|FPRC;

|PROCESO Max;
     #44#0 = "z" * 20;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     aAlfa = Usuario; aAlfa = aAlfa + "1";
     |NOME_DAT #43 aAlfa;
     |DELINDEX #43;

     aAlfa = Usuario; aAlfa = aAlfa + "2";
     |NOME_DAT #44 aAlfa;
     |DELINDEX #44;

     |ABRE #43;
     |BUCLE gopartia;
     |ABRE #2;
     |ABRE #44;
     |BUCLE agifa019;
     |CIERRA #44;
     |CIERRA #2;
     |CIERRA #43;

     |PINPA #0#44;
     |ENTLINEAL #1#44, 1, 4, 22, 0, Min, Max;
     |CONFI "0000NBorrar articulos seleccionadas";
     |SI FSalida = 0;
          |ABRE #19;
          |BUCLE guerw044;
          |CIERRA #19;
     |FINSI;

     |DELINDEX #43;
     |DELINDEX #44;
|FPRC;
