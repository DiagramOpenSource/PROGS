|FICHEROS;
   tecam020 #0;
   tecam031 #31;
|FIN;

|VARIABLES;
   aAlfa = "";
|FIN;

|PROCESO MiraBarra;
   aAlfa = #tecam020#Campo#; |QBF aAlfa;
   aAlfa = aAlfa % -101;
   |SI aAlfa ! "/"; |Y aAlfa ! "\";
      aAlfa = #tecam020#Campo#; |QBF aAlfa; aAlfa = aAlfa + "/";
      #tecam020#Campo# = aAlfa; |PINTA #tecam020#Campo#;
   |FINSI;
|FPRC;

|PROCESO Min31;
     #31#0 = "";
|FPRC;

|PROCESO Max31;
     #31#0 = "zzz";
|FPRC;

|PROCESO GrupoArt;
     |ENTLINEAL #1#31, 1, 2, 20, 0, Min31, Max31;
|FPRC;

|PROGRAMA;
   |ABRE #tecam020;
   |LEE 000#tecam020.p;
   |SI FSalida ! 0;
      |GRABA 020#tecam020.c;
   |FINSI;

   |CLS;
   |CABEZA " Control general ";
   |PINPA #0#0; |PINDA #0#0;
   |ENTLINEAL #1#31, 1, 7, 20, 0, Min31, Max31;
   |ENDATOS #1#0;
   |SI FSalida = 0;
      |GRABA 020#tecam020.a;
   |FINSI;
   |CIERRA #tecam020;
|FPRO;
