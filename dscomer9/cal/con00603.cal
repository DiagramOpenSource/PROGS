|FICHEROS;
     con00603 #0;
     con00626 #626;
|FIN;

|VARIABLES;
     &enTiva = 0;
     &efFiva = @;
     &eaDiva = "";
     &enIva = 0;
     &enRec = 0;
     nModo = 0;
     aAlfa1 = "";
     aAlfa2 = "";
|FIN;

|PROCESO total;
     #0#4 = #0#2 * #0#3;
     |PINTA #0#4;
|FPRC;

|PROCESO IVA603_0; |TIPO 0;
     enTiva = #0Campo;
     efFiva = ~;
     |HAZ SacaIVA;
     |SI eaDiva = "";
          |MENAV "0000No existe tipo iva para esta fecha...";
          |ERROR; |ACABA;
     |SINO;
          #0#6 = enIva; |PINTA #0#6;
     |FINSI;
|FPRC;

|PROCESO IVA603_66; |TIPO 66;
     enTiva = #0Campo;
     efFiva = ~;
     |HAZ ConsuIVA;
     |SI eaDiva = "";
          |ERROR;
     |SINO;
          #0Campo = enTiva; |PINTA #0Campo;
          #0#6 = enIva; |PINTA #0#6;
     |FINSI;
|FPRC;

|PROCESO Tipo2_603; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |DDEFECTO #626;
          |ABRE #626;
          |SELECT #7#626;
          #626#2 = #0#0;
          |LEE 000#626];
          aAlfa1 = #626#2; |QBF aAlfa1;
          aAlfa2 = #0#0; |QBF aAlfa2;
          |SI FSalida = 0; |Y aAlfa1 = aAlfa2;
               |CONFI "0000NExisten movimientos de esta ficha. Continuar con la baja?";
               |SI FSalida ! 0; |ERROR; |FINSI;
          |FINSI;
          |CIERRA #626;
     |FINSI;
|FPRC;
