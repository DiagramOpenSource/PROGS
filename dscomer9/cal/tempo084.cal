|FICHEROS;
     tempo084 #0;
|FIN;

|VARIABLES;
     &enTiva = "";
     &efFiva = @;
     &enIva = 0;
     aAlfa = "";
|FIN;

|PROCESO Pago13; |TIPO 13;
     |HAZ EsSematel;
     |SI FSalida = 0;
          |ATRI I; |PINTA 1326, "PROMOCION........:"; |ATRI 0;
     |FINSI;
|FPRC;

|PROCESO EntraPago; |TIPO 7;
     |HAZ EsSematel;
     |SI FSalida = 0;
          efFiva = ~;
          enTiva = 2;
          |HAZ SacaIVA;
          aAlfa =  "0000<F5>Calculo Neto (suma " + enIva + "%)";
          |MENSA aAlfa;
     |FINSI;
|FPRC;

|PROCESO TeclaPago; |TIP 0;
     |SI FSalida = 13;
          |HAZ EsSematel;
          |SI FSalida = 0;
               #0#1 = #0#1 > enIva;
               |PINTA #0#1;
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;
