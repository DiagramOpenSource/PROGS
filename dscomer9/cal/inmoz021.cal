|FICHEROS;
     inmoz021 #0;
     inmom006 #6;
|FIN;

|VARIABLES;
     aDCB = "";
     aHCB = "";
     aTxtError = "";
     &enSwImp = 0;
     nSwCheq = 0;
     nPrime = 0;
     nImpre = 1;
     nInfor = 1;
     aAlfa1 = "";
     aAlfa2 = "";
|FIN;

|PROCESO Imprime;
     |SI nPrime = 0;
          nPrime = 1;
          |IMPRE 0;
          nImpre = FSalida;
          |SI FSalida ! 0; |ACABA; |FINSI;
          |INFOR "inmoz021";
          nInfor = FSalida;
          |SI FSalida ! 0; |ACABA; |FINSI;
     |FINSI;
     |SI nInfor = 0; |PRINT; |FINSI;
|FPRC;

|PROCESO inmom006;
     |SI #6#8 ! "S";
          |SI nSwCheq = 1;
               aTxtError = "";

               aAlfa1 = #6#3;
               aAlfa2 = #6#12;
               |SI aAlfa1 ! aAlfa2;
                    enSwImp = 1;
                    aTxtError = aTxtError + "Obra ";
                    |HAZ Imprime;
               |FINSI;

               aAlfa1 = #6#4;
               aAlfa2 = #6#13;
               |SI aAlfa1 ! aAlfa2;
                    enSwImp = 2;
                    aTxtError = aTxtError + "Seguidor ";
                    |HAZ Imprime;
               |FINSI;

               aAlfa1 = #6#5;
               aAlfa2 = #6#14;
               |SI aAlfa1 ! aAlfa2;
                    enSwImp = 3;
                    aTxtError = aTxtError + "Serie ";
                    |HAZ Imprime;
               |FINSI;

               aAlfa1 = #6#6;
               aAlfa2 = #6#15;
               |SI aAlfa1 ! aAlfa2;
                    enSwImp = 4;
                    aTxtError = aTxtError + "Ord.Serie ";
                    |HAZ Imprime;
               |FINSI;

               |SI aTxtError ! "";
                    #6#8 = "E";
                    #6#9 = "No coinciden:" + aTxtError;
               |FINSI;
          |SINO;
               #6#8 = "S";
          |FINSI;
          |GRABA 020#6a;
     |FINSI;
     |LIBERA #6;
|FPRC;

|DEFBUCLE inmom006;
     #6#2;
     ;
     #0#0;
     #0#0;
     be;
     NULL, inmom006;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     aDCB = " " * 40;
     aHCB = "z" * 40;

     nSwCheq = 1;
     |BUCLE inmom006;
     |SI nPrime = 0;
          nSwCheq = 0;
          |BUCLE inmom006;
     |SINO;
          |SI nInfor = 0; |PIEINF; |FININF; |FINSI;
          |SI nImpre = 0; |FINIMP; |FINSI;
     |FINSI;
|FPRC;
