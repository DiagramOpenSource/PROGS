 #241#2 = #8#20;
 |LEE 000#241=;
 nFactor = 1;
 |SI FSalida = 0; |Y aMulti = "S";
     |SI #241#6 = "D";
          nFactor = 1 / #241#5;
     |SINO;
          nFactor = 1 * #241#5;
     |FINSI;
 |FINSI;
|FPRC;

