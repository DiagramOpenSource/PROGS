|FPRC;

|PROCESO grabar;
     swno = 0;
     operz = #8#2;
     #1#12 = #8#11; || Serie
     |HAZ leeserie;
     |SI operz = "AV"; |O operz = "BV"; |O operz = "FC"; |O operz = "DP";
                       |O operz = "DV"; |O operz = "SF";
         swno = 1;  ||Es un Cliente
     |FINSI;
     |SI swno = 1; |ACABA; |FINSI;

     || Seleccion de las Operaciones
     |PINTA 0820 , #8#1;      || ...  Pinta Fecha

     |SI sw = 0;
         codigo = #8#0;
         |HAZ leearticul;
         stock6 = 0;
         sw = 1;
     |FINSI;

     |SI codigo ! #8#0;
         codigo = #8#0;
         |HAZ leearticul;
         stock6 = 0;
     |FINSI;

     |DDEFECTO #1;
     c = c + 1;
     stock6 = stock6 + #8#6;

     #1#0  = c;
     #1#1  = #8#10;
     #1#2  = #8#1;
     #1#3  = #8#2;
     #1#4  = #8#3;
     #1#5  = #8#4;
     #1#6  = #8#5;
     #1#7  = #8#6;
     #1#8  = stock6;
     #1#9  = #8#8;
     #1#10 = #8#9 * nFactor;
     #1#11 = #8#0;
     #1#12 = #8#11;
     #1#13 = #7#1;
     #1#14 = #8#14 * nFactor;
     |GRABA 021#1n;
|FPRC;

|PROCESO leearticul;
