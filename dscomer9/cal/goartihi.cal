|PROCESO Genera; |TIPO 7;
/*
     |C_M #0#7,  1, "S";
     |C_M #0#9,  1, "S";
     |C_M #0#11, 1, "S";
     |C_M #0#13, 1, "S";
     |C_M #0#20, 1, "S";
     |C_M #0#22, 1, "S";
     |C_M #0#24, 1, "S";
     |C_M #0#26, 1, "S";
     |C_M #0#28, 1, "S";
*/
     nModo = (FEntrada/100)!0;
     |SI nModo = 1;
          #0#18 = Usuario % 810; |PINTA #0#18;

          |SI #8#32 ! "N";
               |PUSHV 01012380;
               |CLS;
               #9#9 = "";
               #9#0 = "";
               qAlta = "P";
               |PINPA #0#9;
               |PINDA #0#9;
               |ENDATOS #2#9;
               |SI FSalida = 0;
                    |HAZ MiraExiste;
               |SINO;
                    |PTEC 807;
               |FINSI;
               |SI FSalida = 0;
                    |HAZ GrabaEstandar;
                    #0#0  = #9#0;            ||Codigo Partida.
                    #0#7  = #9#1;            ||Seccion;
                    #0#8  = #9#2;            ||Nombre Sec.
                    #0#9  = #9#3;            ||Familia.
                    #0#10 = #9#4;            ||Nombre Fam.
                    #0#11 = #9#5;            ||SubFamilia
                    #0#12 = #9#6;            ||Nombre Sub.
                    #0#13 = #9#7;            ||Capitulo.
                    #0#14 = #9#8;            ||Nombre Cap.
                    #0#20 = #9#11;
                    #0#21 = #9#12;
                    #0#22 = #9#13;
                    #0#23 = #9#14;
                    #0#24 = #9#15;
                    #0#25 = #9#16;
                    #0#26 = #9#17;
                    #0#27 = #9#18;
                    #0#28 = #9#19;
                    #0#29 = #9#20;
/*
                    |C_M #0#7,  1, "N";
                    |C_M #0#9,  1, "N";
                    |C_M #0#11, 1, "N";
                    |C_M #0#13, 1, "N";
                    |C_M #0#20, 1, "N";
                    |C_M #0#22, 1, "N";
                    |C_M #0#24, 1, "N";
                    |C_M #0#26, 1, "N";
                    |C_M #0#28, 1, "N";
*/
               |SINO;
                    |POPV;
                    |ERROR; |ACABA;
               |FINSI;

               |CONFI "0000N¿DESEA BASARSE EN UNA PARTIDA EXISTENTE?";
               |SI FSalida ! 0;
                    |HAZ CreaDesAmpli;
               |SINO;
                    |DBASE Camino;
                    Camino = Camino + "def/goartive.mas";
                    |CARGA_DEF Camino, duppart@;
                    |DBASE Camino;
                    Camino = Camino + "def/goartdes.mas";
                    |CARGA_DEF Camino, dupparl@;
                    |DBASE Camino;
                    Camino = Camino + "def/godeivec.mas";
                    |CARGA_DEF Camino, dupdesc@;
                    |DBASE Camino;
                    Camino = Camino + "def/godeivel.mas";
                    |CARGA_DEF Camino, dupdesl@;
                    |SI  FSalida ] 0;
                         |ABRE #10;
                         |CONSULTA_CLAVE #1#10;
                         |SI FSalida = 0;
                              |HAZ DupliParti;
                              |HAZ DupliDescr;
                         |FINSI;
                         |CIERRA #10;
                    |FINSI;
                    |DESCARGA_DEF #duppart@;
                    |DESCARGA_DEF #dupparl@;
                    |DESCARGA_DEF #dupdesc@;
                    |DESCARGA_DEF #dupdesl@;
               |FINSI;
               |POPV;
               |PINTA #0#0;
               |PINTA #0#7;            ||Seccion;
               |PINTA #0#8;            ||Nombre Sec.
               |PINTA #0#9;            ||Familia.
               |PINTA #0#10;           ||Nombre Fam.
