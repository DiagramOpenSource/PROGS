|PROCESO DupliDescr;
     |ABRE #1;
     |ABRE #2;
     |ABRE #12;
     #12#0 = #10#0;
     |LEE 000#12=;
     |SI FSalida = 0;
          |ABRE #13;
          nLinea = 1;
          #13#0 = #12#0;
          #13#1 = nLinea;
          |LEE 000#13=;
          |PARA; |SI FSalida = 0; |HACIENDO;
               #2#0 = #0#0;
               #2#1 = #13#1;
               #2#2 = #13#2;
               |GRABA 020#2n;
               nLinea = nLinea + 1;
               #13#1 = nLinea;
               |LEE 000#13=;
          |SIGUE;
          |CIERRA #13;
          #1#0 = #0#0;
          #1#1 = #12#1;
          #1#2 = #12#2;
          |GRABA 020#1n;
     |FINSI;
     |CIERRA #12;
     |CIERRA #1;
     |CIERRA #2;
|FPRC;
