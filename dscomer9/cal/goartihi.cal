     |CARGA_DEF aAlfa, goartive@;
     |SI FSalida = 0;
          |ABRE #goartive@;
          #goartive@#0 = #9#0;
          |LEE 000#goartive@.=;
          |SI FSalida = 0;
               #0#0 = #9#0;
