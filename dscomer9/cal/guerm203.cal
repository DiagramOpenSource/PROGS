|FICHEROS;
     guerm203 #0;
     gotrabaj #1;
|FIN;

|VARIABLES;
     f = @;
     nModo = 0;
     aSubContra = "";
     nCon = 0;
|FIN;

|PROCESO Defecto; |TIPO 7;
     |SI #0#3 = "    ";
          f = ~;
          #0#3 = f % -104;
     |FINSI;
|FPRC;

|PROCESO T203_66; |TIPO 66;
     |ABRE #1;
     #1#0 = #0#0;
     |LEE 000#1];
     |CONSULTA_CLAVE #1#1;
     |SI FSalida = 0;
          #0#0 = #1#0; |PINTA #0#0;
          #0#1 = #1#1; |PINTA #0#1;
     |SINO;
          |ERROR;
     |FINSI;
     |CIERRA #1;
|FPRC;

|PROCESO T203_0; |TIPO 0;
     |SI nModo = 1;
          #0#5 = aSubContra;
          |PINTA #0#5;
          |SI aSubContra = "N";
               |ABRE #1;
               #1#0 = #0#0;
               |LEE 000#1=;
               |SI FSalida ! 0;
                    |MENAV "0000No existe trabajador...";
                    |ERROR;
               |FINSI;
               |CIERRA #1;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO T203_7; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |C_M #0#5, 1, "S";
     |SI nModo = 1;
          |SI aSubContra = "fin";
               #0#0 = nCon;
               |LEE 000#0=;
               aSubContra = "";
               |ERROR; |ACABA;
          |FINSI;
          |CONFI "0000NConductor subcontratado?";
          |SI FSalida = 0;
               aSubContra = "S";
          |SINO;
               aSubContra = "N";
          |FINSI;
          |C_M #0#5, 1, "N";
     |SINO;
          aSubContra = "";
     |FINSI;
|FPRC;

|PROCESO T203_30; |TIPO 30;
     aSubContra = "fin";
     nCon = #0#0;
|FPRC;


|PROCESO EntraMail; |TIPO 0;
     |SI #0#5 = "S";
          |C_M #0#6, 1, "S";
     |SINO;
          |C_M #0#6, 1, "N";
          #0#6 = "";
          |PINTA #0#6;
     |FINSI;
|FPRC;
