|FICHEROS;
     guerz068 #0;
     guerm119 #119;
     guerm120 #120;
|FIN;

|VARIABLES;
     &enOferta = 0;
     aDTipo = "";
     aHTipo = "";
     aDImp = "";
     aHImp = "";
|FIN;

|PROCESO Linea;
     |PRINT;
|FPRC;

|PROCESO FinCab;
     |PIEINF;
     |FININF;

     #119#15 = "S";
     |GRABA 020#119a;
     |LIBERA #119;
|FPRC;

|PROCESO Cab;
     |SI #119#9 = "A";
          |INFOR "guera119";
          |SI FSalida ! 0; |MENAV "0000No existe informe 'guera119'"; |FINSI;
     |SINO;
          |INFOR "guerp119";
          |SI FSalida ! 0; |MENAV "0000No existe informe 'guerp119'"; |FINSI;
     |FINSI;
     |SI FSalida ! 0; |ERROR; |ACABA; |FINSI;
|FPRC;

|DEFBUCLE Ofertas;
     #119#1;
     4,5,9,15;
     #0#6, #0#2, #0#4, aDTipo, aDImp;
     #0#7, #0#3, #0#5, aHTipo, aHImp;
     be;
     NULL, Cab, Linea, FinCab;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     aDTipo = #0#9;
     aHTipo = #0#9;
     aDImp = #0#8;
     aHImp = #0#8;
     |SI #0#9 = "T";
          aDTipo = " ";
          aHTipo = "z";
     |FINSI;
     |SI #0#8 = "T";
          aDImp = " ";
          aHImp = "z";
     |FINSI;

     |IMPRE 0;
     |SI FSalida = 0;
          |BUCLE Ofertas;
          |FINIMP;
     |FINSI;
|FPRC;

|PROGRAMA;
     |SI enOferta ! 0;
          #0#8 = "T";
          #0#9 = "T";
          #0#6 = enOferta;
          #0#7 = enOferta;
          #0#0 = "01.01.0000";
          #0#1 = "31.12.9999";
          |HAZ Tipo3;
     |SINO;
          |MANTE #0;
     |FINSI;
|FPRO;
