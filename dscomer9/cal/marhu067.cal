|PROCESO tmpmarhub;
     |PARA nSer = 1; |SI nSer [ 10; |HACIENDO nSer = nSer + 1;
           nPed = nSer + 10;
           |SI #0nPed ! 0;
               aSeri = #0nSer;
               nPedi = #0nPed;
               |BUCLE agifa073;
           |FINSI;
     |SIGUE;
|FPRC;

|PROCESO marhu040b;
     nSum = 0;
     nGrup = 1;
     |ABRE #20;
     |HAZ tmpmarhub;
     |CIERRA #20;
     |SI nSum ! 0;
          |PARA nRom = 1; |SI nRom [ 9; |HACIENDO nRom = nRom + 1;
               sWimp = 0;
               aAlfa = nRom;
               |QBF aAlfa;
               aAlfa = ("00" + aAlfa ) % -101;
               aClav = aPlan + aAlfa;
               |BUCLE marhu041;
               |SI sWimp ! 0;
                    |PIEINF;
               |FINSI;
          |SIGUE;
     |FINSI;
     |DELINDEX #20;
|FPRC;

