|FICHEROS;
     pcegm028 #0;
|FIN;

|VARIABLES;
     aRuta = "";
     aAlfa = "";
     aCar = "";
|FIN;

|PROCESO Oculta; |TIPO 0;
     aAlfa = "*" * 33;
     |PINTA 2038, aAlfa;
|FPRC;

|PROCESO PonBarra; |TIPO 0;
     aAlfa = #0Campo; |QBF aAlfa;
     aCar = aAlfa % -101;
     |SI aCar ! "/"; |Y aCar ! "\";
          #0Campo = aAlfa + "\";
     |FINSI;
|FPRC;

|PROCESO SoloFija; |TIPO 7;
     |SI #0#4 = "F";
          aAlfa = #0#5; |QBF aAlfa;
          |SI aAlfa = "Automatico";
               #0#5 = "";
               |PINTA #0#5;
          |FINSI;
          |C_M #0#5, 1, "S";
     |SINO;
          |C_M #0#5, 1, "N";
          #0#5 = "Automatico";
          |PINTA #0#5;
     |FINSI;
|FPRC;

|PROCESO Browser; |TIPO 0;
     |SI FSalida ! 13; |ACABA; |FINSI;

     aRuta = "D*.*";

     |BROWSE aRuta;

     aRuta = aRuta % 2; |QBF aRuta;
     |SI aRuta = "D"; aRuta = ""; |FINSI;

     |QBF aRuta;
     |SI aRuta = "";
          |ERROR;
     |SINO;
          #0Campo = aRuta;
          |PINTA #0Campo;
     |FINSI;
|FPRC;

|PROGRAMA;
     |ABRE #0;
     |LEE 101#0p;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     |PINPA #0#0;
     |PINDA #0#0;
     |HAZ Oculta;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |GRABA 020#0a;
     |FINSI;
     |CIERRA #0;
|FPRO;
