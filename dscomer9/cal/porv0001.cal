|FICHEROS;
     porv0001 #0;
     porm0003 #3;  || clientes nuevos
     agifa024 #24; || clientes antiguos
|FIN;


|VARIABLES;
     nBotonx1=0;
     nBotonx2=0;
     &aClienteSuper="";
     aCadena1="";
     nExiste=0;
|FIN;

|PROCESO PulsaOk;
     |PTEC 806;
|FPRC;

|PROCESO PulsaCancelar;
     |PTEC 807;
|FPRC;

|PROCESO PonBotones;
     |CONTROL_SIMPLE 0, "BOTON,OK", 2025, 2035, PulsaOk;
     nBotonx1 = FSalida;
     |CONTROL_SIMPLE 0, "BOTON,Cancelar", 2045, 2055, PulsaCancelar;
     nBotonx2 = FSalida;
|FPRC;

|PROCESO QuitaBotones;
     |FIN_CONTROL_WINDOWS nBotonx1;
     |FIN_CONTROL_WINDOWS nBotonx2;
     |PULSATECLA;
|FPRC;


|PROCESO PonNombre;
     aCadena1=aClienteSuper % 101;
     |SI aCadena1="N";
          |ABRE #3;
          aCadena1=aClienteSuper % 205;
          #3#0=aCadena1;
          |LEE 000#3=;
          #0#2=#3#2;
          |CIERRA #3;
     |SINO;
          |ABRE #24;
          #24#0=aClienteSuper;
          |LEE 000#24=;
          #0#2=#24#1;
          |CIERRA #24;
     |FINSI;

|FPRC;

|PROCESO Tipo12; |TIPO 12;

|FPRC;

|PROGRAMA;
     |SI PARAMETRO ! "";
          |CLS;
          |CABEZA "Resultado Visitas";
          |PINPA #0#0;
          |PINDA #0#0;
          |ABRE #0;
          |MANTE #0;
          |CIERRA #0;
     |SINO;
          aCadena1=aClienteSuper;
          |QBF aCadena1;
          |SI aCadena1="";
               |MENAV "0000No Tienes Ningun Cliente Seleccionado!";
          |SINO;
               |ABRE #0;
               |DDEFECTO #0;
               #0#0=aClienteSuper;
               |LEE 101#0=;
               |SI FSalida=0;
                    nExiste=0;
               |SINO;
                    nExiste=1;
               |FINSI;

               |HAZ PonBotones;
               |HAZ PonNombre;
               |PINPA #0#0;
               |PINDA #0#0;
               |ENDATOS #1#0;

               |SI nExiste=0;
                    |GRABA 020#0a;
               |SINO;
                    |GRABA 020#0n;
               |FINSI;

               |LIBERA #0;
               |CIERRA #0;

               |HAZ QuitaBotones;
          |FINSI;
     |FINSI;
|FPRO;
