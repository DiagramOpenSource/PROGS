|FICHEROS;
     guerw017 #0;
     gopre003 #3;
     godeprec #4;
     godeprel #5;
|FIN;

|VARIABLES;
     dVacio    = "                    ";
     hVacio    = "zzzzzzzzzzzzzzzzzzzz";
     aAlfa     = "";
|FIN;

|PROCESO godeprel;
     #3#0 = #5#1; ||Se
     #3#1 = #5#2; ||Pres
     #3#4 = #5#0; ||Par
     |LEE 000#3=;
     |SI FSalida ! 0;
          aAlfa = "0000No existe Presupuesto:" + #5#1 + "|" + #5#2 + "|" + #5#0;
||          |MENAV aAlfa;
          |ACABA;
     |FINSI;

     #4#0 = #5#0;
     #4#1 = #5#1;
     #4#2 = #5#2;
     #4#4 = #5#5;
     |LEE 101#4=;
     |SI FSalida = 0;
          |BORRA 020#4a;
          #4#4 = #3#2;        ||Capi
          |GRABA 020#4n;
     |FINSI;

     |BORRA 020#5a;
     #5#5 = #3#2;        ||Capi
     |GRABA 020#5n;
|FPRC;

|DEFBUCLE godeprel;
     #5#1;
     ;
     dVacio, "     ", 000001, dVacio, 001;
     hVacio, "zzzzz", 999999, dVacio, 999;
     ;
     NULL, godeprel;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |SI #0#0 = "SI";
          |ABRE #3; |SELECT #4#3;
          |ABRE #4;
          |BUCLE godeprel;
          |CIERRA #3;
          |CIERRA #4;
     |FINSI;
|FPRC;
