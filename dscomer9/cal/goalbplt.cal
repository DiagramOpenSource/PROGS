     #3#13 = #2#20;                               ||Articulo.
     |LEE 000#3=;

     |SI FSalida ! 0;
          #3#0 = nLinea;
          |GRABA 020#3c;
     |FINSI;

     #3#7 = #0#3;                                 ||Proveedor.
     #3#8 = #0#4;                                 ||Nombre.
     |ABRE #4;
     #4#0 = #3#1;
     #4#1 = #3#2;
     |LEE 000#4=;

