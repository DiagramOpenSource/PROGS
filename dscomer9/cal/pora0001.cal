|FICHEROS;
     pora0001 #0;
|FIN;

|VARIABLES;
     nModo=0;
     aHora="";
     nClave=0;
|FIN;

|PROCESO PonHora;|TIPO 7;
     nModo = (FEntrada/100)!0;
     |SI nModo = 1;
          |HORA aHora;
          #0Campo=aHora % A105;
     |FINSI;
|FPRC;

|PROCESO min;
     |SI FSalida="POSICION";
          #0#0=nClave;
     |SINO;
          #0#0=001;
     |FINSI;
|FPRC;

|PROCESO max;
     #0#0=999;
|FPRC;


|PROGRAMA;
     |CLS;
     |ABRE #0;

     nClave=1;
     |LEE 000#0u;
     |SI FSalida=0;
          nClave=#0#0;  || ultima clave
     |FINSI;

     |ENTLINEAL #1#0,1,2,22,1,min,max;
     |SI FSalida=0;

     |FINSI;
     |CIERRA #0;
|FPRO;
