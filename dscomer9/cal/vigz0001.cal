|PROCESO BuscaPrecio;
     |DDEFECTO #65;
     |ABRE #65;
     #65#0 = #19#0;
     #65#1 = "31.12.9999";
     |LEE 000#65];
     |SI FSalida = 0;
          |LEE 000#65a;
     |SINO;
          |LEE 000#65u;
     |FINSI;
     |PARA; |SI FSalida = 0; |HACIENDO;
          |SI #65#0 = #19#0; |Y #65#8 ! 0;
               nPrecio = #65#8;
               |SAL;
          |FINSI;
          |LEE 000#65a;
     |SIGUE;
     |CIERRA #65;
|FPRC;

|PROCESO RecalCab;
     #1#6 = nPrecio / #1#3;
     |GRABA 020#1a;
     |LIBERA #1;
