|FICHEROS;
     albm0045 #0;
|FIN;

|VARIABLES;
     aMensaje = "";
     nModo = 0;
     aTipo = "";
|FIN;

|PROCESO TipoMov; |TIPO 0;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          aTipo = #albm0045#1;
          |QBF aTipo;
          |SI aTipo ! "entrada"; |Y aTipo ! "mezcladas"; |Y aTipo ! "fabrica";
               aMensaje = "0000Tipo movimiento Incorrecto [" + aTipo + "]";
               |MENAV aMensaje;
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;
