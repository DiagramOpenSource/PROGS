     |DDEFECTO #54;
     #54#0 = #5#2;
     #54#1 = #5#3;
     |LEE 101#54=;
     |SI FSalida ! 0; |Y #0#5 = "S"; |GRABA 020#54b; |FINSI;
     nHayAlma = FSalida;
     |SI #53#194 = 2; nUni0 = #5#48; |SINO; nUni0 = #5#5; |FINSI;
     |SI #4#43 = "N";
          nUniVta = nUni0;
          nUniSal = nUni0;
     |SINO;
          nUniVta = -nUni0;
          nUniSal = -nUni0;
     |FINSI;

