     nCanti2 = #3#44 - #3#49;
     articul = #3#2;
     almacen = #3#3;
     |HAZ pedive_a; || Actualiza el principal
     swopera = 1;
     |SI fabSN = "S"; |HAZ escandallos; |FINSI;
|FPRC;

