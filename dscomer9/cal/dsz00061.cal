|PROCESO linpedi;
     #53#2 = #3#2;
     |LEE 000#53=;

     |SI #53#194 = 2; nUni0 = #3#44; |SINO; nUni0 = #3#5; |FINSI;
     nImpo = ((((nUni0 * #3#6) < #3#8) < #3#29) < #2#5) < #2#17;
     |SI #42#115 = "S";
          nImpo = nImpo < #2#6;
     |FINSI;

     fFecha = #2#1;
     aCliente = #2#4;
     nImpCliPed = nImpo;
     |HAZ AcumCli;

