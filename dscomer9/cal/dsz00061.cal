     aArticulo = #5#2;
     nAlmacen = #5#3;
     |HAZ AcumArt;
     |HAZ AcumAlm;

     |HAZ linalba2;
     |HAZ ValoraStock;
     |SI nHayArti =  0; |GRABA 020#53a; |FINSI;
     |SI nHayAlma =  0; |GRABA 020#54a; |FINSI;
     |LIBERA #53;
     |LIBERA #54;
|FPRC;

|PROCESO linalba2;
     || Al importe total de la linea le quitamos los dtos. del cliente
