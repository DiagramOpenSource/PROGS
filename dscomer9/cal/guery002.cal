|PROCESO guerm002;
  swlin = 1;
  suma = suma + (#2#4 / 1000);
  |PRINT;
  escab = 1;
  #3#0 = #2#2;
  #3#1 = #2#3;
  |LEE 101#3=;
  |SI FSalida ! 0;
      |DDEFECTO #3;
      #3#0 = #2#2;
      #3#1 = #2#3;
      |GRABA 101#3b;
  |FINSI;
  #3#2 = #3#2 + (#2#4 / 1000);
  |GRABA 101#3a;
  |LIBERA #3;
|FPRC;

|PROCESO guerm001;
  |SI prim = 0;
      prim = 1;
      cliant = #1#3;
      obrant = #1#29;
      suma = 0;
  |FINSI;
  |SI cliant ! #1#3;  |O obrant ! #1#29;
      swlin = 3;
      |PRINT;
      |CIERRA #3;
      suma = 0;
      |BUCLE guerz006;
      swlin = 4;
      |PRINT;
      |DELINDEX #3;
      |ABRE #3;
      |CIERRA #3;
      |ABRE #3;
      |PIEINF;
      suma = 0;
  |FINSI;
  cliant = #1#3;
  obrant = #1#29;
  escab = 0;
  |BUCLELIN 2guerm002#1;
|FPRC;

