|FICHEROS;
     agifa080 #80;
     con00004@ #4;
|FIN;

|VARIABLES;
     &enForma = 0;
     nModo = 0;
     aDef = "";
     &eaSerie = "";
     &enNume = 0;
     &enLin = 0;
|FIN;

|PROCESO BorraSec;
     |DDEF aDef;
     aDef = aDef + "con00004";
     |CARGA_DEF aDef, con00004@;
     |SI FSalida ! 0;
          |MENAV "0000ERROR al cargar 'con00004'";
     |SINO;
          |ABRE #4;
          #4#0 = #80#27;
          #4#1 = #80#0;
          #4#2 = #80#1;
          |BORRA 000#4c;
          |CIERRA #4;
          |DESCARGA_DEF #con00004@;
     |FINSI;
|FPRC;


|PROCESO TallerSecCom;
     nModo = (FEntrada / 100) ! 0;

     |SI enForma = 1;
          eaSerie = #80#27;
          enNume = #80#0;
          enLin = #80#1;
          |SUB_EJECUTA_NP "con00005"; || Pide Seccion
     |FINSI;
     |SI nModo = 3;
          |HAZ BorraSec;
     |FINSI;
|FPRC;
