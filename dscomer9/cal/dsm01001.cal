     nAcumula = nAcumula + nCanti;
     nReparto = nCanti * nPorce;
     nEntero  = (nReparto - .50) ! 0;
     |SI nReparto = nEntero;
         nCantiBlanco = nReparto;
         nTCantiBlanco = nTCantiBlanco + nCantiBlanco;
