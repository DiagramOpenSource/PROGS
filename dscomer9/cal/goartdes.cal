|INCLUYE i_varart;
------------------------------------------------------------------------------
|PROCESO CalLinCom; |TIPO 0;
     #1#5 = #1#5 ! #8#115; |PINTA #1#5;
     #1#7 = #1#5 * #1#6;
     |PINTA #1#7; |PINTA #1#3;|PINTA #1#4;|PINTA #1#8;
|FPRC;
------------------------------------------------------------------------------
|PROCESO TipoArti; |TIPO 3;
     |ABRE #3;
     #3#0 = #1#8;
     |LEE 000#3=;

     |SI FSalida = 0;
          #1#9 = #3#1;                            ||Descripción Articulo.
     |FINSI;

     |CIERRA #3;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO MiraArticulo; |TIPO 0;
     nModo = (FEntrada / 100) ! 0;

     |SI FSalida = 10;
          |CONFI "0000SDESEA CREAR UN ARTICULO";
          |SI FSalida = 0;
               |PUSHV 01012380;
               |SUB_EJECUTA "goaltaar";
               |POPV;
               |QBF aCodAlta;
               |SI aCodAlta = ""; |ERROR; |ACABA; |FINSI;

               #1#2 = aCodAlta;
               |PINTA #1#2;
          |SINO;
               |ERROR; |ACABA;
          |FINSI;
     |FINSI;

     |ABRE #4;
     #4#0 = #1#2;
     |LEE 000#4=;
     |SI FSalida = 0;
          |HAZ MonBas;
          cCodMonDe = cCodMonBase;
          cCodMonA  = #0#5;
          nPrecioComp = #4#133;
          |HAZ EsForestales;
          |SI FSalida = 0;
               nPrecioComp = #4#149 - (#4#149 / 100 * #4#152);
               |SI #4#149 = 0; nPrecioComp = #4#133; |FINSI;
          |FINSI;

          |HAZ DemonAmon;
          |SI nModo = 1;
             #1#3 = #4#1;                            ||Descripcion.
             #1#4 = #4#5;                            ||Unidad de Medida.
             #1#6 = nPrecioComp;                     ||Precio Ul. Comp.
             #1#8 = #4#126;                          ||Tipo Articulo.
          |FINSI;
          |PINTA #1#3;|PINTA #1#4;
          |PINTA #1#5;|PINTA #1#6;
     |SINO;
          |MENAV "0000No existe articulo...";
          |ERROR;
     |FINSI;
     |CIERRA #4;
|FPRC;
-------------------------------------------------------------------------------

|PROCESO BuscaArt2; |TIPO 66;
     |ABRE #4;
     #4#0 = #1#2;
     |LEE 000#4];
     |CONSULTA_CLAVE #1#4;
     |SI FSalida = 0;
          #1#2 = #4#0;
          #1#3 = #4#1;
          |PINTA #1#2;
          |PINTA #1#3;
     |SINO;
          |ERROR;
     |FINSI;
     |CIERRA #4;
|FPRC;

|PROCESO Arti7; |TIPO 7;
     eaArtiAnt = #1#2;
|FPRC;

|PROCESO DobleUnid; |TIPO 7;
     |C_M #1#8, 1, "S";

     |ABRE #4;
     #4#0 = #1#2;
     |LEE 000#4=;
     |SI FSalida = 0; |Y #4#179 = "S";
          ||eaArtiAnt  (se cargar en el tipo 0 de la linea)
          eaArticulo = #1#2;
          enUnidad1 = #1#5;
          enUnidad2 = #1#10;
          enMedida1 = #1#11;
          enMedida2 = #1#12;
          enMedida3 = #1#13;
          eaPartida = "";
          enModoVta = (FEntrada / 100) ! 0;
          |SUB_EJECUTA_NP "dsz99907";
          |SI eaSw2Stock = "S";
               |C_M #1#8, 1, "N";
               #1#5 = enUnidad1; |PINTA #1#5;
               #1#10 = enUnidad2;
               #1#11 = enMedida1;
               #1#12 = enMedida2;
               #1#13 = enMedida3;
          |SINO;
               #1#10 = 0;
               #1#11 = 0;
               #1#12 = 0;
               #1#13 = 0;
          |FINSI;
     |FINSI;
     |CIERRA #4;
|FPRC;
