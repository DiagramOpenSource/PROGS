|PROCESO ImprimeFac;
     #1#49 = #300#0;
     #1#0 = #300#1;
     |LEE 121#1=;
     |SI FSalida = 0;
          |HAZ agifa134;
          |LIBERA #1;
     |FINSI;
|FPRC;

|DEFBUCLE ImpAlb;
     #300#3;
     ;
     INICIO;
     FINAL;
     ;
     NULL, ImprimeFac;
|FIN;

|DEFBUCLE ImpCliAlb;
     #300#2;
     ;
     INICIO;
     FINAL;
     ;
     NULL, ImprimeFac;
|FIN;

|DEFBUCLE ImpFac;
     #300#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, ImprimeFac;
|FIN;

|PROCESO OrdenaFacL;
     #300#0 = #1#49;
     #300#1 = #1#0;
     |LEE 101#300=;
     |SI FSalida ! 0; |GRABA 020#300b; |FINSI;
     #300#2 = #1#2;
     #300#3 = #2#15;
     #300#4 = #2#2;
     |GRABA 020#300a;
     |LIBERA #300;
|FPRC;

|PROCESO OrdenaFacC;
     |DDEFECTO #11;
     #6#0 = #1#11;
     |LEE 000#6=;
     |SI #6#69 < #0#44; |O #6#69 > #0#45; ||Tipo Recibo
          |ERROR;
     |FINSI;
|FPRC;

|DEFBUCLE OrdenaFac;
     #1#1;
     2,1,45,31,11,7;
     #0#9, #0#2 , #0#0 , #0#4 , #0#6 ,  #0#7, #0#42, #0#46;
     #0#10,#0#3 , #0#1 , #0#5 , #0#6 ,  #0#8, #0#43, #0#47;
     be;
     NULL, OrdenaFacC, OrdenaFacL;
|FIN;

|PROCESO Rodacal;
     |HAZ CreaTempo; aTempo = Tempo;
     |NOME_DAT #300 aTempo;
     |DELINDEX #300;

     |ABRE #300;
     |BUCLE OrdenaFac;
     |CIERRA #300;
     |ABRE #1;
     |SI #0#48 = "C";
          |BUCLE ImpCliAlb;
     |FINSI;
     |SI #0#48 = "A";
          |BUCLE ImpAlb;
     |FINSI;
     |SI #0#48 = "F";
          |BUCLE ImpFac;
     |FINSI;
     |CIERRA #1;
     |DELINDEX #300;
     Tempo = aTempo; |HAZ BoraTempo;
|FPRC;
