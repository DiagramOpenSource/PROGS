|FICHEROS;
     malpe720  #720;
     malpe710  #710;
|FIN;

|VARIABLES;
     &enLinea = 0;
     aPeriodo = "";
     aParam = "";
     nVentana = 0;
     aAlfa = "";
|FIN;

|PROCESO malpe710;
     aAlfa = "";
     enLinea = #710#0;
     aAlfa = "malpe717;" + enLinea;
     |SUB_EJECUTA_NP aAlfa;
|FPRC;

|DEFBUCLE malpe710;
     #710#3;
     ;
     aPeriodo, 00001;
     aPeriodo, 99999;
     ;
     NULL, malpe710;
|FIN;

|PROGRAMA;
     aParam = PARAMETRO;
     aParam = aParam % 101;

     |VENTANA 0501, 2799, -1, 1, "";
     nVentana = FSalida;

     |PINPA #0#720;
     |PTEC 802; |PAUSA;

     |ABRE #720;
     |LEE 101#720p;
     |SI FSalida ! 0;
          |DDEFECTO #720;
          |GRABA 020#720b
     |FINSI;

     |SI aParam ! "";
          aPeriodo = aParam;
          |BUCLE malpe710;
     |SINO;
          |MENAV "    No se ha especificado periodo";
     |FINSI;

     |LIBERA #720;
     |CIERRA #720;

     |FINVENTANA nVentana;
     |PULSATECLA;
|FPRO;
