|FICHEROS;
     guery005 #0;

     guerm033 #33;       ||Vehiculos
|FIN;

|VARIABLES;
     aMensaje = "";
     nMes = 0;
     nCampo = 0;
     aInforme = "";
     nSwPie = 0;
     &enKmA = 0;
     &enLiA = 0;
     &enEuA = 0;
     &enKm = 0;
     &enLi = 0;
     &enEu = 0;
     nAux = 0;
     nAux1 = 0;
     &enCoA = 0;
     &enEKA = 0;
     &enCo = 0;
     &enEK = 0;
|FIN;

|PROGRAMA;
     |CLS;
     |ABRE #0;
     |DDEFECTO #0;
     |LEE 101#0p;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |SI #guery005#5 = "SI";
               |HAZ Principal;
          |FINSI;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;

|PROCESO guerm033;
     enLiA = 0;
     enEuA = 0;
     |PARA nCampo = 1; |SI nCampo [ #guery005#3; |HACIENDO nCampo = nCampo + 1;
          nAux = 3 + nCampo;

          nAux = 16 + nCampo;
          enLiA = enLiA + #guerm033#nAux#;

          nAux = 29 + nCampo;
          enEuA = enEuA + #guerm033#nAux#;
     |SIGUE;

     ||Valores para el mes
     nCampo = #guery005#3;
     nAux = 3 + nCampo;

     enKmA = #guerm033#nAux# - #guerm033#3;
     nAux1 = nAux - 1;
     enKm = #guerm033#nAux# - #guerm033#nAux1#;
     nAux = 16 + nCampo;
     enLi = #guerm033#nAux#;
     nAux = 29 + nCampo;
     enEu = #guerm033#nAux#;

     enCoA = (100 * enLiA) / enKmA;
     enCo = (100 * enLi) / enKm;
     enEKA = enEuA / enKmA;
     enEK = enEu / enKm;

     |PRINT;
     nSwPie = 1;
|FPRC;

|DEFBUCLE guerm033;
 #guerm033#1;
 ;
 #guery005#1;
 #guery005#2;
 be;
 NULL, guerm033;
|FIN;

|PROCESO Principal;
     |IMPRE 0;
     |SI FSalida = 0;
          aInforme = "guery005";
          |INFOR aInforme;
          |SI FSalida = 0;
               |BUCLE guerm033;
               |SI nSwPie = 1;
                    |PIEINF;
               |FINSI;
               |FININF;
          |SINO;
               aMensaje = "0000Informe " + aInforme + " inaccesible.";
               |MENAV aMensaje;
          |FINSI;
          |FINIMP;
     |SINO;
          |MENAV "0000Proceso cancelado.";
     |FINSI;
|FPRC;
