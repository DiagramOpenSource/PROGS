
             CUALQUIER COSA QUE TOQUES DE LOS DOS PRC QUE TIENE
             ESTE TAB PUEDE REPERCUTIR EN TODA LA APLICACION DEBES
             TENER MUY CLARO COMO FUNCIONA ESTE PROGRAMA.


|VARIABLES;
     aParametro = "";
|FIN;

 ---------------------------------------------------------------------------
                     QUIEN REPARTE SE LLEVA LA MEJOR PARTE
 ---------------------------------------------------------------------------

|PROGRAMA;
     aParametro = PARAMETRO;

     |HAZ AbreFicheros;

     |SI aParametro = "PV";          || Pedidos Ventas
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ PedidoVenta;
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |SI aParametro = "DP";          || Depositos
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ Deposito;
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |SI aParametro = "AV";          || Albaranes Ventas
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ AlbaranVenta;
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |SI aParametro = "AVP";          || Adjudicacion Albaranes Ventas
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ AlbaranPedidoV;
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |SI aParametro = "FC";          || Facturas Contado
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ FacturasContado;
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |SI aParametro = "PC";          || Pedidos Compras
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ PedidoCompra;
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |SI aParametro = "AC";          || Albaranes Compras
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ AlbaranCompra;
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |SI aParametro = "ACP";          || Adjudicacion Albaranes Compras
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ AlbaranPedidoC;
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |SI aParametro = "PCM_COM";     || Precio Medio Coste Compras
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ PrecioMedioCoste;
         |HAZ AlbaCompraCab;         || Acumulado de importes y Dtos
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |SI aParametro = "PCM";          || Precio Medio Coste
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ PrecioMedioCoste;
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |SI aParametro = "TI";          || TPV
         |HAZ LeArticulo;
         |HAZ LeeAlmacen;
         |HAZ Tpv;
         |HAZ GrabaArticulo;
         |HAZ GrabaAlmacen;
     |FINSI;

     |HAZ CierraFicheros;
|FPRO;
