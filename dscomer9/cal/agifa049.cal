|PROCESO EntraLin7; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;

     |HAZ EsRodacal48;
     |SI FSalida = 0;
          ||MENSA "0000Unidades Componentes: <F5> Modificacion  <F6> Alta ";
          |MENSA "0000<F5> Modificacion Unidades Componentes";
     |FINSI;
     |HAZ EsOrts;
     |SI FSalida = 0; |Y nModo = 4;
          |MENSA "0000<ENTER> Muestra todos los campos de la linea";
     |FINSI;

     |HAZ Es48X;
     |SI FSalida = 0; |Y nModo = 1;
          #1#0 = #0#0;
          #1#1 = 999;
          |LEE 000#1];
          |SI FSalida = 0;
               |LEE 000#1a;
          |SINO;
               |LEE 000#1u;
          |FINSI;
          |SI FSalida = 0; |Y #1#0 = #0#0;
               nLinea = #1#1 + 1;
          |SINO;
               nLinea = 1;
          |FINSI;
          |DDEFECTO #1;
          #1#0 = #0#0;
          #1#1 = nLinea;
     |FINSI;
|FPRC;

