|PROCESO DefeUniPre;
     |HAZ LeeConverUni;
     |HAZ EsTecatel;
     |SI #142#9 = "S"; |Y FSalida = 0;
          |SI #241#6 = "D";
               nNume = #3#69 * #241#5;
               #1#15 = nNume;
          |SINO;
               nNume = #3#69 / #241#5;
               #1#15 = nNume;
          |FINSI;
     |SINO;
          |SI Campo = 9; |ACABA; |FINSI;
          |SI #241#6 = "D";
               nNume = #2#9 * #241#5;
               #1#15 = nNume;
          |SINO;
               nNume = #2#9 / #241#5;
               #1#15 = nNume;
          |FINSI;
     |FINSI;
     |PINTA #1#15;
