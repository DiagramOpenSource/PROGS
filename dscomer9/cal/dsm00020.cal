     nForma = 0;
     nModo = 0;
|FIN;

|PROCESO CargaTeca36;
     |DDEF aAlfa;
     aAlfa = aAlfa + "tecam036";
     |CARGA_DEF aAlfa, tecam036@;
|FPRC;

|PROCESO Tipo13_20; |TIPO 13;
     |HAZ EsDHL;
     |SI FSalida = 0;
          |HAZ CargaTeca36;
          |SI FSalida = 0;
               |DDEFECTO #tecam036@;
               |ABRE #tecam036@;
               #tecam036@#0 = #0#0;
               |LEE 000#tecam036@.=;

               |BLANCO 18102170;
               |PINPA #0#tecam036@;
               |PINDA #0#tecam036@;

               |CIERRA #tecam036@;
               |DESCARGA_DEF #tecam036@;
          |FINSI;
     |FINSI;

     |HAZ EsCoimasa;
     |SI FSalida = 0;
          |PINTA 1850, "Genera Txt:";
          |PINTA 1950, "อออออออออออ";
          |C_V #0#10, 1, "S";
     |FINSI;

     |HAZ EsTagloma;
     |SI FSalida = 0;
          |PINTA 1850, "A Vueltas: ";
          |PINTA 1950, "อออออออออออ";
          |C_V #0#10, 1, "S";
     |FINSI;
|FPRC;

|PROCESO Persona; |TIPO 0;
     |HAZ EsDHL;
     |SI FSalida = 0;
          aAlfa = "tecam036;" + #0#0;
          |SUB_EJECUTA_NP aAlfa;
          |HAZ Tipo13_20;
     |FINSI;
|FPRC;

|PROCESO Tipo2_20; |TIPO 2;
     nForma = 0 +. 1;
     nModo = (FEntrada / 100) ! 0;

     |HAZ EsDHL;
     |SI FSalida = 0;
          |SI nModo = 3;
               |HAZ CargaTeca36;
               |SI FSalida = 0;
                    |DDEFECTO #tecam036@;
                    |ABRE #tecam036@;
                    #tecam036@#0 = #0#0;
                    |BORRA 020#tecam036@.c;
                    |CIERRA #tecam036@;
                    |DESCARGA_DEF #tecam036@;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;
