|FICHEROS;
     sctrm067 #0;
     sctrm001 #100;
|FIN;

|VARIABLES;
     &enPantaLogo = 0;
     &eaNomLogo = "";
     nPanta = 0;
     aAlfa = "";
     nReg = 0;
     nModo = 0;
|FIN;

|PROCESO T67_13; |TIPO 13;
     |ABRE #100; |LEE 000#100p; |CIERRA #100;
     |PINPA #0#0; nPanta = FSalida;
     eaNomLogo = #100#1;
     |HAZ TraeLogoPan;
     enPantaLogo = nPanta;
     |HAZ PintaLogoPan;
|FPRC;

|PROCESO Registro; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          aAlfa = #0#0;
          #0#6 = 999;
          |LEE 000#0];
          |SI FSalida = 0;
               |LEE 000#0a;
          |SINO;
               |LEE 000#0u;
          |FINSI;
          |SI FSalida = 0; |Y #0#0 = aAlfa;
               nReg = #0#6 + 1;
          |SINO;
               nReg = 1;
          |FINSI;
          |DDEFECTO #0;
          #0#0 = aAlfa;
          #0#6 = nReg;
     |FINSI;
|FPRC;
