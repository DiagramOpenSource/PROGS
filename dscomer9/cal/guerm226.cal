|FICHEROS;
     guerm226 #0;
|FIN;

|VARIABLES;
     aAlfa = "";
|FIN;

|PROCESO SoloFija; |TIPO 7;
     |SI #0#1 = "F";
          aAlfa = #0#2; |QBF aAlfa;
          |SI aAlfa = "Automatico";
               #0#2 = "";
               |PINTA #0#2;
          |FINSI;
          |C_M #0#2, 1, "S";
     |SINO;
          |C_M #0#2, 1, "N";
          #0#2 = "Automatico";
          |PINTA #0#2;
     |FINSI;
|FPRC;

|PROCESO DSCorreo; |TIPO 0;
     |QUE_SISTEMA aAlfa;
     |SI aAlfa = "ESBSD";|Y #0#1 = "S";
          |MENAV "    Configuracion no permitida, servidor UNIX. Recomendado: [F]";
          |ERROR;
     |FINSI;
|FPRC;

|PROGRAMA;
     |CLS;
     |ABRE #0;
     |LEE 101#0p;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |GRABA 020#0a;
     |FINSI;
     |CIERRA #0;
|FPRO;
