|FICHEROS;
   tecam006 #0;
   tecam007 #1;
   tecam008 #8;
   referen@ #10;
|FIN;

|VARIABLES;
     nModo = 0;
     fFecha = @;
     aAlfa = "";
     aDef = "";
     i = 0;
     nCampos = 20;
|FIN;

|PROCESO tecam008;
     |BORRA 020#8a;
|FPRC;

|DEFBUCLE tecam008;
     #8#1;
     ;
     #1#0, #1#1, #1#2, #1#3, -999999
     #1#0, #1#1, #1#2, #1#3, 999999;
     be;
     NULL, tecam008;
|FIN;

|PROCESO Tipo2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |BUCLE tecam008;
     |FINSI;
|FPRC;

|PROCESO Min;
     #8#0 = #1#0;
     #8#1 = #1#1;
     #8#2 = #1#2;
     #8#3 = #1#3;
     #8#4 = -99999;
|FPRC;

|PROCESO Max;
     #8#0 = #1#0;
     #8#1 = #1#1;
     #8#2 = #1#2;
     #8#3 = #1#3;
     #8#4 = 99999;
|FPRC;

|PROCESO Series; |TIPO 11;
     nModo = (FEntrada / 100) ! 0;
     |SI FSalida = 13; |Y nModo = 4;
          |PUSHV 0501 2380;
          |ENTLINEAL #1#8, 4, 4, 22, 1, Min, Max;
          |POPV;
     |FINSI;
|FPRC;

|PROCESO Carga;
     |PARA i = 0; |SI i [ nCampos; |HACIENDO i = i + 1;
          #0i = #10i;
     |SIGUE;
     |GRABA 020#0n;
|FPRC;

|DEFBUCLE Carga;
     #10#2001;
     ;
     fFecha;
     fFecha;
     ;
     NULL, Carga;
|FIN;

|PROGRAMA;
     |SI PARAMETRO = "";
          |MANTE #0;
     |SINO;
          fFecha = ~;
          |DDEF aDef;
          aDef = aDef + "tecam006";
          |CARGA_DEF aDef, referen@;
          |SI FSalida = 0;
               aAlfa = Usuario;
               |NOME_DAT #0 aAlfa;
               |DELINDEX #0;

               |MENSA "0000Cargando temporal";
               |ABRE #0;
               |BUCLE Carga;
               |CIERRA #0;

               |MANTE #0;

               |DELINDEX #0;
               |DESCARGA_DEF #referen@;
          |FINSI;
     |FINSI;
|FPRO;
