|PROCESO ProcesaCampo;
     nCampo = -1;
     aCampo = "";
     |PARA nNume2=1;|SI nNume2 [ 100;|HACIENDO nNume2 = nNume2 + 1;
          nNume3    = (nNume2 * 100) + 1;
          aPos      = nNume3;
          aCaracter = aField % aPos;
          |SI nCampo < 6;
               |SI aCaracter = "@";
                    nCampo = nCampo + 1;
                    |SI nCampo = 2;
                        nFech = aCampo;
                        nFech = nFech + 718796;
                        fCampo = nFech;
                        #1#nCampo# = fCampo;
                    |SINO;
                        |SI nCampo = 4;|O nCampo = 5;
                            nNume4 = aCampo;
                            nNume4 = nNume4/100;
                            aCampo = nNume4;
                        |FINSI;
                        #1#nCampo# = aCampo;
                    |FINSI;
                    aCampo = "";
               |SINO;
                    aCampo = aCampo + aCaracter;
               |FINSI;
               |SI aCaracter = "";
                   nCampo = nCampo + 1;
                   #1#nCampo# = aCampo;
               |FINSI;
          |SINO;
               |GRABA 020#1n;
               nCampo = 0;
               aCampo = "";
               |PINTA 1310, #1#0;
               nCuantos = nCuantos + 1;
               |DDEFECTO #1;
               |SAL;
          |FINSI;
     |SIGUE;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     |DELINDEX #1;
     |ABRE #1;
     aAlfa1 = #0#0; |QBF aAlfa1;
     |XABRE "U",aAlfa1,nNume1;
     |SI FSalida ] 0;
         |XLEE nNume1,250,aField;
         |PARA; |SI aField > ""; |HACIENDO;
               |HAZ ProcesaCampo;
               |XLEE nNume1,250,aField;
         |SIGUE;
     |SINO;
          aAlfa1 = "0000[" + (nNume % 5) + "] inaccesible.";
          |MENAV aAlfa1;
     |FINSI;
     |CIERRA #1;
|FPRC;
