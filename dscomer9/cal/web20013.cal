|FICHEROS;
     web20012 #0;
     web20002 #2;
     agifa104 #104;
|FIN;

|VARIABLES;
     i         = 0;
     nPos      = 0;
     aDato     = "";
     nHandle   = 0;
     aAlfa     = "";
     aID       = "";
     aFic      = "";
     aSw       = "";
     aLon      = "";
     aCli      = "";
     nUni      = 0;
|FIN;

|PROCESO agifa104;
     aAlfa = #104#25 + "|" + #104#0 + "|"+ #104#1 + "|" + #104#57 + "|" + #104#19+ "|";
     aAlfa = aAlfa + #104#45 + "|" + #104#46 + "|" + #104#47;
     |XGRABA nHandle, aAlfa;
|FPRC;

|DEFBUCLE agifa104;
     #104#2;
     ;
     aCli, "      ", 000001;
     aCli, "zzzzzz", 999999;
     ;
     NULL, agifa104;
|FIN;

|PROGRAMA;
     aAlfa = PARAMETRO;
     aLon = aAlfa % A0;

     |PARA i = 1; |SI i [ aLon; |HACIENDO i = i + 1;
          nPos = i * 100 + 1;
          aDato = aAlfa % nPos;
          |SI aSw = "Z"; |O aDato = "Z";
               |SI aSw = "Z";
                    aFic = aFic + aDato;
               |SINO;
                    aSw = "Z";
               |FINSI;
          |SINO;
               aID = aID + aDato;
          |FINSI;
     |SIGUE;

     |ABRE #2;
     #2#0 = aID;
     |LEE 000#2=;
     |SI FSalida ! 0;
          aAlfa = "0000No existe identificador:[" + aID + "]";
          |MENSA aAlfa;
     |SINO;
          |XABRE "W", aFic, nHandle;
          |SI FSalida ] 0;
               aCli = #2#3;
               |BUCLE agifa104;
               |XCIERRA nHandle;
          |SINO;
               aAlfa = "0000No puedo crear: [" + aFic + "]";
               |MENSA aAlfa;
          |FINSI;
     |FINSI;
     |CIERRA #2;
|FPRO;
