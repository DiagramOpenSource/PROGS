         #3#47 = aAlfa;
         #3#5  = nCantiBlanco;
         #3#48 = nCantiBlanco2;
         |GRABA 020#3a;
         |LIBERA #3;
     |FINSI;
|FPRC;

|PROCESO PorPrecio;
