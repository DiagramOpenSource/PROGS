|PROCESO liacarti; |TIPO 2;
     || *(2)
     nForma = 0 +. 1;
     enForma = 0 +. 1;
     modo = (FEntrada / 100) ! 0;
     |SI modo = 1; |O modo = 2;
          |HAZ Riesgos71;
          |SI enErrRie = 1; |ERROR; |ACABA; |FINSI;
     |FINSI;
     |SI modo = 2;|O modo = 3;
          |SI #0#12 ! 0;
               |SI enForma = -1;
                    |MENAV "0000La linea proviene de pedido...";
               |FINSI;
               |SI modo = 3; |ERROR; |ACABA; |FINSI;
          |FINSI;

          efFec = #1#1;
          enAlma = #0#3;
          |HAZ MiraHisL;
          |SI enErrHis ! 0;
               |ERROR;
               |ACABA;
          |FINSI;
     |FINSI;
     |SI #0#19 = "S"; |O #0#20 = "S";|O #1#38 = "S"; |O #1#39 = 999999;
          |SI enForma < 0;
              |SI #0#19 = "S";
                  |MENAV "0000Esta linea proviene de un Pedido (use la opcion adecuada)";
              |FINSI;

              |SI #0#20 = "S";
                  |MENAV "0000Esta linea proviene de un Deposito (use la opcion adecuada)";
              |FINSI;

              |SI #1#38 = "S";
                  |MENAV "0000Este albaran esta FACTURADO";
              |FINSI;

              |SI #1#39 = 999999;
                  |MENAV "0000Este albaran esta en modo Exportacion";
              |FINSI;
          |FINSI;
          |ERROR;
          |ACABA;
     |FINSI;

     |SI #0#9 > 0;
         nerror = 1;
     |SINO;
         nerror = -1;
         #0#9  = -#0#9;
         #0#37 = -#0#37;
     |FINSI;

     imneto  = ((((#0#9  < #1#5) < #1#32) < #1#7)) * nerror;
     imnetoD = ((((#0#37 < #1#5) < #1#32) < #1#7)) * nerror;
     #0#9    = #0#9  * nerror;
     #0#37   = #0#37 * nerror;

     ||-------------------Actualiza Historico, Articulo y Almacen
     enForma = 0 +. 1;
     |SI #1#97 ! 0; |Y #0#12 ! 0;     ||Proviene de una Orden de carga
          |HAZ ModifiOrden;
          |SI nError ! 0; |ERROR; |ACABA; |FINSI;
     |FINSI;
     |SI #0#66 ! "R";         ||Roturas, no crea movimiento
          enUniPedi = 0;
          |HAZ AcumulaAVMp;
     |FINSI;
     ||-------------------
     #0#14 = enCoste;
     #1#37 = #1#37 + (imneto - enCoste);

     |SI modo = 1; |O modo = 2;
         |HAZ grapre;                  || Graba Precio Cliente/Articulo
     |FINSI;

     |HAZ CalCabMalpe090;
     |HAZ visual_totales;
     |HAZ Remanente;

     |SI modo = 2; |Y #0#12 ! 0; || de orden de carga
          aCampos = "|NC";
          aCampos = #0#aCampos#;
          ICam = ICam<-;
          |SI nForma = -1;
               |PARA i = 2; |SI i < aCampos; |HACIENDO i = i + 1;
                    |C_M #0i, 0, aAlfa;
                    |C_M #0i, 1, "N";
                    Cam = aAlfa;
                    ICam = ICam + 1;
               |SIGUE;
               |C_M #0#68, 1, "S";
               nSwModi = 1;
          |SINO;
               |PARA i = 2; |SI i < aCampos; |HACIENDO i = i + 1;
                    aAlfa = Cam;
                    ICam = ICam + 1;
                    |C_M #0i, 1, aAlfa;
               |SIGUE;
               nSwModi = 0;
          |FINSI;
     |FINSI;
|FPRC;

