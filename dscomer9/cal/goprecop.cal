|FICHEROS;
     goprecop #0;
     gopre001 #1;
     goparame #10;
|FIN;

|VARIABLES;
     &eaSer    = "";
     &enNum    = 0;

     &enModo   = 0;
     &eaTipoD  = "";
     aAlfa     = "";
     nNum      = 0;
|FIN;

|PROCESO Tipo0 ; |TIPO 0;
     |SI #0#1 < #10#58; |O #0#1 > #10#59;
          aAlfa = "0000Rango parametrizado: " + #10#58 + " a " + #10#59;
          |MENAV aAlfa;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     |SI #0#1 < #10#58; |O #0#1 > #10#59;
          |ACABA;
     |FINSI;

     nNum = #0#1 + #10#54;
     |ABRE #1;
     #1#0 = #0#0;
     #1#1 = nNum;
     |LEE 000#1=;
     |SI FSalida = 0;
          aAlfa = "0000La copia " + #0#0 + "/" + (("000000" + nNum) % -106) + " existe...";
          |MENAV aAlfa;
     |SINO;
          enModo = 1; eaTipoD = "E";
          |MENSA "2400DUPLICANDO PRESUPUESTO...";
          aAlfa = "gopredup" + &59 + #0#0 + (("000000" + #0#1) % -106);
          |SUB_EJECUTA_NP aAlfa;
     |FINSI;
     |CIERRA #1;
|FPRC;

|PROCESO Tipo13; |TIPO 13;
     |SI #10#141 = "O";
          |ATRI R; |PINTA 927, "ORDEN ENTRADA "; |ATRI 0;
     |FINSI;
|FPRC;

|PROGRAMA;
     |ABRE #10; |LEE 000#10p; |CIERRA #10;

     |SI enNum ! 0;
          #0#0 = eaSer;
          #0#1 = enNum;
          |HAZ Tipo3;
     |SINO;
          |CLS;
          |PINPA #0#0;
          |PINDA #0#0;
          |SI #10#52 = "S";
               |ENDATOS #1#0;
          |SINO;
               |MENAV "0000Opcion no activa en parametros...";
          |FINSI;
     |FINSI;
|FPRO;
