     #25#0 = #3#0;
     |LEE 000#25=;
     nUnidades = #25#15;
     |HAZ Genera2Letras;
     #5#2 = aLetras1;
     #5#3 = aLetras2;

     || se hace para que en el endatos de pedir unidades y descuento,
     || se sepan los datos que necesitamos, como nLineaMaxDto y tabla
     || dto-comision

     nCodCli = #0#15;
     nCodArt = #3#0;
