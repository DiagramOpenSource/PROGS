     #124#4 = 0;
     |SI aAlfa1 = "0000000000017"; #124#4 = 1; |FINSI;
     |SI aAlfa1 = "0000000000024"; #124#4 = 2; |FINSI;
     |SI aAlfa1 = "0000000000055"; #124#4 = 5; |FINSI;
     |SI aAlfa1 = "0000000000109"; #124#4 = 10; |FINSI;
     |SI #124#4 = 0;
