     |SINO;
          |SI PARAMETRO = "VER";
               |LEE 101#0=;
               |PINPA #0#0;
               |PINDA #0#0;
               |PAUSA;
          |SINO;
               |LEE 101#0=;
               |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
               |PINPA #0#0;
               |PINDA #0#0;
               |ENDATOS #1#0;
               |GRABA 020#0a;
          |FINSI;
     |FINSI;
     |CIERRA #0;
|FPRO;
