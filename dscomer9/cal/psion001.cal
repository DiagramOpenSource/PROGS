|PROCESO CargaRutas;
     |DDEFECTO #35;
     #35#0 = #0#0;
     #35#1 = #32#0;
     |LEE 101#35=;
     |SI FSalida ! 0; |GRABA 020#35b; |FINSI;
     #35#2 = #32#1;
     |GRABA 020#35a;
     |LIBERA #35;
|FPRC;

|DEFBUCLE CargaRutas;
     #32#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, CargaRutas;
|FIN;

|PROCESO Rutas; |TIPO 0;
     |SI FSalida = 0; |Y #0Campo = "S";
          |PUSHV 0501 2380;
          |BLANCO 818 2354;
          |ABRE #35;
          |BUCLE CargaRutas;
          |CIERRA #35;
          |ENTLINEAL #1#35, 2, 4, 21, 1, Min, Max;
          |POPV;
     |FINSI;
|FPRC;

