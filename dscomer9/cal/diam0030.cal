   |SI #diaz0017#24 = "S";
      #diam0031#0 = #diaz0017#nCont#; nCalc = nCont + 1;
      #diam0031#1 = #diaz0017#nCalc#;
   |SINO;
      #diam0031#1 = nCont;
   |FINSI;
   |GRABA 020#diam0031.n;
   |REPON_FICHA #diam0031;
|FPRC;

|DEFBUCLE CopiaLineas;
#diam0031#1;
;
#diaz0017#0, #diaz0017#1, #diaz0017#2, "                    ";
#diaz0017#0, #diaz0017#1, #diaz0017#2, "zzzzzzzzzzzzzzzzzzzz";
;
NULL,CopiaLineas;
|FIN;

|PROCESO Copiador;
   |SI FSalida ! 10; |ACABA; |FINSI;

   |SALVA_FICHA #diam0030;

   |PUSHV 0501 2380;
   |DDEFECTO #diaz0017;
   |SI Campo = 0; #diaz0017#0 = #diam0030#0; |FINSI;
   |SI Campo ] 1; #diaz0017#1 = #diam0030#1; |FINSI;
   |SI Campo ] 2; #diaz0017#2 = #diam0030#2; |FINSI;
   |PINPA #0#diaz0017; |PINDA #0#diaz0017;
   |ENDATOS #1#diaz0017;
   |SI FSalida = 0;
      |SI #diaz0017#24 = "S";
         |PARA nCont = 3; |SI nCont [ 21; |HACIENDO nCont = nCont + 2;
            |SI #diaz0017#nCont# ! 0;
               #diam0030#0 = #diaz0017#0;
               #diam0030#1 = #diaz0017#1;
               #diam0030#2 = #diaz0017#2;
               |LEE 000#diam0030.=;
               |SI FSalida = 0;
                  #diam0030#0 = #diaz0017#nCont#; nCalc = nCont + 1;
                  #diam0030#1 = #diaz0017#nCalc#;
                  |GRABA 020#diam0030.n;
                  |BUCLE CopiaLineas;
               |FINSI;
            |FINSI;
         |SIGUE;
      |SINO;
         |PARA nCont = #diaz0017#25; |SI nCont [ #diaz0017#26; |HACIENDO nCont = nCont + 1;
            |SI nCont ! #diaz0017#1;
               #diam0030#0 = #diaz0017#0;
               #diam0030#1 = #diaz0017#1;
               #diam0030#2 = #diaz0017#2;
               |LEE 000#diam0030.=;
               |SI FSalida = 0;
                  #diam0030#1 = nCont;
                  |GRABA 020#diam0030.n;
                  |BUCLE CopiaLineas;
               |FINSI;
            |FINSI;
         |SIGUE;
      |FINSI;
   |FINSI;
   |POPV;

   |REPON_FICHA #diam0030;
|FPRC;
