|FPRO;

|PROCESO SoloFija; |TIPO 7;
     |SI #0#9 = "F";
          aAlfa1 = #0#8; |QBF aAlfa1;
          |SI aAlfa1 = "Automatico";
               #0#8 = "";
               |PINTA #0#8;
          |FINSI;
          |C_M #0#8, 1, "S";
     |SINO;
          |C_M #0#8, 1, "N";
          #0#8 = "Automatico";
          |PINTA #0#8;
     |FINSI;
|FPRC;

|PROCESO DSCorreo; |TIPO 0;
     |QUE_SISTEMA aAlfa1;
     |SI aAlfa1 = "ESBSD";|Y #0#9 = "S";
          |MENAV "    Configuracion no permitida, servidor UNIX. Recomendado: [F]";
          |ERROR;
     |FINSI;
|FPRC;
