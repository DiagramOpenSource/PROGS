|FICHEROS;
     qdclor08 #0,MANTE;
     qdclor01 #1;   ||Aux Arti
|FIN;

|VARIABLES;
     aAlfa = "";
     &eaSerie = "";
     &enCodigo = 0;
     &enLinea = 0;
     &eaArticulo = "";
|FIN;

|PROCESO T12; |TIPO 12;
|FPRC;

|PROCESO Defecto;
     |DDEFECTO #1;
     |ABRE #1;
     #1#0 = eaArticulo;
     |LEE 000#1=;
     |CIERRA #1;

     #0#4 = #1#10;
     #0#5 = #1#2;
|FPRC;

|PROGRAMA;
     |ABRE #0;
     #0#0 = eaSerie;
     #0#1 = enCodigo;
     #0#2 = enLinea;
     |SI PARAMETRO = "BAJA";
          |BORRA 000#0c;
     |SINO;
          |LEE 101#0=;
          |SI FSalida ! 0;
               |HAZ Defecto;
               |GRABA 020#0b;
          |SINO;
               |SI #0#6 ! eaArticulo; |HAZ Defecto; |FINSI;
          |FINSI;
          #0#6 = eaArticulo;
          |SI PARAMETRO = "ACT";
               FSalida = 0;
          |SINO;
               |PINDA #0#0;
               |ENDATOS #1#0;
          |FINSI;
          |SI FSalida = 0; |GRABA 020#0a; |FINSI;
     |FINSI;
     |CIERRA #0;
|FPRO;
