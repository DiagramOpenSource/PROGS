|FICHEROS;
     web10005 #0;
|FIN;

|VARIABLES;
     aAlfa = "";
     aAlfa1 = "";
     pass = "";
     numpass = "";
     num = 0;
     aIPRemoto = "";
     aPath = "";
     aAra = "";
|FIN;

|CALCULO finbarra;|TIPO 0;
     aAlfa = #0Campo;
     |QBF aAlfa;
     aAlfa1 = aAlfa % -101;
     |SI aAlfa1 ! "/";
          aAlfa = aAlfa + "/";
          #0Campo = aAlfa;
          |PINTA #0Campo;
     |FINSI;
|FCAL;

|PROGRAMA;
     |CLS;
     |CABEZA "Parametros de DSCorreo";
     |PINPA #0#0;
     |ABRE #0;
     |DDEFECTO #0;
     #0#0 = "A";
     |LEE 101#0=;
     |SI FSalida ! 0;
         |DDEFECTO #0;
         |GRABA 020#0b;
     |FINSI;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
         |GRABA 020#0a;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;

|PROCESO copia;
     |SI #0Campo ! Contenido;
     #0#13 = #0#16;
     pass = #0#13;
     |QBF pass;
     num = 0;
     numpass = pass % num;
     pass = "**********************************************";
     numpass = 10 + numpass;
     pass = pass % numpass;
     #0#16 = pass;
     |PINTA #0#16;
     |FINSI;
|FPRC;

|PROCESO EsMultipuesto; |TIPO 7;
          |ACABA;
     |IP_REMOTO aIPRemoto;
     |QBT aIPRemoto;
     |SI aIPRemoto ! "";
          |QUE_SISTEMA aAlfa1;
          |SI aAlfa1 = "ESBSD";
               #web10005#20 = "L";
          |SINO;
               #web10005#20 = "S";
          |FINSI;
          |PINTA #web10005#20;
     |FINSI;
|FPRC;

|PROCESO DSCorreo; |TIPO 0;
     |QUE_SISTEMA aAlfa1;
     |SI aAlfa1 = "ESBSD";|Y #web10005#20 = "S";
          |MENAV "    Configuracion no permitida, servidor UNIX. Recomendado: [F]";
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO SoloFija; |TIPO 7;
     |SI #web10005#20 = "F";
          aAlfa1 = #web10005#19; |QBF aAlfa1;
          |SI aAlfa1 = "Automatico";
               #web10005#19 = "";
               |PINTA #web10005#19;
          |FINSI;
          |C_M #web10005#19, 1, "S";
     |SINO;
          |C_M #web10005#19, 1, "N";
          #web10005#19 = "Automatico";
          |PINTA #web10005#19;
     |FINSI;
|FPRC;

|PROCESO PathLocal; |TIPO 7;
     aAra = #web10005#14;
     |QBF aAra;
     |SI aAra = "";
          |DBASE aPath;
          |QBF aPath;
          aPath = aPath + "correo/";
          #web10005#14 = aPath;
          |PINTA #web10005#14;
     |FINSI;
|FPRC;
