|PROCESO obrasf30;
     impre = 1;
     |PRINT;
|FPRC;

|DEFBUCLE obrasf30;
      #9#1;
      ;
      000,000,000,000,0000;
      999,999,999,999,9999;
      ;
      NULL,obrasf30;
|FIN;



|PROCESO  mayus;  |TIPO 0;
     #0#1 = #0#1 > " ";
     |PINTA #0#1;
|FPRC;

|PROCESO lista;  |TIPO 3;

          |IMPRE 0;
          |SI FSalida ! 0; |ACABA; |FINSI;
          |SI #0#1 ="OB";
             |INFOR "obrasob8";
          |FINSI;

          |SI #0#1 ="BL";
             |INFOR "obrasbl8";
          |FINSI;

          |SI #0#1 ="PL";
             |INFOR "obraspl8";
          |FINSI;

          |SI #0#1 ="PI";
             |INFOR "obraspi8";
          |FINSI;

          |SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;
          |HAZ CreaTempo; |NOME_DAT #9 Tempo; |DELINDEX #9;
          |ABRE #9;
          |ABRE #3;
          |ABRE #4;
          |ABRE #5;
          |ABRE #6;
          |ABRE #1;
          #3#0 = #0#0;
          |LEE 041#3=;
          |CIERRA #3;
          |BUCLE leepre;
          |CIERRA #9;
          |CIERRA #3;
          |CIERRA #4;
          |CIERRA #5;
          |CIERRA #6;
          |CIERRA #1;
          |BUCLE obrasf30;
          |SI impre = 1;
               |PIEINF;
          |FINSI;
          |HAZ BoraTempo; |DELINDEX #9;
          |FININF;
          |FINIMP;
|FPRC;
