|FICHEROS;
    obras031  #0; || Seleccion.
    obras005  #1; || Cab.   Presupuesto.
    obras006  #2; || Lineas Presupuesto.
    obras001  #3; || Obras.
    obras002  #4; || Bloques.
    obras003  #5; || Plantas.
    obras004  #6; || Pisos.
    obrasf31  #9; ||Fantasma.
|FIN;

|VARIABLES;
     &Tempo = "";
     aAlfa = "";
     nObra = 0;
     nSal   = 0;
     impre = 0;
|FIN;

|PROCESO Lee;
     |SALVA_FICHA  #2;
     #2#0 = nObra;
     |LEE 000#2=;
|FPRC;

|PROCESO Repon;
     |REPON_FICHA #2;
|FPRC;

|PROCESO prolin;
     #1#0 = #2#0;
     #1#1 = #2#1;
     #1#2 = #2#2;
     |LEE 041#1=;
     ||SI #1#4 ! #0#2; |ACABA; |FINSI;

     nObra = #1#0;
     aAlfa = #1#5; |QBF aAlfa;
     |SI aAlfa ! "";             ||Es una copia
           aAlfa = aAlfa % 103;
           |SI #1#0 ! #0#0; |Y aAlfa ! #0#0; |ERROR; |ACABA; |FINSI;
           |SI #0#2 = "O";
               nObra = aAlfa;
               |SI nObra = #0#0; |ACABA; |FINSI;
           |FINSI;
     |SINO;
           |SI #1#0 ! #0#0; |ERROR; |ACABA; |FINSI;
           |SI #1#4 ! #0#2; |ERROR; |ACABA; |FINSI;
     |FINSI;

     |SI #0#1 = "OB";                                        ||N.Obra.
          #9#0 = #2#0;
          #9#1 = 0;
          #9#2 = 0;
          #9#3 = 0;
          #9#10 = #2#2;
          #9#11 = #1#3;
          #9#4 = #3#1;
          |LEE 041#9=;
          nSal = FSalida;
          |HAZ Lee;
          #9#8 = #9#8 + #2#7;
          #9#9 = #9#9 + (#2#7 * #2#8);
          |HAZ Repon;
          |SI nSal = 0; |GRABA 020#9a; |FINSI;
          |SI nSal ! 0; |GRABA 020#9n; |FINSI;
     |FINSI;
     |SI #0#1 = "BL";                                        ||N.Bloque.
          |SI #2#4 = "BL"; |O #2#4 = "PL"; |O #2#4 = "PI";
               #9#0 = #2#0;
               #9#1 = #2#9;
               #9#2 = 0;
               #9#3 = 0;
               #9#4 = #3#1;
               #9#10 = #2#2;
               #9#11 = #1#3;

               #4#0 = #2#0;
               #4#1 = #2#9;
               |LEE 041#4=;
               #9#5 = #4#2;

               |LEE 041#9=;
               nSal = FSalida;
               |HAZ Lee;
               #9#8 = #9#8 + #2#7;
               #9#9 = #9#9 + (#2#7 * #2#8);
               |HAZ Repon;
               |SI nSal = 0; |GRABA 020#9a; |FINSI;
               |SI nSal ! 0; |GRABA 020#9n; |FINSI;
          |FINSI;
     |FINSI;
     |SI #0#1 = "PL";                                        ||N.Planta.
          |SI #2#4 = "PL"; |O #2#4 = "PI";
               #9#0 = #2#0;
               #9#1 = #2#9;
               #9#2 = #2#10;
               #9#3 = 0;
               #9#4 = #3#1;
               #9#10 = #2#2;
               #9#11 = #1#3;

               #4#0 = #2#0;
               #4#1 = #2#9;
               |LEE 041#4=;
               #9#5 = #4#2;

               #5#0 = #2#0;
               #5#1 = #2#9;
               #5#2 = #2#10;
               |LEE 041#5=;
               #9#6 = #5#3;


               |LEE 041#9=;
               nSal = FSalida;
               |HAZ Lee;
               #9#8 = #9#8 + #2#7;
               #9#9 = #9#9 + (#2#7 * #2#8);
               |HAZ Repon;
               |SI nSal = 0; |GRABA 020#9a; |FINSI;
               |SI nSal ! 0; |GRABA 020#9n; |FINSI;
          |FINSI;
     |FINSI;
     |SI #0#1 = "PI";                                        ||N.Piso.
          |SI #2#4 = "PI";
               #9#0 = #2#0;
               #9#1 = #2#9;
               #9#2 = #2#10;
               #9#3 = #2#11;
               #9#4 = #3#1;
               #9#10 = #2#2;
               #9#11 = #1#3;

               #4#0 = #2#0;
               #4#1 = #2#9;
               |LEE 041#4=;
               #9#5 = #4#2;

               #5#0 = #2#0;
               #5#1 = #2#9;
               #5#2 = #2#10;
               |LEE 041#5=;
               #9#6 = #5#3;

               #6#0 = #2#0;
               #6#1 = #2#9;
               #6#2 = #2#10;
               #6#3 = #2#11;
               |LEE 041#6=;
               #9#7 = #6#4;

               |LEE 041#9=;
               nSal = FSalida;
               |HAZ Lee;
               #9#8 = #9#8 + #2#7;
               #9#9 = #9#9 + (#2#7 * #2#8);
               |HAZ Repon;
               |SI nSal = 0; |GRABA 020#9a; |FINSI;
               |SI nSal ! 0; |GRABA 020#9n; |FINSI;
          |FINSI;
     |FINSI;
|FPRC;
