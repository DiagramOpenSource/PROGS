|FICHEROS;
     marhu065 #0;
     marhu066 #1;     ||Resultado Tablas Comis/cli
|FIN;

|VARIABLES;
     aPos   = "";
     aCaracter = "";
     aAlfa1 = "";
     aAlfa2 = "";
     aAlfa3 = "";
     aAlfa4 = "";
     nNume  = 0;
     nNume1 = 0;
     nNume2 = 0;
     nNume3 = 0;
     nNume4 = 0;
     nPara1 = 0;
     nPara2 = 0;
     nPara3 = 0;
     nPara4 = 0;
     fFech1 = @;
     fFech2 = @;
     fFech3 = @;
     fFech4 = @;
     nFEstado = 0;
     nSeleccio = 0;
     nModo = 0;

     aFabre = "";
     aField = "";
     aFcanl = "";
     nCampo = 0;
     aCampo = "";
     nCuantos = 0;
|FIN;
____________________________________/
|PROCESO ProcesaCampo;
     nCampo = -1;
     aCampo = "";
     |PARA nNume2=1;|SI nNume2 [ 100;|HACIENDO nNume2 = nNume2 + 1;
          nNume3    = (nNume2 * 100) + 1;
          aPos      = nNume3;
          aCaracter = aField % aPos;
          nNume1 = &aCaracter;
          |SI aCaracter = ";";
              nCampo = nCampo + 1;
              #1#nCampo# = aCampo;
              aCampo = "";
          |SINO;
              aCampo = aCampo + aCaracter;
          |FINSI;
          |SI nCampo ] 12;
               |GRABA 020#1n;
               nCampo = 0;
               aCampo = "";
               |PINTA 1310, #1#0;     ||Cliente
               |PINTA 1410, #1#1;     ||Tabla DTO1
               |PINTA 1420, #1#2;     ||Linea DTO1
               |PINTA 1510, #1#1;     ||Tabla DTO2
               |PINTA 1520, #1#2;     ||Linea DTO2
               |PINTA 1610, #1#1;     ||Tabla DTO3
               |PINTA 1620, #1#2;     ||Linea DTO3
               |PINTA 1710, #1#1;     ||Tabla DTO4
               |PINTA 1720, #1#2;     ||Linea DTO4
               |PINTA 1810, #1#1;     ||Tabla DTO5
               |PINTA 1820, #1#2;     ||Linea DTO5
               |PINTA 1910, #1#1;     ||Tabla DTO6
               |PINTA 1920, #1#2;     ||Linea DTO6
               nCuantos = nCuantos + 1;
               |DDEFECTO #1;
               |SAL;
          |FINSI;
     |SIGUE;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     |DELINDEX #1;
     |ABRE #1;
     aAlfa1 = #0#0; |QBF aAlfa1;
     |XABRE "U",aAlfa1,nNume4;
     |SI FSalida ] 0;
         |XLEE nNume4,250,aField;
         |PARA; |SI aField > ""; |HACIENDO;
               |HAZ ProcesaCampo;
               |XLEE nNume4,250,aField;
         |SIGUE;
     |SINO;
          aAlfa1 = "0000[" + (nNume % 5) + "] inaccesible.";
          |MENAV aAlfa1;
     |FINSI;
     |CIERRA #1;
|FPRC;
