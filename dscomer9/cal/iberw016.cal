|PROCESO Bucle02;
   aAlfa = #iberw012#2; |QBF aAlfa; aAlfa = aAlfa % -102; nTarjeta = aAlfa;
   nTarjeta = nTarjeta + 1;

   #iberw014#0 = #iberw012#2;
   |LEE 000#iberw014.=;
   |SI FSalida = 0; nTarjeta = #iberw014#1; |FINSI;

   #iberw011#0 = #iberw012#0;
   #iberw011#1 = #iberw012#1;
   |LEE 000#iberw011.=;
   |SI FSalida = 0;
      |SI nTarjeta = 1;
         #iberw011#3 = #iberw011#3 + #iberw012#11;
         #iberw011#7 = #iberw011#7 + #iberw012#9;
      |FINSI;
      |SI nTarjeta = 2;
         #iberw011#4 = #iberw011#4 + #iberw012#11;
         #iberw011#8 = #iberw011#8 + #iberw012#9;
      |FINSI;
      #iberw011#5 = #iberw011#5 + #iberw012#11;
      #iberw011#6 = #iberw011#2 - #iberw011#5;

      nCalc = #iberw011#7 - #iberw011#3;
      |SI nCalc ] #iberw011#6;
         nCalc = #iberw011#6;
      |FINSI;

      |SI #iberw011#3 ! 0;
         #iberw011#9  = nCalc;
         #iberw011#10 = #iberw011#6 - #iberw011#9;
         nCalc = #iberw011#3 + #iberw011#9;
         |SI nCalc < 0;
            #iberw011#10 = nCalc;
            #iberw011#9  = 0 - #iberw011#3;
         |FINSI;
      |SINO;
         |SI #iberw011#4 ! 0;
            #iberw011#10  = nCalc;
            #iberw011#9 = #iberw011#6 - #iberw011#10;
            nCalc = #iberw011#4 + #iberw011#10;
            |SI nCalc < 0;
               #iberw011#9 = nCalc;
               #iberw011#10  = 0 - #iberw011#4;
            |FINSI;
         |SINO;
            #iberw011#9  = nCalc;
            #iberw011#10 = #iberw011#6 - #iberw011#9;
            nCalc = #iberw011#3 + #iberw011#9;
            |SI nCalc < 0;
               #iberw011#10 = nCalc;
               #iberw011#9  = 0 - #iberw011#3;
            |FINSI;
         |FINSI;
      |FINSI;
      |GRABA 020#iberw011.a;
   |FINSI;
|FPRC;

