|FICHEROS;
     extm0001 #0;
|FIN;

|VARIABLES;
     &eaArticulo = "";
|FIN;

|PROGRAMA;
     |ABRE #0;
     #0#0 = eaArticulo;
     |LEE 101#0=;
     |SI PARAMETRO = "SINOEXISTE";
          |SI FSalida ! 0;
               |GRABA 020#0b;
               |PINPA #0#0;
               |PINDA #0#0;
               FSalida = "::ENDATOS::SINBOTONES::";
               |ENDATOS #1#0;
               |SI FSalida = 0; |GRABA 020#0a; |FINSI;
          |FINSI;
     |SINO;
          |SI PARAMETRO = "BAJA";
               |SI FSalida = 0; |BORRA 020#0a; |FINSI;
          |SINO;
               |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
               |PINPA #0#0;
               |PINDA #0#0;
               FSalida = "::ENDATOS::SINBOTONES::";
               |ENDATOS #2#0;
               |SI FSalida = 0; |GRABA 020#0a; |FINSI;
          |FINSI;
     |FINSI;
     |CIERRA #0;
|FPRO;
