|FICHEROS;
     gopre014 #0;
|FIN;

|VARIABLES;
     aClave = "";
     aAlfa = "";
|FIN;

|PROCESO ModiClave; |TIPO 0;
     |SI aClave ! "";
          aAlfa = #0#2; |QBF aAlfa;
          |SI aAlfa ! aClave;
               |MENAV "0000ERROR, Clave incorrecta...";
               |ERROR;
          |SINO;
               |CONFI "0000NModificar la Clave";
               |SI FSalida = 0;
                    |MENSA "0000Introduzca la nueva clave";
                    aClave = "";
                    |ERROR;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|PROGRAMA;
     |ABRE #0;
     |LEE 101#0p;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     aClave = #0#2; |QBF aClave;
     #0#2 = "**********";
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          aAlfa = #0#2; |QBF aAlfa;
          |SI aAlfa = ""; |O aAlfa = "**********";
               #0#2 = aClave;
          |FINSI;
          |GRABA 020#0a;
     |FINSI;
     |CIERRA #0;
|FPRO;
