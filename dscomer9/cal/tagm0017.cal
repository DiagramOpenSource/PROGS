|FICHEROS;
     tagm0017 #0;
     agifa010 #10;
     agifa024 #24;
     agifa204 #204;
     referen@;
|FIN;

|VARIABLES;
     &enSwObsCli = 0;
     aAlfa= "";
     &eaCodigo = "";
     nModo = 0;
     nForma = 0;
|FIN;

|PROCESO Tipo17_13; |TIPO 13;
     |ABRE #204;
     |LEE 000#204=;
     |SI FSalida = 0;
          |ATRI I;
          aAlfa = #204#6; |QBF aAlfa; |PINTA 1307, aAlfa;
          aAlfa = #204#7; |QBF aAlfa; |PINTA 1331, aAlfa;
          aAlfa = #204#8; |QBF aAlfa; |PINTA 1354, aAlfa;
          |ATRI 0;
     |FINSI;
     |CIERRA #204;
|FPRC;

|PROCESO Tipo17_20; |TIPO 20;
     |SI FSalida = "OPCION";
          FSalida = "Obs.Cli.";
     |SINO;
          |LEE 000#0=;
          |SI FSalida = 0;
               eaCodigo = #0#4;
               |SUB_EJECUTA_NP "dsz99994";
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo17_2; |TIPO 2;
     nForma = 0 +. 1;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 2; |Y #0#38 = "S";
          |SI nForma = 1;
                |MENAV "0000Documento pasado a Albaran";
          |FINSI;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO Observa; |TIPO 0;
     |SI enSwObsCli = 1;      || Llamado por tagw0019->tagw0018 Pase Doc.
          eaCodigo = #0#4;
          |SUB_EJECUTA_NP "dsz99994";
     |FINSI;
     |SI #0#44 = "  ";
          |ABRE #24;
          #24#0 = #0#4;
          |LEE 000#24=;
          |SI FSalida = 0;
               #0#44 = #24#20; |PINTA #0#44;
          |FINSI;
          |CIERRA #24;
     |FINSI;
|FPRC;

|PROCESO TagBajaAlb;
     |DDEF aAlfa;
     aAlfa = aAlfa + "tagm0017";
     |CARGA_DEF aAlfa, referen@;
     |SI FSalida = 0;
          |ABRE #referen@; |SELECT #3#referen@;
          #referen@#38 = "A";
          #referen@#39 = #10#52;
          #referen@#40 = #10#0;
          |LEE 101#referen@.=;
          |SI FSalida = 0;
               |SELECT #1#referen@;
               |LEE 121#referen@.=;
               #referen@#38 = "N";
               #referen@#39 = "";
               #referen@#40 = 0;
               |GRABA 020#referen@.a;
          |FINSI;
          |CIERRA #referen@;
          |DESCARGA_DEF #referen@;
     |FINSI;
|FPRC;
