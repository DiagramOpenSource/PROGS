|FICHEROS;
     dsm00115  #0;
     dsm00115@ #1;
|FIN;

|VARIABLES;
     aDef      = "";
     aAlfa     = "";
     nModo     = 0;
|FIN;

|PROCESO Chequea; |TIPO 0;
     nModo = (FEntrada / 100) ! 0;
     |ABRE #1;
     |SELECT #2#1;
     #1#5 = #0#5;
     |LEE 000#1=;
     |SI FSalida = 0; |Y nModo = 1;
          aAlfa = "0000Cliente duplicado. Empresa:" + #1#0;
          |MENAV aAlfa;
          |ERROR;
     |FINSI;
     |CIERRA #1;
|FPRC;

|PROCESO Min;
     #0#0 = 1;
|FPRC;

|PROCESO Max;
     #0#0 = 99999;
|FPRC;

|PROGRAMA;
     |DDEF aDef;
     aDef = aDef + "dsm00115";
     |CARGA_DEF aDef, dsm00115@;
     |SI FSalida = 0;
          |CLS;
          |ENTLINEAL #1#0, -1, 2, 22, 1, Min, Max;
     |SINO;
          |DESCARGA_DEF #dsm00115@;
     |FINSI;
|FPRO;
