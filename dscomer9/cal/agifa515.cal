|FICHEROS;
     agifa515 #0;
|FIN;

|VARIABLES;
    &eswCta = 0;
    &eaCta = "";
     modo = 0;
     deci = 0;
     nume = "";
     resto = 0;
     letra = "";
     base  = 16;
|FIN;

|PROCESO memo; |TIPO 0;
     modo = (FEntrada / 100) ! 0;
     |SI modo ! 1; |ACABA; |FINSI;

     deci = #0#0;
     nume = "";
     |PARA; |SI deci ] base; |HACIENDO;
          resto = deci $ base;          || Redondeo
          deci = ((deci-resto) / base); ||
          |HAZ Letras;
          nume = letra + nume;
     |SIGUE;
     resto = deci;
     |HAZ Letras;
     nume = letra + nume;
     #0#6 = nume;
     |PINTA #0#6;
|FPRC;

|PROCESO Letras;
     |SI resto [ 9;
          letra = resto;
     |SINO;
          resto = resto + 55;
          letra = &resto;
     |FINSI;
|FPRC;

|PROCESO Cta515; |TIPO 0;
     eaCta = #0Campo;
     |HAZ MFiltraPtos;
     #0Campo = eaCta; |PINTA #0Campo;

     |HAZ MCtaContable;
     |SI eswCta = 1;
          |MENAV "    Error. Longitud de Cuenta fuera de Nivel";
          |ERROR;
     |FINSI;
|FPRC;
