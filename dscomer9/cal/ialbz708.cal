     AVISOS

|FICHEROS;
     ialbz008 #0;
|FIN;

|VARIABLES;
     &enGrid = 0;
     aProg = "";
     nPanta = 0;
     nEvtFoco = 0;

     nVentana = 0;
 {-1}aVent = "";
     aVent1 = 0;
     aVent2 = 0;
     aVent3 = 0;
     aVent4 = 0;
     aVent5 = "";
|FIN;

|PROCESO EventoFoco7;
     |VENTANA 0501, 3099, -1, 17, "Avisos";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     aProg = "ialbz010;" + #0#0 + (("0000" + #0#1) % -104);
     |SUB_EJECUTA aProg;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |FINVENTANA nVentana;
     |PULSATECLA;

     |FOCOTECLADO enGrid;
|FPRC;

|PROCESO DestruyePan7;
     |FIN_CONTROL_WINDOWS nEvtFoco;
|FPRC;

|PROCESO Descarga7;
|FPRC;

|PROCESO PintaPanta7;
     |PINPA #7#0;  nPanta = FSalida;

     |CONTROL_SIMPLE nPanta, "EVENTOPAN", 0000, 0000, EventoFoco7;
     nEvtFoco = FSalida;
|FPRC;
