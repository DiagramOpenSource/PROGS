|FICHEROS;
     solm0015 #0;
     solm0002 #2;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO solm0002;
     |SI #2#1 = #0#2;
          #0#5 = #2#7;
     |FINSI;
     #0#4 = #0#4 + #2#7;
|FPRC;

|DEFBUCLE solm0002;
     #2#1;
     ;
     #0#1, 0001;
     #0#1, 9999;
     ;
     NULL, solm0002;
|FIN;

|PROCESO CalcuPo; |TIPO 0;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |BUCLE solm0002;
          #0#3 = #0#5 * 100 / #0#4;
          |PINTA #0#3;
          |PINTA #0#4;
          |PINTA #0#5;
     |FINSI;
|FPRC;

|PROCESO CalRepar; |TIPO 0;
     |SI #0Campo ! Contenido;
          #0#3 = #0#5 * 100 / #0#4;
          |PINTA #0#3;
     |FINSI;
|FPRC;
