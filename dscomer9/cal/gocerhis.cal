|PROCESO CalImp; |TIPO 0;
     |SI Contenido ! #2#10;
          |ABRE #3;
          #3#0 = #2#0;                            ||Serie Pres.
          #3#1 = #2#1;                            ||N.Presupuesto
          #3#2 = #2#14;
          #3#3 = #2#22;                            ||Partida.
          |LEE 000#3=;
          |CIERRA #3;

          |SI #2#10 > #2#11;
               |MENAV "0000EL IMPORTE ES SUPERIOR AL PENDIENTE";
          |FINSI;

          #2#7 = (#2#10 * #2#18) / #2#9;
          #2#20 = #3#14 + #2#7;
          |SI aTipCer = "I";
               #2#12 = (#2#10 * 100) / #2#9;
          |FINSI;
          |PINTA #2#12;
          |PINTA #2#10;
          |PINTA #2#20;
          |PINTA #2#7;
     |FINSI;
|FPRC;

|PROCESO CalTan; |TIPO 0;
     |SI Contenido ! #2#12;
          |ABRE #3;
          #3#0 = #2#0;                            ||Serie Pres.
          #3#1 = #2#1;                            ||N.Presupuesto
          #3#2 = #2#14;
          #3#4 = #2#22;                            ||Partida.
          |LEE 000#3=;
          |SI FSalida = 0;
               #2#10 = #2#9 % #2#12;
               |SI #2#10 > #2#11;
                    #2#10 = #2#11;
               |FINSI;
          |FINSI;
          |CIERRA #3;
          |HAZ CalImp;
     |FINSI;
|FPRC;
