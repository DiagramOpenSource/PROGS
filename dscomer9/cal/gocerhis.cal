|PROCESO Parti7; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo > 2; |ACABA; |FINSI;

     |C_M #2#4, 1, "N";

     |ABRE #10;
     #10#0 = #2#0;
     #10#1 = #2#1;
     |LEE 020#10=;
     |CIERRA #10;
