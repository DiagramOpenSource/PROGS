|PROCESO CamModi;
     |SI aTipCer = "M";
          |C_M #2#10 , 1 , "N";
          |C_M #2#12 , 1 , "N";
          |C_M #2#20 , 1 , "S";
     |FINSI;
     |SI aTipCer = "I";
          |C_M #2#10 , 1 , "S";
          |C_M #2#12 , 1 , "N";
          |C_M #2#20 , 1 , "N";
     |FINSI;
     |SI aTipCer = "P";
          |C_M #2#10 , 1 , "N";
          |C_M #2#12 , 1 , "S";
          |C_M #2#20 , 1 , "N";
     |FINSI;
     |C_M #2#7  , 1,  "N";
     |SI aTipCer = "R";
          |C_M #2#10 , 1 , "N";
          |C_M #2#12 , 1 , "N";
          |C_M #2#20 , 1 , "N";
          |C_M #2#7  , 1,  "S";
     |FINSI;
