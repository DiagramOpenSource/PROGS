|FICHEROS;
     referen@;
|FIN;

|VARIABLES;
     &eaArticulo = "";
     &enCanon = 0;
     aAlfa = "";
|FIN;

|PROCESO TallerCanon;
     |DDEF aAlfa;
     aAlfa = aAlfa + "agifa019";
     |CARGA_DEF aAlfa, referen@;
     |SI FSalida = 0;
          |ABRE #referen@;
          #referen@#0 = eaArticulo;
          |LEE 000#referen@.=;
          |SI FSalida = 0
               enCanon = #referen@#168;
          |SINO;
               enCanon = 0;
          |FINSI;
          |CIERRA #referen@;
          |DESCARGA_DEF #referen@;
     |SINO;
          |MENAV "0000Error al cargar 'agifa019'";
     |FINSI;
|FPRC;
