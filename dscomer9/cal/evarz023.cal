|FICHEROS;
     evarz023 #0;
     evarz024 #1,MANTE;
     evarz025 #2,MANTE;
     dsm00010 #10;
     dsm00014 #14;
     dsm00015 #15;
     dsm00016 #16;
     dsm00071 #71;
     dsm00072 #72;
     dsm00073 #73;
     agifa321 #321;
