      Los procesos marcados con (*) son los necesarios para
      simular un cabeceras/lineas

|FICHEROS;
     dsm00303 #0;
     dsm00304 #1,MANTE;

     dsw00096 #64;       | Tmp Usuarios
|FIN;

|VARIABLES;
     aAlfa = "";
     nGrid1 = -1;
     nRango = 0;
     nPos1 = 0;
     nPos2 = 0;
     nModo = 0;
     nHay = 0;

     {1}aLocal = "";
     aIP = "";
     nHandle = 0;
|FIN;

|PROCESO Evento;
|FPRC;

|PROCESO Min;
     #1#0 = #0#0;
     #1#1 = "  ";
|FPRC;

|PROCESO Max;
     #1#0 = #0#0;
     #1#1 = "zz";
|FPRC;

|PROCESO Tipo14; |TIPO 14;    ||(*)
     |SI nGrid1 = -1;
          nRango = 1 + 4 + 8 + 16 + 32;
          nPos1 = 1307;
          nPos2 = 2489;
          |LINEAL_SIMPLE #1#1, nRango, nPos1, nPos2, Min, Max, Evento;
          nGrid1 = FSalida;
     |SINO;
          |REFRESCACONTROL nGrid1;
     |FINSI;
|FPRC;

|PROCESO Tipo8; |TIPO 8; ||(*)
     |ABRET #1;

     |HAZ CargaUsu;
|FPRC;

|PROCESO Tipo9; |TIPO 9; ||(*)
     |CIERRAT #1;

     |DELINDEX #64;
|FPRC;

|PROCESO BorraLin;
     |BORRA 020#1a;
     |LIBERA #1;
|FPRC;

|DEFBUCLE BorraLin;
     #1#1;
     ;
     #0#0, "  ";
     #0#0, "zz";
     be;
     NULL, BorraLin;
|FIN;

|PROCESO Tipo2; |TIPO 2; ||(*)
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |BUCLE BorraLin;
     |FINSI;
|FPRC;

|PROCESO Tipo0; |TIPO 0;     ||(*)
     |REFRESCACONTROL nGrid1;
|FPRC

|PROCESO Tipo12; |TIPO 12;    ||(*)
     || No quitar
|FPRC;

|PROCESO Tipo30; |TIPO 30;    ||(*)
     nModo = (FEntrada / 100) ! 0;
     |ABRE #1;
     #1#0 = #0#0;
     #1#1 = "";
     |LEE 000#1];
     |SI FSalida ! 0; |O #1#0 ! #0#0;
          |SI nModo = 1;
               |MENAV "0000Se anulan los datos entrados.";
          |SINO;
               |MENAV "0000No existen lineas, se borra la cabecera.";
          |FINSI;
          |BORRA 020#0a;
          |ERROR;
     |FINSI;
     |CIERRA #1;
|FPRC;
---------------------------------------------------
|PROCESO CargaUsu;
     aAlfa = Usuario;
     |NOME_DAT #64 aAlfa;
     |DELINDEX #64;
     |ABRE #64;

     |IP_REMOTO aIP;
     |SI aIP = "";
          |ESPECIFICA "RBASS", aLocal;
          aAlfa = aLocal;
     |SINO;
          |DBASS aAlfa;
     |FINSI;

     aAlfa = aAlfa + "dev/usr/ds_sys.psw";
     |XABRE "", aAlfa, nHandle;
     |SI FSalida = 0;
          |XLEE nHandle, 200, aAlfa;
          |PARA; |SI FSalida ] 0; |HACIENDO;
               #64#0 = aAlfa % 410;
               |GRABA 000#64n;
               |XLEE nHandle, 200, aAlfa;
          |SIGUE;
          |XCIERRA nHandle;
     |FINSI;

     |CIERRA #64;
|FPRC;

|PROCESO ConsultaUsu; |TIPO 0;
     |SI FSalida = 13;
          |ABRE #64;
          #64#0 = #0#0;
          |LEE 000#64];
          |CONSULTA_CLAVE #1#64;
          |SI FSalida = 0;
               #0#0 = #64#0; |PINTA #0#0;
          |SINO;
               |ERROR;
          |FINSI;
          |CIERRA #64;
     |SINO;
          |ABRE #64;
          #64#0 = #0#0;
          |LEE 000#64=;
          |SI FSalida ! 0;
               |MENAV "0000No existe usuario...";
               |ERROR;
          |FINSI;
          |CIERRA #64;
     |FINSI;
|FPRC;
