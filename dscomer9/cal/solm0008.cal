|FICHEROS;
     solm0008 #0;
|FIN;

|VARIABLES;
     nClave = 1;
     nTecla = 0;
     &eaSerie = "";
     &enNumFac = 0;
|FIN;

|PROCESO Tecla; |TIPO 11;
     |SI nTecla ! 0; |ACABA; |FINSI;
     |SI FSalida = 10; |O FSalida = 11; ||F2/F3
          nTecla = FSalida;
          |SI FSalida = 10;
               nClave = nClave - 1;
               |SI nClave < 1; nClave = 3; |FINSI;
          |SINO;
               nClave = nClave + 1;
               |SI nClave > 3; nClave = 1; |FINSI;
          |FINSI;
          |PTEC 807;
     |FINSI;
     |SI FSalida = 13;   || F5
          eaSerie = #0#0;
          enNumFac = #0#1;
          |SUB_EJECUTA_NP "solz0005";
          |PINTA #0#5;
          |ERROR;
     |FINSI;
|FPRC

|PROCESO Min;
     #0#0 = "     ";
     #0#1 = 000000;
     #0#2 = "      ";
     #0#3 = "01.01.0000";
|FPRC;

|PROCESO Max;
     #0#0 = "zzzzz";
     #0#1 = 999999;
     #0#2 = "zzzzzz";
     #0#3 = "31.12.9999";
|FPRC;

|PROGRAMA;
     |CLS;
     |PARA nTecla = 1; |SI nTecla ! 0; |HACIENDO;
          nTecla = 0;
          |PINPA #0#0;
          |SI nClave = 1;
               |ATRI R;
               |PINTA 713, "Serie³Numero";
               |PINTA 539, "Clave:Cliente/Serie       ";
               |ATRI 0;
               |ENTLINEAL #1#0, 1, 4, 22, 0, Min, Max;
          |SINO;
               |SI nClave = 2;
                    |ATRI R;
                    |PINTA 713, "Serie³Numero³Client";
                    |PINTA 539, "Clave:Cliente/Serie/Numero";
                    |ATRI 0;
                    |ENTLINEAL #2#0, 1, 4, 22, 0, Min, Max;
               |SINO;
                    |ATRI R;
                    |PINTA 726, "Client³ F.Factura";
                    |PINTA 539, "Clave:Cliente/F.Factura   ";
                    |ATRI 0;
                    |ENTLINEAL #3#0, 1, 4, 22, 0, Min, Max;
               |FINSI;
          |FINSI;
     |SIGUE;
|FPRO;
