|FICHEROS;
     con00007 #6; || Observa
     con00650 #1; || Vehiculo
     con00003 #3;
     agifa142 #142;
     con00007@;
     agif0041;
|FIN;
------------------------------------------------------------------------------
|VARIABLES;
     nHay = 0;
     aAlfa1 = "";
     aAlfa2 = "";
     nModo = 0;
     aDirectorio    = "";
     aNFichero      = "";
     aVariableCarga = "";
     aFichero       = "";
     aAlfa          = "";

     nVariable      = 0;
     nBaffer        = 0;
     nTotalLinea    = 0;
     nTope          = 0;
     nVari1         = 0;
     nVari2         = 0;

     cDesc         = "";
     eaTipoCodigo  = "";
     eaNombreList  = "";

     enCuadroIni   = 0;
     enCuadroFin   = 0;
     enCampoAncho  = 0;
     enModoEdita   = 0;
     enTotalVaria  = 0;

{100}epPuntEdita   = "";

     cMensa         = "";
     cNumero        = "";

     &eaMatricula = "";
|FIN;
------------------------------------------------------------------------------
|PROCESO GrabaSecuencial;
     IepPuntEdita  = epPuntEdita1 <-;
     |PARA nTotalLinea = 0;  |SI nTotalLinea [ enTotalVaria;  |HACIENDO nTotalLinea = nTotalLinea + 1;
          |AL_BUF nBaffer, nTotalLinea, epPuntEdita;
          IepPuntEdita = IepPuntEdita + 1;
     |SIGUE;
|FPRC;
------------------------------------------------------------------------------
|PROCESO Edicion;
     nTope  = nTotalLinea - 1;
     nVari1 = 0;
     nVari2 = 0;
     enModoEdita = enModoEdita + 100;
     |EDITA nBaffer, 0, nTope, enCuadroIni, enCuadroFin, enModoEdita, eaNombreList, enCampoAncho, nVari1, nVari2;
     enModoEdita = enModoEdita $ 100;
|FPRC;
------------------------------------------------------------------------------
|PROCESO CogePuntero;
     IepPuntEdita  = epPuntEdita1 <-;
     |PARA nTotalLinea = 0;  |SI nTotalLinea [ enTotalVaria;  |HACIENDO nTotalLinea = nTotalLinea + 1;
          |DEL_BUF nBaffer, nTotalLinea, epPuntEdita;
          IepPuntEdita = IepPuntEdita + 1;
     |SIGUE;
|FPRC;
------------------------------------------------------------------------------
|PROCESO Edita;
     |DIMENSIONA  200, nBaffer, 0;

     |HAZ GrabaSecuencial;
     |HAZ Edicion;
     |HAZ CogePuntero;

     |FINDIM nBaffer;
|FPRC;
------------------------------------------------------------------------------
|PROGRAMA;
     enTotalVaria  = 100;
     eaNombreList  = "";
     enCuadroIni   = 0711;
     enCuadroFin   = 2070;
     enCampoAncho  = 60;
     enModoEdita   = 0;

     |ATRI R; |PINTA 0611, "  Observaciones  "; |ATRI 0;

     |ABRE #6;

     ||---------------Bloqueo
     |ABRE #1;
     #1#0 = eaMatricula;
     |LEE 101#1=;
     ||---------------

     |PARA nVariable = 1;  |SI nVariable [ 100;  |HACIENDO nVariable = nVariable + 1;
           IepPuntEdita  = epPuntEdita1 <-;
           IepPuntEdita = IepPuntEdita + (nVariable - 1);

           #6#0 = eaMatricula;
           #6#1 = nVariable;
           |LEE 101#6=;
           |SI FSalida ! 0;
               |SI nVariable = 1;
                    epPuntEdita  = ".";
               |SINO;
                    epPuntEdita  = "";
               |FINSI;
           |SINO;
               epPuntEdita  = #6#2;
           |FINSI;
           |LIBERA #6;
     |SIGUE;
     |ATRI 0;

     |HAZ Edita;

     nHay = 0;
     |PARA nVariable = 1;  |SI nVariable [ 100;  |HACIENDO nVariable = nVariable + 1;
           IepPuntEdita = epPuntEdita1 <-;
           IepPuntEdita = IepPuntEdita + (nVariable - 1);

           |DDEFECTO #6;
           #6#0 = eaMatricula;
           #6#1 = nVariable;
           |LEE 101#6=;
           |SI FSalida ! 0;  |GRABA 020#6b;  |FINSI;

           aAlfa1 = #6#2; |QBT aAlfa1;
           aAlfa2 = epPuntEdita; |QBT aAlfa2;

           |SI aAlfa1 ! aAlfa2;
               #6#2 = epPuntEdita;
               |GRABA 020#6a;
           |FINSI;
           aAlfa = #6#2; |QBF aAlfa;
           |SI aAlfa ! ""; nHay = 1; |FINSI;

           |LIBERA #6;
     |SIGUE;

     |SI nHay = 0;
          #6#0 = eaMatricula;
          #6#1 = 1;
          |LEE 101#6];
          |PARA; |SI FSalida = 0; |Y #6#0 = eaMatricula; |HACIENDO;
               |BORRA 020#6a;
               |LIBERA #6;
               |LEE 101#6s;
          |SIGUE;
     |FINSI;

     ||---------------Desbloqueo
     |LIBERA #1;
     |CIERRA #1;
     ||---------------
     |CIERRA #6;
|FPRO;
------------------------------------------------------------------------------

|PROCESO ControlEmp;
     |SI nModo = 1; |Y #con00003#16 = "N"; |ACABA; |FINSI;
     |SI nModo = 2;
         |SI #con00003#17 = "N"; |Y #con00003#18 = "N"; |ACABA; |FINSI;
     |FINSI;

     |DFICB aAlfa; |QBF aAlfa; |PATH_DAT #agif0041#aAlfa#;
     |ABRE #agif0041; |SALVA_FICHA #agif0041;
     #agif0041#0 = #con00003#0;
     |LEE 000#agif0041.=;
     |SI FSalida = 0;
          |DBASE aAlfa; |QBF aAlfa;
          aAlfa = aAlfa + "def/con00007.mas";
          |CARGA_DEF aAlfa, con00007@;
          |SI FSalida = 0;
               |DFICB aAlfa; |QBF aAlfa;
               aAlfa = aAlfa + (("00000" + #con00003#0) % -105) + "/";
               |PATH_DAT #con00007@#aAlfa#;
               |DDEFECTO #con00007@;
               |ABRE #con00007@;
               #con00007@#0 = #con00007#0;
               #con00007@#1 = #con00007#1;
               |LEE 101#con00007@.=;
               |SI nModo = 3;
                    |SI FSalida = 0;
                         |BORRA 020#con00007@.a; |LIBERA #con00007@;
                    |FINSI;
               |SINO;
                    |SI FSalida ! 0;
                         |SI #con00003#17 = "S";
                              #con00007@#0 = #con00007#0;
                              #con00007@#1 = #con00007#1;
                              #con00007@#2 = #con00007#2;
                              |GRABA 020#con00007@.n;
                         |FINSI;
                    |SINO;
                         |SI #con00003#18 = "S";
                              #con00007@#2 = #con00007#2;
                              |GRABA 020#con00007@.a;
                         |FINSI;
                    |FINSI;
               |FINSI;
               |CIERRA #con00007@; |DESCARGA_DEF #con00007@;
          |FINSI;
     |FINSI;
     |REPON_FICHA #agif0041; |CIERRA #agif0041;
|FPRC;

|DEFBUCLE ControlEmp;
     #con00003#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, ControlEmp;
|FIN;

|PROCESO ObseMatri17; |TIPO 17;
     nModo = 3;
     |ABRE #agifa142; |LEE 001#agifa142.p; |CIERRA #agifa142;

     |MENSA "    Actualizando vehiculos ... ";
     |SI #agifa142#28  = "S"; |BUCLE ControlEmp; |FINSI;
     |BLIN 4;
|FPRC;

|PROCESO ObseMatri16; |TIPO 16;
     nModo = 1;
     |ABRE #agifa142; |LEE 001#agifa142.p; |CIERRA #agifa142;

     |MENSA "    Actualizando vehiculos ... ";
     |SI #agifa142#28  = "S"; |BUCLE ControlEmp; |FINSI;
     |BLIN 4;
|FPRC;
