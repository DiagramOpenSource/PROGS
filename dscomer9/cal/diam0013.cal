|FICHEROS;
     diam0012 #12;
     diam0013 #13;
|FIN;

|VARIABLES;
     nForma = 0;
|FIN;

|PROCESO Tipo2;  |TIPO 2;
     nForma = 0 +. 1;

     |SI #13#5 = "CALCULO";  |Y nForma ! 1;
         |MENAV "      Linea Restringida por el Calculo de Comisiones.";
         |ERROR;
         |ACABA;
     |FINSI;

     |SI #13#5 = "CALCULO";  |ACABA;  |FINSI;

     |SI #13#6 = "S";  |Y nForma ! 1;
         |MENAV "      Linea Restringida esta Contabilizada.";
         |ERROR;
         |ACABA;
     |FINSI;

     |SI #13#6 = "S";  |ACABA;  |FINSI;

     |SI #13#3 = "D";
         #12#1 = #12#1 +. #13#4;
     |SINO;
         #12#2 = #12#2 +. #13#4;
     |FINSI;

     #12#3 = #12#1 - #12#2;

     |PINTA #12#1;
     |PINTA #12#2;
     |PINTA #12#3;
|FPRC;

|PROCESO Tipo5;  |TIPO 5;
     FSalida = "SINBARRA";
|FPRC;
