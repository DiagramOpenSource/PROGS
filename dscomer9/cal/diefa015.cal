|FICHEROS;
     diefa015 #0;
     diefa013 #13;
     agifa112 #112;
|FIN;

|VARIABLES;
     aDia      = "";
     aAlfa     = "";
     nDia      = 0;
     nInc      = 0;
     f         = @;
     fIni      = @;
     fFecha    = @;
     f0        = @;
|FIN;

|PROCESO agifa112;
     nDia = #112#63;
     nInc = #112#64;
     |SI nDia = 0; |O nInc = 0; |O nDia > 7;
          |ACABA;
     |FINSI;
     |SI nDia = 1; aDia = "Lu"; |FINSI;
     |SI nDia = 2; aDia = "Ma"; |FINSI;
     |SI nDia = 3; aDia = "Mi"; |FINSI;
     |SI nDia = 4; aDia = "Ju"; |FINSI;
     |SI nDia = 5; aDia = "Vi"; |FINSI;
     |SI nDia = 6; aDia = "Sa"; |FINSI;
     |SI nDia = 7; aDia = "Do"; |FINSI;

     |PARA fFecha = #0#0; |SI fFecha [ #0#1; |HACIENDO fFecha = fFecha + 1;
          fIni = fFecha;
          f0 = fFecha % 1;
          aAlfa = f0 % 1102;
          |SI aAlfa = aDia; |SAL; |FINSI;
     |SIGUE;

     |PARA fFecha = fIni; |SI fFecha [ #0#1; |HACIENDO fFecha = fFecha + nInc;
          |PARA f = fFecha; |SI f [ #0#1; |HACIENDO f = f + 1;
               f0 = f % 1;
               aAlfa = f0 % 1102;
               |SI aAlfa = aDia; |SAL; |FINSI;
          |SIGUE;
          |DDEFECTO #13;
          #13#0 = f;
          #13#1 = #112#0;
          |GRABA 000#13n;
     |SIGUE;
|FPRC;

|DEFBUCLE agifa112;
     #112#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, agifa112;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |ABRE #13;
     |BUCLE agifa112;
     |CIERRA #13;
|FPRC;
