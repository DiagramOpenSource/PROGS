|FICHEROS;
     dsz00113 #0;
     agifa104 #104;
     agifa073 #73;
     agifa010 #10;
     agifa071 #71;
|FIN;

|VARIABLES;
     &enImp = 0;
     nServi = 0;
     nImpre = -1;
     nInfor = -1;
     nPrime = 0;
     nImp = 0;
|FIN;

|PROCESO agifa071;
     nServi = nServi + #71#5;
     |SI nImp = 1;
          enImp = 2; |PRINT;
     |FINSI;
|FPRC;

|DEFBUCLE agifa071;
     #71#4;
     ;
     #73#28, #73#0, #73#1;
     #73#28, #73#0, #73#1;
     ;
     NULL, agifa071;
|FIN;

|PROCESO agifa073;
     |SI #73#43 ! 0;
          nServi = 0;
          |BUCLE agifa071;
          |SI #73#43 ! nServi;
               |SI nPrime = 0;
                    |IMPRE 0;
                    nImpre = FSalida;
                    |SI FSalida = 0;
                         |INFOR "dsz00113";
                         nInfor = FSalida;
                         |SI FSalida ! 0;
                              |MENAV "0000Error en informe 'dsz00113'";
                         |FINSI;
                    |FINSI;
                    nPrime = 1;
               |FINSI;
               |SI nInfor = 0;
                    nImp = 1;
                    enImp = 1; |PRINT;
                    |BUCLE agifa071;
                    nImp = 0;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO agifa104;
     |SI #104#52 = 0;
          |ERROR;
     |FINSI;
|FPRC;

|DEFBUCLE agifa104;
     #104#7;
     ;
     #0#2, #0#0, 0000, #0#4, #0#6;
     #0#3, #0#1, 9999, #0#5, #0#7;
     ;
     NULL, agifa104, agifa073;
|FIN;


|PROCESO Tipo3; |TIPO 3;
     |BUCLE agifa104;
     |SI nInfor = 0; |PIEINF; |FININF; |FINSI;
     |SI nImpre = 0; |FINIMP; |FINSI;
|FPRC;
