|FICHEROS;
     coima046 #0;
|FIN;

|VARIABLES;
     aAlfa = "";
     aCod = "";
     i = 0;
|FIN;

|PROCESO Parri; |TIPO 0;
     |SI #0#1 = "S";
          #0#2 = 0;
          #0#3 = 999;
          |C_M #0#2, 1, "N"; |PINTA #0#2;
          |C_M #0#3, 1, "N"; |PINTA #0#3;
          |PARA i = 4; |SI i [ 13; |HACIENDO i = i + 1;
               |C_M #0i, 1, "S"; |PINTA #0i;
          |SIGUE;
     |SINO;
          |C_M #0#2, 1, "S"; |PINTA #0#2;
          |C_M #0#3, 1, "S"; |PINTA #0#3;
          |PARA i = 4; |SI i [ 13; |HACIENDO i = i + 1;
               #0i = "";
               |C_M #0i, 1, "N"; |PINTA #0i;
          |SIGUE;
     |FINSI;
|FPRC;

|PROGRAMA;
     aCod = PARAMETRO % 110;
     aAlfa = PARAMETRO % 1104;
     |ABRE #0;
     #0#0 = aCod;
     |LEE 101#0=;
     |SI aAlfa = "ALTA";
          |SI FSalida ! 0;
               |GRABA 020#0b;
               |PINPA #0#0;
               |PINDA #0#0;
               |ENDATOS #1#0;
               |SI FSalida = 0; |GRABA 020#0a; |FINSI;
          |FINSI;
     |FINSI;
     |SI aAlfa = "MODI";
          |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
          |PINPA #0#0;
          |PINDA #0#0;
          |ENDATOS #1#0;
          |SI FSalida = 0; |GRABA 020#0a; |FINSI;
     |FINSI;
     |SI aAlfa = "BAJA";
          |SI FSalida = 0; |BORRA 020#0a; |FINSI;
     |FINSI;
     |CIERRA #0;
|FPRO;
