|PROCESO CheqPCM_PVP70; |TIPO 0;
     eaArticulo = #0#2;
     enPrecio = #0#6;
     enDto1 = #0#8;
     enDto2 = #0#23;
     |HAZ CheqPCM_PVP;

     |SI #279#3 = "S";
          eaArticulo = #0#2;
          enPrecio = #0#6;
          enDto1 = #0#8;
          enDto2 = #0#23;
          nNume =  (enPrecio < enDto1) < enDto2;
          |SI nNume ! nPreDtoAnt;
               |SUB_EJECUTA_NP "dsz00100";
               |SI enError ! 0; |ERROR; |ACABA; |FINSI;
               nPreDtoAnt = nNume;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO GuerFacArti;
     |DDEF aAlfa;
     aAlfa = aAlfa + "guerm058";
     |CARGA_DEF aAlfa, referen@;
     |SI FSalida ! 0;
          |MENAV "0000Error al cargar 'guerm058'";
          |ACABA;
     |FINSI;
     |ABRE #referen@;
     #referen@#0 = #0#2;
     |LEE 000#referen@.=;
     |SI FSalida ! 0;
          |AVISO;
          |CONFI "0000SNo existe tipo facturacion. Darla de alta?";
          |SI FSalida = 0;
               |PUSHV 0501 2380;
               |PINPA #0#referen@;
               |PINDA #0#referen@;
               |ATRI I; |PINTA 1217, #0#4; |ATRI 0;
               |ENDATOS #1#referen@;
               |SI FSalida = 0;
                    |GRABA 020#referen@.n;
               |FINSI;
               |POPV;
          |FINSI;
     |FINSI;
     |CIERRA #referen@;
     |DESCARGA_DEF #referen@;
|FPRC;

|PROCESO poncontr70;
     fiac = Control % A109;
     fiac = fiac + relleno;
     guarda = fiac % A109;
     fiac = Control % A1001;
     Control = Control + fiac;
     Control = Control + relleno;
     Control = Control % A120;
     Control = Control + aponer;
|FPRC;

|PROCESO T070_6_0; |TIPO 0;
