|PROCESO TallerCreaLin70;
     |DDEF aAlfa; aAlfa = aAlfa + "agifa070";
     |CARGA_DEF aAlfa, referen@;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |ABRE #referen@;
     |SI #0#38 > 0;
          nLin = #0#38;
          #referen@#22 = #0#22;
          #referen@#0 = #0#0;
          #referen@#1 = nLin;
          |LEE 121#referen@.=;
     |SINO;
          #referen@#22 = #0#22;
          #referen@#0 = #0#0;
          #referen@#1 = 999;
          |LEE 000#referen@.];
          |SI FSalida = 0;
               |LEE 000#referen@.a;
          |SINO;
               |LEE 000#referen@.u;
          |FINSI;
          |SI FSalida = 0; |Y #referen@#22 = #0#22;  |Y #referen@#0 = #0#0;
               nLin = #referen@#1 + 1;
          |SINO;
               nLin = 1;
          |FINSI;
          |SI nLin [ #0#1; nLin = #0#1 + 1; |FINSI;
          |DDEFECTO #referen@;
          #referen@#22 = #0#22;
          #referen@#0 = #0#0;
          #referen@#1 = nLin;
          |GRABA 020#referen@.b;
     |FINSI;
     aAlfa = "CANON " + #0#4;
     #referen@#2 = "xxxxx";
     #referen@#3 = 1;
     #referen@#4 = aAlfa;
     #referen@#5 = #0#5;
     #referen@#6 = enCanon;
     #referen@#11 = #0#11;
     #referen@#13 = #0#13;
     #referen@#15 = #0#15;
     #referen@#24 = enCanon;
     #referen@#38 = -1;
     |SALVA_DATOS #0;

     |SALVA_DATOS #referen@;
     |REPON_DATOS #0;

     enForma = 1;
     |HAZ AcumulaDE;
     |HAZ TotalLin70;
     |HAZ AcuDeposito;

     |SALVA_DATOS #0;
     |REPON_DATOS #referen@;
     |GRABA 020#referen@.a;

     |REPON_DATOS #0;

     #0#38 = nLin;       || Linea Promocion

     |CIERRA #referen@;
     |DESCARGA_DEF #referen@;
|FPRC;

|PROCESO TallerAcuLin70;
     |DDEF aAlfa; aAlfa = aAlfa + "agifa070";
     |CARGA_DEF aAlfa, referen@;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |ABRE #referen@;
     #referen@#22 = #0#22;
     #referen@#0 = #0#0;
     #referen@#1 = #0#38;     || Usa linea promocion
     |LEE 101#referen@.=;
     |SI FSalida = 0;
          |SALVA_DATOS #0;
          |SALVA_DATOS #referen@;
          |REPON_DATOS #0;
          #0#1 = #referen@#1;
          enForma = -1;
          |HAZ AcumulaDE;
          |HAZ TotalLin70;
          |HAZ AcuDeposito;
          |REPON_DATOS #0;
     |FINSI;
     |CIERRA #referen@;

     |DESCARGA_DEF #referen@;
|FPRC;

|PROCESO TallerBorLin70;
     |DDEF aAlfa; aAlfa = aAlfa + "agifa070";
     |CARGA_DEF aAlfa, referen@;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |ABRE #referen@;
     #referen@#22 = #0#22;
     #referen@#0 = #0#0;
     #referen@#1 = #0#38;     || Usa linea promocion
     |LEE 101#referen@.=;
     nTallerBorLin = FSalida;
     |SI FSalida = 0;
          |BORRA 020#referen@.a;
     |FINSI;
     |CIERRA #referen@;

     |DESCARGA_DEF #referen@;
|FPRC;

|PROCESO TallerCanon70;
     eaArticulo = #0#2; |QBF eaArticulo;
     |HAZ TallerCanon;
     |SI nForma = 1;
          |SI enCanon ! 0;
               aponer = Asino; |HAZ poncontr70;
               |HAZ TallerCreaLin70;
               aponer = Anosi; |HAZ poncontr70;
               |PTEC 809; |PTEC 816; |PTEC 806;
          |SINO;
               |SI #0#38 > 0;      || ha cambiado a un articulo sin canon
                    |HAZ TallerBorLin70;
                    #0#38 = 0;
               |FINSI;
          |FINSI;
     |SINO;
          |SI modo = 3; |Y enModo036 ! 3;
               |HAZ TallerAcuLin70;
               |HAZ TallerBorLin70;
               |SI nTallerBorLin = 0;
                    |PONCONTROL 23, "SI";
                    |PTEC 809; |PTEC 808;
               |FINSI;
          |SINO;
               |SI modo = 2;
                    |HAZ TallerAcuLin70;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO AcuDeposito;
     |HAZ CalCabAgifa036;
