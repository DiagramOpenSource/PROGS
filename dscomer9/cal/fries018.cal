|FICHEROS;
     fries018 #0;
     fries019 #1;
     fries020 #20;
     fries025 #25;
|FIN;

|VARIABLES;
     &enCodigo = 0;
     nNume = 0;
     nModo = 0;
     nForma = 0;
|FIN;

|PROCESO Act19;
     #1#2 = #0#1;
     #1#3 = #0#2;
     #1#4 = #0#3;
     #1#5 = #0#4;
     #1#6 = #0#5;
     |GRABA 020#1a;
     |LIBERA #1;
|FPRC;

|PROCESO Tipo18_3; |TIPO 3;
     |BUCLELIN 0 Act19 #0;
     |CONFI "0000NImprimir propuesta de pedido";
     |SI FSalida = 0;
          |GRABA 020#0a;
          |LIBERA #0;
          |HAZ Tipo4;
          |LEE 121#0a;
     |FINSI;
|FPRC;

|PROCESO Precio; |TIPO 0;
|FPRC;

|PROCESO Nume7; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |LEE 000#0u;
          |SI FSalida = 0;
               nNume = #0#0 + 1;
          |SINO;
               nNume = 1;
          |FINSI;
          |DDEFECTO #0;
          #0#0 = nNume;
     |FINSI;
|FPRC;

|PROCESO Tipo4; |TIPO 4;
     enCodigo = #0#0;
     |SUB_EJECUTA_NP "friez001";
|FPRC;

|PROCESO Tipo19_2; |TIPO 2;
     nForma = 0 +. 1;
     |SI #1#12 = "S";
          |SI nForma = -1;
               |MENAV "0000No se puede modificar, esta servida...";
          |FINSI;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO CamModi; |TIPO 0;
     nModo = (FEntrada / 100) ! 0;
     |C_M #1#8, 1, "N";
     |C_M #1#9, 1, "N";
     |C_M #1#11, 1, "N";
     |SI nModo = 2;
          |C_M #1#8, 1, "S";
          |C_M #1#9, 1, "S";
          |C_M #1#11, 1, "S";
     |FINSI;
|FPRC;

|PROCESO Articulo; |TIPO 0;
     |SI #1Campo ! Contenido;
          |ABRE #20;
          #20#0 = #0#2;
          |LEE 000#20=;
          |CIERRA #20;
          |ABRE #25;
          #25#0 = #1#7;
          |LEE 000#25=;
          |CIERRA #25;
          |SI #20#2 = 1; #1#9 = #25#2; |FINSI;
          |SI #20#2 = 2; #1#9 = #25#5; |FINSI;
          |SI #20#2 = 3; #1#9 = #25#6; |FINSI;
          |SI #20#2 = 4; #1#9 = #25#7; |FINSI;
          |PINTA #1#9;
     |FINSI;
|FPRC;
