|FICHEROS;
     malpe008 #0;   || Este
     malpe004 #1;   || Orden Carga (C)
     malpe005 #2;   ||             (L)
|FIN;

|VARIABLES;
     &eaSerie = "";
     &enOrden = "";
     aAlfa = "";
     informe = "malpe008";
|FIN;

|PROCESO Lineas;
     |PRINT;
|FPRC;

|DEFBUCLE BCarga;
     #1#2;
     ;
     #0#0, "N" , "      ";
     #0#0, "N" , "zzzzzz";
     ;
     NULL, NULL, Lineas;
|FIN;


|PROCESO inicio; |TIPO 3;
     |IMPRE #0;
     |SI FSalida = 0;
          |INFOR informe;
          |SI FSalida = 0;
               |SI eaSerie = "";
                    |BUCLE BCarga;
               |SINO;
                    |ABRE #1;
                    #1#0 = eaSerie;
                    #1#1 = enOrden;
                    |LEE 020#1=;
                    |SI FSalida = 0;
                         |BUCLELIN 2Lineas#1;
                    |FINSI;
                    |CIERRA #1;
               |FINSI;
               |PIEINF;
               |FININF;
          |SINO;
               aAlfa = "0000No existe Informe [" + informe + "]";
               |MENAV aAlfa;
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;

|PROGRAMA;
     |SI eaSerie = "";
          |MANTE #0;
     |SINO;
          |HAZ inicio;
     |FINSI;
|FPRO;
