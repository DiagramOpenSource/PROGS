|PROCESO tipo2w018; |TIPO 2;
     aCliente = #platm013#2;

     nModo = (FEntrada/100) ! 0;

     a = a +.1;

     |PUSHV 0501 2380;
     |ABRET #1;
     |PINPA #0#platm018;

     |SI nModo = 1;
          |DDEFECTO #1;
          #1#0 = #0#0;
          #1#1 = #0#1;
          |LEE 101#1=;
          |SI FSalida ! 0;    |GRABA 020#1b; |FINSI;
          |PINDA #0#1;
          |HAZ Desinst;
          |ENDATOS #1#platm018;
          |SI FSalida = 0;
               #0#0 = #1#0;  ||contrato
               #0#1 = #1#1;  ||instalacion
               #0#2 = #1#2;  ||instalador
               #0#3 = #1#3;  ||nombre instalador
               #0#4 = #1#4;  ||fecha insta
               #0#5 = #1#158; ||n§ direccion
               #0#6 = #1#159; ||direccion
               #0#7 = #1#150; ||plataforma
               |GRABA 020#1a;

               #3#2 = aCliente;
               |DDEFECTO #platm026;
               |ABRE #platm026;
               #platm026#0 = #3#2;
               |LEE 000#platm026.=;
               |CIERRA #platm026;
               #3#3 = #platm026#2;
          |SINO;
               |BORRA 020#1a;
          |FINSI;
          |LIBERA #1;
          a = 0;
      |FINSI;

      |SI nModo = 2;|Y a = -1;
           #1#0 = #0#0;
           #1#1 = #0#1;
           |LEE 101#1=;
           |SI FSalida = 0;
                |HAZ Desinst;
                |PINDA #0#1;
                |ENDATOS #2#platm018;
                |SI FSalida = 0;
                     #0#0 = #1#0;  ||contrato
                     #0#1 = #1#1;  ||instalacion
                     #0#2 = #1#2;  ||instalador
                     #0#3 = #1#3;  ||nombre instalador
                     #0#4 = #1#4;  ||fecha insta
                     #0#5 = #1#158; ||n§ direccion
                     #0#6 = #1#159; ||direccion
                     #0#7 = #1#150; ||plataforma
                     |GRABA 020#1a;
                |FINSI;
           |FINSI;
           |LIBERA #1;
      |FINSI;

      |SI nModo = 3;
           #1#0 = #0#0;
           #1#1 = #0#1;
           |LEE 101#1=;
           |SI FSalida = 0;
                |PINDA #0#1;
                |ENDATOS #3#platm018;
                |SI FSalida = 0;
                     |BORRA 020#1a;
                |FINSI;
           |FINSI;
           |LIBERA #1;
           a = 0;
      |FINSI;

      |SI nModo = 4;
           #1#0 = #0#0;
           #1#1 = #0#1;
           |LEE 000#1=;
           |SI FSalida = 0;
                |PINDA #0#1;
                |PAUSA;
           |FINSI;
           a = 0;
      |FINSI;

      |CIERRAT #1;
      |POPV;

      |PINTA #3#2;
      |PINTA #3#3;
|FPRC;

