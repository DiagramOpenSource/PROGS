|FICHEROS;
     iber9999  #0;
|FIN;

|VARIABLES;
     ||Para el tema de Arreglar la direccion, quitandole la autentificacion
     ||ArreglaDir
     aDirToFix = "";
     aLongTotal = "";
     nLongTotal = 0;
     aDosPuntos = ":";
     nCaracter = 0;
     aNumeroExt = "";
     nNumeroExt = 0;
     aCaracter = "";

     &eaAlfaDir = "";
     &eaDirecOk = "";
     ||Fin variables para ArreglaDir
|FIN;

|PROCESO ArreglaDir;
     eaDirecOk = "";
     aDirToFix = eaAlfaDir;
     |QBF aDirToFix;
     aLongTotal = aDirToFix % A0;
     nLongTotal = aLongTotal;
     |PARA nCaracter = 1; |SI nCaracter [ nLongTotal; |HACIENDO nCaracter = nCaracter + 1;
          aNumeroExt = ("00" + nCaracter) % -102;
          aNumeroExt = "-1" + aNumeroExt;
          nNumeroExt = aNumeroExt;
          aCaracter = aDirToFix % nNumeroExt;
          aCaracter = aCaracter % 101;
          |SI aCaracter = ":";
               |SAL;
          |SINO;
               eaDirecOk = aCaracter + eaDirecOk;
          |FINSI;
     |SIGUE;
|FPRC;
