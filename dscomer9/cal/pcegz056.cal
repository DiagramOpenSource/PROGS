|PROCESO AsignaRec;
     #0#0 = nCodCob;
     |BORRA 020#0c;
     #2#0 = aEmpRec;
     #2#1 = nCodRec;
     |BORRA 020#2c;
     |REFRESCACONTROL nGrid0;
     |REFRESCACONTROL nGrid2;

     |ABRE #56;
     #56#0 = nCodCob;
     #56#1 = "R";
     #56#4 = aEmpRec;
     #56#5 = nCodRec;
     |GRABA 020#56n;
     |CIERRA #56;
     nCodCob = 0;
     nCodRec = 0;
|FPRC;

|PROCESO AsignaPed;
     #0#0 = nCodCob;
     |BORRA 020#0c;
     #1#1 = aSerPed;
     #1#2 = nCodPed;
     |BORRA 020#1c;
     |REFRESCACONTROL nGrid0;
     |REFRESCACONTROL nGrid1;

     |ABRE #56;
     #56#0 = nCodCob;
     #56#1 = "P";
     #56#2 = aSerPed;
     #56#3 = nCodPed;
     |GRABA 020#56n;
     |CIERRA #56;
     nCodCob = 0;
     nCodPed = 0;
|FPRC;

|PROCESO Recibos;
     nHayRec = 1;
     #2#0 = aEmpCarte;
     #2#1 = #3#40;
     #2#2 = #3#0;
     #2#3 = #3#1;
     #2#4 = #3#2;
     #2#5 = #3#32;
     #2#6 = #3#6;
     #2#7 = #3#99;
     #2#8 = #3#65;
     #2#10 = #3#3;
     |GRABA 020#2n;
|FPRC;

|DEFBUCLE Recibos;
     #3#1;
     14, 12;
     000000001, "P", #33#11;
     999999999, "P", #33#11;
     be;
     NULL, Recibos;
|FIN;

|PROCESO dscam045;
     |SI nEmpCarte ! #45#6;
          nEmpCarte = #45#6;

          aEmpCarte = ("00000" + #45#6) % -105;
          aAlfa = aBase + "dscarter/fich/" + aEmpCarte + "/";
          |PATH_DAT #3#aAlfa#;
          |BUCLE Recibos;
     |FINSI;
|FPRC;

|DEFBUCLE dscam045;
     #45#1;
     ;
     nEmpresa, 001;
     nEmpresa, 999;
     ;
     NULL, NULL, NULL, NULL, NULL, dscam045;
     #45#6;
|FIN;

|PROCESO LlenaRecPen;
     |DBASS aBase;
     aAlfa = aBase + "dscarter/fich/";
     |PATH_DAT #45 aAlfa;

     nEmpresa = aEmp;
     nEmpCarte = 0;

     |BUCLE dscam045;
|FPRC;

|PROCESO QuitaMarca2;
     #pcegz052@#9 = "";
     |GRABA 020#pcegz052@.a;
|FPRC;

