|FIN;

|PROCESO Desmarca2;
     |DDEF aAlfa;
     aAlfa = aAlfa + "pcegz052";
     |CARGA_DEF aAlfa, pcegz052@;
     |SI FSalida = 0;
          |PATH_DAT #pcegz052@#aFich#;
          |NOME_DAT #pcegz052@#aTmp2#;
          |BUCLE QuitaMarca2;
          |DESCARGA_DEF #pcegz052@;
     |FINSI;
|FPRC;

