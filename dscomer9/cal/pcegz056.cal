|FIN;

|PROCESO Desmarca1;
     |DDEF aAlfa;
     aAlfa = aAlfa + "pcegz051";
     |CARGA_DEF aAlfa, pcegz051@;
     |SI FSalida = 0;
          |PATH_DAT #pcegz051@#aFich#;
          |NOME_DAT #pcegz051@#aTmp1#;
          |BUCLE QuitaMarca1;
          |DESCARGA_DEF #pcegz051@;
     |FINSI;
