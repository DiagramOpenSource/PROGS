|PROCESO Desmarca0;
     |DDEF aAlfa;
     aAlfa = aAlfa + "pcegz050";
     |CARGA_DEF aAlfa, pcegz050@;
     |SI FSalida = 0;
          |PATH_DAT #pcegz050@#aFich#;
          |NOME_DAT #pcegz050@#aTmp0#;
          |BUCLE QuitaMarca0;
          |DESCARGA_DEF #pcegz050@;
     |FINSI;
|FPRC;

|PROCESO EventoGrid2;
     |SI FSalida = 4;
          |LEE 000#2=;
          |SI FSalida ! 0; |ACABA; |FINSI;

          |SI #2#9 = "*";
               #2#9 = "";
               nCodRec = 0;
          |SINO;
               |SI nCodRec ! 0;
                    |HAZ Desmarca2;
               |FINSI;
               #2#9 = "*";
               aEmpRec = #2#0;
               nCodRec = #2#1;
          |FINSI;
          |GRABA 020#2a;
          |REFRESCACONTROL nGrid2;
    |FINSI;
|FPRC;

|PROCESO EventoGrid1;
     |SI FSalida = 4;
          |LEE 000#1=;
          |SI FSalida ! 0; |ACABA; |FINSI;

          |SI #1#6 = "*";
               #1#6 = "";
               nCodPed = 0;
          |SINO;
               |SI nCodPed ! 0;
                    |HAZ Desmarca1;
               |FINSI;
               #1#6 = "*";
               nCodPed = #1#2;
               aSerPed = #1#1;
          |FINSI;
          |GRABA 020#1a;
          |REFRESCACONTROL nGrid1;
     |FINSI;
|FPRC;

|PROCESO EventoGrid0;
     |SI FSalida = 4;
          |LEE 000#0=;
          |SI FSalida ! 0; |ACABA; |FINSI;

          |SI #0#16 = "*";
               #0#16 = "";
               nCodCob = 0;
          |SINO;
               |SI nCodCob ! 0;
                    |HAZ Desmarca0;
               |FINSI;
               #0#16 = "*";
               nCodCob = #0#0;
          |FINSI;
          |GRABA 020#0a;
          |REFRESCACONTROL nGrid0;
     |FINSI;
|FPRC;


|PROCESO Pedidos;
     #1#0 = #4#11;
