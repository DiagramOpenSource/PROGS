|FICHEROS;
     primy001 #0;        || este def
     agifa024 #1;        || clientes
     dsm00018 #2;        || observaciones
|FIN;

|VARIABLES;
     aAlfa = "";
|FIN;

|PROCESO dsm00018;
     |PRINT;
|FPRC;

|DEFBUCLE dsm00018;
     #2#1;
     ;
     #1#0,"CL",001;
     #1#0,"CL",999;
     ;
     NULL,dsm00018;
|FIN

|PROCESO agifa024;
     |INFOR "primy001";
     |SI FSalida = 0;
          |PRINT;
          |PIEINF;
          |FININF;
          |SI #0#8 = "S";|O #0#8 = "s";
               |INFOR "primy002";
               |SI FSalida = 0;
                    |BUCLE dsm00018;
                    |PIEINF;
                    |FININF;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE agifa024;
     #1#1;
     25,9,181;
     #0#0,#0#2,#0#4,#0#6;
     #0#1,#0#3,#0#5,#0#7;
     ;
     NULL, NULL, NULL, NULL, NULL,agifa024;
     #1#9;
|FIN;

|PROCESO Inicio;    |TIPO 3;
     |IMPRE 1;
     |SI FSalida = 0;
          |BUCLE agifa024;
          |FINIMP;
     |FINSI;
|FPRC;

|PROCESO llena;     |TIPO 0;
     aAlfa = ("000000" + #0Campo);
     |QBF aAlfa;
     aAlfa = aAlfa % -106;
     #0Campo = aAlfa;
     |PINTA #0Campo;
|FPRC;
