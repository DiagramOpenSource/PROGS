          |SI pedido ! 0; |Y FSalida = 14; |ACABA; |FINSI;
          |SI FSalida = 13; |Y Campo = 9; |ACABA; |FINSI;
          serie   = #0#0;     || SERIE.
          pedido  = #0#1;     || NUMERO PEDIDO.
          lineap  = #0#2;     || LINEA PEDIDO.
          codigo  = #0#4;     || CODIGO ARTICULO.
