|FPRC;

|PROCESO Escape;
     |SI FSalida = 1;
         |LEE 040#0c;
     |FINSI;

     |SI FSalida = 4;
         nSal = 0;
         |PTEC 807;
     |FINSI;
|FPRC;

|PROCESO BusArti2;
     #0#0 = aSer;
     #0#1 = nPed;
     #0#2 = nLin;
     |LEE 000#0];
     |LEE 000#0s;
     |PARA; |SI FSalida = 0; |HACIENDO;
          |SI #0#4 = aArti;
               aSer = #0#0;
               nPed = #0#1;
               nLin = #0#2;
               nHay = 1;
               |SAL;
          |FINSI;
          |LEE 000#0s;
     |SIGUE;
|FPRC;

|PROCESO Marca; |TIPO 0;
     |SI FSalida = 0; |Y #0Campo = Contenido;
          |SI #0#12 = "*";
               #0#12 = "";
               nUniMarcadas = nUniMarcadas - #0#8;
               #0#13 = nUniMarcadas; |PINTA 2266, #0#13;
          |SINO;
               |HAZ CheqPedido;
               |SI nError = 0;
                    #0#12 = "*";
                    nUniMarcadas = nUniMarcadas + #0#8;
                    #0#13 = nUniMarcadas; |PINTA 2266, #0#13;
               |FINSI;
          |FINSI;
          |GRABA 020#0a;
          |PINTA #0#12;
     |FINSI;
|FPRC;

|PROGRAMA;
     |HAZ EsEvaristo; nSwEvaristo = FSalida;
     |HAZ EsBerria; nSwBerria = FSalida;

     |ABRE #142; |LEE 000#142p; |CIERRA #142;

     |NOME_DAT #0 eaFicPdte; |DELINDEX #0;

     |SI enTipo = 0;                   || Desde Cabecera
          dcliente = "      ";
          hcliente = "zzzzzz";
     |SINO;                            || Desde Linea
          dcliente = cliente;
          hcliente = cliente;
     |FINSI;

     |SI pedido = 0;
          nDPedido = 1;
          nHPedido = 999999;
     |SINO;
          nDPedido = pedido;
          nHPedido = pedido;
     |FINSI;

     |ABRE #0;
     |ABRE #19;
     |ABRE #agifa104;
     |BUCLE agit0000;
     |CIERRA #agifa104;
     |CIERRA #19;

     |SI enDesdeX = 0; |PUSHV 0501 2380; |PINPA #0#0; |FINSI;

     |LEE 000#0p;

  |ET Otra;
     |SI enDesdeX = 0;
          |PINPA #0#0;
          #0#13 = nUniMarcadas; |PINTA 2266, #0#13;
          |ENTLINEAL #1#0,1,4,20,0,mini,maxi;
     |SINO;
         nSal  = 1;
         |SI FSalida = 0;
             nModo = 2 + 4 + 8 + 16;
             |LINEAL_SIMPLE #1#0, nModo, 0501, 2699, NULL, NULL, Escape;
             |SI nSal = 0;
                 serie   = #0#0;     || SERIE.
                 pedido  = #0#1;     || NUMERO PEDIDO.
                 lineap = 0;    || Pedido Completo
                 codigo  = #0#4;     || CODIGO ARTICULO.
                 descrip = #0#5;     || DESCRIPCION.
                 unid    = #0#8;     || UNIDADES.
                 cliente = #0#9;     || Cliente
             |FINSI;
         |FINSI;
     |FINSI;
     |SI aArti ! "";
          nHay = 0;
          |HAZ BusArti2;
          |SI nHay = 0;
               |MENAV "0000El articulo no se encuentra en las lineas pendientes.";
          |FINSI;
          aArti = "";
          #0#0 = aSer;
          #0#1 = nPed;
          #0#2 = nLin;
          |LEE 000#0=;
          |VETE Otra;
     |FINSI;

     |SI enDesdeX = 0; |POPV; |FINSI;

     |SELECT #2#0;
     #0#12 = "*";
     #0#0 = "";
     #0#1 = 0;
     #0#2 = 0;
     |LEE 000#0];
     |SI FSalida = 0; |Y #0#12 = "*";
          pedido = #0#1;
          cliente = #0#9;
          |CIERRA #0;
     |SINO;
          pedido = 0;
          |CIERRA #0;
          |DELINDEX #0; Tempo = eaFicPdte; |HAZ BoraTempo;
     |FINSI;
|FPRO;
