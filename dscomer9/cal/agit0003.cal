|PROCESO Condiciones;
     |ABRE #24;
     #24#0 = cliente;
     |LEE 000#24=;
     |CIERRA #24;

     |ABRE #91;
     #91#0 = #24#20;
     |LEE 000#91=;
     |CIERRA #91;

     |ABRE #25;
     #25#0 = #24#25;
     |LEE 000#25=;
     |CIERRA #25;

     |SI nSw = 0;
          nSw = 1;
          #43#0 = #24#20;
          #43#1 = #91#1;
          #43#2 = #24#25;
          #43#3 = #25#1;
          #43#4 = #24#37;          || dto.especial
          #43#5 = #24#38;          || dto.p.p.
          #43#6 = #24#156;         || dto.acumul.
     |FINSI;


     |BLANCO 1213 2368;
     |PINPA #0#43;
     |PINDA #0#43;

     |SI nSwEvaristo = 0;
          |C_V #43#2, 1, "N";
          |C_V #43#3, 1, "N";
          |PINTA 1715, "³                                                  ³";
     |FINSI;

     |ENDATOS #2#43;
|FPRC;

|PROCESO T12; |TIPO 12;
     |MENU aMenu;

     nSwModi = FSalida;
     |SI nSwModi > 1;
          |DDEF aDef;
          aDef = aDef + "agit0001";
          |CARGA_DEF aDef, referen@;
          |SI FSalida = 0;
               |NOME_DAT #10 aTempo0;
               aSerAnt   = "";
               nPedAnt   = 0;
               |ABRE #2;
               |SI nSwModi = 3;
                    |BUCLE ModiPediTodos;
               |SINO;
                    |HAZ ModiPedi;
               |FINSI;
               |CIERRA #2;
               |DESCARGA_DEF #referen@;
          |SINO;
               aDef = "0000Error al cargar:" + aDef;
               |MENAV aDef;
          |FINSI;
     |FINSI;
|FPRC;
