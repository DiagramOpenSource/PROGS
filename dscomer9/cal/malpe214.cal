|FICHEROS;
     tmp00000 #0; || Tmp Facturas
     agifa134 #134;
     agifa059 #59;
     agifa010 #10;
     agifa071 #71;
     malpe087 #87;
     malpe088 #88;
|FIN;

|VARIABLES;
     aAlfa = "";
     &Tempo = "";
     nSw = 0;
     nSwImprime = 0;
     nSwImpre = 1;
     nSwInfor = 1;
|FIN;

|PROCESO Imprime;
     |SI nSwImprime = 0;
          nSwImprime = 1;
          |CONFI "S0000Imprimir precios distintos albaran y pedido";
          |SI FSalida = 0;
               |IMPRE 0;
               nSwImpre = FSalida;
               |SI FSalida = 0;
                    |INFOR "malpe214";
                    nSwInfor = FSalida;
                    |SI FSalida ! 0;
                         |MENAV "0000Error en informe malpe214";
                    |FINSI;
               |FINSI;
          |FINSI;
     |FINSI;
     |SI nSwInfor = 0;
          |PRINT;
     |FINSI;
|FPRC;

|PROCESO LinAlbaran;
     #87#25 = #71#31;
     #87#0 = #71#12;
     |LEE 000#87=;
     |SI FSalida = 0;
          #88#28 = #71#31;
          #88#0 = #71#12;
          #88#1 = #71#21;
          |LEE 000#88=;
          |SI FSalida = 0;
               |SI #88#6 ! #71#6;
                    |HAZ Imprime;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO LinFactura;
     #10#52 = #59#15;
     #10#0 = #59#2;
     |LEE 020#10=;
     |SI FSalida = 0;
          |BUCLELIN 0LinAlbaran#10;
     |FINSI;
|FPRC;

|PROCESO Factura
     #134#49 = #0#0;
     #134#0 = #0#1;
     |LEE 020#134=;
     |SI FSalida = 0;
          |BUCLELIN 0LinFactura#134;
     |FINSI;
|FPRC;

|DEFBUCLE Factura;
     #0#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, Factura;
|FIN;

|PROGRAMA;
     aAlfa = PARAMETRO;
     |NOME_DAT #0aAlfa;

     |ABRE #134;
     |ABRE #10;
     |ABRE #87;
     |ABRE #88;
     |BUCLE Factura;
     |CIERRA #88;
     |CIERRA #87;
     |CIERRA #10;
     |CIERRA #134;

     |SI nSwInfor = 0; |PIEINF; |FININF; |FINSI;
     |SI nSwImpre = 0; |FINIMP; |FINSI;
|FPRO;
