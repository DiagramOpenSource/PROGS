               |HAZ CargaDef;
               |ABRE #300; |SELECT #2#300;
               #300#9 = "A";
               #300#5 = #3#3;
               #300#10 = "31.12.9999";
               #300#11 = "23:59";
               |LEE 000#300];
               |SI FSalida = 0;
                    |LEE 000#300a;
               |SINO;
                    |LEE 000#300u;
               |FINSI;
               |SI FSalida = 0; |Y #300#5 = #3#3; |Y #300#9 = "A";
                    |HAZ LaPutaHora;
               |SINO;
                    |HORA aHora;
                    #5#6 = ~;
                    #5#11 = aHora;
               |FINSI;
               |PINTA #5#6; |PINTA #5#11;
               |CIERRA #300;
               |HAZ DescargaDef;
          |FINSI;
     |FINSI;
|FPRC;


