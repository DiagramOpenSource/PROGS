|PROCESO Desencripta;
     camufz = "";
     |PARA para1 = 1; |SI para1 [ 20; |HACIENDO para1 = para1 + 2;
          nume1 = (para1 * 100) + 1;
          alfa1 = clave % nume1;
          nume1 = ((para1 + 1) * 100) + 1;
          alfa2 = clave % nume1;

          nume1 = &alfa2;
          nume1 = nume1 - 65;

          camuf = &alfa1;
          camuf = camuf - 65;
          camuf = ((camuf * 26) + nume1) / 2;

          camufz = camufz + &camuf;
     |SIGUE;
|FPRC;

|PROCESO Encripta;
     camufz = "";
     |PARA para1 = 1; |SI para1 [ 10; |HACIENDO para1 = para1 + 1;
          nume1 = (para1 * 100) + 1;
          alfa1 = clave % nume1;

          camuf = &alfa1;
          camuf = camuf * 2;

          nume1 = ((camuf - (camuf $ 26)) / 26 ) + 65;
          camufz = camufz + &nume1;

          nume1 = (camuf $ 26) + 65;
          camufz = camufz + &nume1;
     |SIGUE;
|FPRC;

