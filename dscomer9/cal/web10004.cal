|FICHEROS;
     web10004 #0;
     web10002 #2;
     web10003 #3;
     agifa019 #19;
|FIN;

|VARIABLES;
     nHandle   = 0;
     nErr      = 0;
     nBoton1   = 0;
     nLong     = 0;

     aHora     = "";
     aFic      = "";
     aAlfa     = "";
     aAlfa1    = "";
     aLong     = "";
     aReg      = "";
     aRuta     = "";
|FIN;

|PROCESO web10002;
     #19#0 = #2#1;
     |LEE 000#19=;
     |SI FSalida ! 0;
          |BORRA 020#2a;
          |ACABA;
     |FINSI;

     aAlfa = #2#6; |QBF aAlfa; aAlfa = ("00" + aAlfa) % -102; aReg = aAlfa;
     aAlfa = #2#7; |QBF aAlfa; aReg = aReg + "|" + aAlfa;
     aAlfa = #2#8; |QBF aAlfa; aReg = aReg + "|" + aAlfa;
     aAlfa = #2#1; |QBF aAlfa; aReg = aReg + "|" + aAlfa;
     aAlfa = #2#9; |QBF aAlfa; aReg = aReg + "|" + aAlfa;
     |SI #2#4 = "S";
          aAlfa = "1";
     |SINO;
          aAlfa = "0";
     |FINSI;
     |QBF aAlfa; aReg = aReg + "|" + aAlfa;
     aAlfa = #2#5; |QBF aAlfa; aReg = aReg + "|" + aAlfa;
     aAlfa = #2#2; |QBF aAlfa; aReg = aReg + "|" + aAlfa;
     aAlfa = #2#3; |QBF aAlfa; aReg = aReg + "|" + aAlfa;
     |XGRABA nHandle, aReg;
|FPRC;


|DEFBUCLE web10002;
     #2#1;
     ;
     INICIO;
     FINAL;
     be;
     NULL, web10002;
|FIN;

|PROCESO Boton1;
     |PTEC 824;
|FPRC;

|PROCESO Browser;
     |SI FSalida ! 10; |ACABA; |FINSI;

     aRuta = #0#0;  |QBF aRuta;
     |SI aRuta = "";
         aRuta = "\*.*";
     |FINSI;
     aRuta = "F" + aRuta;

     |BROWSE aRuta;

     aRuta = aRuta % 299;

     #0#1 = aRuta;
     #0#1 = #0#1 > #0#1;
     |PINTA #0#1;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     aFic = #0#1; |QBF aFic;

     |XABRE "WC", aFic, nHandle;
     |SI FSalida ] 0;
          |ABRE #19;
          |BUCLE web10002;
          |CIERRA #19;
          |XCIERRA nHandle;

          |HORA aHora;
          |ABRE #3;
          |LEE 000#3u;
          #3#0 = #3#0 + 1;
          #3#1 = ~;
          #3#2 = aHora;
          #3#3 = Usuario % 810;
          #3#4 = #0#2;
          |GRABA 020#3n;
          |CIERRA #3;
     |SINO;
          |MENAV "0000ERROR al Crear el Fichero...";
     |FINSI;
|FPRC;

|PROCESO Quita; |TIPO 0;
     |ESTADO_CONTROL nBoton1, "DISABLE";
|FPRC;

|PROCESO Pone; |TIPO 7;
     |ESTADO_CONTROL nBoton1, "ENABLE";
|FPRC;

|PROGRAMA;
     |CONTROL_SIMPLE 0, "BOTON,...", 1162, 1166, Boton1;
     nBoton1 = FSalida;

     |CLS;
     |ABRE #0;
     |LEE 101#0p;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |GRABA 020#0a;
     |FINSI;
     |CIERRA #0;

     |FIN_CONTROL_WINDOWS nBoton1;
     |PULSATECLA;
|FPRO;
