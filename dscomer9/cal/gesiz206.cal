|PROCESO BtnReabPar2;
     |LEE 101#referb2@.=;
     |SI FSalida ! 0; |ACABA; |FINSI;
     |SI #referb2@#3 = "A";
          |MENAV "0000Parte abierto...";
     |SINO;
          aAlfa = "0000NReabrir parte:" + #referb2@#0+"/"+(("000000"+#referb2@#1)%-106)+(("000"+#referb2@#2)%-103);
          |CONFI aAlfa;
          |SI FSalida = 0;
               #referb2@#3 = "A";
               |GRABA 020#referb2@.a;
               |REFRESCACONTROL enGridb2;
          |FINSI;
     |FINSI;
     |LIBERA #referb2@;
|FPRC;

|PROCESO BtnHisMaqPar2;
