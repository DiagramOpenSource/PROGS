|FICHEROS;
     agifa034 #0;
     agifa036 #1;
     agifa070 #2;
     dsm00005 #905;
|FIN;

|VARIABLES;
     nLinV = 0;
     linea = 0;
     &enErr = 0;
|FIN;

|PROCESO Tipo8; |TIPO 8;
     |HAZ ControlUsuDV;
     |SI enErr ! 0; |PTEC 807; |ACABA; |FINSI;
|FPRC;

|CALCULO lincont;|TIPO 0;
     |SI #2#5 ! 0;
        linea = 1;
        |ERROR;
     |FINSI;
|FCAL;

|CALCULO fichcont;|TIPO 0;
     linea = 0;
     |BUCLELIN 2lincont#1;
|FCAL;

|PROCESO BorraCompo;
     |BORRA 020#905a;
     |LIBERA #905;
|FPRC;

|DEFBUCLE dsm00005;
     #905#1;
     ;
     "DP", #2#22, #2#0, nLinV, "      ";
     "DP", #2#22, #2#0, nLinV, "zzzzzz";
     be;
     NULL, BorraCompo;
|FIN;

|CALCULO borlin;|TIPO 0;
     |SI linea = 0;
          nLinV = #2#1;
          |BUCLE dsm00005;

          |BORRA 021#2a;
     |FINSI;
|FCAL;

|CALCULO borfich;|TIPO 0;
     |SI linea = 0;
         |BORRA 021#1a;
     |FINSI;
|FCAL;

|DEFBUCLE agifa036;
     #1#1;
     3,1;
     #0#6 , #0#0 , #0#2 , #0#4;
     #0#7 , #0#1 , #0#3 , #0#5;
     e;
     NULL, fichcont, borlin, borfich;
|FIN;

|CALCULO depbo;|TIPO 3;
     |SI #0#8 = "SI";
          |BUCLE agifa036;
     |FINSI;
|FCAL;
