|FICHEROS;
     albz0006 #0;
     agifa104 #104;
     albm0010 #10;
|FIN;

|VARIABLES;
     aAlfa = "";
     nHay = 0;
|FIN;

|PROCESO Min;
     #0#0 = "     ";
     #0#1 = 1;
|FPRC;

|PROCESO Max;
     #0#0 = "zzzzz";
     #0#1 = 999999;
|FPRC;

|PROCESO Tecla;
     |SI #0Campo = Contenido; |Y FSalida = 0;
          |SI #0#5 = "*";
               #0#5 = "";
          |SINO;
               #0#5 = "*";
          |FINSI;
          |GRABA 020#0a;
          |PINTA #0#5;
     |FINSI;
|FPRC;

|PROCESO Tmp;
     #10#0 = #0#0;
     #10#1 = #0#1;
     |LEE 121#10=;
     |SI FSalida = 0;
          |SI #0#5 ! " ";
               #10#6 = "S";
               |GRABA 020#10a;
          |FINSI;
          |LIBERA #10;
     |FINSI;
|FPRC;

|DEFBUCLE Tmp;
     #0#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, Tmp;
|FIN;

|PROCESO albm0010;
     #104#25 = #10#0;
     #104#0 = #10#1;
     |LEE 000#104=;
     |SI FSalida = 0;
          #0#0 = #10#0;
          #0#1 = #10#1;
          #0#2 = #104#1;
          #0#3 = #104#4;
          #0#4 = #104#8;
          |GRABA 020#0n;
          nHay = 1;
     |FINSI;
|FPRC;

|DEFBUCLE albm0010;
     #10#1;
     6;
     "      ", 000001, "N";
     "zzzzzz", 999999, "N";
     ;
     NULL, albm0010;
|FIN;

|PROGRAMA;
     |CLS;
     |ENTLINEAL #1#0, 1, 7, 22, 1, Min, Max;
     |MENSA "0000Cargando Temporal....";
     aAlfa = Usuario;
     |QBF aAlfa;
     |NOME_DAT #0 aAlfa; |DELINDEX #0;
     |ABRE #0;
     |ABRE #104;
     |BUCLE albm0010;
     |CIERRA #104;
     |CIERRA #0;
     |SI nHay = 0;
          |MENAV "0000No hay registros...";
     |SINO;
          |ENTLINEAL #1#0, 1, 4, 22, 0, Min, Max;
          |CONFI "0000NActualizar Pedido...";
          |SI FSalida = 0;
               |ABRE #10;
               |BUCLE Tmp;
               |CIERRA #10;
          |FINSI;
     |FINSI;
     |DELINDEX #0;
|FPRO;
