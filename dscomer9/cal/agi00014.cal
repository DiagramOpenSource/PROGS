|FICHEROS;
     agi00014 #0;
     agifa142 #142;
|FIN;

|VARIABLES;
     r_alfa3        = "";
     r_long3        = "";
     r_num3         = 0;
     r_masc3        = "000000000000000000000000000";
     &proveedor     = "";
|FIN;

____________________________________/ LLENA DE CEROS EL CODIGO DEL PROVEEDOR.
|PROCESO llena_ceros; |TIPO 6;
     |ABRE #142;
     |LEE 000#142p;
     |SI FSalida = 0; |Y #142#46 = "S";
          r_alfa3 = #0Campo; |QBF r_alfa3;
          #0Campo= r_masc3;
          r_long3 = #0Campo % A0;
          r_alfa3 = r_masc3 + r_alfa3;
          r_num3 = -(r_long3 + 100);
          #0Campo = r_alfa3 % r_num3;
          |PINTA #0Campo;
     |FINSI;
     |CIERRA #142;
|FPRC;

___________________________/ HACE UNA LLAMADA AL RECALCULO DE DEFECTOS Y PVP.
|PROCESO llama; |TIPO 30;
     |CONFI "2400S¿Desea recalcular los defectos y los precios de venta? ";
     |SI FSalida = 0;
          proveedor = #0#0;
          |SUB_EJECUTA_NP "agi00015.tab";
     |FINSI;
|FPRC;
