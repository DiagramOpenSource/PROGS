     &cProveAlba    = "";
     &cNombrAlba    = "";

     &fFechaAlba    = @;

     &nNumerAlba    = 0;
     &nTotComTra    = 0;
     &nTotVenTra    = 0;
     &nTotComBas    = 0;
     &nTotVenBas    = 0;

     &cCodMonBase   = "";
     &cCodMonTrab   = "";
     &cCodMonConv   = "";
     &cCodMonActu   = "";
     &cDesMonBase   = "";
     &cDesMonTrab   = "";

     &nNumerPres    = 0;
     &nDeci_PBase   = 0;
     &nDeci_TBase   = 0;
     &nDeci_PTrab   = 0;
     &nDeci_TTrab   = 0;
     &nDeci_TotVis  = 0;
     &nEurComTrab   = 0;
     &nEurComBase   = 0;
     &nEurVenTrab   = 0;
