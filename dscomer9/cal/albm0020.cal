|FICHEROS;
   albm0020 #0;
   agifa112 #112;
|FIN;

|VARIABLES;
   nModo = 0;
   aAlfa  = "";
   aAlfa1 = "";
|FIN;

|PROCESO Tipo0m020; |TIPO 0;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          aAlfa = "00" + #0#0;
          |ABRE #112;
          #112#0 = aAlfa;
          |LEE 000#112=;
          |SI FSalida = 0;
               #0#1 = #112#1; |PINTA #0#1;
          |FINSI;
          |CIERRA #112;
     |FINSI;
|FPRC;

|PROCESO Tipo6m020; |TIPO 6;
   aAlfa = #albm0020#0; |QBF aAlfa;
   |PARA; |SI aAlfa ! ""; |HACIENDO;
      aAlfa1 = aAlfa % 0101; aAlfa = aAlfa % 0299;
      |SI aAlfa1 < "0"; |O aAlfa1 > "9";
         |AVISO; |ERROR; |SAL;
      |FINSI;
   |SIGUE;
|FPRC;
