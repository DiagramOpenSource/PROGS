==================== Llamadas del agifa189 ========================
|PROCESO GrSolBorraTmp;
     |SI eaTmpSol ! "";
          |DELINDEX #grsol003@; Tempo = eaTmpSol; |HAZ BoraTempo;
          |DESCARGA_DEF #grsol003@;
     |FINSI;
|FPRC;

|PROCESO GrSolCreaTmp;
     |DDEF aDef;
     aDef = aDef + "grsol003";
     |CARGA_DEF aDef, grsol003@;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |HAZ CreaTempo;
     eaTmpSol = Tempo;
     |NOME_DAT #grsol003@#eaTmpSol#; |DELINDEX #grsol003@;
|FPRC;

|PROCESO GrSolFabrica;
     aArti = #189#2;;
     |HAZ ParametroArti;
     |SI aSerFab = "S";
          eaSerie = #189#11;
          enNumero = #189#0;
          enLinea = #189#1;
          enUniSol = #189#7;
          |SUB_EJECUTA_NP "grsol003";
          #189#7 = enUniSol;
          |PINTA #189#7;

          |C_M #189#7, 1, "N";

     |SINO;
          |C_M #189#7, 1, "S";
     |FINSI;
|FPRC;

|PROCESO ActSerFab;
     |DDEFECTO #2;
     #2#0 = #3#4;
     #2#1 = #3#0;
     #2#2 = #3#1;
     #2#3 = #189#9;
     #2#4 = #3#5;
     #2#5 = #3#6;
     |GRABA 020#2n;
|FPRC;

|DEFBUCLE ActSerFab;
     #3#1004;
     ;
     #189#11, #189#0, #189#1, 001;
     #189#11, #189#0, #189#1, 999;
     ;
     NULL, ActSerFab;
|FIN;

|PROCESO GrSolActFab;
     |DDEF aDef;
     aDef = aDef + "grsol002";
     |CARGA_DEF aDef, grsol002@;
     |SI FSalida = 0;
          |ABRE #2;
          |BUCLE ActSerFab;
          |CIERRA #2;
          |DESCARGA_DEF #grsol002@;
     |FINSI;
|FPRC;
