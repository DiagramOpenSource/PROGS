|FIN;

|PROCESO ParametroArti;
     |DDEF aDef;
     aDef = aDef + "grsol001";
     |CARGA_DEF aDef, grsol001@;
     |SI FSalida = 0;
          |DDEFECTO #1;
          |ABRE #1;
          #1#0 = aArti;
          |LEE 101#1=;
          |CIERRA #1;
          aSerFab = #1#1;
          nUniFab = #1#2;
          |DESCARGA_DEF #grsol001@;
     |FINSI;
|FPRC;

