|VARIABLES;
     &efFecha = @;
     &eaAbono = "";
     &eaSerie = "";
     &enNumero = 0;
     &enLinea = 0;
     &enUniSol = 0;
     &eaTmpSol = "";
     &Tempo = "";
     aDef = "";
     aSerFab = "";
     nUniFab = "";
