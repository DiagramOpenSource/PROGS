|FICHEROS;
     agifa029 #0;
     referen@;
|FIN;

|VARIABLES;
     aDef = "";
     aAlfa = "";
     &enAlmacen = 0;
     nSeccion = 0;
|FIN;

|PROCESO T13_29; |TIPO 13;
     |HAZ EsTaller;
     |SI FSalida = 0;
          |PINTA 1510, "Seccion.....:";
          |ATRI I;
          nSeccion = 0;
          |DDEF aDef;
          aAlfa = aDef + "con00006";
          |CARGA_DEF aAlfa, referen@;
          |SI FSalida = 0;
               |DDEFECTO #referen@;
               |ABRE #referen@;
               #referen@#0 = #0#0;
               |LEE 000#referen@.=;
               |SI FSalida ! 0; |GRABA 020#referen@.n; |FINSI;
               |CIERRA #referen@;
               nSeccion = #referen@#1;
               |PINTA 1524, #referen@#1;
               |DESCARGA_DEF #referen@;
          |FINSI;
          aAlfa = aDef + "con00002";
          |CARGA_DEF aAlfa, referen@;
          |SI FSalida = 0;
               |DDEFECTO #referen@;
               |ABRE #referen@;
               #referen@#5 = nSeccion;
               #referen@#6 = nSeccion;
               #referen@#0 = nSeccion;
               |LEE 000#referen@.=;
               |CIERRA #referen@;
               |PINTA 1529, #referen@#1;
               |DESCARGA_DEF #referen@;
          |FINSI;
          |ATRI 0;
     |FINSI;
|FPRC;

|PROCESO T0_29; |TIPO 0;
     |SI FSalida = 0;
          |HAZ EsTaller;
          |SI FSalida = 0;
               enAlmacen = #0#0;
               |SUB_EJECUTA_NP "con00006";
               |HAZ T13_29;
          |FINSI;
     |FINSI;
|FPRC;
