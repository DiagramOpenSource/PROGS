|FICHEROS;
     guerz045 #0;
     gopartia #10;
     agifa077 #77;
|FIN;

|VARIABLES;
     nReg = 0;
     nSwImpre = 0;
     nSwInfor = 0;
     nSwPrime = 0;
|FIN;

|PROCESO gopartia;
     |SI #10#28 = "00.00.0000"; |Y #10#34 = "M";
          #77#50 = #10#4;
          #77#0 = #10#5;
          |LEE 000#77=;
          |SI FSalida = 0;
               |SI #0#1 = "S";
                    #10#28 = #77#1;
                    |GRABA 020#10a;
               |FINSI;
               |SI #0#0 = "S";
                    |SI nSwPrime = 0;
                         nSwPrime = 1;
                         |IMPRE 0;
                         nSwImpre = FSalida;
                         |SI FSalida = 0;
                              |INFOR "guerz045";
                              nSwInfor = FSalida;
                              |SI FSalida ! 0;
                                   |MENAV "0000No existe informe 'guerz045'";
                              |FINSI;
                         |FINSI;
                    |FINSI;
                    |SI nSwImpre = 0; |Y nSwInfor = 0;
                         |PRINT;
                    |FINSI;
               |FINSI;
          |FINSI;
     |FINSI;
     |LIBERA #10;
|FPRC;

|DEFBUCLE gopartia;
     #10#1;
     ;
     INICIO;
     FINAL;
     be;
     NULL, gopartia;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |SI #0#0 = "S"; |O #0#1 = "S";
          |ABRE #77;
          |BUCLE gopartia;
          |CIERRA #77;
     |FINSI;
     |SI #0#0 = "S";
          |SI nSwPrime ! 0;
               |SI nSwInfor = 0; |PIEINF; |FININF; |FINSI;
               |SI nSwImpre = 0; |FINIMP; |FINSI;
          |SINO;
               |MENAV "0000No hay registros...";
          |FINSI;
     |FINSI;
|FPRC;
