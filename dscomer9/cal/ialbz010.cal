     |ESTADO_CONTROL nBtnActualiza, "DISABLE";
     |ESTADO_CONTROL nBtnNuevoAviso, "DISABLE";
     |ESTADO_CONTROL nBtnNuevaEntre, "DISABLE";
     |ESTADO_CONTROL nBtnDocumento, "DISABLE";

     |HAZ CargaCartera;
     |HAZ PintaPan1;
     |HAZ PintaPan2;
     |HAZ PintaPan3;
     |HAZ PintaPan4;
     |HAZ PintaPan5;
     |HAZ PintaPan6;
     |HAZ PintaPan7;
     ||...

     |SI eaAuto = "1";
          |HAZ Posiciona;
     |SINO;
          aCliente = PARAMETRO; |QBF aCliente;
          |SI aCliente ! "";
               #0#0 = aCliente;
               eaAuto = "1"; |HAZ BtnActualiza;
          |FINSI;
     |FINSI;

     eaAuto = ""; || salida normal del programa

     eaCliDoc = #0#0;
     |PINDA #0#0;
     |ESTADO_CONTROL nBtnActualiza, "ENABLE";
     |PARA; |SI; |HACIENDO;
          |ENDATOS #1#0;
          |SI FSalida ! 0; |SAL; |FINSI;
     |SIGUE;

     |PULSATECLA;        || sin esto se cuelga
     |HAZ GrabaTxt;
