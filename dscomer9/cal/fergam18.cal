|FICHEROS;
     fergam18;

     fergaw13,MANTE;
|FIN;

|VARIABLES;
     nPanta     = 0;
     nBtn1      = 0;
     nBtn2      = 0;
     nFSalida   = 0;

     &eaCliente = "";
     &enDirEnt  = 0;
|FIN;

|PROCESO T12ferm18;  |TIPO 12;
|FPRC;

|PROCESO Comentarios;
     nFSalida = FSalida;

     |PUSHV 0501 2380;

     |PINPA #0#fergaw13;


     |SI nBtn1 = nFSalida;
         #fergaw13#0 = #fergam18#5;
         #fergaw13#1 = #fergam18#6;
         #fergaw13#2 = #fergam18#7;
     |FINSI;

     |SI nBtn2 = nFSalida;
         #fergaw13#0 = #fergam18#8;
         #fergaw13#1 = #fergam18#9;
         #fergaw13#2 = #fergam18#10;
     |FINSI;

     |PINDA #0#fergaw13;
     |ENDATOS #1#fergaw13;
     |SI FSalida = 0;
         |SI nBtn1 = nFSalida;
             #fergam18#5 = #fergaw13#0;
             #fergam18#6 = #fergaw13#1;
             #fergam18#7 = #fergaw13#2;
         |FINSI;

         |SI nBtn2 = nFSalida;
             #fergam18#8  = #fergaw13#0;
             #fergam18#9  = #fergaw13#1;
             #fergam18#10 = #fergaw13#2;
         |FINSI;
     |FINSI;

     |POPV;
|FPRC;

|PROGRAMA;
     |DDEFECTO #fergam18;
     |ABRE #fergam18;
     #fergam18#0 = eaCliente;
     #fergam18#1 = enDirEnt;
     |LEE 101#fergam18.=;
     |SI PARAMETRO = "BAJA";
          |SI FSalida = 0; |BORRA 020#fergam18.a; |FINSI;
     |SINO;
          |SI FSalida ! 0; |GRABA 020#fergam18.b; |FINSI;

          |PINPA #0#fergam18;  nPanta = FSalida;
          |PINDA #0#fergam18;

          |CONTROL_SIMPLE nPanta, "BOTON,{{197}}", 1934, 1936, Comentarios;
          nBtn1 = FSalida;

          |CONTROL_SIMPLE nPanta, "BOTON,{{197}}", 2034, 2036, Comentarios;
          nBtn2 = FSalida;

          FSalida = "::ENDATOS::SINBOTONES::";
          |ENDATOS #1#fergam18;
          |SI FSalida = 0; |GRABA 020#fergam18.a; |FINSI;

          |FIN_CONTROL_WINDOWS nBtn1;
          |FIN_CONTROL_WINDOWS nBtn2;
     |FINSI;
     |CIERRA #fergam18;
|FPRO;
