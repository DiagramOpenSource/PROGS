|FICHEROS;
     agi00017 #0;
     agifa081 #1;        || Pedidos de compras(C).
     agifa067 #2;        || Pedidos de compras(L).
|FIN;

|VARIABLES;
     &aDivisa  = "";
     &aMoneda  = "";
     informe   = "";
     provant   = "";
|FIN;

|PROCESO implin;
     ||컴컴컴컴컴컴컴컴컴컴컴Modulo 002743
     |HAZ EsGuerola;
     |SI FSalida = 0;
          |HAZ GuerImpPed;
     |FINSI;
     ||컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴
     |PRINT;
|FPRC;


|DEFBUCLE leelin;
     #2#1;
     ;
     #1#25,#1#0,001;
     #1#25,#1#0,999;
     ;
     NULL,implin;
|FIN;

|PROCESO imprime;
     aDivisa = #1#27;
     aMoneda = #1#29;
     |HAZ Divisas081;
     |QBF provant;
     |SI provant = ""; provant = #1#4; |FINSI;
     |SI provant ! #1#4;
          |PIEINF;
          |BUCLE leelin;
     |SINO;
          |BUCLE leelin;
     |FINSI;
|FPRC;

|DEFBUCLE lee_pedidos;
     #1#2;
     25,0,1;
     #0#0, #0#2,#0#4,#0#6;
     #0#1, #0#3,#0#5,#0#7;
     ;
     NULL,imprime;
|FIN;

_________________________________________________________________/ PRINCIPIO.
|PROCESO inicio; |TIPO 3;
          |IMPRE 0;
          |SI FSalida = 0;
               informe = #0#8;
               |QBF informe;
               |SI informe = ""; informe = "agi00017"; |FINSI;
               |INFOR informe;
               |SI FSalida = 0;
                    |BUCLE lee_pedidos;
                    |PIEINF;
                    |FININF;
               |FINSI;
               |FINIMP;
          |FINSI;
|FPRC;

|PROGRAMA;
     |HAZ SacaIVA;
     |HAZ MiraLogos; || Copia Logos para el Crystal
     |MANTE #0;
|FPRO;
