|PROCESO ProcesaCampo;
     nNume1 = &aField;
     |SI nNume1 = 13;         || final linea DOS
          #1#nCampo# = aCampo;
          nCampo = nCampo + 1;
          aCampo = "";
          |GRABA 020#1n;
          nCampo = 0;
          aCampo = "";
          |PINTA 1310, #1#0;
          nCuantos = nCuantos + 1;
          |DDEFECTO #1;
          |ACABA;
     |FINSI;
     |SI nNume1 = 10;         || final linea UNIX
          |ACABA;
     |FINSI;

     |SI aField = "@";
          #1#nCampo# = aCampo;
          nCampo = nCampo + 1;
          aCampo = "";
          |ACABA;
     |FINSI;
     |SI aField = "#";|Y nCampo ! 1;         || multivalor
               || en la descripcion pueden venir "#"
          |ACABA;
     |FINSI;
     |SI aField = "|";                       || subvalor
          |ACABA;
     |FINSI;

     aCampo = aCampo + aField;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     |DELINDEX #1;
     |ABRE #1;
     aAlfa1 = #0#0; |QBF aAlfa1;
     aFabre = "rb  " + aAlfa1;
     |FABRE aFabre;
     |SI FSalida ] 0;
          aFcanl = FSalida;
          aField = aFcanl + " 1";
          |FLEE aField;
          |PARA; |SI FSalida = 1; |HACIENDO;
               |HAZ ProcesaCampo;
               aField = aFcanl + " 1";
               |FLEE aField;
          |SIGUE;
          |FCIERRA aFcanl;
          aAlfa1 = "0000Importadas [" + nCuantos + "] formas de pago";
          |MENAV aAlfa1;
     |SINO;
          aAlfa1 = "0000[" + (aFabre % 5) + "] inaccesible.";
          |MENAV aAlfa1;
     |FINSI;
     |CIERRA #1;
|FPRC;
