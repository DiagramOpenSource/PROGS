|FICHEROS;
     dscam003@;
|FIN;

|VARIABLES;
     aAcc = "";
     aRec = "";
     aEmp = "";

     aAlfa = "";
     aPathDat = "";
     aPathDef = "";
     aPathPan = "";
|FIN;

|PROGRAMA;
     aAcc = PARAMETRO % 101;
     aEmp = PARAMETRO % 205;
     aRec = PARAMETRO % 709;

     |SI aAcc = "I";
          aAlfa = "0000Impresion recibo:" + aRec;
          |MENAV aAlfa;
     |FINSI;
     |SI aAcc = "V";
          |DBASS aAlfa;
          aPathDef = aAlfa + "dscarter/def/";
          aPathPan = aAlfa + "dscarter/pan/";
          aPathDat = aAlfa + "dscarter/fich/" + aEmp + "/";
          aAlfa = aPathDef + "dscam003";
          |CARGA_DEF aAlfa, dscam003@;
          |SI FSalida = 0;
               |PATH_DAT #dscam003@#aPathDat#;
               |PATH_PAN #dscam003@#aPathPan#;
               |CLS;
               |ABRE #dscam003@;
               #dscam003@#40 = aRec;
               |LEE 020#dscam003@.=;
               |SI FSalida = 0;
                    |PINPA #0#dscam003@;
                    |PINDA #0#dscam003@;
                    |PAUSA;
               |FINSI;
               |CIERRA #dscam003@;
               |DESCARGA_DEF #dscam003@;
          |FINSI;
     |FINSI;
|FPRO;
