|FICHEROS;
     albz0009 #0;
     albm0004 #4;
     albw0010 #900;
     agifa324 #324;
|FIN;

|VARIABLES;
     &EURODB = 0;
     nPanta   = 0;
     nGrid    = 0;
     nRango   = 0;
     nBoton1  = 0;
     nSwBoton = 0;
     nTecla   = 0;
     nLectura = 0;
     nWid     = 0;
     nId      = 0;
     nSid     = 0;
     nWidGuarda = 0;
     nIdGuarda  = 0;
     nSidGuarda = 0;

     aAlfa    = "";

     &enDobleClik = 0;
     &efFechaPre  = @;
     &eaHora      = "";
     &enDesdeX = 0;
|FIN;

|PROCESO Adjudica;
     |DDEFECTO #900;
     |PINPA #0#900;
     |PINDA #0#900;
     |ENDATOS #1#900;
|FPRC;

|PROCESO Boton1;
     nSwBoton = 1;
     |PTEC 807;
|FPRC;

|PROCESO PintaCliente;  |TIPO 7;
     aAlfa = #4#14;
     aAlfa = aAlfa % 130;

     nLectura = 1;
     nWid     = nWidGuarda;
     nId      = nIdGuarda;
     nSid     = nSidGuarda;
     |PCONTEXTO nLectura, nWid, nId, nSid;

     |ATRI R;
     |PINTA 0765, aAlfa;
     |ATRI r;

     nLectura = 1;
     |PCONTEXTO nLectura, nWidGuarda, nIdGuarda, nSidGuarda;
|FPRC;

|PROCESO Evento4;
     nTecla   = FSalida;
     |SI nTecla = 1;
         |LEE 040#4=;
         |SI FSalida ! 0;  |DDEFECTO #4;  |FINSI;
     |FINSI;

     |SI nTecla = 4;
         |LEE 040#4=;
         |SI FSalida ! 0;  |ACABA;  |FINSI;

         enDobleClik  = 1;
         efFechaPre   = #4#0;
         eaHora       = #4#1;

         |DDEFECTO #900;
         #900#0 = #4#2;
         #900#1 = #4#14;
         |HAZ Tipo12w10;

         |SI #4#15 = "S";
             |REFRESCACONTROL;
         |SINO;
             FSalida = "SKIPDEFAULT";
         |FINSI;
     |FINSI;

     |HAZ PintaCliente;
|FPRC;

|PROCESO Tipo80;  |TIPO 80;
     |ABRET #4;

     FSalida = 2899;
|FPRC;

|PROCESO Tipo90;  |TIPO 90;
     |CIERRAT #4;
|FPRC;

|PROGRAMA;
     enDesdeX = 1;
     |CLS;

     |ABRE #agifa324;
     |DDEFECTO #agifa324;
     #agifa324#0 = "EUR";
     |LEE 000#agifa324.=;
     EURODB = #agifa324#9;
     |CIERRA #agifa324;

     |PARA;  |SI;  |HACIENDO;
          |PINPA #0#0;
          nPanta = FSalida;

          nLectura = 0;
          |PCONTEXTO nLectura, nWidGuarda, nIdGuarda, nSidGuarda;


          ||TODO CCC. PETA POR EL ICONO 166
||          |CONTROL_SIMPLE nPanta,"BOTON,{{166}} Adjudicar a Pedidos Ventas", 0570, 0595, Boton1;
             |CONTROL_SIMPLE nPanta,"BOTON,Adjudicar a Pedidos Ventas", 0570, 0595, Boton1;
          nBoton1  = FSalida;

          nSwBoton   = 0;
          nRango     = 1 + 4 + 8 + 16 + 128;
          |LINEAL_SIMPLE #1#4, nRango, 0902, 2598, NULL, NULL, Evento4;

          |DESTRUYECONTROL nBoton1;
          |PULSATECLA;

          |SI nSwBoton = 0;  |SAL;  |FINSI;

          |HAZ Adjudica;
     |SIGUE;
|FPRO;

|PROCESO PintaCli; |TIPO 0;
     |HAZ PintaCliente;
|FPRC;
