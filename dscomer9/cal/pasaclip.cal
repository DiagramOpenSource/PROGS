||Pasa clientes/proveedores a la Basica o la Integral.
||se usa en Compaq, pero se puede usar en cualquier aplicacion/programa


|FICHEROS;
     pasaclip #0;

     agifa142 #142;
|FIN;

|VARIABLES;
   &PRMNT_CliPro = "";
   &PRMNT_CodCliPro = "";

   &eaClPr      = "";
   &eaCodigoNum = "";
   &enSwRegraba = 0;          ||Esta variable, sirve para REGRABAR registros
                              ||existentes en INTEGRAL|BASICA. (en resgistro de NIFS)

   aAlfa = "";
|FIN;

|PROGRAMA;
     |ABRE #agifa142;
     |LEE 000#agifa142.p;
     |SI FSalida ! 0; |DDEFECTO #agifa142; |FINSI;
     |CIERRA #agifa142;

     eaClPr = PRMNT_CliPro;
     |QBF eaClPr;
     eaCodigoNum = PRMNT_CodCliPro;
     |QBF eaCodigoNum;

     |SI #agifa142#208 = "S";
          aAlfa = "dsz00097;" + eaClPr + eaCodigoNum;  || Alta contabilidad y basica
          |SUB_EJECUTA_NP aAlfa;
     |SINO;
          enSwRegraba = 1;
          |SUB_EJECUTA_NP "agifa141";
          enSwRegraba = 0;
     |FINSI;
|FPRO;
