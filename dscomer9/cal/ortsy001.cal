|PROCESO ContaEtiqueta_1;
     |SI #0#12 = "S";
          |ABRE #13;
          |SELECT #2#13;
          #13#1 = #11#0;
          #13#2 = #11#1;
          |LEE 000#13=;
          |SI FSalida = 0; |CIERRA #13; |ACABA; |FINSI;
          |CIERRA #13;
     |FINSI;

     fFecha = "01.01.0000";

     |ABRE #13;
     |SELECT #1#13;
     #13#0 = fFecha;
     #13#1 = #11#0;
     #13#2 = #11#1;
     |LEE 000#13=;
     |SI FSalida ! 0;
          |DDEFECTO #13;
          #13#0 = fFecha;
          #13#1 = "";
          #13#2 = 0;
          |LEE 101#13=;
          |SI FSalida ! 0; |GRABA 020#13b; |FINSI;
          #13#3 = #13#3 + 1;
          |SI #13#3 > 999;
               #13#3 = 1;
          |FINSI;
          |GRABA 020#13a;

          #13#0 = fFecha;
          #13#1 = #11#0;
          #13#2 = #11#1;

          #13#4 = "N";
          #13#5 = "N";

          fFecha = ~;
          f = fFecha % A1;
          aAlfa = f % 1102;
          aAlfa = aAlfa > "";
          |SI aAlfa = "LU"; #13#6 = "P"; |FINSI;
          |SI aAlfa = "MA"; #13#6 = "R"; |FINSI;
          |SI aAlfa = "MI"; #13#6 = "V"; |FINSI;
          |SI aAlfa = "JU"; #13#6 = "Z"; |FINSI;
          |SI aAlfa = "VI"; #13#6 = "A"; |FINSI;
          |SI aAlfa = "SA"; #13#6 = "A"; |FINSI;
          |SI aAlfa = "DO"; #13#6 = "A"; |FINSI;

          |GRABA 020#13n;
     |FINSI;
     |CIERRA #13;
|FPRC;

