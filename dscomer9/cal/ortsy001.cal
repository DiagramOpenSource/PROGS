|PROCESO PedCombiUni;
     |SI nCampo [ 25;
          |SI aCombi = "";
               aAlfa = #118#4 + " a ";
               aCombi = #118#5;
               aTira = #118#6;
          |FINSI;
          |SI aCombi ! #118#5; |O aTira ! #118#6;
               aAlfa = aAlfa + aFin;
               #11nCampo = aAlfa;
               nCampo = nCampo + 1;
               |GRABA 020#11a;
               aLon = aAlfa % A0;
               |SI aLon > 50; |Y nCampo [ 25;
                    aAlfa = aAlfa % 51;
                    #11nCampo = aAlfa;
                    nCampo = nCampo + 1;
                    |GRABA 020#11a;
               |FINSI;

               aAlfa = #118#4 + " a ";
               aCombi = #118#5;
               aTira = #118#6;
          |FINSI;
          aFin = #118#4 + " " + #117#5; |QBF aFin;
          aFin = aFin + "-" + #118#5;
          |SI #118#6 ! "    ";
               aFin = aFin + "-" + #118#6;
          |FINSI;
     |FINSI;
|FPRC;

