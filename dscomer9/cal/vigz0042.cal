               |C_M #8para1,1,"S"; |C_V #8para1,1,"S";
               |PINTA #8para1;
         |SIGUE;
     |SINO;
         |PARA para1 = 18; |SI para1 [ 37; |HACIENDO para1 = para1 + 1;
               |C_M #8para1,1,"N"; |C_V #8para1,1,"N";
         |SIGUE;
