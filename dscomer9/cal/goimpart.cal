|FICHEROS;
     goimpart #0;
     goparame #1;                                 ||Parametros Obra.
     agifa019 #2;                                 ||Materiales Obra.
     agifa060 #3;                                 ||Familias.
     goartive #4;                                 ||SubFamilias.
     agifa120 #5;                                 ||Linea(Seccion).
     agifa553 #6;
|FIN;
----------------------------------------------------------------------
|VARIABLES;
     cTexto         = "";
     cCodigo        = "";

     &cCodMonBase   = "";
     &cCodMonTrab   = "";
     &cCodMonDe     = "";
     &cCodMonA      = "";
     &cCodMonConv   = "";
     &cCodMonActu   = "";
     &cDesMonBase   = "";
     &cDesMonTrab   = "";
     &nDeci_PBase   = 0;
     &nDeci_TBase   = 0;
     &nDeci_PTrab   = 0;
     &nDeci_TTrab   = 0;
     &nDeci_TotVis  = 0;
     &nEurComTrab   = 0;
     &nEurComBase   = 0;
     &nEurVenTrab   = 0;
     &nEurVenBase   = 0;
     &nPreComTrab   = 0;
     &nPreComBase   = 0;
     &nPreVenTrab   = 0;
     &nPreVenBase   = 0;
     &nPrecioComp   = 0;
     &nPrecioVent   = 0;

|FIN;
----------------------------------------------------------------------
|PROCESO MiraSecc;
     |ABRE #6;
     |DDEFECTO #6;
     #6#0 = #4#7;                                 ||Codigo Seccion.
     |LEE 101#6=;

     |SI FSalida ! 0;
          #6#1 = #4#8;                            ||Descripcion SubFamilia;
          |GRABA 020#6c;
     |FINSI;

     |LIBERA #6;
     |CIERRA #6;
|FPRC;
----------------------------------------------------------------------
|PROCESO MiraSubf;
     |ABRE #5;
     |DDEFECTO #5;
     #5#0 = #4#11;                                ||Codigo SubFamilia.
     |LEE 101#5=;

     |SI FSalida ! 0;
          #5#1 = #4#12;                           ||Descripcion SubFamilia;
          |GRABA 020#5c;
     |FINSI;

     |LIBERA #5;
     |CIERRA #5;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO MiraFami;
     |ABRE #3;
     |DDEFECTO #3;
     #3#0 = #4#9;                                 ||Codigo Familia.
     |LEE 101#3=;

     |SI FSalida ! 0;
          |PUSHV 01012380;
          |CLS;
          #3#1 = #4#10;                           ||Descricpcion de Familia.
          |PINPA #0#3;
          |PINDA #0#3;
          |ENDATOS #1#3;

          |SI FSalida = 0;
               |GRABA 020#3c;
          |FINSI;

          |POPV;
     |FINSI;

     |LIBERA #3;
     |CIERRA #3;
|FPRC;
-------------------------------------------------------------------------------
|PROCESO GrabaArti;

     |SI #4#6 = " ";
          |ACABA;
     |FINSI;

     |SI #4#6=#0#1;|O #4#6=#0#2;|O #4#6=#0#3;|O #4#6=#0#4;|O #4#6=#0#5;|O #4#6=#0#6;|O #4#6=#0#7;|O #4#6=#0#8;

          cCodigo = #4#0;
          |QBF cCodigo;
          cCodigo = cCodigo % -101;

          |SI cCodigo = "$";
               |ACABA;
          |FINSI;

          |DDEFECTO #2;
          #2#0 = #4#0;                                 ||Código Material.
          |LEE 101#2=;

          |SI FSalida ! 0;
               |GRABA 020#2c;
          |FINSI;

          #2#1 = #4#1;                                 ||Descripcion.
          |HAZ MiraFami;
          #2#2 = #4#9;                                 ||Familia.
          |HAZ MiraSubf;
          #2#3 = #4#11;                                ||Subfamilia.
          #2#5 = #4#2;                                 ||Unidad Medida.
          |HAZ MiraSecc;
          #2#125 = #4#7;                               ||Seccion.
          #2#127 = #4#13;                              ||Capitulo.
          #2#134 = #4#0;                               ||Cod. Proveedor.
          #2#195 = #4#1;                               ||Descripcion Proveedor.

          |SI #0#17 = "S";
               cCodMonDe = #4#5;
               cCodMonA  = cCodMonBase;
               nPrecioComp = #4#15;
               |HAZ DemonAmon;

               #2#28  = nPrecioComp;                   ||Ultimo Precio Proveedor.
               #2#29  = 0;                             ||Ultimo Dto% 1
               #2#129 = 0;                             ||Ultimo Dto% 2
               #2#209 =0;
               #2#133 = #2#28 < #2#29;                 ||Ultimo Precio Neto.
               #2#133 = #2#133 < #2#129;
               #2#133 = #2#133 < #2#209;
               #2#9   = nPrecioComp;                   ||Precio Medio de Compra.
               #2#126 = #4#6;                          ||Tipo Articulo.
          |FINSI;

          |SI #0#18 = "S";
               #2#105 = #1#21;                         ||% Aumento PVP1.
               #2#18 = #2#9 > #1#21;                   ||Precio Venta 1.
          |FINSI;

          |GRABA 020#2a;
          cTexto = "Importando Articulo: " + #2#0;
          |PINTA 0608, cTexto;
     |FINSI;
|FPRC;
-------------------------------------------------------------------------------
|DEFBUCLE PasaArti;
     #4#6;
     ;                                            ||Campos Extras.
     " ";
     "z";
     ;                                            ||b/e Bolqueo-Ordenacion.
     NULL, GrabaArti;
|FIN;
-------------------------------------------------------------------------------
|PROCESO Entrada; |TIPO 0;

     |SI #0#0 = "S";
          |HAZ MonBas;
          |ABRE #1;
          #1#0 = "A";
          |LEE 000#1=;

          |SI FSalida = 0;
               |ABRE #2;
               |BUCLE PasaArti;
               |LIBERA #2;
               |CIERRA #2;
          |SINO;
               |MENAV "0000NO ESTAN DEFINIDOS LOS PARAMETROS DE OBRA";
          |FINSI;

          |CIERRA #1;

     |FINSI;

     |PTEC 802;
|FPRC;
----------------------------------------------------------------------
