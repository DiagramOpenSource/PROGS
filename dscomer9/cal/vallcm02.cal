|FICHEROS;
     vallcm02 #0;
|FIN;

|VARIABLES;
     aAlfa = "";
|FIN;

|PROCESO SoloFija; |TIPO 7;
     |SI #0#2 = "F";
          aAlfa = #0#3; |QBF aAlfa;
          |SI aAlfa = "Automatico";
               #0#3 = "";
          |FINSI;
          |C_M #0#3, 1, "S";
          |PINTA #0#3;
     |SINO;
          |C_M #0#3, 1, "N";
          #0#3 = "Automatico";
          |PINTA #0#3;
     |FINSI;
|FPRC

|PROCESO DSCorreo; |TIPO 0;
     |QUE_SISTEMA aAlfa;
     |SI aAlfa = "ESBSD";|Y #0#2 = "S";
          |MENAV "    Configuracion no permitida, servidor UNIX. Recomendado: [F]";
          |ERROR;
     |FINSI;
|FPRC;

|PROGRAMA;
     |ABRE #0;
     |LEE 101#0p;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0; |GRABA 020#0a; |FINSI;
     |CIERRA #0;
|FPRO;
