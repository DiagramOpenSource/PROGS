     |SI cTipo = "~C"; |Y #13#1 = "S";
          |PINTA 1732, "TIPO (~C)";
          |HAZ TipoC;
     |FINSI;
     |SI cTipo = "~D"; |Y #13#2 = "S";
          |PINTA 1732, "TIPO (~D)";
          |HAZ TipoD;
     |FINSI;
     |SI cTipo = "~T"; |Y #13#3 = "S";
          |PINTA 1732, "TIPO (~T)";
          |HAZ TipoT;
     |FINSI;

     #13#5 = #1#0;
     #13#6 = nSuma;
     |PINTA #13#5;
     |PINTA #13#6;

