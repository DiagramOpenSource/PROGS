                              #9#2 = cCad;
                              |GRABA 020#9c;
                         |FINSI;
                         |SI nMira = 0; |Y cCad = "%";
                              #9#2 = cCad;
                              |GRABA 020#9c;
                         |FINSI;
                         nColumna  = 1;
                         cPrinFila = nFila;
                    |FINSI;
                    cFila    = "00" + nFila;
