                    cColumna = cColumna % -102;
                    nFila    = nFila + 1;
                    nColumna = nColumna + 1;
                    cPosicion = cFila + "01";
                    cCaracter = cCadena % cPosicion;
               |SIGUE;
               nColumna  = nColumna - 2;
               cColumna  = "00" + nColumna;
               cColumna  = cColumna % -102;
               cPosicion = cPrinFila + cColumna;
               cPrecio   = cCadena % cPosicion;
               |HAZ ContraValor;
