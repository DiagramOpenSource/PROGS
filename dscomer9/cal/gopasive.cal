                    cPosicion = cFila + "01";
                    cCaracter = cCadena % cPosicion;
               |SIGUE;
          |FINSI;
     |FINSI;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO Recalculo;
     |LEE 010#1p;
     |PARA; |SI FSalida = 0; |HACIENDO;
          #1#4 = "N";
          |GRABA 020#1a;
          nLinea = 1;
          |PARA; |SI FSalida = 0; |HACIENDO nLinea = nLinea + 1;
               #9#0 = #1#0;
               #9#1 = nLinea;
               |LEE 010#9=;
               |SI FSalida = 0;
                    #1#4 = "S";
                    |GRABA 020#1a;
                    |HAZ Busca;
               |FINSI;
          |SIGUE;
          |LEE 010#1s;
     |SIGUE;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO Busca;
     cIveAnt = #1#0;
     #1#0 = #9#2;
     |LEE 010#1=;
     |SI FSalida = 0;
          #9#3 = #1#1;
          #9#4 = #1#2;
          #9#6 = #1#15;
          #9#7 = #9#5 * #9#6;
          #9#8 = #1#6;
          #9#9 = #1#16;
          |GRABA 020#9a;
     |FINSI;
     #1#0 = cIveAnt;
     |LEE 010#1=;
     |SI FSalida ! 0;
          |ACABA;
     |FINSI;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO MiraMoneda;
     #10#0 = "A";
     |LEE 010#10=;
     |SI FSalida = 0;
          cCodMon = #10#9;
          #11#0 = #10#9;
          |LEE 010#11=;
          |SI FSalida = 0;
               nBase = #11#2;
          |FINSI;
          #11#0 = #0#2;
          |LEE 010#11=;
          |SI FSalida = 0;
               nIve = #11#2;
          |FINSI;
     |FINSI;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO ContraValor;
     nPrecio = cPrecio;
     nPrecio = nPrecio / nIve;
     nPrecio = nPrecio * nBase;
     #1#15   = nPrecio;
     #1#5    = cCodMon;
|FPRC;
-----------------------------------------------------------------------------
|PROCESO TipoCompo;
     |SI cCaracter = "|";
          cCaracter = "0";
     |FINSI;
     #12#0 = cCaracter;
     |LEE 010#12=;
     |SI cCaracter = "0";
          |SI FSalida ! 0;
               #12#1 = "Sin clasificar";
               |GRABA 020#12n;
          |FINSI;
     |FINSI;
     |SI cCaracter = "1";
          |SI FSalida ! 0;
               #12#1 = "Mano de obra";
               |GRABA 020#12n;
          |FINSI;
     |FINSI;
     |SI cCaracter = "2";
          |SI FSalida ! 0;
               #12#1 = "Maquinaria y medios auxiliares";
               |GRABA 020#12n;
          |FINSI;
     |FINSI;
     |SI cCaracter = "3";
          |SI FSalida ! 0;
               #12#1 = "Materiales";
               |GRABA 020#12n;
          |FINSI;
     |FINSI;
     #1#6  = #12#0;
     #1#16 = #12#1;
|FPRC;
-----------------------------------------------------------------------------
