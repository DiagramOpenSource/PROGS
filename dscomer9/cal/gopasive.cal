|PROCESO TipoT;

     ||Extraemos el codigo del Articulo.

     nColumna  = 1;
     cCaracter = "";
     cCodigo = "";
     |PARA nFila = 4; |SI; |HACIENDO nFila = nFila + 1;
          cFila     = "000" + nFila;
          cFila     = cFila % -103;
          cPosicion = cFila + "01";
          cCaracter = cCadena % cPosicion;
          |SI cCaracter = "|";
               |SAL;
          |SINO;
               cCodigo = cCodigo + cCaracter;
          |FINSI;
     |SIGUE;

     #2#0 = cCodigo;
     |LEE 010#2=;
     |SI FSalida = 0;
          |ACABA;
     |FINSI;

     #1#0 = cCodigo;
     |LEE 010#1=;

     |SI FSalida = 0;
          #2#1 = #1#1;
     |FINSI;

     #3#0 = #2#0;
     #3#1 = 1;
     |LEE 010#3=;

     |SI FSalida ! 0;

          |QBF cCadena;
          cColumna = cCadena % 0;
          nColumna = cColumna;
          cDesLin = "";

          |PARA nFila = nFila; |SI nFila [ nColumna ;|HACIENDO nFila = nFila + 1;

               cFila     = "000" + nFila;
               cFila     = cFila % -103;
               cPosicion = cFila + "01";
               cCaracter = cCadena % cPosicion;

               |SI cCaracter ! "|";
                    cDesLin = cDesLin + cCaracter;
               |FINSI;

               cTamano = cDesLin % 0;
               nTamano = cTamano;

               |SI nTamano = 60;
                    |HAZ GrabaLin;
               |FINSI;

               |SI nFila = nColumna; |Y cCaracter ! "|";
                    cCadena   = f + " " + 250;
                    |FLEE cCadena;
                    |QBF cCadena;
                    cColumna = cCadena % 0;
                    nColumna = cColumna;
                    nFila = 2;
               |FINSI;

               |SI nFila = nColumna; |Y cCaracter = "|";
                    |HAZ GrabaLin;
               |FINSI;

          |SIGUE;
     |FINSI;

     |GRABA 020#2c;

|FPRC;
