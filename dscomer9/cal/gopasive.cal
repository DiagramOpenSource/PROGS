               cFila     = "00" + nFila;
               cFila     = cFila % -102;
               cPrinFila = cFila;
               cPosicion = cFila + cColumna;
               cCaracter = cCadena % cPosicion;
               |PARA; |SI cCaracter ! "|"; |HACIENDO;
                    cFila    = "00" + nFila;
                    cFila    = cFila % -102;
