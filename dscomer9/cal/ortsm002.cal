|FICHEROS;
     ortsm002 #0;
     ortsm003 #1;
     ortsm004 #2;
|FIN;

|VARIABLES;
     nModo     = 0;
|FIN;

|PROCESO Min;
     #2#0 = #1#0;
     #2#1 = #1#1;
     #2#2 = #1#2;
     #2#3 = "                    ";
|FPRC;

|PROCESO Max;
     #2#0 = #1#0;
     #2#1 = #1#1;
     #2#2 = #1#2;
     #2#3 = "zzzzzzzzzzzzzzzzzzzz";
|FPRC;

|PROCESO Tecla; |TIPO 11;
     |SI FSalida = 13;
          |SI #1#12 = "S";
               |PUSHV 0501 2380;
               |ENTLINEAL #1#2, -3, 4, 22, 1, Min, Max;
               |POPV;
          |SINO;
               |MENAV "0000Articulo sin desglose...";
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Borra;
     |BORRA 020#2a;
|FPRC;

|DEFBUCLE Borra;
     #2#1;
     ;
     #1#0, #1#1, #1#2, "                   ";
     #1#0, #1#1, #1#2, "zzzzzzzzzzzzzzzzzzzz";
     be;
     NULL, Borra;
|FIN;

|PROCESO Tipo2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |BUCLE Borra;
     |FINSI;
|FPRC;
