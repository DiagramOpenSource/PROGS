|FICHEROS;
     guerz023 #0;
     gomantho #1;
     gomantl1 #2;
     gopre003 #3;
     gopartid #5;
|FIN;

|VARIABLES;
     aAlfa = "";
     nLinea = 0;
     nConta = 0;
|FINS;

|PROCESO gopre003;
     nLinea = nLinea + 1;
     #5#0 = #2#3;        ||Serie Pres.
     #5#1 = #2#4;        ||N§. Pres.
     #5#2 = #1#0;        ||Serie Expe.
     #5#3 = #1#1;        ||N§. Expe.
     #5#4 = nLinea;      ||N§. Linea
     #5#5 = #3#4;        ||Partida
     #5#6 = #3#5;        ||Desc. Partida.
     #5#7 = #3#10;       ||Importe Venta.
     #5#8 = #3#11;       ||Importe Coste.
     #5#9 = #3#16*#3#7;  ||Cantidad MO.
     #5#14 = #3#7;       ||Cantidad Partida.
     |GRABA 000#5n;
     |SI FSalida = 0;
          nConta = nConta + 1;
     |FINSI;
|FPRC;

|DEFBUCLE gopre003;
     #3#1;
     ;
     #2#3, #2#4, "                    ", 001;
     #2#3, #2#4, "zzzzzzzzzzzzzzzzzzzz", 999;
     ;
     NULL, gopre003;
|FIN;

|PROCESO gomantl1;
     nLinea = 0;
     |BUCLE gopre003;
|FPRC;

|DEFBUCLE gomantho;
     #1#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, NULL, gomantl1;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |SI #0#0 = "SI";
          |ABRE #5;
          |BUCLE gomantho;
          |CIERRA #5;

          aAlfa = "0000Generados [" + nConta + "]";
          |MENAV aAlfa;
     |FINSI;
|FPRC;
