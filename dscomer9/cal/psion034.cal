|FICHEROS;
     psion034 #0;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO Situ88; |TIPO 88;
     |ABRE #0;
     |DDEFECTO #0;
     #0#0 = 0;
     |LEE 101#0=;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     #0#1 = "Sin visitar";
     #0#2 = 0;
     |GRABA 020#0a;

     |DDEFECTO #0;
     #0#0 = 1;
     |LEE 101#0=;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     #0#1 = "Visitado y vendido";
     #0#2 = 1;
     |GRABA 020#0a;
     |CIERRA #0;
|FPRC;

|PROCESO Situ2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3; |O nModo = 2;
          |SI #0#0 = 0; |O #0#0 = 1;
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Color; |TIPO 0;
     |SI #0#0 ] 2;
          |SI #0#2 < 2; |O #0#2 > 9;
               |MENAV "0000ERROR, posibles valores 2-9";
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;
