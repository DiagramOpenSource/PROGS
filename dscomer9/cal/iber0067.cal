|FICHEROS;
     iber0066 #0;
     iber0067 #1;
|FIN;

|VARIABLES;
     nModo     = 0;
|FIN;

|PROCESO Calcula; |TIPO 0;
     #1#9 = (#1#6 - #1#7) < #1#8;
     |PINTA #1#9;
|FPRC;

|PROCESO Tipo67_2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;

     |SI #1#11 ! "M";
          |MENAV "0000Tarifa oficial, no se puede modificar...";
          |ERROR;
     |SINO;
          #0#6 = ~;
          #1#10 = #0#6;
          |PINTA #0#6;
          |PINTA #1#10;
     |FINSI;
|FPRC;

|PROCESO Alta0; |TIPO 0;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          #1#11 = "M";
          |PINTA #1#11;
     |FINSI;
|FPRC;
