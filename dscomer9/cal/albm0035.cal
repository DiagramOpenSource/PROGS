|FICHEROS;
     albm0035 #0;
     albm0032 #32;
|FIN;

|VARIABLES;
   nDesc = 0;

   nNueva = 0;
|FIN;

|PROCESO MiraOtros;
   nDesc = Campo + 1;
   |SI #albm0035#Campo# = "S";
      |C_M #albm0035#nDesc#,1,"S";
   |SINO;
      #albm0035#nDesc# = ""; |PINTA #albm0035#nDesc#;
      |C_M #albm0035#nDesc#,1,"N";
   |FINSI;
|FPRC;

|PROCESO Decision;
   |SI #albm0035#Campo# = "S";
      |SI Campo = 39; #albm0035#40 = "N"; #albm0035#41 = "N"; |FINSI;
      |SI Campo = 40; #albm0035#39 = "N"; #albm0035#41 = "N"; |FINSI;
      |SI Campo = 41; #albm0035#39 = "N"; #albm0035#40 = "N"; |FINSI;
      |PINTA #albm0035#39; |PINTA #albm0035#40; |PINTA #albm0035#41;
   |FINSI;
|FPRC;

|PROCESO Tipo12m35;  |TIPO 12;
     || Para que no pegunte el S o N
|FPRC;

|PROCESO Tipo80_35; |TIPO 80;
/*
     FSalida = 3299;
*/
|FPRC;
