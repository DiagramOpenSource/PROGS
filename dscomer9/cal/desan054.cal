|PROCESO TagPostFormado;
     |ABRE #19;
     #19#0 = #5#2;
     |LEE 000#19=;
     |CIERRA #19;
     aEsPostFormaso = "N";
     |DDEF aAlfa;
     aAlfa = aAlfa + "tagm0056";
     |CARGA_DEF aAlfa, tagm0056@;
     |SI FSalida = 0;
          |ABRE #tagm0056@;
          |SI #5#12 ! 0; || Con Pedido
               #tagm0056@#0 = "PV";
               #tagm0056@#1 = #5#31;
               #tagm0056@#2 = #5#12;
               #tagm0056@#3 = #5#21;
               |LEE 000#tagm0056@.=;
               |SI FSalida = 0;
                    descrip = #5#4;
                    nTotal = #5#9;
                    nTotalD = #5#37;
                    |SALVA_FICHA #5;
                    |SELECT #4#5;
                    #5#31 = #tagm0056@#1;
                    #5#12 = #tagm0056@#2;
                    #5#21 = #tagm0056@#5;
                    |LEE 000#5=;
                    |SI FSalida = 0;
                         nTotal = nTotal + #5#9;
                         nTotalD = nTotalD + #5#37;
                         nPrecio = #5#6;
                         nPrecioD = #5#36;
                    |FINSI;
                    |SELECT #1#5;
                    |REPON_FICHA #5;
                    |SI #19#194 = 2;
                         nPrecio = nPrecio + (#5#6 * #5#48 / #5#5);
                         nPrecioD = nPrecioD + (#5#36 * #5#48 / #5#5);
                    |SINO;
                         nPrecio = nPrecio + #5#6;
                         nPrecioD = nPrecioD + #5#36;
                    |FINSI;
                    #5#4 = descrip;
                    #5#6 = nPrecio;
                    #5#36 = nPrecioD;
                    #5#9 = nTotal;
                    #5#37 = nTotalD;
                    precio = #5#6;
               |SINO;
                    |SELECT #2#tagm0056@;
                    #tagm0056@#5 = #5#21;
                    |LEE 000#tagm0056@.=;
                    |SI FSalida = 0;
                         aEsPostFormaso = "S";
                    |FINSI;
               |FINSI;
          |SINO;         || Normal (sin pedido)
               #tagm0056@#0 = "AV";
               #tagm0056@#1 = #5#29;
               #tagm0056@#2 = #5#0;
               #tagm0056@#3 = #5#1;
               |LEE 000#tagm0056@.=;
               |SI FSalida = 0;
                    descrip = #5#4;
                    nTotal = #5#9;
                    nTotalD = #5#37;
                    |SALVA_FICHA #5;
                    #5#1 = #tagm0056@#5;
                    |LEE 000#5=;
                    |SI FSalida = 0;
                         nTotal = nTotal + #5#9;
                         nTotalD = nTotalD + #5#37;
                         nPrecio = #5#6;
                         nPrecioD = #5#36;
                    |FINSI;
                    |REPON_FICHA #5;
                    |SI #19#194 = 2;
                         nPrecio = nPrecio + (#5#6 * #5#48 / #5#5);
                         nPrecioD = nPrecioD + (#5#36 * #5#48 / #5#5);
                    |SINO;
                         nPrecio = nPrecio + #5#6;
                         nPrecioD = nPrecioD + #5#36;
                    |FINSI;
                    #5#4 = descrip;
                    #5#6 = nPrecio;
                    #5#36 = nPrecioD;
                    #5#9 = nTotal;
                    #5#37 = nTotalD;
