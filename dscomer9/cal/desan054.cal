|PROCESO &sacacuenta;|| sustituye los 3 ultimos digitos de eaCuenta por '***'
     eaCuenta = #3#30;
     |QBF eaCuenta;
     aLon = eaCuenta % A0;
     nPos = aLon;
     |SI nPos [ 3;
          eaCuenta = "***";
     |SINO;
          nPos = nPos - 3;
          nPos = 100 + nPos;
          eaCuenta = eaCuenta % nPos;
          eaCuenta = eaCuenta + "***";
     |FINSI;
|FPRC;

|PROCESO BogeReferencia;
     |DDEF aAlfa;
