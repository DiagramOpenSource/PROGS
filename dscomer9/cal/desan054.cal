     |CARGA_DEF aAlfa, bogem027@;
     |SI FSalida = 0;
          |ABRE #bogem027@;
          |SELECT #2#bogem027@;
          #bogem027@#20 = #4#52;
          #bogem027@#21 = #4#0;
          |LEE 000#bogem027@.=;
          |SI FSalida = 0;
               enReferencia = #bogem027@#0;
               enAlbaran = #bogem027@#19;
          |SINO;
               enReferencia = 0;
               enAlbaran = 0;
          |FINSI;
          |CIERRA #bogem027@;
          |DESCARGA_DEF #bogem027@;
     |FINSI;
|FPRC;

