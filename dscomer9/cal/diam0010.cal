|FICHEROS;
     diam0010 #10;
     diam0011 #11;
     diam0012 #12,MANTE;
     diam0014 #14;
|FIN;

|VARIABLES;
     nTecla  = 0;
     nCambio = 0;
     nLineal = 0;
|FIN;

|RUTINA ClaveBaja10;
     #10#0 = 0000;
     #10#1 = 01;
     #10#2 = "  ";
|FRUT;

|RUTINA ClaveAlta10;
     #10#0 = 9999;
     #10#1 = 99;
     #10#2 = "zz";
|FRUT;

|RUTINA ClaveBaja11;
     #11#0 = #10#0;
     #11#1 = #10#1;
     #11#2 = #10#2;
     #11#3 = " ";
     #11#4 = "     ";
     #11#5 = 0;
|FRUT;

|RUTINA ClaveAlta11;
     #11#0 = #10#0;
     #11#1 = #10#1;
     #11#2 = #10#2;
     #11#3 = "z";
     #11#4 = "zzzzz";
     #11#5 = 999999;
|FRUT;

|RUTINA ClaveBaja14;
     #14#0 = 0000;
     #14#1 = 01;
     #14#2 = #10#2;
|FRUT;

|RUTINA ClaveAlta14;
     #14#0 = 9999;
     #14#1 = 99;
     #14#2 = #10#2;
|FRUT;

|PROCESO Tipo7m10;  |TIPO 7;
     |ENTLINEAL #1#11, -3, 7, 14, 0, ClaveBaja11, ClaveAlta11;

     |ABRE #11;
     #11#0 = #10#0;
     #11#1 = #10#1;
     #11#2 = #10#2;
     #11#3 = "";
     #11#4 = 0;
     |LEE 040#11];
     |SI FSalida = 0;  |Y #11#0 = #10#0;  |Y #11#1 = #10#1;  |Y #11#2 = #10#2;
         |PINTA #11#8;   |PINTA #11#9;
         |PINTA #11#10;  |PINTA #11#11;
         |PINTA #11#12;  |PINTA #11#13;
         |PINTA #11#14;  |PINTA #11#15;
         |PINTA #11#16;  |PINTA #11#17;
         |PINTA #11#18;  |PINTA #11#19;
         |PINTA #11#20;  |PINTA #11#21;
         |PINTA #11#22;  |PINTA #11#23;
         |PINTA #11#24;
     |FINSI;
     |CIERRA #11;
|FPRC;

|PROCESO Tipo11m10;  |TIPO 11;
     nTecla = FSalida;
     |SI nTecla = 10;
         |ENTLINEAL #1#11, -3, 4, 14, 0, ClaveBaja11, ClaveAlta11;
         |ERROR;
     |FINSI;

     |SI nTecla = 11;
         |SUB_EJECUTA_NP "diaz0008";
         |PONCONTROL 23, "SI";
         |PTEC 812;
     |FINSI;

     |SI nTecla = 12;
         |PTEC 807;
         nCambio = 1;

         |SI nLineal = 0;
             nLineal = 1;
         |SINO;
             nLineal = 0;
         |FINSI;
     |FINSI;

     |SI nTecla = 13;
         |PUSHV 0501 2380;
         |ENTLINEAL #1#14, -1, 2, 22, 1, ClaveBaja14, ClaveAlta14;
         |POPV;
         |ERROR;
     |FINSI;

     |SI nTecla = 14;
         |ABRE #12;
         #12#0 = #10#2;
         |LEE 040#12=;
         |SI FSalida = 0;
             |PUSHV 0501 2380;
             |PINPA #0#12;
             |PINDA #0#12;
             |ENDATOS #4#12;
         |SINO;
             |MENAV "0000 No hay liquidacion de este Tecnico.";
         |FINSI;
         |CIERRA #12;

         |POPV;
         |ERROR;
     |FINSI;
|FPRC;

|PROGRAMA;
     |CLS;
     |CABEZA "Comisiones";
     |PINPA #0#10;

     |PTEC 806;
     |PTEC 824;
     nCambio = 0;
     nLineal = 0;
     |PARA;  |SI;  |HACIENDO;
          |SI nLineal = 0;
              |C_M #10#2, 1, "N";  |C_M #10#0, 1, "S";

              |ENTLINEAL #1#10, -1, 4, 14, 0, ClaveBaja10, ClaveAlta10;
          |SINO;
              |C_M #10#2, 1, "S";  |C_M #10#0, 1, "N";

              |ENTLINEAL #2#10, -1, 4, 14, 0, ClaveBaja10, ClaveAlta10;
          |FINSI;

          |SI nCambio = 0;  |SAL;  |FINSI;

          nCambio = 0;
     |SIGUE;
|FPRO;
