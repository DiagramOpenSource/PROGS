|FICHEROS;
     guim0007 #0;
|FIN;

|VARIABLES;
     &enSwMail = 0;
     &ser_alb  = "";
     &num_alb = 0;
     nSwEnvia = 0;
     aAlfa = "";
|FIN;

|PROCESO Tecla; |TIPO 11;
     |SI FSalida = 12; || F4
          |LEE 101#0=;
          |SI FSalida = 0;
               |HAZ EnviaPend;
          |FINSI;
     |FINSI;
     |SI FSalida = 13; ||F5;
          |CONFI "0000NEnviar todos los albaranes pendientes";
          |SI FSalida = 0;
               |PTEC 807;
               nSwEnvia = 1;
          |SINO;
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Min;
     #0#0 = "     ";
     #0#1 = 1;
|FPRC;

|PROCESO Max;
     #0#0 = "zzzzz";
     #0#1 = 999999;
|FPRC;

|PROCESO EnviaPend;
     ser_alb = #0#0;
     num_alb = #0#1;
     enSwMail = 1;
     |SUB_EJECUTA_NP "guiz0001";

     #0#5 = ~;
     |HORA aAlfa;
     #0#6 = aAlfa;
     #0#7 = Usuario % 810;
     #0#8 = #0#8 + 1;
     |GRABA 020#0a;
     |LIBERA #0;
|FPRC;

|DEFBUCLE EnviaPend;
     #0#1;
     6;
     "     ", 000001, "        ";
     "zzzzz", 999999, "        ";
     be;
     NULL, EnviaPend;
|FIN;

|PROGRAMA;
     |SI PARAMETRO = "AUTO"; |O PARAMETRO = "auto";
          |BUCLE EnviaPend;
     |SINO;
          |CLS;
          |ENTLINEAL #1#0, 1, 4, 22, 1, Min, Max;
          |SI nSwEnvia = 1;
               |BUCLE EnviaPend;
          |FINSI;
     |FINSI;
|FPRO;
