     IaGet = aGet1<-;
     |ESPECIFICA "GETDATACONTROL", aGet;
     aAlfa = FSalida; |QBT aAlfa;
     #0#0 = aAlfa; |PINTA #0#0;

     |REFERENCIA_DEF aGet1, #0;
     aGet2  = 1;    || numero de campo
     IaGet = aGet1<-;
     |ESPECIFICA "GETDATACONTROL", aGet;
     aAlfa = FSalida; |QBT aAlfa;
     aAlfa = ("000000" + aAlfa) % -106;
     #0#1 = aAlfa; |PINTA #0#1;

     |SI nCampo = 0;
          |SI #0#0 = "                              ";
               #0#2 = "< Todas las m quinas >"; |PINTA #0#2;
          |SINO;
               |ABRE #2;
               #2#0 = #0#0;
               |LEE 000#2=;
               |SI FSalida = 0
                    #0#2 = #2#1; |PINTA #0#2;
                    |SI nSwAct = 0;
                         #0#1 = #2#6; |PINTA #0#1;
                    |FINSI;
                    nCampo = 1;
               |SINO;
                    |MENAV "0000No existe c¢digo de m quina...";
                    |ERROR;
               |FINSI;
               |CIERRA #2;
          |FINSI;
     |FINSI;
