|PROCESO Imprimir;
     Impresora = "CrystalReport";
     |IMPRE -1;
     |SI FSalida = 0;
          |SI aTipo = "M"; aInf = #1#31; |FINSI;
          |SI aTipo = "O"; aInf = #1#32; |FINSI;
          |SI aTipo = "X"; aInf = #1#33; |FINSI;
          |SI aTipo = "D"; aInf = #1#34; |FINSI;
          |SI aTipo = "K"; aInf = #1#35; |FINSI;
          |INFOR aInf;
          |SI FSalida = 0;
               |SI aTipo = "M"; |O aTipo = "X";
                    |SI #0#0 = "                              "; |Y #0#1 = "000000";
                         |BUCLE gesim007_4;
                    |SINO;
                         |SI #0#0 = "                              ";
                              |BUCLE gesim007_2;
                         |SINO;
                              |BUCLE gesim007_3;
                         |FINSI;
                    |FINSI;
               |SINO;
                    |SI #0#0 = "                              "; |Y #0#1 = "000000";
                         |BUCLE gesim022_4;
                    |SINO;
                         |SI #0#0 = "                              ";
                              |BUCLE gesim022_2;
                         |SINO;
                              |BUCLE gesim022_3;
                         |FINSI;
                    |FINSI;
               |FINSI;
               |FININF;
          |SINO;
               aAlfa = "0000Error en informe:" + aInf;
               |MENAV aAlfa;
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;

