|FICHEROS;
     dsz00090 #0;
     agifa513 #513;
     agifa572 #572;
     agifa501 #501;
|FIN;

|VARIABLES;
     aSesion = "";
     aConta = "";
     aAlfa = "";
     aRes = "";
     nPos = 0;
     i = 0;
     aCli = "";
     aLin = "";
     aLon = "";
|FIN;

|PROCESO Corrige;
     #501#36 = aSesion;
     #501#0 = aConta;
     |LEE 000#501=;
     |SI FSalida = 0;
          |PRINT;
          #513#19 = #501#35;
          #513#20 = #501#57;
          #513#21 = #501#58;
          |GRABA 020#513a;
     |FINSI;
|FPRC;


|PROCESO Cobros;
     aAlfa = #513#3; |QBF aAlfa;
     aRes = aAlfa - "Cobro de credito ";
     |SI aRes = aAlfa;
          aRes = aAlfa - "Credito ";
          |SI aRes ! aAlfa;
               aCli = aRes % 106;
               aRes = aRes % 8;
               aLon = aRes % A0;
               aLin = "";
               |PARA i = 1; |SI i [ aLon; |HACIENDO i = i + 1;
                    nPos = i * 100 + 1;
                    aAlfa = aRes % nPos;
                    |SI aAlfa = "/"; |SAL; |FINSI;
                    aLin = aLin + aAlfa;
               |SIGUE;
               #572#0 = aCli;
               #572#1 = aLin;
               |LEE 000#572=;
               |SI FSalida = 0;
                    aSesion = #572#13;
                    aConta = #572#3;
                    |HAZ Corrige;
               |FINSI;
          |FINSI;
     |SINO;
          aConta = aRes % 706;
          aSesion = #0#2;
          |HAZ Corrige;
     |FINSI;
|FPRC;

|DEFBUCLE Cobros;
     #513#3;
     ;
     "      ", #0#0, "     ", 000001;
     "zzzzzz", #0#1, "zzzzz", 999999;
     be;
     NULL, Cobros;
|FIN;


|PROCESO Tipo3; |TIPO 3;
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR "dsz00090";
          |SI FSalida = 0;
               |ABRE #572;
               |ABRE #501;
               |BUCLE Cobros;
               |CIERRA #501;
               |CIERRA #572;
               |PIEINF;
               |FININF;
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;
