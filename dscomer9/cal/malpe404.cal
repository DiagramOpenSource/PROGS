|FICHEROS;
malpe404 #0;
malpe401 #1; ||LINEAS de Ciclos
malpe403 #2; || Tmp. de Ciclos
|FIN;

|VARIABLES;
almacen = 0;
ciclo = 0;
|FIN;

|PROCESO acumul;
|SI #2#1 ! almacen; |O #2#2 ! ciclo;
     #1#0 = #2#1;
     #1#1 = #2#2;
     |LEE 001#1=;
     |SI FSalida ! 0 ; |ERROR; |FINSI;
     #1#6 = 0;
     |GRABA 001#1a;
     |LIBERA #1;
     almacen = #2#1; ciclo = #2#2;
|FINSI;
#1#6 = #1#6 + #2#7;
#1#7 = #1#5 - #1#6;
|GRABA 001#1a;
|LIBERA #1;
|FPRC;


|DEFBUCLE acumula;
     #2#1;
     ;
     "                    " , 00001 , 0;
     "zzzzzzzzzzzzzzzzzzzz" , 99999 , 999999;
     ;
     NULL , NULL , NULL , NULL , NULL , acumul;
     #2#1,#2#2;
|FIN;

|PROCESO tipo3; |TIPO 3;
|SI FSalida = 0; |Y #0#0 = "S";
     |ABRE #1;
     |BUCLE acumula;
     |CIERRA #1;
|FINSI;
|FPRC;
