|FICHEROS;
     referen@ #1;
     agifa019 #19;
|FIN;

|VARIABLES;
     &enSwNoCopia = 0; || Para desactivar la actualizacion
     aPath = "";
     aAlfa = "";
     aDef = "";
     nModo = 0;
|FIN;

|PROCESO AuxArtiCons;    || Tambien esta en dsz99948
     |DDEF aDef;
     aDef = aDef + "goartaux";
     |CARGA_DEF aDef, referen@;
     |SI FSalida = 0;
          aAlfa = Usuario % 810;
          |ABRE #1;
          #1#0 = #19#0;
          #1#1 = ~;
          #1#2 = aAlfa;
          |SI nModo = 1;
               |GRABA 000#1n;
          |FINSI;
          |SI nModo = 3;
               |BORRA 000#1c;
          |FINSI;
          |CIERRA #1;
          |DESCARGA_DEF #referen@;
     |FINSI;
|FPRC;
