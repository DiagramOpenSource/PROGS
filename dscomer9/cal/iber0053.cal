|FICHEROS;
     iber0053 #0;
     iber0054 #1,MANTE;
|FIN;

|VARIABLES;
     nModo  = 0;
     nForma = 0;

     &eaCliente = "";
|FIN;

||INC-LUYE llena_c;

|PROCESO Borralo;
     |BORRA 020#1a;
     |LIBERA #1;
|FPRC;

|DEFBUCLE iber0054;
     #1#1;
     ;
     #0#0, #0#1, #0#2, 001;
     #0#0, #0#1, #0#2, 999;
     be;
     NULL, Borralo;
|FIN;

|RUTINA ClaveBaja1;
     #1#0 = #0#0;
     #1#1 = #0#1;
     #1#2 = #0#2;
     #1#3 = 001;
|FRUT;

|RUTINA ClaveAlta1;
     #1#0 = #0#0;
     #1#1 = #0#1;
     #1#2 = #0#2;
     #1#3 = 999;
|FRUT;

|PROCESO Tipo7;  |TIPO 7;
     |ENTLINEAL #1#1, -4, 7, 22, 0, ClaveBaja1, ClaveAlta1;
|FPRC;

|PROCESO Tipo2;  |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
         |BUCLE iber0054;
         eaCliente = #0#0;
         |SUB_EJECUTA_NP "iberz001";
     |SINO;
         nForma = 0 +. 1;
         |SI nForma = 1;
             |ENTLINEAL #1#1, -4, 1, 22, 0, ClaveBaja1, ClaveAlta1;
             eaCliente = #0#0;
             |SUB_EJECUTA_NP "iberz001";
         |FINSI;
     |FINSI;
|FPRC;

|RUTINA ClaveBaja0;
     #0#0 = "      ";
     #0#1 = 1;
     #0#2 = "01.01.1900";
|FRUT;

|RUTINA ClaveAlta0;
     #0#0 = "zzzzzz";
     #0#1 = 99;
     #0#2 = "31.12.2999";
|FRUT;

|PROGRAMA;
     |ENTLINEAL #1#0, -1, 1, 22, 1, ClaveBaja0, ClaveAlta0;
|FPRO;
