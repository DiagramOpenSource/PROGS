|FICHEROS;
      dsy00008 #0;
      tlfw0010 #10;
      agifa513 #513;
|FIN;

|VARIABLES;
     &eaFichero = "";
     &efDFech = @;
     &efHFech = @;
     &enDCaja = 0;
     &enHCaja = 0;

     &eCaja  = 0;
     &eFecha = "";
     nHay    = 0;
|FIN;

|PROCESO agifa513;
     nHay = 1;
     |PRINT;
|FPRC;

|DEFBUCLE agifa513;
     #513#3;
     5
     "      ", #0#0, "     ", 000001, #0#2;
     "zzzzzz", #0#1, "zzzzz", 999999, #0#3;
     ;
     NULL, agifa513;
|FIN;

|PROCESO TmpSematel;
     #513#14 = #10#2;
     #513#0 = #10#3;
     #513#1 = #10#8;  ||Cli
     #513#5 = #10#10; ||Caja
     #513#11 = #10#9; ||Fecha
     #513#3 = "";     ||concepto
     #513#4 = #10#4;  ||importe
     |PRINT;

     nHay = 1;
|FPRC;

|DEFBUCLE TmpSematel;
     #10#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, TmpSematel;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR "dsy00008";
          |SI FSalida = 0;
               |BUCLE agifa513;
               |HAZ EsSematel;
               |SI FSalida = 0;
                    efDFech = #0#0;
                    efHFech = #0#1;
                    enDCaja = #0#2;
                    enHCaja = #0#3;
                    eaFichero = Usuario;
                    |SUB_EJECUTA_NP "tlfz0001;IMPRECOBRO";
                    |NOME_DAT #10 eaFichero;
                    |BUCLE TmpSematel;
                    |DELINDEX #10;
               |FINSI;
               |SI nHay ! 0;
                    |PIEINF;
               |FINSI;
               |FININF;
          |SINO;
               |MENAV "0000Falta informe...";
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;


|PROGRAMA;
     |SI eCaja = 0;
          |MANTE #0;
     |SINO;
          #0#0 = eFecha;
          #0#1 = eFecha;
          #0#2 = eCaja;
          #0#3 = eCaja;
          |HAZ Tipo3;
     |FINSI;
|FPRO;
