|PROCESO LeeDDX;
     fdef = def + fich; fdes = org + fich;
     |LEEDEDEF fdef, fdes;
     |SI exp = 1; |Y FSalida = -1;
          fich = fdes + ".dat"; |FBORRA fich;
          fich = fdes + ".ixx"; |FBORRA fich;
          fich = fdes + ".ddx"; |FBORRA fich;
          |LEEDEDEF fdef, fdes;
          |MENAV "0000Fichero exportacion reparado...";
     |FINSI;
     eaNomDef = fich;
     |HAZ CamposDef;
     num = enCamposDef;
|FPRC;

|PROCESO BusCampos;
     |DFICE org; |QBF org;
     |DBASE des; |QBF des;
     |DDEF  def; |QBF def;
