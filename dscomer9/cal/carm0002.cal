|FICHEROS;
     carm0002 #0;
     carm0001 #1;
|FIN;

|VARIABLES;
     nModo     = 0;
|FIN;

|PROCESO Coste6; |TIPO 6;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo ! 3;
          |ABRE #1;
          #1#0 = #0#1;
          #1#1 = "01.01.0000";
          |LEE 000#1];
          |SI FSalida = 0; |Y #1#0 = #0#1;
          |SINO;
               |MENAV "0000Tipo coste inexistente...";
               |ERROR;
          |FINSI;
          |CIERRA #1;
     |FINSI;
|FPRC;

|PROCESO Coste66; |TIPO 66;
     |ABRE #1;
     #1#0 = #0#1;
     |LEE 000#1];
     |CONSULTA_CLAVE #1#1;
     |SI FSalida = 0;
          #0#1 = #1#0;
          |PINTA #0#1;
     |SINO;
          |ERROR;
     |FINSI;
     |CIERRA #1;
|FPRC;
