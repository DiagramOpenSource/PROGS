|FICHEROS;
     pora0002 #0;
     porconrp #10;
     pora0003 #3;
|FIN;

|VARIABLES;
     nModo=0;
     aHora="";
     aParametro = "";
     aRepre = "";
     aMensaje = "";
     aDesRepre = "";
     aHasRepre = "";
|FIN;

|PROCESO PonHora;|TIPO 7;
     nModo = (FEntrada/100)!0;
     |SI nModo = 1;
          |HORA aHora;
          #0#2=aHora % A105;
     |FINSI;
|FPRC;

|PROCESO min;
     #0#0 = 001;
     #0#5 = aDesRepre;
|FPRC;

|PROCESO max;
     #0#0 = 999;
     #0#5 = aHasRepre;
|FPRC;

|PROGRAMA;
     aParametro = PARAMETRO;
     |QBF aParametro;
     |SI aParametro = "";
          |ABRE #porconrp;
          |DDEFECTO #porconrp;
          |LEE 000#porconrp.p;
          aRepre = #porconrp#0;
          |CIERRA #porconrp;
          |C_M #pora0002#5, 1, "N";
          aDesRepre = "  ";
          aHasRepre = "zz";
     |SINO;
          |CLS;
          |PINPA #0#pora0003;
          |PINDA #0#pora0003;
          |ENDATOS #1#pora0003;
          |SI FSalida = 0;
               aDesRepre = #pora0003#0;
               aHasRepre = #pora0003#1;
          |SINO;
               aDesRepre = "  ";
               aHasRepre = "zz";
          |FINSI;
     |FINSI;
     |CLS;
     |ABRE #0;
     |LEE 000#0u;
     |ENTLINEAL #1#0, 1, 2, 22, 1, min, max;
     |SI FSalida=0;

     |FINSI;
     |CIERRA #0;
|FPRO;

|PROCESO PonRepre; |TIPO 7;
     |SI aParametro = "";
          #pora0002#5 = aRepre;
          |PINTA #pora0002#5;
     |FINSI;
|FPRC;
