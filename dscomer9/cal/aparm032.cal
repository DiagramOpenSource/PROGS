|FICHEROS;
     aparm032 #0;
     aparm033 #33;
|FIN;

|VARIABLES;
     nPuntero = 0;
     aAlfa = "";
     nBuffer = 0;
     nLineas = 0;
     nModo = 0;
     nLin = 0;
     nCol = 0;
|FIN;

|PROCESO CogeLineas32;
     nPuntero = 1;
     |PARA nPuntero = 1; |SI nPuntero [ 999; |HACIENDO nPuntero = nPuntero + 1;
          #33#0 = #0#0;
          #33#1 = nPuntero;
          |LEE 000#33=;
          |SI FSalida = 0;
               aAlfa = #33#2; |AL_BUF nBuffer, nPuntero, aAlfa;
               |BORRA 020#33a;
          |SINO;
               |SAL;
          |FINSI;
     |SIGUE;
|FPRC;

|PROCESO PoneLineas32;
     |PARA nPuntero = 999; |SI nPuntero ] 0; |HACIENDO nPuntero = nPuntero - 1;
          |DEL_BUF nBuffer, nPuntero, aAlfa;
          |SI aAlfa ! ""; nLineas = nPuntero; |SAL; |FINSI;
     |SIGUE;
     |PARA nPuntero = 1; |SI nPuntero [ nLineas; |HACIENDO nPuntero = nPuntero + 1;
          |DEL_BUF nBuffer, nPuntero, aAlfa;
          #33#0 = #0#0;
          #33#1 = nPuntero;
          #33#2 = aAlfa;
          |GRABA 020#33n;
     |SIGUE;
|FPRC;

|PROCESO Observa32;
     |ABRE #33;
     |PINPA #0#33;
     |DIMENSIONA 200, nBuffer, 0;
     |HAZ CogeLineas32;
     nModo = 100; nLin = 10; nCol = 78;
     |EDITA nBuffer, 1, 10, 1202, 2279, nModo, "", 0, nLin, nCol;
     |HAZ PoneLineas32;
     |FINDIM nBuffer;
     |CIERRA #33;
|FPRC;

|PROCESO Tipo32_20; |TIPO 20;
     |SI FSalida = "OPCION";
          FSalida = "Observa.";
     |SINO;
          |LEE 101#0=;
          |SI FSalida ! 0; |ACABA; |FINSI;

          |PUSHV 0501 2380;
          |HAZ Observa32;
          |POPV;

          |LIBERA #0;
     |FINSI;
|FPRC;

|PROCESO Borra33;
     |BORRA 020#33a;
|FPRC;

|DEFBUCLE Borra33;
     #33#1;
     ;
     #0#0, 001;
     #0#0, 999;
     be;
     NULL,  Borra33;
|FIN;


|PROCESO Tipo32_2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |BUCLE Borra33;
     |FINSI;
|FPRC;
