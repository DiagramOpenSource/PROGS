|FICHEROS;
     vigz0051 #0;
     vigm0002 #2;
     vigm0003 #3;
|FIN;

|VARIABLES;
     i = 0;
|FIN;

|PROCESO LimParri; |TIPO 0;
     |PARA i = 1; |SI i [ 74; |HACIENDO i = i + 1;
          |C_M #0i, 1, "N";
     |SIGUE;

     |SI #0#0 = "L";
          |C_M #0#1, 1, "S";
          |C_M #0#2, 1, "S";
     |SINO;
          |PARA i = 3; |SI i [ 74; |HACIENDO i = i + 1;
               |C_M #0i, 1, "S";
          |SIGUE;
     |FINSI;
|FPRC;

|PROCESO Formulas;
     |PRINT;
|FPRC;

|PROCESO Fin;
     |PIEINF;
|FPRC;

|DEFBUCLE Formulas;
     #2#1;
     ;
     #0#1;
     #0#2;
     ;
     NULL, NULL, Formulas, Fin;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR "vigz0051";
          |SI FSalida = 0;
               |SI #0#0 = "L";
                    |BUCLE Formulas;
               |SINO;
                    |ABRE #2;
                    |PARA i = 3; |SI i [ 74; |HACIENDO i = i + 1;
                         #2#0 = #0i;
                         |LEE 000#2=;
                         |SI FSalida = 0; |Y #0i > 0;
                              |BUCLELIN 2 Formulas #2;
                              |PIEINF;
                         |FINSI;
                    |SIGUE;
                    |CIERRA #2;
               |FINSI;
               |FININF;
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;

|PROCESO Consulta; |TIPO 66;
     |ABRE #2;
     #2#0 = #0Campo;
     |LEE 000#2];
     |CONSULTA_CLAVE #1#2;
     |SI FSalida = 0;
          #0Campo = #2#0; |PINTA #0Campo;
     |SINO;
          |ERROR;
     |FINSI;
     |CIERRA #2;
|FPRC;

|PROCESO Mensa; |TIPO 7;
     |MENSA "0000<F11> Consulta formulas";
|FPRC;
