|FICHEROS;
     tpv00011 #0;
     tpv00012 #1;
|FIN;

|VARIABLES;
     s_n  = "";
     i    = 0;
|FIN;

|PROCESO feliz; |TIPO 0;
     #1#3 = #1#3 > "";
     |PINTA #1#3;
     s_n = #1#3;
     |PARA i = 4; |SI i [ 15; |HACIENDO i = i + 1;
          |C_M #1i, 1, s_n;
     |SIGUE;
|FPRC;

|PROCESO periodo; |TIPO 0;
     i = (Campo - 4) $ 3;
     |SI i ! 2; |Y #1#3 = "S"; |Y Campo > 4;
          |SI i = 0;
               i = Campo - 2;
          |SINO;
               i = Campo - 1;
          |FINSI;
          |SI #1Campo [ #1i; |Y #1Campo ! 0; |Y #1i ! 0;
               |MENAV "0000Error en periodo de horas...";
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;
