|FICHEROS;
     pcegm057 #0;
     pcegm058 #1;
     pcegm059 #2;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO Min;
     #2#0 = #1#0;
     #2#1 = #1#1;
     #2#2 = " " * 8;
|FPRC;

|PROCESO Max;
     #2#0 = #1#0;
     #2#1 = #1#1;
     #2#2 = "z" * 8;
|FPRC;

|PROCESO Detalle; |TIPO 11;
     |SI FSalida = 13;
          |PUSHV 0501 2380;
          |ENTLINEAL #1#2, 3, 2, 22, 1, Min, Max;
          |POPV;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO BorraDetalle;
     |BORRA 020#2a;
|FPRC;

|DEFBUCLE BorraDetalle;
     #2#1;
     ;
     #1#0, #1#1, "        ";
     #1#0, #1#1, "zzzzzzzz";
     be;
     NULL, BorraDetalle;
|FIN;

|PROCESO Tipo2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |BUCLE BorraDetalle;
     |FINSI;
|FPRC;
