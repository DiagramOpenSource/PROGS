____________________________________/
|PROCESO ProcesaCampo;
     nCampo = -1;
     aCampo = "";
     |PARA nNume2=1;|SI nNume2 [ 100;|HACIENDO nNume2 = nNume2 + 1;
          nNume3    = (nNume2 * 100) + 1;
          aPos      = nNume3;
          aCaracter = aField % aPos;
          nNume1 = &aCaracter;
          |SI aCaracter ! "";
               |SI aCaracter = ";";
                    nCampo = nCampo + 1;
                    #1#nCampo# = aCampo;
                    aCampo = "";
               |SINO;
                    aCampo = aCampo + aCaracter;
               |FINSI;
          |SINO;
               nCampo = nCampo + 1;
               #1#nCampo# = aCampo;
               |GRABA 020#1n;
               nCampo = 0;
               aCampo = "";
               |PINTA 1310, #1#0;     ||codigo
               |PINTA 1410, #1#1;     ||dto1i
               |PINTA 1415, #1#2;     ||com1i
               |PINTA 1510, #1#3;     ||dto2i
               |PINTA 1515, #1#4;     ||com2i
               |PINTA 1610, #1#5;     ||dto3i
               |PINTA 1615, #1#6;     ||com3i
               |PINTA 1710, #1#7;     ||dto4i
               |PINTA 1715, #1#8;     ||com4i
               |PINTA 1810, #1#9;     ||dto5i
               |PINTA 1815, #1#10;     ||com5i
               |PINTA 1420, #1#11;     ||dto1e
               |PINTA 1425, #1#12;     ||com1e
               |PINTA 1520, #1#13;     ||dto2e
               |PINTA 1525, #1#14;     ||com2e
               |PINTA 1620, #1#15;     ||dto3e
               |PINTA 1625, #1#16;     ||com3e
               |PINTA 1720, #1#17;     ||dto4e
               |PINTA 1725, #1#18;     ||com4e
               |PINTA 1820, #1#19;     ||dto5e
               |PINTA 1825, #1#20;     ||com5e
               nCuantos = nCuantos + 1;
               |DDEFECTO #1;
               |SAL;
          |FINSI;
     |SIGUE;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     |DELINDEX #1;
     |ABRE #1;
     aAlfa1 = #0#0; |QBF aAlfa1;
     |XABRE "U",aAlfa1,nNume1;
     |SI FSalida ] 0;
         |XLEE nNume1,250,aField;
         |PARA; |SI aField > ""; |HACIENDO;
               |HAZ ProcesaCampo;
               |XLEE nNume1,250,aField;
         |SIGUE;
     |SINO;
          aAlfa1 = "0000[" + (nNume % 5) + "] inaccesible.";
          |MENAV aAlfa1;
     |FINSI;
     |CIERRA #1;
|FPRC;
