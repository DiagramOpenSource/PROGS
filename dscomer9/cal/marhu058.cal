|FICHEROS;
     marhu058 #0;
     marhu059 #1;     ||Resultado Tablas Comis/dto
|FIN;

|VARIABLES;
     aPos   = "";
     aCaracter = "";
     aAlfa1 = "";
     aAlfa2 = "";
     aAlfa3 = "";
     aAlfa4 = "";
     nNume  = 0;
     nNume1 = 0;
     nNume2 = 0;
     nNume3 = 0;
     nNume4 = 0;
     nPara1 = 0;
     nPara2 = 0;
     nPara3 = 0;
     nPara4 = 0;
     fFech1 = @;
     fFech2 = @;
     fFech3 = @;
     fFech4 = @;
     nFEstado = 0;
     nSeleccio = 0;
     nModo = 0;

     aFabre = "";
     aField = "";
     aFcanl = "";
     nCampo = 0;
     aCampo = "";
     nCuantos = 0;
