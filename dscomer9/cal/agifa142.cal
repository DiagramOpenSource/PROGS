|PROCESO LogosCrystal; |TIPO 7;
     |SI FSalida ! 0; |Y FSalida ! 2; |ACABA; |FINSI;

     |DBASE aBase;  |QBF aBase;

     aDirLogos  = aBase + "Documentos";  |MKDIR aDirLogos;
     aDirLogos  = aDirLogos + "/logos";  |MKDIR aDirLogos;
     aAlfa = "00000" + #48#0;
     aAlfa = aAlfa % -105;
     aDirLogos  = aDirLogos + "/" + aAlfa;  |MKDIR aDirLogos;
     aDirLogos  = aDirLogos + "/";

     aDirPatron = aBase + "patrones/logos/";
     aFichero   = aDirPatron + "zlogos.tgz";

     |XABRE "E", aFichero, 0;
     |SI FSalida < 0;
          aAlfa = "NO EXISTE:" + aFichero;
          |PINTA 2401, aAlfa; |AVISO;
     |SINO;
          |DESCOMPRIME aFichero;
          aFichero   = aDirPatron + "zlogos.tar";
          |DETAR aFichero, aDirPatron;
          |FBORRA aFichero;
     |FINSI;

     aDir = aDirPatron + "*.bmp";
     |_PDIR aDir;
     |PARA; |SI FSalida ] 0; |HACIENDO;
          aOrigen  = aDir;                             |QBF aOrigen;
          aDestino = aDirLogos + (aDir - aDirPatron);  |QBF aDestino;
          |COPIA_FICHERO aOrigen, aDestino;
          |FBORRA aDir;

          |_SDIR aDir;
     |SIGUE;

     aAbre = aDirLogos + "logos.txt";
     |XABRE "W", aAbre, nHandle;
     |SI FSalida < 0;
          aAlfa = "0000No puedo crear:" + aAbre;
          |MENAV aAlfa; |ACABA;
     |FINSI;

     nHay = 0;
     aDir = aDirLogos + "*.bmp";
     |_PDIR aDir;
     |PARA; |SI FSalida ] 0; |HACIENDO;
          aAlfa = (aDir - aDirLogos) + &13 + &10;
          |XGRABA nHandle, aAlfa;
          nHay = 1;
          |_SDIR aDir;
     |SIGUE;
     |XCIERRA nHandle;

     |SI nHay ! 0;
          |LEESECUENCIAL aAbre, nBaffer, nTotalLinea, 0, 0;

          |PUSHV 0501 2380;
          |PARA; |SI;  |HACIENDO;
                 nOpcion = 1;
                 |BMENUG nBaffer, 0, 1, nTotalLinea, 1116, nOpcion;
                 nOpcion     = FSalida;
                 nCogeOpcion = nOpcion - 1;
                 |SI FSalida [ nTotalLinea; |Y FSalida ] 1;
                     |DEL_BUF nBaffer, nCogeOpcion, aDestino;

                     |SAL;
                 |FINSI;
          |SIGUE;
          |POPV;

          #0#228 = aDestino;  |PINTA #0#21;

          |FBORRA aAbre;
     |FINSI;
|FPRC;

|PROCESO CogeLogo;
     |SI FSalida = 13; || F5;
          aAlfa = "F*.bmp";
          |BROWSE aAlfa;
          |SI aAlfa = "F";
               |ERROR;
          |SINO;
               aOrigen = aAlfa % 2;
               aLon = aAlfa % A0;
               aDestino = "";
               |PARA i = 1; |SI i [ aLon; |HACIENDO i = i + 1;
                    nPos = -(i * 100 + 1);
                    aLetra = (aOrigen % nPos);
                    |SI aLetra = "/"; |O aLetra = "\"; |SAL; |FINSI;
                    aDestino = aLetra + aDestino;
               |SIGUE;
               #0#228 = aDestino; |PINTA #0#228;
               |DBASE aAlfa;
               aAlfa1 = "00000" + #48#0;
               aAlfa1 = aAlfa1 % -105;
               aDestino = aAlfa + "Documentos/logos/" + aAlfa1 + "/" + aDestino;
               aIPRemoto = "";
               |IP_REMOTO aIPRemoto;  |QBT aIPRemoto;
               |SI aIPRemoto ! "";
                    |XABRE "E", aDestino, 0;
                    |SI FSalida ] 0;
                         |CONFI "0000NReemplazar fichero";
                    |SINO;
                         FSalida = 0;
                    |FINSI;
                    |SI FSalida = 0;
                         |BLIN 24; |PINTA 2401, "Copiando...";
