|FICHEROS;
     dsz00134 #0;
     enlactas@;
     agifa141 #141;
|FIN;

|VARIABLES;
     aAlfa = "";
     aPath = "";
     aEmp = "";
     i = 0;
|FIN;

|PROGRAMA;
     |DFICE aEmp;
     aEmp = aEmp % -202; || dscepsxx es de 2 digitos

     |DBASS aAlfa;
     aPath = aAlfa + "dscepsax/def/enlactas";
     |CARGA_DEF aPath, enlactas@;
     |SI FSalida = 0;
          aPath = aAlfa + "dscepsax/fich/" + aEmp + "/";
          |PATH_DAT #enlactas@#aPath#;
          |ABRE #enlactas@;
          |LEE 020#enlactas@.=;
          |SI FSalida = 0;
               |ABRE #141;
               |LEE 101#141=;
               |SI FSalida ! 0; |GRABA 020#141b; |FINSI;
               |PARA i = 0; |SI i [ 12; |HACIENDO i = i + 1;
                    #141i = #enlactas@#i#;
               |SIGUE;
               |GRABA 020#141a;
               |CIERRA #141;
               |SUB_EJECUTA_NP "agifa141;cepsaxx";
          |FINSI;
          |CIERRA #enlactas@;
          |DESCARGA_DEF #enlactas@;
     |FINSI;
|FPRO;
