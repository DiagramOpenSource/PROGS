|FICHEROS;
     goparasi #0;
     gopartia #1,MANTE;
     gomatcab #2;
|FIN;
------------------------------------------------------------------------------
|VARIABLES;
     &cParam        = "";
     nModo          = 0;
     nValor         = 0;
|FIN;
------------------------------------------------------------------------------
|PROCESO EntrPart; |TIPO 0;
     nModo = (FEntrada/100)!0;
     |SI nModo = 1; |O nModo = 3;
          |ERROR10;
          |ACABA;
     |FINSI;
     cParam = "ALBARANES";
     |PUSHV 01012380;
     |ENTLINEAL #1#1, 6, 2, 21, 1, MinParArt, MaxParArt;
     |PTEC 802;
     |POPV;
     |SI #0#6 ! #0#5;
         |LINEAS_BI_ATRI #0#6, P, I, 0;
     |SINO;
         |LINEAS_BI_ATRI #0#6, I, I, 0;
     |FINSI;
     |PINDA #0#0;
|FPRC;
------------------------------------------------------------------------------
|PROCESO MinParArt;
     #1#0 = #0#10;
     #1#1 = #0#11;
     #1#4 = #0#0;
     #1#5 = #0#1;
     #1#6 = 1;
     #1#7 = 1;
|FPRC;
------------------------------------------------------------------------------
|PROCESO MaxParArt;
     #1#0 = #0#10;
     #1#1 = #0#11;
     #1#4 = #0#0;
     #1#5 = #0#1;
     #1#6 = 999;
     #1#7 = 999;
|FPRC;
------------------------------------------------------------------------------
|PROCESO PinCli; |TIPO 7;
     |ATRI I;
     |PINTA 0813, #2#18;
     |PINTA 0821, #2#19;
     |ATRI 0;
     |SI #0#6 ! #0#5;
         |LINEAS_BI_ATRI #0#6, P, I, 0;
     |SINO;
         |LINEAS_BI_ATRI #0#6, I, I, 0;
     |FINSI;
|FPRC;
------------------------------------------------------------------------------
