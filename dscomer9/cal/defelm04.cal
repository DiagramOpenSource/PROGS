|FICHEROS;
     defelm04 #0;
     defelm05 #1,MANTE;
     dsm00010 #910;
|FIN;

|INCLUYE i_varart;

|VARIABLES;
     nModo = 0;
     nForma = 0;
     aAlfa = "";
     nNume = 0;
     nPos = 0;
     aCampo = "";
|FIN;

|PROCESO Tipo12; |TIPO 12;
|FPRC;

|PROCESO BorraTalla;
     |BORRA 020#1a;
|FPRC;

|DEFBUCLE BorraTalla;
     #1#1;
     ;
     #0#0, "      ";
     #0#0, "zzzzzz";
     be;
     NULL, BorraTalla;
|FIN;

|PROCESO Tipo2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     nForma = 0 +. 1;
     |SI nModo = 3;
          |BUCLE BorraTalla;
     |SINO;
          |SI nForma = 1;
               |HAZ Columna;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Min;
     #1#0 = #0#0;
     #1#1 = "      ";
|FPRC;

|PROCESO Max;
     #1#0 = #0#0;
     #1#1 = "zzzzzz";
|FPRC;

|PROCESO Columna;
     |ENTCOLUMNA #1#1, 2, nModo, 5, 0, Min, Max;
|FPRC;

|PROCESO Tipo7; |TIPO 7;
     nModo = 7;
     |HAZ Columna;
|FPRC;

|PROCESO Tipo13; |TIPO 13;
     |ABRE #910; |LEE 000#910p; |CIERRA #910;

     |PINPA #0#1;
     aAlfa = #910#20; |QBF aAlfa;
     aAlfa = (aAlfa + "..........") % 110;
     |ATRI R; |PINTA 1304, aAlfa; |ATRI 0;

     nNume = #910#21;
     |C_A #1#1, 1, nNume;
     nModo = 7;
     |HAZ Columna;
|FPRC;

|PROCESO Rellena; |TIPO 6;
     aAlfa = #910#21 * #910#23;
     |SI #910#22 = "N";
          nNume = #1#1; aCampo = nNume;
     |SINO;
          aCampo = #1#1; |QBF aCampo;
     |FINSI;
     nPos = -1 * (#910#21 + 100)
     aAlfa = (aAlfa + aCampo) % nPos;
|FPRC;

|PROCESO Talla2; |TIPO 2;
     #0#3 = #0#3 +. #1#3; |PINTA #0#3;
|FPRC;
