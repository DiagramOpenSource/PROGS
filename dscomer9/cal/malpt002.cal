|FICHEROS;
     malpt002 #0;
|FIN;

|VARIABLES;
     aAlfa = "";
     modo   = 0;
     &n_dec = 0;
     &n_pre = 0;
|FIN;

|PROCESO acepta; |TIPO 11;
     |SI FSalida = 9;
          |SI #0#5 = #0#7;
               |MENAV "0000ннн Esta linea esta Servida !!!";
               |ERROR;
          |SINO;
               |SI #0#12 = " ";
                    #0#12 = "*";
               |SINO;
                    #0#12 = " ";
               |FINSI;
               |PINTA #0#12;
               |GRABA 021#0a;
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO no_alta; |TIPO 0;
     modo = (FEntrada / 100) ! 0;
     |SI modo = 1;
          |AVISO;
          |ERROR;
     |FINSI;
     |SI modo = 2;
          |SI #0#5 = #0#7;
               |MENAV "0000ннн Esta linea esta Servida !!!";
               |AVISO;
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO no_baja; |TIPO 1;
     modo = (FEntrada / 100) ! 0;
     |SI modo = 3;
          |AVISO;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO uni; |TIPO 0;
     #0#5  = #0#5 ! n_dec;
     #0#11 = #0#5 / #0#13;
     #0#12 = "*";
     |PINTA #0#5;
     |PINTA #0#11;
     |PINTA #0#12;
|FPRC;

|PROCESO palets; |TIPO 0;
     #0#5 = #0#11 * #0#13;
     |HAZ uni;
|FPRC;


|PROCESO lrese; |TIPO 0;
     aAlfa = #0#14; |QBF aAlfa;
     |SI aAlfa ! ""; |Y #0#10 ! #0#14;
          |MENAV "0000Lote Incorrecto...";
     |FINSI;
|FPRC;
