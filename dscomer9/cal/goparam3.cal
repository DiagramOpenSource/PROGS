|FICHEROS;
     goparam3 #0;
     goparam4 #1;
     goparam2 #2;
     goparam1 #3;
|FIN;
------------------------------------------------------------------------------
|VARIABLES;
     &cTitulo3      = "";
     &cTitulo4      = "";
     &cCombina      = "";
|FIN;
------------------------------------------------------------------------------
|PROCESO Marca3; |TIPO 0;

     |SI FSalida = 0;
          #0#0 = "*";
          |PINTA #0#0;
          cCombina = #3#1 + #2#1 + #0#1;

          |SI cTitulo4 ! "";
               |ENTLINEAL #1#1, 1, 4, 22, 0, MinPar3, MaxPar3;
          |FINSI;

          |SI cTitulo4 = "";
               |PTEC 807;
               |PTEC 807;
               |PTEC 807;
          |FINSI;

     |FINSI;

|FPRC;
------------------------------------------------------------------------------
|PROCESO MinPar3;
     #1#1 = " ";
|FPRC;
------------------------------------------------------------------------------
|PROCESO MaxPar3;
     #1#1 = "z";
|FPRC;
------------------------------------------------------------------------------
