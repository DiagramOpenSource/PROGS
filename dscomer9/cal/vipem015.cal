|PROCESO ModiAlta;
     |VENTANA 0501, 3299, -1, 1, ""; nVentana = FSalida;

     |PINPA #0#812; nPanta2 = FSalida;
     |PINDA #0#812;

     |HAZ BotonesPanta2;

     |ABRE #812;

     |LEE 101#916=;

     aCadena ="|NC";
     aAlfa = #916#aCadena#;
     nCampoHasta = aAlfa;
     nCampoHasta = nCampoHasta - 1;

     |SI nModo ! 1;
          |PARA nCampoD = 0;  |SI nCampoD [ nCampoHasta;   |HACIENDO nCampoD = nCampoD + 1;
                nCampoH = nCampoD;
                #812nCampoH = #916nCampoD;
          |SIGUE;

          |C_M #812#0, 1, "N";
     |SINO;
          |C_M #812#0, 1, "S";
          |DDEFECTO #812;
     |FINSI;

     |PINPA #0#812;
     |PINDA #0#812;

     |SI nModo = 1;  |ENDATOS #1#812;  |FINSI;
     |SI nModo = 2;  |ENDATOS #2#812;  |FINSI;

     |SI FSalida = 0;
          |LIBERA #916;
          |HAZ GrabaDatos;
     |SINO;
          |LIBERA #916;
