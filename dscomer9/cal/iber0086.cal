|FICHEROS;
     iber0086 #0;
     iber0078 #78;
     iber0079 #79;
     iber0080 #80;
     iber0016 #16;
     iber0019 #19;
|FIN;

|VARIABLES;
     &enObs    = 0;
     aParam    = "";
     aCentro   = "";
     aInforme  = "iber0086";
|FIN;

|PROCESO iber0080;
     |PRINT;
|FPRC;

|DEFBUCLE iber0080;
     #80#1;
     ;
     #79#0, #79#1, #79#2, #79#3, 002;
     #79#0, #79#1, #79#2, #79#3, 999;
     ;
     NULL, iber0080;
|FIN;

|PROCESO iber0079;
     |PRINT;
     enObs = 1;
     |BUCLE iber0080;
     enObs = 0;
|FPRC;

|DEFBUCLE iber0079;
     #79#1;
     6;
     #78#0, #78#1, #78#2, 001, #0#6;
     #78#0, #78#1, #78#2, 999, #0#7;
     ;
     NULL, NULL, NULL, NULL, NULL, iber0079;
     #79#6, #79#7, #79#4, #79#5;
|FIN;

|PROCESO Lineas;
     #16#0 = #78#0;
     |LEE 000#16=;
     |SI FSalida ! 0; |DDEFECTO #16; |FINSI;

     #19#0 = #78#0;
     #19#1 = #78#1;
     |LEE 000#19=;
     |SI FSalida ! 0; |DDEFECTO #19; |FINSI;

     |INFOR aInforme;
     |SI FSalida = 0;
          |BUCLE iber0079;
          |PIEINF;
          |FININF;
     |FINSI;
|FPRC;

|DEFBUCLE iber0078;
     #78#1;
     ;
     #0#0, #0#4, #0#2;
     #0#1, #0#5, #0#3;
     ;
     NULL, NULL, NULL, NULL, NULL, Lineas;
     #78#0, #78#2, #78#1;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |IMPRE 0;
     |SI FSalida = 0;
          |ABRE #16;
          |ABRE #19;
          |BUCLE iber0078;
          |CIERRA #16;
          |CIERRA #19;
          |FINIMP;
     |FINSI;
|FPRC;

|PROCESO DefCentro;  |TIPO 7;
     |SI #0#0 = 0;
          |DFICE aCentro;
          aCentro = aCentro % -403;
          #0#0 = aCentro; |PINTA #0#0;
          #0#1 = aCentro; |PINTA #0#1;
     |FINSI;
|FPRC;

|PROGRAMA;
     aParam = PARAMETRO; |QBF aParam;
     |SI aParam ! "";
