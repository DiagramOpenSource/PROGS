|VARIABLES;
     posicion                 = "";
     posi                     = "";
     num_alfa                 = "";
     n1                       = "";
     n2                       = "";
     n3                       = "";
     n4                       = "";
     n5                       = "";
|FIN;

||________________________________/ PROCESO PRINCIPAL

|PROCESO mira_num;
     |SI #0#9 = 0;
          puente_a = "     0";
     |SINO;
          puente_a = "      " + #0#9; |QBF puente_a;
          puente_a = puente_a % -106;
     |FINSI;
     num_alfa = puente_a % -101;   posicion = "30";    |HAZ busca_numero;
     num_alfa = puente_a % -201;   posicion = "25";    |HAZ busca_numero;
     num_alfa = puente_a % -301;   posicion = "20";    |HAZ busca_numero;
     |SI #0#9 > 999; |O #0#9 < -999;
          |ATRI R;
          |PINTA 1017 , "лл";
          |ATRI r;
     |SINO;
          |PINTA 1017 , "  ";
     |FINSI;
     num_alfa = puente_a % -401;   posicion = "12";    |HAZ busca_numero;
     num_alfa = puente_a % -501;   posicion = "07";    |HAZ busca_numero;
     num_alfa = puente_a % -601;   posicion = "02";    |HAZ busca_numero;
|FPRC;

|PROCESO busca_numero;
     |SI num_alfa = "0"; |Y num_alfa ! " ";  |HAZ cero;     |HAZ pinta_numero; |FINSI;
     |SI num_alfa = "1";      |HAZ uno;      |HAZ pinta_numero;  |FINSI;
     |SI num_alfa = "2";      |HAZ dos;      |HAZ pinta_numero;  |FINSI;
     |SI num_alfa = "3";      |HAZ tres;     |HAZ pinta_numero;  |FINSI;
     |SI num_alfa = "4";      |HAZ cuatro;   |HAZ pinta_numero;  |FINSI;
     |SI num_alfa = "5";      |HAZ cinco;    |HAZ pinta_numero;  |FINSI;
     |SI num_alfa = "6";      |HAZ seis;     |HAZ pinta_numero;  |FINSI;
     |SI num_alfa = "7";      |HAZ siete;    |HAZ pinta_numero;  |FINSI;
     |SI num_alfa = "8";      |HAZ ocho;     |HAZ pinta_numero;  |FINSI;
     |SI num_alfa = "9";      |HAZ nueve;    |HAZ pinta_numero;  |FINSI;
     |SI num_alfa = "-";      |HAZ raya;     |HAZ pinta_numero;  |FINSI;
     |SI num_alfa = " ";      |HAZ blanco;   |HAZ pinta_numero;  |FINSI;
|FPRC;

|PROCESO pinta_numero;
     |ATRI I;
     posi = "06" + posicion;  |PINTA posi , n1;
     posi = "07" + posicion;  |PINTA posi , n2;
     posi = "08" + posicion;  |PINTA posi , n3;
     posi = "09" + posicion;  |PINTA posi , n4;
     posi = "10" + posicion;  |PINTA posi , n5;
     |ATRI i;
|FPRC;

|PROCESO blanco;
     n1 = "    ";
     n2 = "    ";
     n3 = "    ";
     n4 = "    ";
     n5 = "    ";
|FPRC;

|PROCESO raya;
     n1 = "    ";
     n2 = "    ";
     n3 = "лллл";
     n4 = "    ";
     n5 = "    ";
|FPRC;

|PROCESO cero;
     n1 = "лллл";
     n2 = "л  л";
     n3 = "л  л";
     n4 = "л  л";
     n5 = "лллл";
|FPRC;

|PROCESO uno;
     n1 = " лл ";
     n2 = " лл ";
     n3 = "  л ";
     n4 = "  л ";
     n5 = "лллл";
|FPRC;

|PROCESO dos;
     n1 = "лллл";
     n2 = "   л";
     n3 = "лллл";
     n4 = "л   ";
     n5 = "лллл";
|FPRC;

|PROCESO tres;
     n1 = "лллл";
     n2 = "   л";
     n3 = "лллл";
     n4 = "   л";
     n5 = "лллл";
|FPRC;

|PROCESO cuatro;
     n1 = "л  л";
     n2 = "л  л";
     n3 = "лллл";
     n4 = "   л";
     n5 = "   л";
|FPRC;

|PROCESO cinco;
     n1 = "лллл";
     n2 = "л   ";
     n3 = "лллл";
     n4 = "   л";
     n5 = "лллл";
|FPRC;

|PROCESO seis;
     n1 = "лллл";
     n2 = "л   ";
     n3 = "лллл";
     n4 = "л  л";
     n5 = "лллл";
|FPRC;

|PROCESO siete;
     n1 = "лллл";
     n2 = "   л";
     n3 = "  л ";
     n4 = " л  ";
     n5 = " л  ";
|FPRC;

|PROCESO ocho;
     n1 = "лллл";
     n2 = "л  л";
     n3 = "лллл";
     n4 = "л  л";
     n5 = "лллл";
|FPRC;

|PROCESO nueve;
     n1 = "лллл";
     n2 = "л  л";
     n3 = "лллл";
     n4 = "   л";
     n5 = "лллл";
|FPRC;

|PROCESO pan; |TIPO 14;
     |SI modo ] 1; |Y #agifa505#31 = "S";
          |HAZ mira_num;
||          |PINTA 0602 , "                            лллл";
||          |PINTA 0702 , "                            л  л";
||          |PINTA 0802 , "                            л  л";
||          |PINTA 0902 , "                            л  л";
||          |PINTA 1002 , "                            лллл";
     |FINSI;
