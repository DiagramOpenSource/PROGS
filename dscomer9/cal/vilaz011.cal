|FICHEROS;
     vilaz011 #0
     vilam002 #2;
     vilaw003 #3;
     vilam004 #4;
|FIN;

|VARIABLES;
     aInforme = "";
     aAlfa = "";
     i = 0;
|FIN;

|PROCESO Print;
     |PRINT;
|FPRC;

|DEFBUCLE Clave5;
     #3#5;
     ;
     INICIO;
     FINAL;
     ;
     NULL, Print;
|FIN;

|DEFBUCLE Clave4;
     #3#4;
     ;
     INICIO;
     FINAL;
     ;
     NULL, Print;
|FIN;

|DEFBUCLE Clave3;
     #3#3;
     ;
     INICIO;
     FINAL;
     ;
     NULL, Print;
|FIN;

|DEFBUCLE Clave2;
     #3#2;
     ;
     INICIO;
     FINAL;
     ;
     NULL, Print;
|FIN;

|PROCESO PreVto;
     |SI #0#6 = "S";
          |SI #4#7 ! "S"; |ACABA; |FINSI;
     |FINSI;
     |SI #0#6 = "X";
          |SI #4#7 ! "N"; |Y #4#7 ! "P"; |ACABA; |FINSI;
     |FINSI;
     |SI #0#6 = "N";
          |SI #4#7 ! "N"; |ACABA; |FINSI;
     |FINSI;

     |PARA i = 0; |SI i [ 12; |HACIENDO i = i + 1;
          #3i = #4i;
     |SIGUE;
     |GRABA 020#3n;
|FPRC;

|DEFBUCLE PreVto;
     #4#2;
     ;
     #0#0, #0#2, #0#4;
     #0#1, #0#3, #0#5;
     ;
     NULL, PreVto;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     aAlfa = Usuario; |NOME_DAT #3 aAlfa; |DELINDEX #3;

     |IMPRE 0;
     |SI FSalida = 0;
          |SI #0#7 = 1; aInforme = "vilaza11"; |FINSI;
          |SI #0#7 = 2; aInforme = "vilazb11"; |FINSI;
          |SI #0#7 = 3; aInforme = "vilazc11"; |FINSI;
          |SI #0#7 = 4; aInforme = "vilazd11"; |FINSI;
          |INFOR aInforme;
          |SI FSalida = 0;
               |ABRE #3;
               |BUCLE PreVto;
               |CIERRA #3;

               |SI #0#7 = 1; |BUCLE Clave2; |FINSI;
               |SI #0#7 = 2; |BUCLE Clave3; |FINSI;
               |SI #0#7 = 3; |BUCLE Clave4; |FINSI;
               |SI #0#7 = 4; |BUCLE Clave5; |FINSI;

               |PIEINF;
               |FININF;
          |SINO;
               aAlfa = "0000Error en informe '" + aInforme + "'";
               |MENAV aAlfa;
          |FINSI;
          |FINIMP;
     |FINSI;

     |DELINDEX #3;
|FPRC;
