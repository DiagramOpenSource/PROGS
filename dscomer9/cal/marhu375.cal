|FICHEROS;
     marhu375 #0;
     agifa153 #1;
|FIN;

|VARIABLES;
     aMensaje = "";
     &stock = " ":
     &sust = " ";
     &tabla = 0;
     &tipo = " ";
     programa = "";
|FIN;

|PROCESO VerifArticulo; |TIPO 0;
     |SI #1#2 = "A";
     |SINO;
          aMensaje = "0000Error: El tipo de cambio especificado no es de articulo [" + #1#2 + "]";
          |MENAV aMensaje;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO inicio; |TIPO 3;
     tabla = #0#1;
     sust = #0#3;
     stock = #0#6;
     |SI #0#0 = "SI"
          |SI #1#2 = "A";     ||Cambio El codigo de Articulo
               tipo = "A";
               programa = "marhu376.run";
          |FINSI;
          |SUB_EJECUTA_NP programa;
     |FINSI;
|FPRC;
