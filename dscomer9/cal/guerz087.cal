|FICHEROS;
     guerz087 #0;
     gomantho #1;
     gopartia #2;
     gosubaux #3;
|FIN;

|VARIABLES;
     &Tempo = "";
     aSubcon = "";
     aD = "";
     aH = "";
     nHay = 0;
     aAlfa = "";
|FIN;

|PROCESO CreaAux;
     |SI #0#7 = "*";
          |DDEFECTO #3;
          #3#0 = "A";
          #3#1 = #0#0;
          #3#2 = #0#1;
          #3#3 = #0#2;
          #3#4 = #0#3;
          #3#5 = aSubcon;
          #3#7 = #0#9;
          #3#8 = #0#8;
          |GRABA 020#3n;
     |FINSI;
|FPRC;

|DEFBUCLE CreaAux;
     #0#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, CreaAux;
|FIN;

|PROCESO gopartia;
     #3#0 = #2#34;
     #3#1 = #2#4;
     #3#2 = #2#5;
     #3#3 = #2#6;
     #3#4 = #2#7;
     |LEE 000#3=;
     |SI FSalida ! 0;
          #0#0 = #2#4;
          #0#1 = #2#5;
          #0#2 = #2#6;
          #0#3 = #2#7;
          #0#4 = #2#20;
          #0#5 = #2#30;
          #0#6 = #2#10;
          #0#8 = #2#12;
          #0#9 = #2#28;
          |GRABA 020#0n;
          nHay = 1;
     |FINSI;
|FPRC;

|DEFBUCLE gopartia;
     #2#3;
     34, 17;
     #1#0, #1#1, "     ", 000001, aD, aD, "A", #1#11;
     #1#0, #1#1, "zzzzz", 999999, aH, aH, "A", #1#11;
     ;
     NULL, gopartia;
|FIN;

|PROCESO Min;
     #0#0 = "     ";
     #0#1 = 000001;
     #0#2 = 001;
     #0#3 = 001;
|FPRC;

|PROCESO Max;
     #0#0 = "zzzzz";
     #0#1 = 999999;
     #0#2 = 999;
     #0#3 = 999;
|FPRC;

|PROCESO Tecla; |TIPO 0;
     |SI FSalida = 0; |Y #0Campo = Contenido;
          |SI #0#7 = "*";
               #0#7 = "";
          |SINO;
               #0#7 = "*";
          |FINSI;
          |GRABA 020#0a;
     |FINSI;
|FPRC;

|PROGRAMA;
     aD = " " * 20;
     aH = "z" * 20;
     |ABRE #1;
     aAlfa = PARAMETRO % 105; #1#0 = aAlfa;
     aAlfa = PARAMETRO % 606; #1#1 = aAlfa;
     aSubcon = PARAMETRO % 1206;
     |LEE 020#1=;
     |SI FSalida = 0;
          |HAZ CreaTempo; |NOME_DAT #0 Tempo; |DELINDEX #0;
          |ABRE #0;
          |ABRE #3;
          |BUCLE gopartia;
          |CIERRA #3;
          |CIERRA #0;
          |SI nHay = 0;
               |MENAV "0000No hay registros...";
          |SINO;
               |CLS;
               |PINPA #0#0;
               |ENTLINEAL #1#0, 1, 4, 22, 0, Min, Max;
               |CONFI "0000N¿Es correcta la selección?";
               |SI FSalida = 0;
                    |ABRE #3;
                    |BUCLE CreaAux;
                    |CIERRA #3;
               |FINSI;
          |FINSI;
          |HAZ BoraTempo; |DELINDEX #0;

     |FINSI;
     |CIERRA #1;
|FPRO;
