|FICHEROS;
     ialbm015 #0;
     ialbm016 #1;
|FIN;

|VARIABLES;
     aAlfa = "";
     nConta = 0;
     nModo = 0;
|FIN;

|PROCESO Importe; |TIPO 0;
     #1#8 = #1#6 * #1#7;
     |PINTA #1#8;
|FPRC;

|PROCESO Imprime;
     |PRINT;
|FPRC;

|PROCESO Tipo4; |TIPO 4;
     |SELECT #1#0;
     |LEE 000#0=;
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR "ialbm015";
          |SI FSalida = 0;
               |BUCLELIN 2 Imprime #0;
               |PIEINF;
               |FININF;
          |SINO;
               |MENAV "0000No existe informe 'ialbm015'";
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;

|PROCESO Conta; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          aAlfa = #0#0;
          #0#1 = 999999;
          |LEE 000#0];
          |SI FSalida = 0;
               |LEE 000#0a;
          |SINO;
               |LEE 000#0u;
          |FINSI;
          |SI FSalida = 0; |Y #0#0 = aAlfa;
               nConta = #0#1 + 1;
          |SINO;
               nConta = 1;
          |FINSI;
          |DDEFECTO #0;
          #0#0 = aAlfa;
          #0#1 = nConta;
     |FINSI;
|FPRC;
