|FICHEROS;
     sebalo07 #0;
     sebalo01 #1;
     sebalo02 #2;
     sebalo06 #6;
|FIN;

|VARIABLES;
     &Tempo = "";
     {-1}aMenu = "";
     aMenu1 = "2400";
     aMenu2 = "1";
     aMenu3 = "Opciones: ";
     aMenu4 = "RAC";
     aMenu5 = " Repasar ";
     aMenu6 = " Aceptar ";
     aMenu7 = " Cancelar ";
     nConta = 0;
|FIN;

|PROCESO Devolucion;
     |LEE 000#6u;
     |SI FSalida = 0;
          nConta = #6#0 + 1;
     |SINO;
          nConta = 1;
     |FINSI;
     |DDEFECTO #6;
     #6#0 = nConta;
     #6#4 = #0#5;
     #6#5 = #0#0;
     #6#6 = #0#1;
     #6#7 = -#0#4;
     #6#8 = #0#3;
     |GRABA 020#6n;
|FPRC;

|DEFBUCLE Devolucion;
     #0#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, Devolucion;
|FIN;

|PROCESO Actualiza;
     |ABRE #6;
     |BUCLE Devolucion;
     |CIERRA #6;
|FPRC;

|PROCESO Min;
     #0#0 = "      ";
     #0#1 = "   ";
|FPRC;

|PROCESO Max;
     #0#0 = "zzzzzz";
     #0#1 = "zzz";
|FPRC;

|PROCESO Tipo0; |TIPO 0;
     |SI #0#1 = "   ";
          #0#1 = #1#1; |PINTA #0#1;
          |ABRE #2;
          #2#0 = #0#1;
          |LEE 000#2=;
          |SI FSalida = 0;
               #0#3 = #2#1; |PINTA #0#3;
          |FINSI;
          |CIERRA #2;

     |FINSI;
|FPRC;

|PROGRAMA;
     |ABRE #1; |LEE 000#1p; |CIERRA #0;

     |HAZ CreaTempo; |NOME_DAT #0 Tempo; |DELINDEX #0;
     |PINPA #0#0;
     |PARA; |SI; |HACIENDO;
          |ENTLINEAL #1#0, 1, 1, 22, 0, Min, Max;
          |MENU aMenu;
          |SI FSalida = 2; |HAZ Actualiza; |SAL; |FINSI;
          |SI FSalida = 3; |SAL; |FINSI;
     |SIGUE;
     |HAZ BoraTempo; |DELINDEX #0;
|FPRO;
