|FICHEROS;
     otpz0002 #0;
     agifa024 #24;

     :/dscarter/def/dscam003 #203;
     :/dscarter/def/dscam045 #245;
|FIN;

|VARIABLES;
     aBase        = "";
     aPathFich    = "";
     aAlfa1       = "";
     aAlfa2       = "";

     nEmpresaFact = 0;
     nEmpresaCart = 0;
|FIN;

|PROCESO dscam045;
     nEmpresaCart = #245#6;
     |ERROR10;
|FPRC;

|DEFBUCLE dscam045;
     #245#1;
     2;
     nEmpresaFact, 001, "V";
     nEmpresaFact, 999, "V";
     ;
     NULL, dscam045;
|FIN;

|PROCESO dscam003;
     aAlfa1 = #24#32;  |QBF aAlfa1;
     aAlfa2 = #203#8;  |QBF aAlfa2;
     |SI aAlfa1 ! aAlfa2;
         |PRINT;
         |ACABA;
     |FINSI;

     aAlfa1 = #24#33;  |QBF aAlfa1;
     aAlfa2 = #203#9;  |QBF aAlfa2;
     |SI aAlfa1 ! aAlfa2;
         |PRINT;
         |ACABA;
     |FINSI;

     aAlfa1 = #24#31;   |QBF aAlfa1;
     aAlfa2 = #203#10;  |QBF aAlfa2;
     |SI aAlfa1 ! aAlfa2;
         |PRINT;
         |ACABA;
     |FINSI;

     aAlfa1 = #24#30;   |QBF aAlfa1;
     aAlfa2 = #203#11;  |QBF aAlfa2;
     |SI aAlfa1 ! aAlfa2;
         |PRINT;
     |FINSI;
|FPRC;

|DEFBUCLE dscam003;
     #203#11;
     4;
     #24#0, "01.01.0000", "     ", #0#0;
     #24#0, "31.12.2999", "zzzzz", #0#1;
     ;
     NULL, dscam003;
|FIN;

|PROCESO agifa024;
     |BUCLE dscam003;
|FPRC;

|DEFBUCLE agifa024;
     #24#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, agifa024;
|FIN;

|PROCESO Tipo3;  |TIPO 3;
     |IMPRE 0;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |INFOR "otpz0002";
     |SI FSalida ! 0;  |FINIMP; |ACABA;  |FINSI;

     |DBASS aBase;  |QBF aBase;
     aPathFich = aBase + "dscarter/fich/";
     |PATH_DAT #245 aPathFich;

     nEmpresaFact = #48#0;
     nEmpresaCart = 0;
     |BUCLE dscam045;

     aPathFich = aBase + "dscarter/fich/" + (("00000" + nEmpresaCart) % -105) + "/";
     |PATH_DAT #203 aPathFich;

     |BUCLE agifa024;

     |PIEINF;
     |FININF;
     |FINIMP;
|FPRC;
