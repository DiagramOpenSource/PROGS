|FICHEROS;
     agif0019  #1;
     agifa019  #2;
     agifa017  #3;
|FIN;

|VARIABLES;
     &eaArticulo = "";
|FIN;

|PROGRAMA;
  |ET inicio;
     |DDEFECTO #1;
     |CLS;
     |CABEZA "Consulta Articulo";
     |PINPA #0#1;
     |PINDA #0#1;
     |ENDATOS #1#1;
     |SI FSalida = 0;
            |HAZ art;
            |VETE inicio;
     |FINSI;
|FPRO;

|PROCESO art;
     |ABRE #2;
     |ABRE #3;
     #2#0 = #1#0;
     #3#0 = #1#0;
     #3#1 = #1#1;
     |LEE 000#2=;
     |SI FSalida = 0;
          |LEE 000#3=;
          |SI FSalida = 0;
               #1#2 = #2#1;   || Descripcion
               #1#3 = #2#6;   || Stock
               #1#4 = #2#18;  || Precio
               #1#5 = #3#2;   || Localizacion
               #1#6 = #3#5;   || Stock Almacen
               |PINTA #1#2;
               |PINTA #1#3;
               |PINTA #1#4;
               |PINTA #1#5;
               |PINTA #1#6;
          |FINSI;
     |FINSI;
     |CIERRA #2;
     |CIERRA #3;
     |PAUSA;
|FPRC;

|PROCESO Llena019; |TIPO 6;
     eaArticulo = #1Campo;
     |HAZ LlenaArticulo;
     |SI FSalida = 0;
          #1Campo = eaArticulo;
          |PINTA #1Campo;
     |SINO;
          |ERROR; |ACABA;
     |FINSI;
|FPRC;
