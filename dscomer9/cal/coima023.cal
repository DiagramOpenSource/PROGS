|FICHEROS;
     coima023 #0;
|FIN;

|VARIABLES;
     aIPRemoto = "";
     aOrigen = "";
     aDestino = "";
     aAlfa = "";
|FIN;

|PROCESO Edita; |TIPO 11;
     |SI FSalida ! 13; |ACABA; |FINSI; ||F5

     aIPRemoto = "";
     |IP_REMOTO aIPRemoto;  |QBT aIPRemoto;

     |DBASE aOrigen;
     aOrigen = aOrigen + "004082/adjuntos/" + #0#1; |QBF aOrigen;

     |RMKDIR "C:\DOCUMENTOS\";
     aDestino = "C:\DOCUMENTOS\"+ #0#1; |QBF aDestino;

     |SI aIPRemoto ! "";
          |ENVIA_FICHERO aOrigen, aDestino;
     |SINO;
          |COPIA_FICHERO aOrigen, aDestino;
     |FINSI;
     |SI FSalida = 0;
          aAlfa = "cmd " + &34 + " /c START /WAIT " + aDestino + &34;
          |RSYSTEM aAlfa;
          |FBORRA aDestino;
     |FINSI;
     |ERROR;
|FPRC;

|PROCESO Min;
     #0#0 = 1;
|FPRC;

|PROCESO Max
     #0#0 = 99999999;
|FPRC;

|PROGRAMA;
     |CLS;
     |ENTLINEAL #1#0, 1, 4, 22, 1, Min, Max;
|FPRO;
