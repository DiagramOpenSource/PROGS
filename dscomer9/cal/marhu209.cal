|FICHEROS;
     marhu209 #0;
|FIN;

|VARIABLES;
     &ext1      = "";
     &nRieTotal = 0;
     &nRieUtili = 0;
     &eaTag     = "";
     aParam     = "";
     nSal       = 0;
|FIN;

|PROCESO Extracto; |TIPO 0;
     |SI #0#0 = Contenido; |Y FSalida = 0;
          |PUSHV 0501 2380;
          ext1 = #0#4;
          |SUB_EJECUTA_NP "caextrap.run";
          |POPV;
     |FINSI;
|FPRC;

|PROCESO Min;
     #0#0 = "      ";
|FPRC;

|PROCESO Max;
     #0#0 = "zzzzzz";
|FPRC;

|PROCESO marhu209;
     eaTag = "PORTATIL" + #0#0 + #0#1;
     |HAZ SacaRiesgo;
     |SI nRieUtili ] nRieTotal; |Y nRieTotal ! 0;
     |SINO;
          |BORRA 020#0a;
     |FINSI;
|FPRC;

|DEFBUCLE marhu209;
     #0#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, marhu209;
|FIN;

|PROGRAMA;
     aParam = PARAMETRO;
     |NOME_DAT #0 aParam;

     |BUCLE marhu209;

     |ABRE #0;
     |LEE 000#0p;
     nSal = FSalida;
     |CIERRA #0;

     |SI nSal = 0;
          |CONFI "0000SVer Clientes con riesgo superado...";
          |SI FSalida = 0;
               |ENTLINEAL #1#0, -1, 4, 22, 1, Min, Max;
          |FINSI;
     |FINSI;
|FPRO;
