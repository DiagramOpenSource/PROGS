|FICHEROS;
    retmpcar #0;
    m2incpre #1;  ||  Premios, Sorteos, loteria, etc.
    m2jerant #2;  ||  Rendimientos netos ejercicios anteriores.
    m2increg #3;  ||  Incremento y disminucion Patrimonial Regular.
    m2incire #4;  ||  Incremento y disminucion Patrimonial Irregular.
    m2incpen #5;  ||  Incrementos pendientes por operaciones a plazo.
|FIN;

|VARIABLES;
    &swdef = 0;
    &empresa   = 0;
    &fichero = "";
    &lin_tmp = 0;
    &path  = "";
    relleno = "                    ";
|FIN;

|PROCESO graba_tmp;
         |DDEFECTO #0;
         lin_tmp = lin_tmp + 1;
         #0#0 = lin_tmp;
         |SI swdef = 15; #0#1 = #3#13; |FINSI;
         |SI swdef = 16; #0#1 = #4#13; |FINSI;
         |SI swdef = 17; #0#1 = #1#5;  |FINSI;
         |SI swdef = 18; #0#1 = #5#13; |FINSI;
         |SI swdef = 19; #0#1 = #2#5;  |FINSI;
         #0#1 = relleno + #0#1;
         |GRABA 020#0n;
|FPRC;

|DEFBUCLE reincpen;
          #5#1;
          ;
          empresa, 01;
          empresa, 99;
          e;
          NULL,graba_tmp;
|FIN;

|DEFBUCLE reincire;
          #4#1;
          ;
          empresa, 01;
          empresa, 99;
          e;
          NULL,graba_tmp;
|FIN;


|DEFBUCLE reincreg;
          #3#1;
          ;
          empresa, 01;
          empresa, 99;
          e;
          NULL,graba_tmp;
|FIN;

|DEFBUCLE rejerant;
          #2#1;
          ;
          empresa, 01;
          empresa, 99;
          e;
          NULL,graba_tmp;
|FIN;

|DEFBUCLE reincpre;
          #1#1;
          ;
          empresa, 01;
          empresa, 99;
          e;
          NULL,graba_tmp;
|FIN;

|PROGRAMA;
     |PATH_DAT #1 path;
     |PATH_DAT #2 path;
     |PATH_DAT #3 path;
     |PATH_DAT #4 path;
     |PATH_DAT #5 path;
     |NOME_DAT #1 "reincpre";
     |NOME_DAT #2 "rejerant";
     |NOME_DAT #3 "reincreg";
     |NOME_DAT #4 "reincire";
     |NOME_DAT #5 "reincpen";
     |NOME_DAT #0 fichero;
     |ABRE #0;
     |SI swdef = 15; |BUCLE reincreg; |FINSI;
     |SI swdef = 16; |BUCLE reincire; |FINSI;
     |SI swdef = 17; |BUCLE reincpre; |FINSI;
     |SI swdef = 18; |BUCLE reincpen; |FINSI;
     |SI swdef = 19; |BUCLE rejerant; |FINSI;
     |CIERRA #0;
|FPRO;
