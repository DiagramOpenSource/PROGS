|PROCESO imp_1;
    nume1 = puntero + 1;
|SI #1nume1 = 1;
    |ABRE #7;
    |PATH_DAT #6 path;
        error = -1;
    |PARA; |SI error ! 0; |HACIENDO;
        |ABRE #6;
            error = FSalida;
    |SIGUE;
        #7#0 = codi;
        #7#1 = 1;             || bucle de lectura de lineas
    |LEE 001#7=;
    |SI FSalida ! 0;  |LEE 001#7];  |FINSI;
    |PARA; |SI FSalida = 0;|Y #7#0 = codi; |HACIENDO;
            #6#0 = #7#0;
            #6#1 = #7#1;
        |LEE 001#6=;
        |SI FSalida = 0;   |BORRA 020#6a;   |FINSI;
        |PARA nume1 = 0; |SI nume1 [ campos_1; |HACIENDO nume1 = nume1 + 1;
                #6nume1 = #7nume1;
        |SIGUE;
        |GRABA 020#6n;
        |BORRA 020#7a;
        |LEE 001#7s;
    |SIGUE;
    |CIERRA #6;
    |CIERRA #7;
|FINSI;
|FPRC;

