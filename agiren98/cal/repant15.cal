  anyo2   = #0#25 % -102;
  anyon1  = anyo1;
  anyon2  = anyo2;
  difer   = anyon1 - anyon2;

                             #0#27 = 1.020;
  |SI #0#25 = "31.12.1994";  #0#27 = 1.083;  |FINSI;
  |SI anyo2 = 95;            #0#27 = 1.083;  |FINSI;
