|PROCESO continua;
  |SI #0#11 > #0#28;
      #0#14 = 0;
      #0#16 = 0;
      #0#18 = 0;
      #0#22 = 0;
  |SINO;
      #0#13 = 0;
      #0#15 = 0;
      #0#17 = 0;
      #0#19 = 0;
      #0#20 = 0;
      #0#21 = 0;
  |FINSI;

  |SI #0#11 > #0#28;
      |SI #0#19 > #0#17; #0#19 = #0#17; |FINSI;
      #0#20 = #0#17 - #0#19;

      |SI modelito = 1; |Y Campo < 20;
          #0#21 = #0#20;
      |FINSI;

      |SI #0#21 > #0#20;  #0#21 = #0#20;  |FINSI;

      #0#14 = 0;
      #0#16 = 0;
      #0#18 = 0;
      #0#22 = 0;
  |FINSI;

  |SI #0#11 [ #0#28;
