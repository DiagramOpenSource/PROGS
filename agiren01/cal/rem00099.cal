|PROCESO ValorExento;  |TIPO 7;
     |ABRE #100;
     #100#0 = #20#0;
     |LEE 040#100=;
     |SI FSalida ! 0;  |DDEFECTO #100;  |FINSI;
     |CIERRA #100;

     #20#23 = 0;
     |SI #20#1 = "A1"; |Y #20#22 = "S";  |Y #100#10 = "S";
         #20#23 = #401#90;
         |SI #20#3 = "GANANCIAL ";
              #20#23 = #401#91;
         |FINSI;
         |SI #20#23 > #20#10;
             #20#23 = #20#10;
         |FINSI;
         |PINTA #20#23;
     |FINSI;
|FPRC;

|PROCESO Titulos;  |TIPO 5;
