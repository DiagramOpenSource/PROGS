|PROCESO Seleccion; |TIPO 0;
     |SI #0#0 = "S";
         |CAMPO_MODIFICA #0#01, 1, "S";
         |CAMPO_MODIFICA #0#02, 1, "S";

         |PARA nCampo = 3; |SI nCampo [ 32; |HACIENDO nCampo = nCampo + 1;
               #0nCampo = 0; |PINTA #0nCampo;
               |CAMPO_MODIFICA #0nCampo, 1, "N";
         |SIGUE;
     |SINO;
         #0#1 = 00001; |PINTA #0#1;    |CAMPO_MODIFICA #0#01, 1, "N";
         #0#2 = 99999; |PINTA #0#2;    |CAMPO_MODIFICA #0#02, 1, "N";

         |PARA nCampo = 3; |SI nCampo [ 32; |HACIENDO nCampo = nCampo + 1;
               |CAMPO_MODIFICA #0nCampo, 1, "S";
         |SIGUE;
     |FINSI;
|FPRC;

|PROCESO Importe;    || Calculo para poner las comas en el importe
     enCanti   = nImporte;
     enSwTrata = 1;
     eaMoneda  = "EUROS  ";
     |HAZ SeparaImporte;
     aImporte   = eaImporte;
|FPRC;

|PROCESO AbreWord1;
     |DBASE aPathDocu;
     |QBF aPathDocu;
     aPathDocu = aPathDocu + "word/";


     aAlfa = "C:/MODELOS";                   |RMKDIR aAlfa;
     aAlfa = "C:/MODELOS/DOCUMENTOS";        |RMKDIR aAlfa;

     aOrigen   = aPathDocu + aDocumento;
     aDestino  = "C:/MODELOS/DOCUMENTOS/" + aDocumento;

     aIPRemoto = "";
     |IP_REMOTO aIPRemoto;
     |QBT aIPRemoto;

     |SI aIPRemoto ! "";
         |ENVIA_FICHERO aOrigen, aDestino;
     |SINO;
         |COPIA_FICHERO aOrigen, aDestino;
     |FINSI;

     nWord  = 1;
     aAbre1 = aDestino;

     |MENSA "      Abriendo el Documento, espere un Momento.";

     |WORD_ABRE nWord, aAbre1;
     nWord1 = nWord;
     |WORD_PROPIEDADES nWord1, "Visible", "NO";

     |MENSA "      Asignando Campos, espere un Momento.";
|FPRC;

|PROCESO AbreWord2;
     |DBASE aPathDocu;
     |QBF aPathDocu;
     aPathDocu = aPathDocu + "word/";

     aAlfa = "C:/MODELOS";                   |RMKDIR aAlfa;
     aAlfa = "C:/MODELOS/DOCUMENTOS";        |RMKDIR aAlfa;

     aOrigen   = aPathDocu + aDocumento;
     aDestino  = "C:/MODELOS/DOCUMENTOS/" + aDocumento;

     aIPRemoto = "";
     |IP_REMOTO aIPRemoto;
     |QBT aIPRemoto;

     |SI aIPRemoto ! "";
         |ENVIA_FICHERO aOrigen, aDestino;
     |SINO;
         |COPIA_FICHERO aOrigen, aDestino;
     |FINSI;

     nWord  = 2;
     aAbre2 = aDestino;

     |WORD_ABRE nWord, aAbre2;

     nWord2 = nWord;
     |WORD_PROPIEDADES nWord2, "Visible", "NO";
|FPRC;

|PROCESO ImprimeWord;
     |MENSA "      Imprimiendo Documento, espere un Momento.";

     aAsigna = "";
     aIdenti = "!<<PEGAR>>";
     |WORD_ASIGNA_TEXTO nWord1, aIdenti, aAsigna;

     |WORD_PROPIEDADES nWord1, "limpiafinal", "";
     |SI #0#44 = "V";
         |WORD_PROPIEDADES nWord1, "Visible", "SI";
         FSalida = "NO";
         |PARA;  |SI FSalida = "NO";  |HACIENDO;
                 |PULSATECLA;
                 |WORD_PROPIEDADES nWord1, "Quit", "";
         |SIGUE;
     |SINO;
         |SI nDialogo = 0;
             |WORD_IMPRIME nWord1, "Dialogo";
             nDialogo = 1;
         |SINO;
             |WORD_IMPRIME nWord1, "";
         |FINSI;
         |SLEEP 10;
         |WORD_SALVA nWord1, "";
     |FINSI;

     |RFBORRA aAbre1;
|FPRC;

|PROCESO CompletaLineas;
     |PARA nCampo = nContador; |SI nCampo [ 50; |HACIENDO nCampo = nCampo + 1;
           aAsigna = "";
           aIdenti = "!#"+ nCampo + "#";
           |WORD_ASIGNA_TEXTO nWord2, aIdenti, aAsigna;
     |SIGUE;

     |WORD_SALVA nWord2, "";

     |WORD_CARGA_TEXTO nWord1, "!<<PEGAR>>", aAbre2;
     |RFBORRA aAbre2;
|FPRC;

|PROCESO AbreOtro;
     |WORD_SALVA nWord2, "";

     |WORD_CARGA_TEXTO nWord1, "!<<PEGAR>>", aAbre2;

     |RFBORRA aAbre2;

     |HAZ AbreWord2;
|FPRC;

|PROCESO Asigna;
     nContador = nContador + 1;
     aIdenti   = "!#" + nContador + "#";
     |WORD_ASIGNA_TEXTO nWord2, aIdenti, aAsigna;
     |SI FSalida ! 0;
         aAlfa1 = aAsigna;
         |HAZ AbreOtro;

         aAsigna   = "RENTA : " + (("00000" + #1#0) % -105) + " " + #1#1;
         nContador = 1;
         aIdenti   = "!#" + nContador + "#";
         |WORD_ASIGNA_TEXTO nWord, aIdenti, aAsigna;

         |SI nSwLiqui = 0;
             aAsigna   = "";
             nContador = 2;
             aIdenti   = "!#" + nContador + "#";
             |WORD_ASIGNA_TEXTO nWord, aIdenti, aAsigna;

             aAsigna   = aAlfa1;
             nContador = 3;
             aIdenti   = "!#" + nContador + "#";
             |WORD_ASIGNA_TEXTO nWord, aIdenti, aAsigna;
         |SINO;
             aAsigna   = "LIQUIDACION " + #1001#2;
             nContador = 2;
             aIdenti   = "!#" + nContador + "#";
             |WORD_ASIGNA_TEXTO nWord, aIdenti, aAsigna;

             aAsigna   = "";
             nContador = 3;
             aIdenti   = "!#" + nContador + "#";
             |WORD_ASIGNA_TEXTO nWord, aIdenti, aAsigna;

             aAsigna   = aAlfa1;
             nContador = 4;
             aIdenti   = "!#" + nContador + "#";
             |WORD_ASIGNA_TEXTO nWord, aIdenti, aAsigna;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO FiltroImpre;
     |SI nImporte = 0;  |Y #0#43 = "N";
         |ACABA;
     |FINSI;

     |HAZ Asigna;
|FPRC;

|PROCESO ConverAyN;
     |SI nImporte = 0;  |Y #0#43 = "N";
         |ACABA;
     |FINSI;

     |HAZ Importe;

     aAsigna = aAlfa1 + aImporte;
     |HAZ Asigna;
|FPRC;

|PROCESO ConverAyNN;
     |SI nImporte1 = 0;  |Y nImporte2 = 0;  |Y #0#43 = "N";
         |ACABA;
     |FINSI;

     nImporte = nImporte1;  |HAZ Importe;  aAlfa2 = aImporte;
     nImporte = nImporte2;  |HAZ Importe;  aAlfa3 = aImporte;

     aAsigna = aAlfa1 + aAlfa2 + " " + aAlfa3;
     |HAZ Asigna;
|FPRC;

