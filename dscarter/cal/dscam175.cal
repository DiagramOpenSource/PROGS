      |SI FSalida = 0;
         |BUCLE BorraTodos;
      |FINSI;
   |FINSI;
*/
|FPRC;

|RUTINA inf175;
   #dscam175#0 = " ";
   #dscam175#1 = 000000001;
|FRUT;

|RUTINA sup175;
   #dscam175#0 = "z";
   #dscam175#1 = 999999999;
|FRUT;

|PROGRAMA;
   |CLS;

   |ENTLINEAL #1#dscam175, -2, 4, 21, 1, inf175, sup175;
|FPRO;
