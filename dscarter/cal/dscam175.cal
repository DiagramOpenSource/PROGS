|FICHEROS;
   dscam175 #0;
|FIN;

|VARIABLES;
   aAlfa = "";
|FIN;

|PROCESO MiraTecla; |TIPO 11;
   |SI FSalida = 11;
      aAlfa = "    " + &191 + " Esta seguro de eliminar el bloqueo seleccionado ? ";
      |CONFI aAlfa;
      |SI FSalida = 0;
         |BORRA 020#dscam175.a;
      |FINSI;
      |ERROR; |PTEC 808; |PONCONTROL 23, "SI";
   |FINSI;
