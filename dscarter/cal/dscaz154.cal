   |SI #dscam004#5 ! "REM"; |Y #dscam004#5 ! "COB"; |Y #dscam004#5 ! "RPT";
    |Y #dscam004#5 ! "IMP"; |Y #dscam004#5 ! "CAC";
       |ACABA;
   |FINSI;
   |SI #dscam004#5 = "REM"; |O #dscam004#5 = "COB";
    |O #dscam004#5 = "RPT"; |O #dscam004#5 = "CAC";
      nSignoOp = 1;
   |FINSI;
   |SI #dscam004#5 = "IMP";
      nSignoOp = -1;
   |FINSI;

   aAlfa = "Comprobando recibo " + #dscam004#17;
