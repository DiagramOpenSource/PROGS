|FICHEROS;
   dscam094 #0;
   canempr@ #1;
|FIN;

|VARIABLES;
   &Fichero = ""; &Tipo = ""; &Cmp = ""; &CmpNum = 0;
   Tecla = 0;
   Comodin = "";
|FIN;

|PROCESO ListaConta;
   Tecla = FSalida;
   |SI Tecla = 2; |ACABA; |FINSI;
   Comodin = #48#16; |QBF Comodin;
   |SI Comodin = ""; |ACABA; |FINSI;
   Comodin = Comodin + "def/canempre.mas";
   |CARGA_DEF Comodin, canempr@;
   |SI FSalida = 0;
      Comodin = #48#16; |QBF Comodin;
      Comodin = Comodin + "fich/";
      |PATH_DAT #1 Comodin; |ABRE #canempr@;
      |SI Tecla = 10;
         |CONSULTA_CLAVE #1#canempr@;
         |SI FSalida ] 0;
            Fichero = "Empresas"; Tipo = "C"; Cmp = "Codigo Empresa"; |HAZ ObtenDato;
            #dscam094#2 = #canempr@#CmpNum#; |PINTA #dscam094#2;
            Fichero = "Empresas"; Tipo = "C"; Cmp = "Nombre Comercial"; |HAZ ObtenDato;
            #dscam094#3 = #canempr@#CmpNum#; |PINTA #dscam094#3;
         |FINSI;
         |ERROR;
      |FINSI;
      Fichero = "Empresas"; Tipo = "C"; Cmp = "Codigo Empresa"; |HAZ ObtenDato;
      #canempr@#CmpNum# = #dscam094#2;
      |LEE 000#canempr@.];
      |SI FSalida = 0; |Y #canempr@#CmpNum# = #dscam094#2;
         Fichero = "Empresas"; Tipo = "C"; Cmp = "Nombre Comercial"; |HAZ ObtenDato;
         #dscam094#3 = #canempr@#CmpNum#; |PINTA #dscam094#3;
      |SINO;
         |MENAV "    Empresa contable inexistente "; |ERROR;
      |FINSI;
      |CIERRA #canempr@; |DESCARGA_DEF #canempr@;
   |FINSI;
|FPRC;

|RUTINA inf;
   #dscam094#0 = " ";
   #dscam094#1 = "     ";
|FRUT;

|RUTINA sup;
   #dscam094#0 = "z";
   #dscam094#1 = "zzzzz";
|FRUT;

|PROGRAMA;
   |ENTLINEAL #1#dscam094,-2,1,20,1,inf,sup;
|FPRO;
