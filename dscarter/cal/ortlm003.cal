|FICHEROS;
   ortlm003 #0;

   agif091@ #100;
|FIN;

|VARIABLES;
   aAlfa  = "";
   aAlfa1 = "";

   nModoEntrada = 0;
   nFlag        = 0;
|FIN;

|PROCESO T02m001; |TIPO 2;
   nModoEntrada = (FEntrada / 100) ! 0;
   nFlag = 0 +. 1;
   |SI nModoEntrada = 1; |Y nFlag = 1;
      aAlfa = #48#17; |QBF aAlfa;
      |SI aAlfa ! "";
         aAlfa1 = aAlfa + "def/agifa091.mas";
         |CARGA_DEF aAlfa1, agif091@;
         |SI FSalida = 0;
            aAlfa1 = aAlfa + "fich/" + (("00000" + #48#15) % -105) + "/";
            |PATH_DAT #agif091@#aAlfa1#;
            |ABRE #agif091@;
            #agif091@#0 = #ortlm003#0;
            |LEE 000#agif091@.=;
            |SI FSalida = 0;
               #ortlm003#1 = #agif091@#1; |PINTA #ortlm003#1;
            |SINO;
               |MENAV "    Forma de pago inaccesible"; |ERROR;
            |FINSI;
            |CIERRA #agif091@;
            |DESCARGA_DEF #agif091@;
         |FINSI;
      |FINSI;
   |FINSI;
|FPRC;

|PROCESO T66m001; |TIPO 66;
   aAlfa = #48#17; |QBF aAlfa;
   |SI aAlfa ! "";
      aAlfa1 = aAlfa + "def/agifa091.mas";
      |CARGA_DEF aAlfa1, agif091@;
      |SI FSalida = 0;
         aAlfa1 = aAlfa + "fich/" + (("00000" + #48#15) % -105) + "/";
         |PATH_DAT #agif091@#aAlfa1#;
         |ABRE #agif091@;
         |CONSULTA_CLAVE #1#agif091@;
         |SI FSalida = 0;
            #ortlm003#0 = #agif091@#0;
            #ortlm003#1 = #agif091@#1;
            |GRABA 020#ortlm003.c;
         |FINSI;
         |CIERRA #agif091@;
         |DESCARGA_DEF #agif091@;
      |FINSI;
   |FINSI;
   |PONCONTROL 23, "si";
|FPRC;

|RUTINA Infm001;
   #ortlm003#0 = "  ";
|FRUT;

|RUTINA Supm001;
   #ortlm003#0 = "zz";
|FRUT;

|PROGRAMA;
   |ABRE #ortlm003;
   |CLS;
   |CABEZA "DIAS DE VENCIMIENTO DE LA FORMA DE PAGO";

   |PINPA #0#0; |PINDA #0#0;
   |ENTLINEAL #1#ortlm003, 1, -1, 21, 1, Infm001, Supm001;

   |CIERRA #ortlm003;
|FPRO:
