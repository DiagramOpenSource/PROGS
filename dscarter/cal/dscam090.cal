|FICHEROS;
   dscam090 #0;
|FIN;

|VARIABLES;
   nCalc    = 0;
   nCalc1   = 0;
   nSwAcaba = 0;
   nTecla   = 0;
|FIN;

|PROCESO Calcula;
   |ACABA;
   nTecla = FSalida;
   |SI nTecla = 2; |ACABA; |FINSI;

   nCalc = (Campo + 1) / 3;
   nCalc1 = nCalc ! 0;
   |SI nCalc = nCalc1; |Y #dscam090#Campo# ! 999999999.99; |Y nSwAcaba = 0;
      nCalc = Campo + 2;
      nCalc1 = #dscam090#Campo# + 1;
      #dscam090#nCalc# = nCalc1;       |PINTA #dscam090#nCalc#;
      nCalc = nCalc + 1;
      |SI #dscam090#nCalc# = 0;
         #dscam090#nCalc# = 999999999.99; |PINTA #dscam090#nCalc#;
      |FINSI;
   |SINO;
      nCalc = Campo / 3;
      nCalc1 = nCalc ! 0;
      |SI nCalc = nCalc1;
         nCalc = Campo - 1;
         |SI #dscam090#nCalc# = 999999999.99;
            nSwAcaba = 1; |PTEC 815;
         |FINSI;
      |FINSI;
   |FINSI;
|FPRC;

|PROGRAMA;
   |ABRE #dscam090;
   |LEE 000#dscam090.p;
   |SI FSalida ! 0;
      |DDEFECTO #dscam090; |GRABA 020#dscam090.c;
   |FINSI;

   nSwAcaba = 0;
   |CLS;
   |CABEZA " Tabla para el calculo de timbres ";
   |PINPA #0#0; |PINDA #0#0;
   |ENDATOS #1#0;
   |SI FSalida = 0;
      |GRABA 020#dscam090.a;
   |FINSI;
   |CIERRA #dscam090;
|FPRO;
