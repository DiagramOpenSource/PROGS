|FICHEROS;
   dscam099 #0;
   dscam100 #1;

   agifm003@ #100;
|FIN;

|VARIABLES;
   aAlfa = "";
   nSw = 0;
|FIN;

|PROCESO ConsGrupos; |TIPO 66;
   #agifm003@#0 = #dscam100#1;
   |LEE 000#agifm003@.];
   |CONSULTA_CLAVE #1#agifm003@;
   |SI FSalida = 0;
      #dscam100#1 = #agifm003@#0;
      #dscam100#2 = #agifm003@#1; |PINTA #dscam100#2;
   |FINSI;
   |ERROR;
|FPRC;

|PROCESO Intro; |TIPO 3;
   #agifm003@#0 = #dscam100#1;
   |LEE 000#agifm003@.=;
   |SI FSalida = 0;
      #dscam100#2 = #agifm003@#1;
   |SINO;
      |MENAV "    No existe el grupo indicado"; |ERROR;
   |FINSI;
|FPRC;

|PROCESO Carga; |TIPO 8;
   aAlfa = #48#17; |QBF aAlfa;
   aAlfa = aAlfa + "def/agifm003.mas";
   |CARGA_DEF aAlfa, agifm003@;
   |SI FSalida = 0;
      aAlfa = #48#17; |QBF aAlfa;
      aAlfa = aAlfa + "fich/" + (("00000" + #48#15) % -105) + "/";
      |QBF aAlfa; |PATH_DAT #100 aAlfa; |ABRE #agifm003@
      nSw = 0;
   |SINO;
      |MENAV "    No puedo encontrar el fichero de gestion de grupos de clientes";
      nSw = 1;
      |PTEC 807;
   |FINSI;
|FPRC;

|PROCESO Descarga; |TIPO 9;
   |SI nSw = 0;
      |CIERRA #agifm003@; |DESCARGA_DEF #agifm003@;
   |FINSI;
|FPRC;
