|FICHEROS;
   dscam101 #0;
   dscam102 #1;

   agifa091@ #100;
|FIN;

|VARIABLES;
   aAlfa = "";
   nSw = 0;
|FIN;

|PROCESO ConsGrup83; |TIPO 66;
   #agifa091@#0 = #dscam102#1;
   |LEE 000#agifa091@.];
   |CONSULTA_CLAVE #1#agifa091@;
   |SI FSalida = 0;
      #dscam102#1 = #agifa091@#0;
      #dscam102#2 = #agifa091@#1; |PINTA #dscam102#2;
   |FINSI;
   |ERROR;
|FPRC;

|PROCESO Intro83; |TIPO 3;
   #agifa091@#0 = #dscam102#1;
   |LEE 000#agifa091@.=;
   |SI FSalida = 0;
      #dscam102#2 = #agifa091@#1;
   |SINO;
      |MENAV "    No existe el grupo indicado"; |ERROR;
   |FINSI;
|FPRC;

|PROCESO Carga83; |TIPO 8;
   aAlfa = #48#17; |QBF aAlfa;
   aAlfa = aAlfa + "def/agifa091.mas";
   |CARGA_DEF aAlfa, agifa091@;
   |SI FSalida = 0;
      aAlfa = #48#17; |QBF aAlfa;
      aAlfa = aAlfa + "fich/" + (("00000" + #48#15) % -105) + "/";
      |QBF aAlfa; |PATH_DAT #100 aAlfa; |ABRE #agifa091@
      nSw = 0;
   |SINO;
      |MENAV "    No puedo encontrar el fichero de gestion de grupos de clientes";
      nSw = 1;
      |PTEC 807;
   |FINSI;
|FPRC;

|PROCESO Descarga83; |TIPO 9;
   |SI nSw = 0;
      |CIERRA #agifa091@; |DESCARGA_DEF #agifa091@;
   |FINSI;
|FPRC;
