|FICHEROS;
   dscam085 #0;

   dscam083 #10;
|FIN;

|VARIABLES;
   &PRMNT_enPerfil = 0;

   aAlfa    = "";
   aAntTipo = "";

   nAntPerf = 0;
|FIN;

|PROCESO MiraPerfil;
   |SI #dscam085#1 = "F";
      |C_M #dscam085#2,1,"S";
   |SINO;
      |C_M #dscam085#2,1,"N";
      #dscam085#2 = 00; |PINTA #dscam085#2;
      |DDEFECTO #dscam083;
      |PINTA 1225, #dscam083#1;
   |FINSI;
|FPRC;

|PROCESO TomaTipo; |TIPO 7;
   aAntTipo = #dscam085#1; nAntPerf = #dscam085#2;
|FPRC;

|PROCESO Tipo03m085; |TIPO 3;
   |GRABA 020#dscam085.a;
   |SALVA_FICHA #dscam085;
   |SI aAntTipo ! #dscam085#1; |O nAntPerf ! #dscam085#2;
      aAlfa = Usuario % 0810; |QBF aAlfa;
      aAlfa = (aAlfa + (" " * 10)) % 0110;
      |SI aAlfa = #dscam085#0;
         |PUSHV 0501 2380;
         |HAZ Entrada;
         |POPV;
      |FINSI;
   |FINSI;
   |REPON_FICHA #dscam085;
|FPRC;
