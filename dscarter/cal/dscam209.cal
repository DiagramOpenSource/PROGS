|FICHEROS;
   dscam209 #0;

   &cacuent@ #100;
|FIN;

|VARIABLES;
   &MaxDigit = 0;

   aAlfa  = "";
   aAlfa1 = "";
   aAlfa2 = "";

   nCalc  = 0;

   nTecla = 0;
|FIN;

|PROCESO MiraCuenta;
   || Convierto los puntos de la cuenta contable ...
   aAlfa = #dscam209#Campo#; |QBF aAlfa;
   aAlfa = aAlfa - ".";
   |SI FSalida ! 0;
      |PARA; |SI FSalida ! 0; |HACIENDO;
         nCalc = ((FSalida / 100) ! 0) + 99; aAlfa1 = aAlfa % nCalc;
         nCalc = FSalida + 98;               aAlfa2 = aAlfa % nCalc;
         aAlfa = aAlfa1 + aAlfa2; aAlfa = aAlfa % 0; nCalc = aAlfa;
         nCalc = MaxDigit - nCalc;
         aAlfa = aAlfa1 + ("0" * nCalc) + aAlfa2;

         aAlfa = aAlfa - ".";
      |SIGUE;
      #dscam209#Campo# = aAlfa; |PINTA #dscam209#Campo#;
   |FINSI;

   |SI nTecla = 10;
      #cacuent@#0 = #dscam209#Campo#;
      |LEE 000#cacuent@.];
      |CONSULTA_CLAVE #1#cacuent@;
      |SI FSalida = 0;
         #dscam209#Campo# = #cacuent@#0; |PINTA #dscam209#Campo#;
      |FINSI;
      |ERROR; |ACABA;
   |FINSI;

   #cacuent@#0 = #dscam209#Campo#;
   |LEE 000#cacuent@.=;
   |SI FSalida ! 0;
      |MENAV "    No existe la cuenta contable indicada";
      |ERROR; |ACABA;
   |FINSI;
|FPRC;

|PROCESO Tipo11209; |TIPO 11;
     |SI FSalida ! 7;
          nTecla = FSalida; |HAZ MiraCuenta;
     |FINSI;
|FPRC;

|PROCESO Tipo00209;
     |SI FSalida ! 7;
          nTecla = FSalida; |HAZ MiraCuenta;
     |FINSI;
|FPRC;

|PROGRAMA;
   |HAZ Tipo8;

   |ABRE #dscam209;
   |LEE 000#dscam209.p;
   |SI FSalida ! 0;
      |DDEFECTO #dscam209;
      |GRABA 020#dscam209.c;
   |FINSI;

   |CLS;
   |CABEZA "Parametros Cash-Flow";
   |PINPA #0#0; |PINDA #0#0;
   |ENDATOS #1#0;
   |SI FSalida = 0;
      |GRABA 020#dscam209.a;
   |FINSI;

   |CIERRA #dscam209;
   |HAZ Tipo9;
|FPRO;
