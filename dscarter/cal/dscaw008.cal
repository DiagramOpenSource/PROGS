|FICHEROS;
   dscaw008 #0;
|FIN;

|VARIABLES;
   Cont = 0;
|FIN;

|PROCESO MiraEnlace;
   |SI #dscaw008#Campo# = "X";
      |PARA Cont = 0; |SI Cont [ 6; |HACIENDO Cont = Cont + 1;
         |SI Cont = 6; Cont = 10; |FINSI;
         |SI Campo ! Cont; #dscaw008#Cont# = " "; |PINTA #dscaw008#Cont#; |FINSI;
         |SI Cont = 10; Cont = 6; |FINSI;
      |SIGUE;
   |FINSI;
|FPRC;

|PROCESO MiraCampo;
   |SI #dscaw008#Campo# = "X";
      |PARA Cont = 6; |SI Cont [ 9; |HACIENDO Cont = Cont + 1;
         |SI Campo ! Cont; #dscaw008#Cont# = " "; |PINTA #dscaw008#Cont#; |FINSI;
      |SIGUE;
   |FINSI;
|FPRC;
