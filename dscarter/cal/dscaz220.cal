          |LEE 000#gestr070@.p;
          |PARA; |SI FSalida = 0; |HACIENDO;
               aCta = #gestr070@#10; |QBF aCta;
               |SI aCta = aAlfa;
                    #8#32 = #gestr070@#0;
                    |GRABA 020#8a;

                    #agifa079@#66 = #8#0;
                    #agifa079@#0 = #8#1;
                    |LEE 101#agifa079@.=;
