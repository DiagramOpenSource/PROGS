|FICHEROS;
   dscam001 #10,MANTE;
   dscam002 #11,MANTE;
|FIN;

|VARIABLES;
   aAlfa = "";
   aEnt  = "";
   aEmp  = "";
   aSuc  = "";

   nCalc = 0;
   nSw = 0;
|FIN;

|PROGRAMA;
   aSuc = "";
   aAlfa = PARAMETRO;
   aAlfa = aAlfa - ",";
   nCalc = FSalida + 98; aEnt = aAlfa % nCalc;
   aEmp = aAlfa - aEnt;

   nSw = 1;
   aEnt = aEnt - "/";
   |SI FSalida ! 0;
      nCalc = FSalida + 98; aSuc = aEnt % nCalc;
      aEnt = aEnt - aSuc;
      nSw = 2;
   |FINSI;

   |DBASE aAlfa; |QBF aAlfa;
   aAlfa = aAlfa + "fich/" + aEmp + "/";
   |PATH_DAT #10 aAlfa; |PATH_DAT #11 aAlfa;
   |ABRE #dscam002; |ABRE #dscam001;
   |CLS;
   |SI nSw = 1;
      |CABEZA " Entidades bancarias ";
      #dscam001#0 = aEnt;
      |PINPA #0#10; |PINDA #0#10;
      |CONFI "    NDar de alta ? ";
      |SI FSalida = 0;
         |GRABA 020#dscam001.b;
         |ENDATOS #1#dscam001;
         |SI FSalida = 0;
            |GRABA 020#dscam001.a;
         |SINO;
            |BORRA 020#dscam001.a; |ERROR;
         |FINSI;
         |LIBERA #dscam001;
      |FINSI;
   |SINO;
      #dscam001#0 = aEnt;
      |LEE 000#dscam001.=;
      |CABEZA " Sucursales bancarias ";
      #dscam002#2 = #dscam001#1;

      #dscam002#0 = aEnt;
      #dscam002#1 = aSuc;
      |PINPA #0#11; |PINDA #0#11;
      |CONFI "    NDar de alta ? ";
      |SI FSalida = 0;
         |GRABA 020#dscam002.b;
         |ENDATOS #1#dscam002;
         |SI FSalida = 0;
            |GRABA 020#dscam002.a;
         |SINO;
            |BORRA 020#dscam002.a; |ERROR;
         |FINSI;
         |LIBERA #dscam002;
      |FINSI;
      |CIERRA #dscam002;
   |FINSI;
|FPRO;
