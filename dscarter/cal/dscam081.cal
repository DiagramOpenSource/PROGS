|FICHEROS;
   dscam081 #0;
   dscam082 #1;
|FIN;

|VARIABLES;
   aAlfa = "";

   nTipoRec = 0;
   nCont    = 0;
|FIN;

|PROCESO PintaTipo; |TIPO 13;
   |HAZ TipoClasif;
|FPRC;

|PROCESO MiraSiEsta;
   nTipoRec = #dscam082#1; nCont = 0;
   |SALVA_FICHA #dscam082;
   |SELECT #2#dscam082;
   #dscam082#1 = nTipoRec;
   |LEE 000#dscam082.=;
   |PARA; |SI FSalida = 0; |Y #dscam082#1 = nTipoRec; |HACIENDO;
      nCont = nCont + 1;
      |LEE 000#dscam082.s;
   |SIGUE;
   |SELECT #1#dscam082;
   |REPON_FICHA #dscam082;
   |SI nCont > 1;
       |MENAV "    El tipo de recibo ya existe en otra clasificacion";
       |ERROR;
   |FINSI;
|FPRC;

|PROCESO TipoClasif;
   aAlfa = "            ";
   |SI #dscam081#2 = "C"; aAlfa = "Contado     "; |FINSI;
   |SI #dscam081#2 = "R"; aAlfa = "Remesable   "; |FINSI;
   |SI #dscam081#2 = "P"; aAlfa = "Talon/Pagare"; |FINSI;
   |SI #dscam081#2 = "A"; aAlfa = "A aceptar   "; |FINSI;
   |SI #dscam081#2 = "O"; aAlfa = "Confirming  "; |FINSI;
   |SI #dscam081#2 = "F"; aAlfa = "Factoring   "; |FINSI;
   |ATRI I; |PINTA 0964, aAlfa; |ATRI N;
|FPRC;
