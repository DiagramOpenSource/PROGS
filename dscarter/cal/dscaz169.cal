|FICHEROS;
   dscaz169 #0;

   dscaw137 #10,MANTE;
   dscam051 #11;
|FIN;

|VARIABLES;
   &PRMNT_Init = 0;
   &aInforme   = "";
   aAlfa       = "";

   nBoton1 = 0;
   nBoton2 = 0;
|FIN;

|RUTINA Inf;
   #dscaw137#0 = 00000;
   #dscaw137#1 = 0;
   #dscaw137#2 = 00;
   #dscaw137#3 = "01.01.0000";
   #dscaw137#4 = 000000;
   #dscaw137#5 = 0000;
|FRUT;

|RUTINA Sup;
   #dscaw137#0 = 99999;
   #dscaw137#1 = 9;
   #dscaw137#2 = 99;
   #dscaw137#3 = "31.12.9999";
   #dscaw137#4 = 999999;
   #dscaw137#5 = 9999;
|FRUT;

|PROCESO Tipo5; |TIPO 5;
   #dscaw137#12 = "1"; |LINEAS_BI_ATRI #dscaw137#12,S,D,-1;
|FPRC;

|PROCESO Impresion;
   |PRINT;
|FPRC;

|DEFBUCLE Impresion;
#dscaw137#1;
;
INICIO;
FINAL;
;
NULL, Impresion;
|FIN;

|PROCESO Imprimir;
   |IMPRE 0;
   |SI FSalida = 0;
      aInforme = "Impresion resumen contable"; |HAZ ObtenInforme;
      |SI aInforme = "Impresion resumen contable"; aInforme = "dscaz169"; |FINSI;
      |INFOR aInforme;
      |SI FSalida = 0;
         |BUCLE Impresion;
         |PIEINF; |FININF;
      |FINSI;
      |FINIMP;
   |FINSI;
|FPRC;

|PROCESO Salir;
   |PTEC 807;
|FPRC;

|PROGRAMA;
   |SI PARAMETRO ! "";
      |ABRE #dscam051;
      |LEE 000#dscam051.p;
      |SI FSalida ! 0; |DDEFECTO #dscam051; |FINSI;
      |CIERRA #dscam051;

      |SI #dscam051#118 = "N"; |O #dscam051#1 = "N"; |ACABA; |FINSI;

      |SI PARAMETRO = "INICIO";
         PRMNT_Init = 1;
         aAlfa = "rc" + Usuario; |NOME_DAT #dscaw137#aAlfa#;
         |DELINDEX #dscaw137;
      |FINSI;
      |SI PARAMETRO = "FINAL";
         |PUSHV 0617 2364;

         |SI #dscam051#125 = "S";
            |CONTROL_SIMPLE 0, "BOTON,{{205}}Imprimir", 2326, 2339, Imprimir;
            nBoton1 = FSalida;

            |CONTROL_SIMPLE 0, "BOTON,{{1}}Salir", 2341, 2354, Salir;
            nBoton2 = FSalida;
         |FINSI;

         aAlfa = "rc" + Usuario; |NOME_DAT #dscaw137#aAlfa#;
         |ABRE #dscaw137;
         |BLANCO 0610 2372;
         |ENTLINEAL #1#dscaw137, -6, 4, 18, 1, Inf, Sup;
         |CIERRA #dscaw137; |DELINDEX #dscaw137;
         |SI #dscam051#125 = "S";
            |FIN_CONTROL_WINDOWS nBoton1;
            |FIN_CONTROL_WINDOWS nBoton2;
         |FINSI;

         |POPV;
         PRMNT_Init = 0;
      |FINSI;
   |FINSI;
|FPRO;
