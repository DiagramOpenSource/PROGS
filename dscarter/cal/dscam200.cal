|FICHEROS;
   dscam200 #0;
   dscam201 #1;

   dscam058 #10;
|FIN;

|VARIABLES;
   aAlfa        = "";

   nModoEntrada = 0;
   nFlag        = 0;
|FIN;

|PROCESO RevisaTR2;
   #dscam201#0 = #dscam200#0;
   #dscam201#1 = #dscam200#1;
   #dscam201#2 = #dscam058#0;
   |LEE 000#dscam201.=;
   |SI FSalida ! 0;
      |DDEFECTO #dscam201;
      #dscam201#0 = #dscam200#0;
      #dscam201#1 = #dscam200#1;
      #dscam201#2 = #dscam058#0;
      #dscam201#3 = #dscam058#1;
      |GRABA 020#dscam201.n;
   |FINSI;
|FPRC;

|DEFBUCLE RevisaTR2;
#dscam058#1;
;
INICIO;
FINAL;
;
NULL, RevisaTR2;
|FIN;

|PROCESO RevisaTR1;
   #dscam058#0 = #dscam201#2;
   |LEE 000#dscam058.=;
   |SI FSalida ! 0;
      |BORRA 020#dscam201.a;
   |FINSI;
|FPRC;

|DEFBUCLE RevisaTR1;
#dscam201#1;
;
#dscam200#0, #dscam200#1, 01;
#dscam200#0, #dscam200#1, 99;
;
NULL, RevisaTR1;
|FIN;

|PROCESO Tipo00C002F200;
   |SI #dscam200#2 = "N"; |ACABA; |FINSI;
   nModoEntrada = (FEntrada / 100) ! 0;
   nFlag = 0 +. 1;

   |ABRE #dscam058; |ABRE #dscam201;
   |BUCLE RevisaTR1;  |BUCLE RevisaTR2;
   |CIERRA #dscam201; |CIERRA #dscam058;
|FPRC;

|PROCESO PintaEstado;
   aAlfa = "";
   |SI #dscam200#0 = "V";
      |SI #dscam200#1 = "C"; aAlfa = "Parcialmente cobrado     "; |FINSI;
      |SI #dscam200#1 = "e"; aAlfa = "Entrega pdte.compensar   "; |FINSI;
      |SI #dscam200#1 = "E"; aAlfa = "Compensado parcialmente  "; |FINSI;
      |SI #dscam200#1 = "F"; aAlfa = "Recepcionado Confirme    "; |FINSI;
      |SI #dscam200#1 = "I"; aAlfa = "Impagado                 "; |FINSI;
      |SI #dscam200#1 = "L"; aAlfa = "Recibo Liquidado         "; |FINSI;
      |SI #dscam200#1 = "M"; aAlfa = "Gestionado por cobrador  "; |FINSI;
      |SI #dscam200#1 = "P"; aAlfa = "Pendiente de cobro       "; |FINSI;
      |SI #dscam200#1 = "p"; aAlfa = "Entrega pdte.cobro       "; |FINSI;
      |SI #dscam200#1 = "R"; aAlfa = "Remesado                 "; |FINSI;
      |SI #dscam200#1 = "T"; aAlfa = "Pagare/Talon de cliente  "; |FINSI;
      |SI #dscam200#1 = "V"; aAlfa = "Recibo enviado al cliente"; |FINSI;
   |FINSI;
   |SI #dscam200#0 = "C";
      |SI #dscam200#1 = "C"; aAlfa = "Parcialmente pagado      "; |FINSI;
      |SI #dscam200#1 = "e"; aAlfa = "Pendiente de compensar   "; |FINSI;
      |SI #dscam200#1 = "E"; aAlfa = "Englobado en documento   "; |FINSI;
      |SI #dscam200#1 = "H"; aAlfa = "Parcialmente compensada  "; |FINSI;
      |SI #dscam200#1 = "L"; aAlfa = "Recibo Liquidado         "; |FINSI;
      |SI #dscam200#1 = "P"; aAlfa = "Pendiente de pago        "; |FINSI;
   |FINSI;
   |ATRI I; |PINTA 1028, aAlfa; |ATRI N;
|FPRC;
