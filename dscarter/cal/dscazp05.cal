|FICHEROS;
     dscam012 #12;
     dscam051 #51;
|FIN;

|VARIABLES;
     nConta = 0;
|FIN;

|PROCESO ContaRemesa;
     |ABRE #dscam051; |LEE 000#dscam051.p; |CIERRA #dscam051;

     nConta = 1;
     |SI #dscam051#134 = "S";
          |LEE 000#dscam012.p;
          |PARA; |SI FSalida = 0; |HACIENDO;
               |SI #dscam012#0 ! nConta;
                    |SAL;
               |FINSI;
               |LEE 000#dscam012.s;
               nConta = nConta + 1;
          |SIGUE;
     |SINO;
          |LEE 000#dscam012.u;
          |SI FSalida = 0;
               nConta = #dscam012#0 + 1;
          |FINSI;
     |FINSI;
     |DDEFECTO #dscam012;
     #dscam012#0 = nConta;
|FPRC;
