   |ENDATOS #1#0;
   |SI FSalida = 0;
      |IMPRE 0;
      |SI FSalida ! 0; |ACABA; |FINSI;
      aImp = Impresora;

      aAlfa = "tp1" + Usuario; |NOME_DAT #dscaw185#aAlfa#;
      |DELINDEX #dscaw185; |ABRE #dscaw185;

      |ABRE #dscam008;
