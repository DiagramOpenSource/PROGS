            aAlfa = #dscaz018#6; |QBF aAlfa;
            fFecha = #dscaz018#7; fFecha = fFecha % 2;
            aAlfa1 = fFecha % 1199; |QBF aAlfa1;
            eaEncab = aAlfa + ", " + aAlfa1;

