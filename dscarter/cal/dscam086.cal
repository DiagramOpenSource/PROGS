|FICHEROS;
   dscam086 #0;

   dscam087 #10,MANTE;
   dscam088 #11;
   dscam089 #12,MANTE;
   dscaw061 #13,MANTE;

   Ref0001@ #100;
   &Referen@ #1000;
|FIN;

|VARIABLES;
   aAlfa = "";

   nModo = 0;
   nFlag = 0;
|FIN;

|PROCESO Pintas; |TIPO 13;
   |ENTLINEAL #1#dscam087,2,7,14,1,infm087,supm087;
   |ENTLINEAL #1#dscam089,3,7,22,1,infm089,supm089;
|FPRC;

|PROCESO Tipo13Clas; |TIPO 13;
   |ENTLINEAL #1#dscam089,3,7,22,1,infm089,supm089;
|FPRC;

|PROCESO TClas; |TIPO 3;
   |ENTLINEAL #1#dscam089,3,1,22,1,infm089,supm089;
   |ENTLINEAL #1#dscam089,3,7,22,1,infm089,supm089;
|FPRC;

|PROCESO TRecs;
   |ENTLINEAL #1#dscam087,2,1,14,1,infm087,supm087;
   |ENTLINEAL #1#dscam087,2,7,14,1,infm087,supm087;
|FPRC;

|RUTINA infm086;
  #dscam086#0 = 01;
|FRUT;

|RUTINA supm086;
  #dscam086#0 = 99;
|FRUT;

|RUTINA infm087;
  #dscam087#0 = #dscam086#0;
  #dscam087#1 = 01;
|FRUT;

|RUTINA supm087;
  #dscam087#0 = #dscam086#0;
  #dscam087#1 = 99;
|FRUT;

|RUTINA infm089;
  #dscam089#0 = #dscam087#0;
  #dscam089#1 = #dscam087#1;
  #dscam089#2 = 01;
|FRUT;

|RUTINA supm089;
  #dscam089#0 = #dscam087#0;
  #dscam089#1 = #dscam087#1;
  #dscam089#2 = 99;
|FRUT;

|PROCESO MiraCond; |TIPO 2;
   nModo = (FEntrada / 100) ! 0;
   nFlag =  0 +. 1;
   |SI nFlag < 0; |ACABA; |FINSI;

   |SI nModo = 2; |Y #dscam089#5 ! " ";
      #dscaw061#0 = #dscam089#4;
      #dscaw061#1 = #dscam089#5;
      #dscaw061#3 = #dscam089#6;
      #dscaw061#4 = #dscam089#7;
      #dscaw061#5 = #dscam089#8;
      |SI #dscaw061#1 = "A";
         #dscaw061#2 = "Alfanumerico";
         |C_M #dscaw061#3,1,"S"; |C_M #dscaw061#4,1,"N"; |C_M #dscaw061#5,1,"N";
         |C_V #dscaw061#3,1,"S"; |C_V #dscaw061#4,1,"N"; |C_V #dscaw061#5,1,"N";
      |FINSI;
      |SI #dscaw061#1 = "N";
         #dscaw061#2 = "Numerico";
         |C_M #dscaw061#3,1,"N"; |C_M #dscaw061#4,1,"S"; |C_M #dscaw061#5,1,"N";
         |C_V #dscaw061#3,1,"N"; |C_V #dscaw061#4,1,"S"; |C_V #dscaw061#5,1,"N";
      |FINSI;
      |SI #dscaw061#1 = "F";
         #dscaw061#2 = "Fecha";
         |C_M #dscaw061#3,1,"N"; |C_M #dscaw061#4,1,"N"; |C_M #dscaw061#5,1,"S";
         |C_V #dscaw061#3,1,"N"; |C_V #dscaw061#4,1,"N"; |C_V #dscaw061#5,1,"S";
      |FINSI;
      FSalida = 10; |HAZ Condicion;
   |FINSI;
|FPRC;

|PROCESO Condicion; |TIPO 11;
   |SI FSalida = 10;
      |PUSHV  0846 1580;
      |BLANCO 0846 1580;
      |DBASE aAlfa; |QBF aAlfa;
      aAlfa = aAlfa + "def/" + #dscam086#1 + ".mas";
      |CARGA_DEF aAlfa, Referen@;
      |SI FSalida = 0;
         |PINPA #0#dscaw061; |PINDA #0#dscaw061;
         |ENDATOS #1#dscaw061;
         |SI FSalida = 0;
            #dscam089#4 = #dscaw061#0;
            #dscam089#5 = #dscaw061#1;
            #dscam089#6 = #dscaw061#3;
            #dscam089#7 = #dscaw061#4;
            #dscam089#8 = #dscaw061#5;
            |GRABA 020#dscam089.a;
         |FINSI;
         |DESCARGA_DEF #Referen@;
      |FINSI;
      |POPV;
      |ERROR;
   |FINSI;
|FPRC;

|PROGRAMA;
   |CLS;
   |CABEZA " Adjudicacion procesos a tipos de recibo ";
   aAlfa = "dscam088"; |NOME_DAT #0 aAlfa;
   |ENTLINEAL #1#dscam087,2,7,14,1,infm087,supm087;
   |ENTLINEAL #1#dscam089,3,7,22,1,infm089,supm089;
   |ENTLINEAL #1#dscam086,1,4,22,1,infm086,supm086;
|FPRO;
