|FICHEROS;
   dscam065 #0;
|FIN;

|VARIABLES;
   &aInforme = "";

   Comodin = "";
   aAlfa   = "";

   aClaveBuena = "";
   aClave = "";
   nTecla = 0;
   nSalir = 0;
   SwAlta = 0;
|FIN;

|PROCESO Tipo13m65; |TIPO 13;
   nTecla = FSalida;
   |SI nTecla = 10;
      |ENCAMPO #1#dscam065;
      |GRABA 020#dscam065.a;
      |PTEC 804; |PONCONTROL 23, "SI";
   |FINSI;
|FPRC;

|PROCESO Tipo11m65; |TIPO 11;
   nTecla = FSalida;
   |SI nTecla ] 13; |Y nTecla [ 20;
      nTecla = nTecla + 40;
      aClave = aClave + &nTecla;
      |SI aClave = aClaveBuena;
         SwAlta = 1; |PTEC 807; |ACABA;
      |SINO;
         |ERROR; |ACABA;
      |FINSI;
   |SINO;
      |SI aClave ! aClaveBuena; aClave = ""; |FINSI;
   |FINSI;
|FPRC;

|RUTINA inf65;
   #dscam065#0 = " " * 30;
|FRUT;

|RUTINA sup65;
   #dscam065#0 = "z" * 30;
|FRUT;

|PROGRAMA;
   aClaveBuena = "8576"; SwAlta = 0;
   |CLS;
   |CABEZA "Parametrizacion de informes";
   |ABRE #dscam065;

   |HAZ EsLuxe;
   |SI FSalida = 0;
      aInforme = "Carta impagado vertical"; |HAZ ObtenInforme;
      |SI aInforme = "Carta impagado vertical";
         |DDEFECTO #dscam065;
         #dscam065#0 = aInforme;
         #dscam065#1 = "luxe0107";
         |GRABA 020#dscam065.n;
      |FINSI;
      aInforme = "Carta impagado horizontal"; |HAZ ObtenInforme;
      |SI aInforme = "Carta impagado horizontal";
         |DDEFECTO #dscam065;
         #dscam065#0 = aInforme;
         #dscam065#1 = "luxe0106";
         |GRABA 020#dscam065.n;
      |FINSI;
   |FINSI;

   |PINPA #0#0; |PINDA #0#0;
   |PARA; |SI nSalir = 0; |HACIENDO;
      |SI SwAlta = 0;
         |ENTLINEAL #1#dscam065,-1,4,21,1,inf65,sup65;
         |SI aClave ! aClaveBuena; nSalir = 1; |FINSI;
      |SINO;
         |ENTLINEAL #1#dscam065,-1,1,21,1,inf65,sup65;
         nSalir = 1;
      |FINSI;
   |SIGUE;
   |CIERRA #dscam065;
|FPRO;
