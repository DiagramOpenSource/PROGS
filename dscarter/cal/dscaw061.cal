|FICHEROS;
   dscaw061 #0;
   dscam086 #10;

   &Referen@ #1000;
|FIN;

|VARIABLES;
   aAlfa = "";
|FIN;

|PROCESO NumeroCampo;
   aAlfa = "|C" + (("000" + #dscaw061#0) % -103) + "TIPO";
   #dscaw061#1 = #Referen@#aAlfa#;
   |SI #dscaw061#1 = "A";
      #dscaw061#2 = "Alfanumerico";
      |C_M #dscaw061#3,1,"S"; |C_M #dscaw061#4,1,"N"; |C_M #dscaw061#5,1,"N";
      |C_V #dscaw061#3,1,"S"; |C_V #dscaw061#4,1,"N"; |C_V #dscaw061#5,1,"N";
   |FINSI;
   |SI #dscaw061#1 = "N";
      #dscaw061#2 = "Numerico";
      |C_M #dscaw061#3,1,"N"; |C_M #dscaw061#4,1,"S"; |C_M #dscaw061#5,1,"N";
      |C_V #dscaw061#3,1,"N"; |C_V #dscaw061#4,1,"S"; |C_V #dscaw061#5,1,"N";
   |FINSI;
   |SI #dscaw061#1 = "F";
      #dscaw061#2 = "Fecha";
      |C_M #dscaw061#3,1,"N"; |C_M #dscaw061#4,1,"N"; |C_M #dscaw061#5,1,"S";
      |C_V #dscaw061#3,1,"N"; |C_V #dscaw061#4,1,"N"; |C_V #dscaw061#5,1,"S";
   |FINSI;
   |PINTA #dscaw061#1; |PINTA #dscaw061#2;
   |BLANCO 1348 1378;
   |PINTA #dscaw061#3; |PINTA #dscaw061#4; |PINTA #dscaw061#5;
|FPRC;
