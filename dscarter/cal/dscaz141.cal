|PROCESO Formato;
   |SI aTipo = "I";
      Calc = Field;
      Calc = (Calc * Cambio) ! Redond;
      |SI Moneda = "E"; Calc = Calc * 100; |FINSI;
      Field = Calc;
   |FINSI;

   |SI aTipo = "A";
      |QBF Field;
      aChar1 = Field % 0; nCalc1 = aChar1;
      |PARA nCont = 1; |SI nCont [ nCalc1; |HACIENDO nCont = nCont + 1;
         nCalc2 = (nCont * 100) + 1;
         aChar1 = Field % nCalc2; aChar2 = "";
         |SI aChar1 = "§"; aChar2 = "º"; |FINSI;
         |SI aChar1 = "¦"; aChar2 = "ª"; |FINSI;
         |SI aChar1 = "¤"; aChar2 = "ñ"; |FINSI;
         |SI aChar1 = "¥"; aChar2 = "Ñ"; |FINSI;
         |SI aChar1 = " "; aChar2 = "á"; |FINSI;
         |SI aChar1 = "‚"; aChar2 = "é"; |FINSI;
         |SI aChar1 = "¡"; aChar2 = "í"; |FINSI;
         |SI aChar1 = "¢"; aChar2 = "ó"; |FINSI;
         |SI aChar1 = "£"; aChar2 = "ú"; |FINSI;

         |SI aChar2 ! "";
            nCalc2 = 99 + nCont; aParte1 = Field % nCalc2;
            aChar1 = aParte1 + aChar1;  aParte2 = Field - aChar1;
            Field = aParte1 + aChar2 + aParte2;
         |FINSI;
      |SIGUE;

      aParte2 = " " * NumDig;
      aParte1 = Field + aParte2;
      Calc = 100 + NumDig;
      Field = aParte1 % Calc;
   |SINO;
      Field = "0" * NumDig;
