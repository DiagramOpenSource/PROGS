|PROCESO AltaCliente;
     enDesdeRenta = enEmpresa;
     |SUB_EJECUTA_NP ":integral/dsam0000";
|FPRC;

|PROCESO ModCliente;
     enCodCli = enEmpresa;
     |HAZ LeePersona;
     |SI enError = 0;  |Y enSwDeDonde = 1;
         enDesdeRenta = enEmpresa;
         |SUB_EJECUTA_NP ":integral/dsam0000";
     |FINSI;
|FPRC;

|PROCESO Liquidacion;
     aAlfa = ":ren2010/rem01001;" + enEmpresa;
     |SUB_EJECUTA aAlfa;
|FPRC;

|PROCESO Patrones;
     |SUB_EJECUTA ":basica/dspatron;r11";
|FPRC;

|PROGRAMA;
     aAlfa  = PARAMETRO % 105;   enEmpresa = aAlfa;
     aBoton = PARAMETRO % 615;   |QBF aBoton;

     |SI aBoton = "ALTACLIENTE";  |HAZ AltaCliente; |FINSI;
     |SI aBoton = "MODCLIENTE";   |HAZ ModCliente;  |FINSI;
     |SI aBoton = "LIQUIDACION";  |HAZ Liquidacion; |FINSI;
     |SI aBoton = "PATRONES";     |HAZ Patrones;    |FINSI;
|FPRO;
