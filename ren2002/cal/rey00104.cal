    |FINSI;
    |SI #rem00021#32 ! 0;
       Calc = (#rem00021#15 * #rem00021#32) / 365;
       |SI #rem00021#17 = "S";
          Calc = Calc % 1.1;
       |SINO;
          Calc = Calc % 2;
       |FINSI;
       #rem00006#29 = #rem00006#29 + (Calc % Parte);
    |FINSI;
    |SI #rem00021#31 ! 0;
       |SI #rem00021#37 = "A";
          #rem00006#29 = #rem00006#29 + (((#rem00021#38 * #rem00021#31) / 365) % Parte);
       |SINO;
          #rem00006#29 = #rem00006#29 + (#rem00021#38 % Parte);
       |FINSI;
    |FINSI;

