|PROCESO BuscaIncidencias;
     #2#0 = #4#0;
     #2#1 = #4#1;
     #2#2 = #4#2;
     #2#3 = #4#3;
     #2#4 = #4#4;
     |LEE 040#2=;
     |SI FSalida ! 0;   |DDEFECTO #2;  |FINSI;

     |SI #4#4 = "117";  |ACABA;  |FINSI;
     |SI #4#4 = "118";  |ACABA;  |FINSI;
     |SI #4#4 = "119";  |ACABA;  |FINSI;
     |SI #4#4 = "a  ";  |ACABA;  |FINSI;
