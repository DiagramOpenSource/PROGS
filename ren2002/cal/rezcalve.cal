     |SI #4#4 = "122";  |ACABA;  |FINSI;

     |SI #4#4 = "TM1";  |O #4#4 = "TM2";
         |SI #3#5 ! "714";
             |ACABA;
         |FINSI;
     |FINSI;

     nImporte1 = #2#6 + #2#9;
     nImporte2 = #4#6 + #4#9;
     |SI nImporte1 ! nImporte2;
         #1001#0 = #3#0;
         #1001#1 = #3#1;
         #1001#2 = #3#2;
         #1001#5 = #3#5;
         #1001#7 = #3#7;
         #1002#4 = #4#4;
         #1002#5 = #4#5;
         #1002#6 = #4#6;
         enAnt   = #2#6;
         |PRINT;
         nSwPrint = 1;
     |FINSI;
|FPRC;

