|PROCESO CogeVersion;
     aMenu = aPathBase + "ren20022.men";
     |XABRE "U", aMenu, nHandle;
     |SI FSalida ] 0;
         |XLEE nHandle, 250, aAlfa;
         aAlfa = aAlfa % 403;
     |FINSI;
     |XCIERRA nHandle;
|FPRC;

|PROCESO Tipo3;  |TIPO 3;
     |DBASE aPathRenta;  |QBT aPathRenta;
     aPathBase  = aPathRenta;
     aPathRenta = aPathRenta + "def/";

     |ABRE #0;
     #0#0 = 1;
     |LEE 101#0=;
     |SI FSalida ! 0;
         |DDEFECTO #0;
         #0#0 = 1;
         |GRABA 020#0b;
     |FINSI;

     |IMPRE 0;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |INFOR "rezcalve";
     |SI FSalida ! 0;  |FINIMP;  |ACABA;  |FINSI;

     |HAZ CogeVersion;
     #0#1 = aAlfa;

     |GRABA 020#0a;
     |LIBERA #0;
     |CIERRA #0;

     |DFICE aDirDatos;  |QBT aDirDatos;

     |HAZ BorraLiquiMala;

     enEmpresa = 0;
