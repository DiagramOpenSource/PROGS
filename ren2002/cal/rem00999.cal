|FICHEROS;
     rem00999 #1000;
|FIN;

|VARIABLES;
     nCampo = 0;
     nNume  = 0;
     aAlfa  = "";

     {50}paAlfa = "";
|FIN;

|RUTINA ClaveBaja1000;
     #1000#0 = 0;
     #1000#1 = "   ";
|FRUT;

|RUTINA ClaveAlta1000;
     #1000#0 = 99;
     #1000#1 = "zzz";
|FRUT;

|PROCESO Tipo5;  |TIPO 5;
       |LEE 040#1000p;
       |LINEAS_BI_ATRI #1000#3, S, D, -1;
|FPRC;

|PROCESO Tipo2;  |TIPO 2;
     #1000#3 = "D";
     |SI #1000#1 = "   ";  #1000#3 = "T";  |FINSI;
|FPRC;

|PROGRAMA;
     |ENTLINEAL #1#1000, -1, 2, 22, 1, ClaveBaja1000, ClaveAlta1000;
|FPRO;
