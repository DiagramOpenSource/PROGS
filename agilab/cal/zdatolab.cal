         |SI nCampo = 22;  aAlfa = "    Administracion hacienda publica."; |FINSI;
      |FINSI;
   |FINSI;
   |SI aMod = "labcentr";
      |SI #labcontr#55 = "S";
         |SI nCampo = 6;  aAlfa = "    <F3> Seleccione poblacion";        |FINSI;
      |SINO;
         |SI nCampo = 6;  aAlfa = "    Localidad.";          |FINSI;
      |FINSI;
   |FINSI;
