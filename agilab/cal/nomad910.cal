|PROCESO FormateaContrato;
     |QBT aAlfa1;
     aAlfa1 = ("000" + aAlfa1) % -103;
     |SI aAlfa1 = "0Z1"; aAlfa1 = "181"; |FINSI;
     |SI aAlfa1 = "0Z2"; aAlfa1 = "182"; |FINSI;
