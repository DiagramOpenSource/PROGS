     |HAZ FormateaContrato;
     #labbasel#6 = aAlfa1;
     |GRABA 020#labbasel.a;
     |LIBERA #labbasel;
|FPRC;

|DEFBUCLE labbasel;
     #labbasel#1;
     ;
         1,     1, 000;
     99999, 99999, 999;
     be;
     NULL, labbasel_1;
|FIN;

