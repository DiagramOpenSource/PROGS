          #0#75 = #0#1;
          |PINTA #0#75;
     |FINSI;
|FPRC;

|PROCESO Parametro; |TIPO 7;
     aParam = PARAMETRO;
     |QBF aParam;
     |SI nLabHisto = 1;
          |SI #0#50 = "X"; aParam = "A"; |FINSI;
          |SI #0#51 = "X"; aParam = "B"; |FINSI;
          |SI #0#52 = "X"; aParam = "C"; |FINSI;
          |SI #0#84 = "X"; aParam = "D"; |FINSI;
          |SI #0#53 = "X"; aParam = "E"; |FINSI;
     |FINSI;
     |HAZ TipoAntes;
     |SI aParam = "A"; #0#50 = "X"; |PINTA #0#50; |FINSI;
     |SI aParam = "B"; #0#51 = "X"; |PINTA #0#51; |FINSI;
     |SI aParam = "C"; #0#52 = "X"; |PINTA #0#52; |FINSI;
     |SI aParam = "D"; #0#84 = "X"; |PINTA #0#84; |FINSI;
     |SI aParam = "E"; #0#53 = "X"; |PINTA #0#53; |FINSI;
     |HAZ TipoDespues;
|FPRC;

|PROCESO ConsultaTra_b7; |TIPO 66;
     |ABRE #labtraba;
     |CONSULTA_CLAVE #1#labtraba;
     |SI FSalida = 0;
          #0#75 = #labtraba#0;
          #0#76 = #labtraba#1;
          |PINTA #0#75;
          |PINTA #0#76;
     |FINSI;
     |CIERRA #labtraba;
|FPRC;

|PROCESO Activa97; |TIPO 7;
     |SI #0#96 = "S";
          |CAMPO_MODIFICA #0#97, 1, "S";
     |SINO;
          |CAMPO_MODIFICA #0#97, 1, "N";
          #0#97 = ""; |PINTA #0#97;
     |FINSI;
|FPRC;
