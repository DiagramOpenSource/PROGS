    |SELECT #2#4;

        #2#0 = #0#54;
        #2#1 = #0#55;
    |LEE 000#2=;
    |SI FSalida ! 0;
            #0#56 = "<Trabajador inaccesible o inexistente>";
        |PDEFECTO #0, 57, 61;
    |SINO;
            #0#56 = #2#2;     || nombre
            #0#57 = #2#7;     || dni
            #0#58 = #2#24;    || oficina INEM

            #4#1 = #2#0;
