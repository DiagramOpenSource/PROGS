|PROCESO Nacido102; |TIPO 7;
     |MODULO aAlfa1;
     |SI aAlfa1 ! "labtraba";
          |ABRE #2;
          #2#0 = #0#3;
          #2#1 = #0#4;
          |LEE 000#2=;
          |SI FSalida = 0;
               fFech2 = #2#11;          aAlfa2 = fFech2;
               aAlfa2 = aAlfa2 % -104;  nNume2 = aAlfa2;
               |SI nNume2 ! 0;
                    #102#1 = #0#2 - nNume2;
                    |PINTA #102#1;
               |FINSI;
          |FINSI;
          |CIERRA #2;
     |SINO;
          fFech2 = #labtraba#11;          aAlfa2 = fFech2;
          aAlfa2 = aAlfa2 % -104;  nNume2 = aAlfa2;
          |SI nNume2 ! 0;
               #102#1 = nAnyo - nNume2;
               |PINTA #102#1;
          |FINSI;
     |FINSI;
|FPRC;

