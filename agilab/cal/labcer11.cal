|PROCESO Defectos; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          fFechaHoy = ~;

          aAlfa = fFechaHoy;
          aAlfa = aAlfa % -104;
          nNume = aAlfa;
          #0#5 = nNume;       || el a¤o
          |PINTA #0#5;

          aAlfa = fFechaHoy;
          aAlfa = aAlfa % 402;
          nNume = aAlfa;
          #0#6 = nNume;       || el mes
          |PINTA #0#6;
     |FINSI;
|FPRC;

|PROCESO topemes;     || halla el ultimo dia del mes
     nume2 = 0;
     nume1 = nAny $ 4;    || a¤o historico
     |SI nume1 = 0;
          nume2 = 1;
     |FINSI;
     nTopemes = 31;
     |SI nMes = 2;
          nTopemes = 28 + nume2;
     |FINSI;
     |SI nMes = 4; |O nMes = 6; |O nMes = 9; |O nMes = 11;
          nTopemes = 30;
     |FINSI;
|FPRC;

|PROCESO Fechas;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          nAny = #0#5;
          nMes = #0#6;
          |HAZ topemes;

          aMes = #0#6;
          aMes = ("00" + aMes) % -102;
          aAny = #0#5;
          aDia = "01";
          aFecha = aDia + "." + aMes + "." + aAny;
          #0#3 = aFecha;
          |PINTA #0#3;

          aDia = nTopemes;
          aFecha = aDia + "." + aMes + "." + aAny;
          #0#4 = aFecha;
          |PINTA #0#4;
     |FINSI;
|FPRC;

