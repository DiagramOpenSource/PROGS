|PROCESO RecogeMensual;
     |ABRE #2;
     |ABRE #3;
     |ABRE #labtraba;

     nMesDesde = #0#6;
     nMesHasta = #0#6;

     |BUCLE nomcalca;

     |CIERRA #labtraba;
     |CIERRA #3;
     |CIERRA #2;
|FPRC;

|PROCESO RecogeHco;
     |ABRE #2;
     |ABRE #3;
     |ABRE #labtraba;

     nMesDesde = #0#6;
     nMesHasta = #0#6;
     nAny = #0#5 - 2000;

     |BUCLE nomhicca;

     |CIERRA #labtraba;
     |CIERRA #3;
     |CIERRA #2;
|FPRC;

|PROCESO CargaInicial;
     aAlfa = "    S¿Desea realizar una carga inicial de trabajadores con expediente";
     aAlfa = aAlfa + " de Regulación de Empleo?";
     |CONFI aAlfa;
     |SI FSalida = 0;
          |PARA; |SI aAlfa ! "M"; |Y aAlfa ! "H"; |Y aAlfa ! "C"; |HACIENDO;
               |PUSHV 11201760;
               |COLOR 7,1;
               |ATRI I;
               ||            2    3         4         5
               ||            67890123456789012345678901234
               |PINTA 1225, "¨Desea recoger los datos del";
               |PINTA 1325, "mantenimiento Mensual  o del";
               |PINTA 1425, "    Hist¢rico? (M/H):       ";
               E_sup = 1;
               E_inf = "";
               aAlfa = "M";
               aAlfa = 1448 ? 1;
               |QBF aAlfa;
               |SI aAlfa ! "M"; |Y aAlfa ! "H"; |Y aAlfa ! "C";
                    aMensa = "    Introduzca M para Mensual, H para Histórico ";
                    aMensa = aMensa + "o C para Cancelar";
                    |MENAV aMensa;
               |FINSI;
               |POPV;
          |SIGUE;
          |SI aAlfa = "M";
               |HAZ RecogeMensual;
          |FINSI;
          |SI aAlfa = "H";
               |HAZ RecogeHco;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Muestra; |TIPO 14;
     |ENTLINEAL #1#2, -5, 7, 20, 0, inferior_cer02, superior_cer02;
     |ATRI I;

     nPrimerTra = 0;
     |BUCLE HayAlgo;
     |ENTLINEAL #1#3, -6, 7, 20, 0, inferior_cer03, superior_cer03;
     |ATRI I;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     swAlgo = 0;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |HAZ CargaInicial;
     |FINSI;
     |BUCLE HayAlgo;
     |ENTLINEAL #1#2, -5, 2, 20, 0, inferior_cer02, superior_cer02;
     |SI nModo = 1; |O nModo = 2;
          |CONFI "    S¿Desea generar el fichero xml para su envío?";
          |SI FSalida = 0;
               |HAZ Envio;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Envio;
     aEmpresa = #0#0;
     |QBF aEmpresa;
     aEmpresa = ("00000" + aEmpresa) % -105;

     aCampanya = #0#1;
     |QBF aCampanya;
     aCampanya = ("000000000" + aCampanya) % -109;

     aMes = #0#6;
     aMes = ("00" + aMes) % -102;
     aAny = #0#5;

     aAlfa = "labcer14;"+ aEmpresa + aCampanya + aAny + aMes;
     |SUB_EJECUTA aAlfa;
|FPRC;

|PROCESO Tipo20; |TIPO 20;
     |HAZ Defectos;
     |SI FSalida = "OPCION";
          FSalida = "Envio XML";
     |SINO;
          nModo = FEntrada;
          |SI nModo = 0;
               aAlfa = "    ATENCION. Ha de entrar en un expediente para poder generar el fichero.";
               |MENAV aAlfa;
          |SINO;
               |HAZ Envio;
          |FINSI;
     |FINSI;
|FPRC;
