|PROCESO GrabaHueco;
     #3#7 = nLineaG;
     aDia = nDesde; aDia = ("00" + aDia) % -102;
     aFecha = aDia + aMesAny;
     #3#8 = aFecha;
     aDia = nHasta; aDia = ("00" + aDia) % -102;
     aFecha = aDia + aMesAny;
     #3#9 = aFecha;
     #3#10 = 03;
     #3#11 = "D¡as de actividad";
     #3#12 = " ";
     #3#16 = 00;
     |GRABA 020#3n;
|FPRC;

|PROCESO CodigoActividad;
     |SI aTipo = "D";
          #3#10 = 01;
          #3#11 = "Sin actividad";
     |FINSI;
     |SI aTipo = "E"; |O aTipo = "M"; |O aTipo = "P"; |O aTipo = "A";
          #3#10 = 04;
          #3#11 = "I.T., Maternidad";
     |FINSI;
     |SI aTipo = "V";
          #3#10 = 05;
          #3#11 = "Vacaciones y descansos retribuidos";
     |FINSI;
|FPRC;

|PROCESO CompDias;
     aTipo = aTipo1;
     |SI aTipo = "D";
     |O aTipo = "E"; |O aTipo = "M"; |O aTipo = "P"; |O aTipo = "A";
     |O aTipo = "V";
          |SI nDesde1 = 0; nDesde1 = nPrimerDia; |FINSI;
          |SI nHasta1 = 0; nHasta1 = nUltimoDia; |FINSI;
     |FINSI;

     aTipo = aTipo2;
     |SI aTipo = "D";
     |O aTipo = "E"; |O aTipo = "M"; |O aTipo = "P"; |O aTipo = "A";
     |O aTipo = "V";
          |SI nDesde2 = 0; nDesde2 = nPrimerDia; |FINSI;
          |SI nHasta2 = 0; nHasta2 = nUltimoDia; |FINSI;
     |FINSI;

     aTipo = aTipo3;
     |SI aTipo = "D";
     |O aTipo = "E"; |O aTipo = "M"; |O aTipo = "P"; |O aTipo = "A";
     |O aTipo = "V";
          |SI nDesde3 = 0; nDesde3 = nPrimerDia; |FINSI;
          |SI nHasta3 = 0; nHasta3 = nUltimoDia; |FINSI;
     |FINSI;

     aTipo = aTipo4;
     |SI aTipo = "D";
     |O aTipo = "E"; |O aTipo = "M"; |O aTipo = "P"; |O aTipo = "A";
     |O aTipo = "V";
          |SI nDesde4 = 0; nDesde4 = nPrimerDia; |FINSI;
          |SI nHasta4 = 0; nHasta4 = nUltimoDia; |FINSI;
     |FINSI;
|FPRC;

|PROCESO RellenaHueco;
     |SI nVoyPorDia < nDesde; |Y swUltimo = 0;
          #3#7 = nLinea - 1;

          nDesdeG = nVoyPorDia;
          aDia = nDesdeG; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#8 = aFecha;

          nHastaG = nDesde - 1;
          aDia = nHastaG; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#9 = aFecha;

          #3#10 = 03;
          #3#11 = "D¡as de actividad";
          #3#12 = " ";
          #3#16 = 00;
          |GRABA 020#3n;
     |FINSI;
     |SI nVoyPorDia [ nUltimoDia; |Y swUltimo = 1;
          #3#7 = nLinea + 1;

          nDesdeG = nVoyPorDia;
          aDia = nDesdeG; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#8 = aFecha;

          nHastaG = nUltimoDia;
          aDia = nHastaG; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#9 = aFecha;

          #3#10 = 03;
          #3#11 = "D¡as de actividad";
          #3#12 = " ";
          #3#16 = 00;
          |GRABA 020#3n;
     |FINSI;
|FPRC;

|PROCESO Carga;
     |HAZ CompDias;

     |DDEFECTO #2;
     |DDEFECTO #3;

     fFechaIni = #0#3;
     fFechaFin = #0#4;

     |SI fFechaAlta > fFechaIni;
          fFechaIni = fFechaAlta;
     |FINSI;
     |SI fFechaBaja < fFechaFin;
          fFechaFin = fFechaBaja;
     |FINSI;

     #2#0 = #0#0;
     #2#1 = #0#1;
     #2#2 = #0#2;
     #2#3 = fFechaIni;
     #2#4 = fFechaFin;
     #2#5 = #labtraba#1;
     #2#6 = #labtraba#2;
     #2#7 = #0#5;
     #2#8 = #0#6;
     |GRABA 020#2n;

     #3#0 = #0#0;
     #3#1 = #0#1;
     #3#2 = fFechaIni;
     #3#3 = fFechaFin;
     #3#4 = #0#4;
     #3#5 = #labtraba#1;
     #3#6 = #labtraba#2;

     #3#7 = 0000;
     #3#8 = #2#3;
     #3#9 = #2#4;
     #3#10 = 00;
     #3#11 = "Inicio/fin per¡odos ERE";
     #3#12 = " ";
     #3#16 = 04;         || coeficiente por defecto para ERE: 1.25
     #3#18 = #0#5;
     #3#19 = #0#6;
     |GRABA 020#3n;

     aMes = #0#6;
     aMes = ("00" + aMes) % -102;
     aAny = #0#5;
     aMesAny = "." + aMes + "." + aAny;

     swUltimo = 0
     nLinea = 0005;

     aAlfa = fFechaIni;
     aAlfa = aAlfa % 102;
     nPrimerDia = aAlfa;

     aAlfa = fFechaFin;
     aAlfa = aAlfa % 102;
     nUltimoDia = aAlfa;

     nVoyPorDia = nPrimerDia;

     aTipo = aTipo1; nDesde = nDesde1; nHasta = nHasta1;
     |SI aTipo = "D";
     |O aTipo = "E"; |O aTipo = "M"; |O aTipo = "P"; |O aTipo = "A";
     |O aTipo = "V";
          |HAZ RellenaHueco;

          #3#7 = nLinea;
          aDia = nDesde1; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#8 = aFecha;
          aDia = nHasta1; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#9 = aFecha;
          |HAZ CodigoActividad;
          #3#12 = " ";
          #3#16 = 00;
          |GRABA 020#3n;

          nLinea = nLinea + 5;
          nVoyPorDia = nHasta + 1;
     |FINSI;

     aTipo = aTipo2; nDesde = nDesde2; nHasta = nHasta2;
     |SI aTipo = "D";
     |O aTipo = "E"; |O aTipo = "M"; |O aTipo = "P"; |O aTipo = "A";
     |O aTipo = "V";
          |HAZ RellenaHueco;

          #3#7 = nLinea;
          aDia = nDesde2; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#8 = aFecha;
          aDia = nHasta2; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#9 = aFecha;
          |HAZ CodigoActividad;
          #3#12 = " ";
          #3#16 = 00;
          |GRABA 020#3n;

          nLinea = nLinea + 5;
          nVoyPorDia = nHasta + 1;
     |FINSI;

     aTipo = aTipo3; nDesde = nDesde3; nHasta = nHasta3;
     |SI aTipo = "D";
     |O aTipo = "E"; |O aTipo = "M"; |O aTipo = "P"; |O aTipo = "A";
     |O aTipo = "V";
          |HAZ RellenaHueco;

          #3#7 = nLinea;
          aDia = nDesde3; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#8 = aFecha;
          aDia = nHasta3; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#9 = aFecha;
          |HAZ CodigoActividad;
          #3#12 = " ";
          #3#16 = 00;
          |GRABA 020#3n;

          nLinea = nLinea + 5;
          nVoyPorDia = nHasta + 1;
     |FINSI;

     aTipo = aTipo4; nDesde = nDesde4; nHasta = nHasta4;
     |SI aTipo = "D";
     |O aTipo = "E"; |O aTipo = "M"; |O aTipo = "P"; |O aTipo = "A";
     |O aTipo = "V";
          |HAZ RellenaHueco;

          #3#7 = nLinea;
          aDia = nDesde4; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#8 = aFecha;
          aDia = nHasta4; aDia = ("00" + aDia) % -102;
          aFecha = aDia + aMesAny;
          #3#9 = aFecha;
          |HAZ CodigoActividad;
          #3#12 = " ";
          #3#16 = 00;
          |GRABA 020#3n;

          nLinea = nLinea + 5;
          nVoyPorDia = nHasta + 1;
     |FINSI;
     |SI nVoyPorDia ! 1;
          swUltimo = 1;
          |HAZ RellenaHueco;
     |FINSI;
|FPRC;

