|FICHEROS;
     labcerm3 #0;
     labcerm1;
|FIN;

|VARIABLES;
     nCampo = 0;
     nCampo1 = 0;
     nCampo2 = 0;
     &aFecha = "";
|FIN

|PROCESO DatosOtro_m3;
     |SI #0#3 ! "X";
          |ACABA;
     |FINSI;
     |ABRE #labcerm1;
     #labcerm1#0 = #0#0;
     #labcerm1#1 = #0#1;
     #labcerm1#2 = #0#2;
     |LEE 000#labcerm1.=;
     |SI FSalida = 0;
          #0#6 = #labcerm1#23;
          #0#7 = #labcerm1#24;
          #0#8 = #labcerm1#25;
          #0#9 = #labcerm1#30;
          |PINTA #0#6;
          |PINTA #0#7;
          |PINTA #0#8;
          |PINTA #0#9;
     |FINSI;
     |CIERRA #labcerm1;
|FPRC;

|PROCESO Fecha_m3;
     aFecha = #labcerm3#Campo#;
     |HAZ CompFecha_plb;
|FPRC;

|PROCESO ActivaUnoNO_m3;
     |SI #0nCampo = "N";
          |CAMPO_MODIFICA #0Campo, 1, "S";
     |SINO;
          |CAMPO_MODIFICA #0Campo, 1, "N";
          nCampo1 = Campo;
          nCampo2 = Campo + 1;
          |PDEFECTO #0, nCampo1, nCampo2;
          |PINTA #0Campo;
     |FINSI;
|FPRC;

|PROCESO ActivaUno_m3;
     |SI #0nCampo = "S"; |O #0nCampo = "X";
          |CAMPO_MODIFICA #0Campo, 1, "S";
     |SINO;
          |CAMPO_MODIFICA #0Campo, 1, "N";
          nCampo1 = Campo;
          nCampo2 = Campo + 1;
          |PDEFECTO #0, nCampo1, nCampo2;
          |PINTA #0Campo;
     |FINSI;
|FPRC;

|PROCESO Activa3_m3; |TIPO 7;
     nCampo = 3;
     |HAZ ActivaUno_m3;
|FPRC;

|PROCESO Activa13_m3; |TIPO 7;
