|FPRC;

|PROCESO Activa2122_m3;
     |SI #0#21# = "          "; |Y #0#22# = "          ";
          |CAMPO_MODIFICA #0Campo, 1, "S";
     |SINO;
          |CAMPO_MODIFICA #0Campo, 1, "N";
          nCampo1 = Campo;
          nCampo2 = Campo + 1;
          |PDEFECTO #0, nCampo1, nCampo2;
          |PINTA #0Campo;
     |FINSI;
|FPRC;

