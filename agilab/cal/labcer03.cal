|PROCESO AyudaActiv; |TIPO 7;
     || AYUDA DE OPCIONES
     || 4         5         6         7         8
     || 01234567890123456789012345678901234567890
     |PUSHV 06462380;
     swAyuda = 1;
     |COLOR 0,7;
     |ATRI R;
     ||            4  5         6         7         8
     ||            7890123456789012345678901234567890
     |PINTA 0747, "     - Opciones disponibles -     ";
     |PINTA 0847, "01. D며s no trabajados en el mes  ";
     |PINTA 0947, "    (d며s pago de la prestaci묥)  ";
     |PINTA 1047, "02. D며s sin actividad por no     ";
     |PINTA 1147, "    acudir al llamamiento         ";
     |PINTA 1247, "03. D며s trabajados en el per멾do ";
     |PINTA 1347, "04. D며s IT (excluidos de pago de ";
     |PINTA 1447, "    prestaci蚤 por este sistema)  ";
     |PINTA 1547, "05. Descanso semanal              ";
