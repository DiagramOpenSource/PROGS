|PROCESO LineaCero;
     |SI #3#7 = 0;
          |CAMPO_MODIFICA #3#10, 1, "N";
          #3#10 = 0;
          |PINTA #3#10;
          #3#11 = "Inicio/fin campa바 trabajador";
          |PINTA #3#11;

          |CAMPO_MODIFICA #3#17, 1, "S";

          |ABRE #labtraba;
          #labtraba#0 = #3#0;
          #labtraba#1 = #3#5;
          |LEE 000#labtraba.=;
          |SI #labtraba#42 = "C"; |O #labtraba#42 = "F";
               |CAMPO_MODIFICA #3#16, 1, "S";
          |SINO;
               |CAMPO_MODIFICA #3#16, 1, "N";
               #3#16 = 0;
               |PINTA #3#16;
               |GRABA 020#3a;
          |FINSI;
          |CIERRA #labtraba;
     |SINO;
          |CAMPO_MODIFICA #3#10, 1, "S";
          |CAMPO_MODIFICA #3#16, 1, "N";

          #3#17 = 0;
          |PINTA #3#17;
          ||GRABA 020#3a;
          |CAMPO_MODIFICA #3#17, 1, "N";

          |ABRE #labtraba;
          #labtraba#0 = #3#0;
          #labtraba#1 = #3#5;
          |LEE 000#labtraba.=;
          |SI #labtraba#42 = "C"; |O #labtraba#42 = "F";
               || nada, solo en linea 0
          |SINO;
               #3#16 = 0;
               |PINTA #3#16;
               ||GRABA 020#3a;
          |FINSI;
          |CIERRA #labtraba;
     |FINSI;

     |SI #3#12 = "S";
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO RestauraActiv;
     swAyuda = 0;
     |POPV;

     |SI #3#10 ] 01; |Y #3#10 [ 06;
          |SI #3#10 = 1;
               #3#11 = "D며s no trabajados";
          |FINSI;
          |SI #3#10 = 2;
               #3#11 = "Sin actividad por no acudir al llamamiento";
          |FINSI;
          |SI #3#10 = 3;
               #3#11 = "D며s trabajados";
          |FINSI;
          |SI #3#10 = 4;
               #3#11 = "D며s IT";
          |FINSI;
          |SI #3#10 = 5;
               #3#11 = "Descanso semanal";
          |FINSI;
          |SI #3#10 = 6;
               #3#11 = "Vacaciones anuales retribuidas";
          |FINSI;
          |PINTA #3#11;
     |FINSI;
|FPRC;

|PROCESO FechaIni; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |SALVA_FICHA #3;
          |LEE 000#3a;
          |SI FSalida = 0;
               fPropIni = #3#8;
          |FINSI;
          |REPON_FICHA #3;
          #3#8 = fPropIni;
          |PINTA #3#8;
     |FINSI;
|FPRC;

|PROCESO FechaFin; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          fPropFin = #3#8;
          nNume = fPropFin;
          nNume = nNume + 1;
          fPropFin = nNume;
          #3#9 = fPropFin;
          |PINTA #3#9;
     |FINSI;
|FPRC;

|PROCESO CompFechas;
     |SI #3#9 < #3#8;
          aAlfa = "    La fecha de fin del intervalo no puede ser mayor que la fecha de inicio.";
          aAlfa = aAlfa + " Revise las fechas."
          |MENAV aAlfa;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO CompFecha1;
     |SI #3#8 < fIniCamp; |O #3#8 > fFinCamp;
          aAlfa = "    La fecha de inicio del intervalo ha de estar comprendida";
          aAlfa = aAlfa + " entre las fechas de inicio/fin campa人 del trabajador."
          |MENAV aAlfa;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO CompFecha2;
     |SI #3#9 < fIniCamp; |O #3#9 > fFinCamp;
          aAlfa = "    La fecha de fin del intervalo ha de estar comprendida";
          aAlfa = aAlfa + " entre las fechas de inicio/fin campa人 del trabajador."
          |MENAV aAlfa;
          |ERROR;
     |FINSI;
|FPRC;
