|FICHEROS;
     labtraba;
     labcer02 #2;
     labcer03 #3;
|FIN;

|VARIABLES;
     swAyuda = 0;
     nModo = 0;
     aAlfa = "";
     nNume = 0;
     fPropIni = @;
     fPropFin = @;
     &fIniCamp = @;
     &fFinCamp = @;
|FIN;

|PROCESO Tipo5; |TIPO 5;
     #3#12 = "S";
     |LINEAS_BI_ATRI #3#12, S, I, -1;
|FPRC;

|PROCESO Devuelve; |TIPO 11;
     nModo = (FEntrada / 100) ! 0;

     ||SI #3#12 = "S"; |Y nModo = 2; |Y FSalida = 13;
     |SI nModo = 2; |Y FSalida = 13;
          |SI #3#12 = "S";
               aAlfa = "    S" + &191 + "Desea dejar el movimiento seleccionado como ";
               aAlfa = aAlfa + "pendiente de env癌?";
               || MENAV aAlfa;
               |CONFI aAlfa;
               |SI FSalida = 0;
                    #3#12 = "N";
                    #3#13 = "01.01.0000";
                    #3#14 = "";
                    #3#15 = " ";
                    |GRABA 020#3a;
               |FINSI;
               |PONCONTROL 23, "si";
               |ACABA;
          |FINSI;
               /*
               |SI #3#12 = "N";
                    #3#12 = "S";
                    |GRABA 020#3a;
                    |PONCONTROL 23, "si";
                    |ACABA;
               |FINSI;
               */
     |SINO;
          |ACABA;
     |FINSI;
|FPRC;

|PROCESO Tipo2_cer03; |TIPO 2;
     #3#1 = #2#1;
     #3#3 = #2#3;
     #3#4 = #2#4;
     #3#6 = #2#6;
|FPRC;

|PROCESO Mensa; |TIPO 7;
     |SI swAyuda = 1;
          |POPV;
          swAyuda = 0;
     |FINSI;
     |SI #3#7 = 0;
          |MENSA "    La linea 0000 se reserva para fechas de comienzo/fin campa바 del trabajador";
     |SINO;
          |SI #3#12 = "S";
               |MENSA "    Numero de linea. Pulse < F5 > para dejar un movimiento como pendiente";
          |SINO;
               |MENSA "    Numero de linea.";
          |FINSI;
     |FINSI;
|FPRC;

