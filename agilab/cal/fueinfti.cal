|PROCESO impresion;
|DDEFECTO #3;
    #4#0 = #2#0;
    |LEE 000#4=;
    #3#0 = #2#0;
    #3#1 = #2#5;
    |LEE 000#3=;
    |SI empresa ! #1#0;|O ejercicio ! #1#1;
        lineames = 1;
        linea = 1;
        empresa = #1#0;
        ejercicio = #1#1;
    |FINSI;
    |SI mes ! #1#4;
        lineames = 1;
        mes = #1#4;
    |FINSI;
    |PRINT;
    linea = 0;
    lineames = 0;
    seleccio = 1;
|FPRC;

|PROCESO imprelin;
|BUCLELIN 2impresion#1;
|FPRC;

|DEFBUCLE fueinfti0;
 #1#1;
 ;
 #0#0, #0#2, #0#4;
 #0#1, #0#3, #0#5;
 ;
 NULL, imprelin;
|FIN;

|PROCESO tipo3; |TIPO 3;
|IMPRE 0;
|SI FSalida = 0;
    |INFOR "fueinfti";
    |ABRE #3;
    |ABRE #4;
    |BUCLE fueinfti0;
    |CIERRA #3;
    |CIERRA #4;
    |SI seleccio ! 0;
        |PIEINF;
    |SINO;
        |MENAV "    Seleccion vacia ...";
    |FINSI;
    |FININF;
|SINO;
