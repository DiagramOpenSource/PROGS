|FIN;

|PROCESO FuncOpc2;
     |SI FSalida = 13;
          |SI #labllenv#12 = "*";
               #labllenv#12 = " ";
               |PINTA #labllenv#12;
          |SINO;
               #labllenv#12 = "*";
               |PINTA #labllenv#12;
          |FINSI;
          |GRABA 020#labllenv.a;
          |LIBERA #labllenv;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO FuncOpc3;
     |SI FSalida = 13;
          |SI #labllenv#11 = "N";
               |HAZ ElProceso;
               eaFichero = aFichero;
               |HAZ EnvioNetContrata;
               #labllenv#11 = "S";
               |PINTA #labllenv#11;
               |GRABA 020#labllenv.a;
               |ERROR;
          |SINO;
               aAlfa = "    ATENCION. El llamamiento de este trabajador ya ha sido ";
               aAlfa = aAlfa + "marcado como enviado";
               |MENAV aAlfa;
               aAlfa = "    S¿Desea volver a enviarlo?";
               |CONFI aAlfa;
               |SI FSalida = 0;
                    |HAZ ElProceso;
                    eaFichero = aFichero;
                    |HAZ EnvioNetContrata;
                    #labllenv#11 = "S";
                    |PINTA #labllenv#11;
                    |GRABA 020#labllenv.a;
                    |ERROR;
               |FINSI;
          |FINSI;
     |FINSI;
     |SI FSalida = 14;
          |SI #labllenv#11 = "N";
               #labllenv#11 = "S";
               |PINTA #labllenv#11;
          |SINO;
               #labllenv#11 = "N";
               |PINTA #labllenv#11;
          |FINSI;
          |GRABA 020#labllenv.a;
          |LIBERA #labllenv;
          |ERROR;
     |FINSI;
|FPRC;

