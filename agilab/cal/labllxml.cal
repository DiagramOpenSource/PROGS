|PROCESO ElProceso;
     |ABRE #1;
     |ABRE #2;
     |ABRE #labtraba;
     |ABRE #labcentr;
     |ABRE #labpadre;

     |ABRE #labcontr;
     |LEE 000#labcontr.p;
     |CIERRA #labcontr;
     |HAZ LeeEmpresa;

     aRuta = #labcontr#51;
     |QBF aRuta;
     aAlfa = aRuta % -101;
     |SI aAlfa ! "\"; |Y aAlfa ! "/";
          aRuta = aRuta + "/";
     |FINSI;
     aRuta = aRuta + "Llamamientos/";

     |RMKDIR aRuta;

     aRuta = aRuta + aFichero;
     |XABRE "CBW", aRuta, nHandle;
     |SI FSalida ] 0;
          nNivel = 1;
          aAlfa = "<?xml version=" + &34 + "1.0" + &34 + " encoding=" + &34 + "ISO-8859-1" + &34 + " ?>";
          |HAZ Linea;

          nNivel = 1;
          aAlfa = "<LLAMAMIENTOS>";
          |HAZ Linea;

          || DATOS TRABAJADOR

          |SI nOpcion = 3;
               nDesdeTra = #labllenv#5;
               nHastaTra = #labllenv#5;
          |SINO;
               nDesdeTra = 00001;
               nHastaTra = 99999;
          |FINSI;

          |BUCLE Trabajadores;

          nNivel = 1;
          aAlfa = "</LLAMAMIENTOS>";
          |HAZ Linea;
     |FINSI;
     |XCIERRA nHandle;

     |SI nOpcion = 2;
          |SI nNumTra ! 0;
               aRuta = aRuta - aFichero;
               aAlfa = "    El fichero se ha generado correctamente en " + aRuta;
               |MENAV aAlfa;

               aAlfa1 = nNumTra;
               aAlfa2 = nNumMov;

               aAlfa = "    Se incluyen un total de " + aAlfa1 + " llamamientos ";
               aAlfa = aAlfa + "de trabajadores.";
               |MENAV aAlfa;
          |SINO;
               aAlfa = "    No se han encontrado trabajadores para enviar.";
               |MENAV aAlfa;
          |FINSI;
     |FINSI;
     |CIERRA #labpadre;
     |CIERRA #labcentr;
     |CIERRA #labtraba;
     |CIERRA #2;
     |CIERRA #1;
|FPRC;

