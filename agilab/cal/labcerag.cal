                    |LEE 000#nomcalli.=;
                    |SI FSalida = 0;
                         #0para2 = #nomcalli#9;     || nro. de jornadas
                    |FINSI;
                    #nomcalli#4 = 922;
                    |LEE 000#nomcalli.=;
                    |SI FSalida = 0;
                         #0para2 = #0para2 + #nomcalli#9;
                    |FINSI;
