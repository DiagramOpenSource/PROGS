|PROCESO BuscaJorDias;
     swMen = 0;
     swHis = 0;
     |SI #labtraba#42 = "E";
          |SI nume1 [ hasta;
               #3#2 = cmes;
               #3#3 = 0;
               #3#4 = 903;
               |LEE 000#3=;
               |SI FSalida = 0;
                    #0para2 = #3#9;     || nro. de jornadas
               |FINSI;
               #3#4 = 922;
               |LEE 000#3=;
               |SI FSalida = 0;
                    #0para2 = #0para2 + #3#9;     || nro. de jornadas
               |FINSI;
               |SI #0para2 ! 0;
                    swHis = 1;
                    |HAZ Bases;
               |FINSI;
          |FINSI;
          |SI #0para2 = 0;
               |SI nume1 = hasta;       || si es el ultimo mes
                    #nomcalli#0 = #0#0;   || si no esta en el historico busco
                    #nomcalli#1 = #0#1;   || en el mensual
