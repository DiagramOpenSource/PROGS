|FICHEROS;
     nomverag;
|FIN;

|VARIABLES;
     aParam = "";
     aBase = "";
     aAlfa = "";
     aMenu = "";
     aArchivo = "";
     nCanal = 0;
     aLinea = "";
     aVersion = "";
     &enVersionAgitool = 0;
|FIN;

|PROGRAMA;
     |CLS;

     aParam = PARAMETRO;
     |QBF aParam;

     |DBASS aBase;
     |QBF aBase;
     aMenu = aBase + "agitool/agitool.men";
     aArchivo = aMenu;

     |XABRE "U", aArchivo, nCanal;
     |SI FSalida ] 0;
          |XLEE nCanal, 100, aLinea;
     |FINSI;
     |XCIERRA nCanal;

     aLinea = aLinea - "#:Utilidades DIAGRAM ";
     aVersion = aLinea;

     aAlfa = aVersion - "." - ".";
     enVersionAgitool = aAlfa;

     |SI aParam = "MENU";
          aAlfa = "    Se encuentra instalada la versión " + aVersion;
          aAlfa = aAlfa + " de agitool";
          |MENAV aAlfa;
     |FINSI;
|FPRO;
