|FICHEROS;
     labtraba;
     labcer12 #2;
     labcer13 #3;
|FIN;

|VARIABLES;
     swAyuda = 0;
     nModo = 0;
     aAlfa = "";
     nNume = 0;
     fPropIni = @;
     fPropFin = @;
     &fIniCamp = @;
     &fFinCamp = @;
|FIN;

|PROCESO Tipo5; |TIPO 5;
     #3#12 = "S";
     |LINEAS_BI_ATRI #3#12, S, I, -1;
|FPRC;

|PROCESO Devuelve; |TIPO 11;
     nModo = (FEntrada / 100) ! 0;

     |SI nModo = 2; |Y FSalida = 13;
          |SI #3#12 = "S";
               aAlfa = "    S" + &191 + "Desea dejar el movimiento seleccionado como ";
               aAlfa = aAlfa + "pendiente de env癌?";
               |CONFI aAlfa;
               |SI FSalida = 0;
                    #3#12 = "N";
                    #3#13 = "01.01.0000";
                    #3#14 = "";
                    #3#15 = " ";
                    |GRABA 020#3a;
               |FINSI;
               |PONCONTROL 23, "si";
               |ACABA;
          |FINSI;
     |SINO;
          |ACABA;
     |FINSI;
|FPRC;

|PROCESO Tipo2_cer03; |TIPO 2;
     #3#1 = #2#1;
     #3#3 = #2#3;
     #3#4 = #2#4;
     #3#6 = #2#6;
|FPRC;

|PROCESO Mensa; |TIPO 7;
     |SI swAyuda = 1;
          |POPV;
          swAyuda = 0;
     |FINSI;
     |SI #3#7 = 0;
          |MENSA "    La linea 0000 se reserva para fechas de comienzo/fin del periodo de ERE";
     |SINO;
          |SI #3#12 = "S";
               |MENSA "    Numero de linea. Pulse < F5 > para dejar un movimiento como pendiente";
          |SINO;
               |MENSA "    Numero de linea.";
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO AyudaIC; |TIPO 7;
     || AYUDA DE OPCIONES

     |PUSHV 05402380;
     swAyuda = 1;
     |COLOR 0,7;
     |ATRI R;

     || 3     4         5         6         7         8
     || 45678901234567890123456789012345678901234567890
     |PINTA 0745, "     - Opciones disponibles -       ";
     |PINTA 0845, "Per멾dos de  actividad de trabajador";
     |PINTA 0945, "fijo discontinuo:                   ";
     |PINTA 1045, "01 - Por finalizaci묥 de campa바    ";
     |PINTA 1145, "02 - Finalizaci묥 por excedencia    ";
     |PINTA 1245, "     durante la campa바             ";
     |PINTA 1345, "03 - Durante la campa바             ";
     |PINTA 1445, "04 - Por inicio de campa바          ";
     |ATRI 0;
|FPRC;

|PROCESO RestauraIC;
     swAyuda = 0;
     |POPV;
     |SI #3#16 = 3;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO AyudaCoef; |TIPO 7;
     || AYUDA DE OPCIONES
     || 4         5         6         7         8
     || 01234567890123456789012345678901234567890
     |PUSHV 05502380;
     swAyuda = 1;
     |COLOR 0,7;
     |ATRI R;
     |PINTA 0751, " - Opciones disponibles -  ";
     |PINTA 0851, "00 - 1.000                 ";
     |PINTA 0951, "04 - 1.250                 ";
     |PINTA 1051, "05 - 1.300                 ";
     |PINTA 1151, "06 - 1.350                 ";
     |PINTA 1251, "07 - 1.400                 ";
     |PINTA 1351, "08 - 1.450                 ";
     |PINTA 1451, "09 - 1.500                 ";
     |ATRI 0;
|FPRC;

|PROCESO RestauraCoef;
     swAyuda = 0;
     |POPV;
     |SI #3#16 = 1; |O #3#16 = 2; |O #3#16 = 3;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO AyudaActiv; |TIPO 7;
     || AYUDA DE OPCIONES
     || 4         5         6         7         8
     || 01234567890123456789012345678901234567890
     |PUSHV 05502380;
     swAyuda = 1;
     |COLOR 0,7;
     |ATRI R;
     |PINTA 0751, " - Opciones disponibles -  ";
     |PINTA 0851, "01 - Sin actividad         ";
     |PINTA 0951, "02 - Sin actividad por no  ";
     |PINTA 1051, "     acudir al trabajo.    ";
     |PINTA 1151, "03 - D며s de actividad     ";
     |PINTA 1251, "04 - I.T., Maternidad      ";
     |PINTA 1351, "05 - Descanso semanal      ";
     |PINTA 1451, "06 - Vacaciones anuales re-";
     |PINTA 1551, "     tribuidas (excluidas  ";
     |PINTA 1651, "     de pago de prestaci묥)";
     |ATRI 0;
|FPRC;

|PROCESO LineaCero;
     |SI #3#7 = 0;
          |CAMPO_MODIFICA #3#10, 1, "N";
          #3#10 = 0;
          |PINTA #3#10;
          #3#11 = "Inicio/fin periodo ERE";
          |PINTA #3#11;

          |CAMPO_MODIFICA #3#16, 1, "S";
          |CAMPO_MODIFICA #3#17, 1, "S";
/*
          |ABRE #labtraba;
          #labtraba#0 = #3#0;
          #labtraba#1 = #3#5;
          |LEE 000#labtraba.=;
          |SI #labtraba#42 = "C"; |O #labtraba#42 = "F";
               |CAMPO_MODIFICA #3#16, 1, "S";
          |SINO;
               |CAMPO_MODIFICA #3#16, 1, "N";
               #3#16 = 0;
               |PINTA #3#16;
               |GRABA 020#3a;
          |FINSI;
          |CIERRA #labtraba;
*/
     |SINO;
          |CAMPO_MODIFICA #3#10, 1, "S";
          |CAMPO_MODIFICA #3#16, 1, "N";

          #3#17 = 0;
          |PINTA #3#17;
          ||GRABA 020#3a;
          |CAMPO_MODIFICA #3#17, 1, "N";

          |ABRE #labtraba;
          #labtraba#0 = #3#0;
          #labtraba#1 = #3#5;
          |LEE 000#labtraba.=;
/*
          |SI #labtraba#42 = "C"; |O #labtraba#42 = "F";
               || nada, solo en linea 0
          |SINO;
*/
               #3#16 = 0;
               |PINTA #3#16;
               ||GRABA 020#3a;
          ||FINSI;
          |CIERRA #labtraba;
     |FINSI;

     |SI #3#12 = "S";
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO RestauraActiv;
     swAyuda = 0;
     |POPV;

     |SI #3#10 ] 01; |Y #3#10 [ 06;
          |SI #3#10 = 1;
               #3#11 = "Sin actividad";
          |FINSI;
          |SI #3#10 = 2;
               #3#11 = "Sin actividad por no acudir al llamamiento";
          |FINSI;
          |SI #3#10 = 3;
               #3#11 = "D며s de actividad";
          |FINSI;
          |SI #3#10 = 4;
               #3#11 = "I.T., Maternidad";
          |FINSI;
          |SI #3#10 = 5;
               #3#11 = "Descanso semanal";
          |FINSI;
          |SI #3#10 = 6;
               #3#11 = "Vacaciones anuales retribuidas";
          |FINSI;
          |PINTA #3#11;
     |FINSI;
|FPRC;

|PROCESO FechaIni; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |SALVA_FICHA #3;
          |LEE 000#3a;
          |SI FSalida = 0;
               fPropIni = #3#8;
          |FINSI;
          |REPON_FICHA #3;
          #3#8 = fPropIni;
          |PINTA #3#8;
     |FINSI;
|FPRC;

|PROCESO FechaFin; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          fPropFin = #3#8;
          nNume = fPropFin;
          nNume = nNume + 1;
          fPropFin = nNume;
          #3#9 = fPropFin;
          |PINTA #3#9;
     |FINSI;
|FPRC;

|PROCESO CompFechas;
     |SI #3#9 < #3#8;
          aAlfa = "    La fecha de fin del intervalo no puede ser mayor que la fecha de inicio.";
          aAlfa = aAlfa + " Revise las fechas."
          |MENAV aAlfa;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO CompFecha1;
     |SI #3#8 < fIniCamp; |O #3#8 > fFinCamp;
          aAlfa = "    La fecha de inicio del intervalo ha de estar comprendida";
          aAlfa = aAlfa + " entre las fechas de inicio/fin campa人 del trabajador."
          |MENAV aAlfa;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO CompFecha2;
     |SI #3#9 < fIniCamp; |O #3#9 > fFinCamp;
          aAlfa = "    La fecha de fin del intervalo ha de estar comprendida";
          aAlfa = aAlfa + " entre las fechas de inicio/fin campa人 del trabajador."
          |MENAV aAlfa;
          |ERROR;
     |FINSI;
|FPRC;
