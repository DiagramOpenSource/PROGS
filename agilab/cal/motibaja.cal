|FICHEROS;
     motibaja #3;
|FIN;

|VARIABLES;
     nModo = 0;
     nPos1 = 0;
     nPos2 = 0;
|FIN;

|PROCESO inferior;
     #motibaja#0 = 01;
|FPRC;

|PROCESO superior;
     #motibaja#0 = 99;
|FPRC;

|PROGRAMA;
     |CLS;
     |ABRE #motibaja;
     |SI PARAMETRO = "MODIF";
          |ENTLINEAL #1#motibaja, -1, 2, 22, 1, inferior, superior;
     |SINO;
          |CONSULTA_CLAVE #1#motibaja;
     |FINSI;
     |CIERRA #motibaja;
|FPRO;
