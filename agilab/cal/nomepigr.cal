|FICHEROS;
     nomepigr #0;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO Los700;
     nModo = (FEntrada / 100) ! 0;
     |SI #0#0 ] 700;
          |SI nModo ! 4;
               |MENAV "    Estan reservados los epigrafes a partir de Codigo 700 desde el 01/01/2007";
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI #0#0 ] 700;
          |SI nModo ! 4;
               |MENAV "    Estan reservados los epigrafes a partir de Codigo 700 desde el 01/01/2007";
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;
