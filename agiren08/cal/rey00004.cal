|PROCESO Carga1;
    co1 = #1#0;
    no1 = #1#5;
    di1 = #1#24 + " " + #1#25;
    |QBF di1;
    di1 = di1 + " " + #1#26 + " " + #1#27 + " " + #1#28 + " " + #1#29;
    cp1 = (("00" + #1#30) % -102) + (("000" + #1#31) % -103);
    po1 = #1#32;
    pr1 = #2#2;
|FPRC;

|PROCESO Carga2;
    co2 = #1#0;
    no2 = #1#5;
    di2 = #1#24 + " " + #1#25;
    |QBF di2;
    di2 = di2 + " " + #1#26 + " " + #1#27 + " " + #1#28 + " " + #1#29;
