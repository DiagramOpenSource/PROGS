|FICHEROS;
     ver00001 #100;
     retabla1 #401;
     retabla2 #402;
     rem00008 #8;
     rem00500 #500;
|FIN;

|VARIABLES;
     &enError    = 0;
     &eaRenta    = "";

      nSwHay     = 0;
      nLimite    = 0;
      nPorce     = 0;
      nCanti1    = 0;
      nCanti2    = 0;
      nCanti3    = 0;
      nCanti4    = 0;
      nDias      = 0;
      nValor     = 0;
      nRecalculo = 0;

      aAlfa   = "";
