|PROCESO CalcuRenta;
    |SI #rem00002#2 ! "R"; |Y #rem00002#2 ! "A"; |ACABA;  |FINSI;

    |SI #rem00021#30 ! 0;
       Calc = (#rem00021#15 * #rem00021#30) / 366;
       |SI #rem00021#17 = "S";
          Calc = Calc % 1.1;
       |SINO;
          Calc = Calc % 2;
       |FINSI;
       #rem00006#29 = Calc % nParte;
    |FINSI;

    |SI #rem00021#32 ! 0;
       Calc = (#rem00021#15 * #rem00021#32) / 366;
       |SI #rem00021#17 = "S";
          Calc = Calc % 1.1;
       |SINO;
          Calc = Calc % 2;
       |FINSI;
       #rem00006#29 = Calc % nParte;
    |FINSI;

    |SI #rem00021#31 ! 0;
       |SI #rem00021#37 = "A";
          #rem00006#29 = ((#rem00021#38 * #rem00021#31) / 366) % nParte;
       |SINO;
          #rem00006#29 = #rem00021#38 % nParte;
       |FINSI;
    |FINSI;

    |SI #rem00002#4 = "VARIOS    ";
        #rem00006#29 = #rem00005#29 % nParte;
    |SINO;
        #rem00006#29 = #rem00005#29;
    |FINSI;

    #rem00006#30 = #rem00005#30;
    #rem00006#31 = #rem00006#29 % #rem00006#30;

    |SI #2#2 = "R";     |PDEFECTO #3, 20, 29;    |FINSI;   || solo renta
|FPRC;

|PROCESO CalcuPatri;
     |SI #reparame#15 = "S";
         |SI aTit = "TITULAR"; Comodin = "ZPATRITIT"; |FINSI;
         |SI aTit = "CONYUGE"; Comodin = "ZPATRICON"; |FINSI;

         |ABRE #rem01002;           || Busco las casillas de la liquidacion del patrimonio
         #rem01002#0 = #rem00002#0; || Codigo
         #rem01002#1 = 0;           || Complementaria
         #rem01002#2 = Comodin;     || Tipo contribuyente
         #rem01002#3 = 1;           || Bloque
         #rem01002#4 = "023";       || Casilla
         |LEE 040#rem01002.=;
         |SI FSalida = 0;
             #rem00006#20 = #rem01002#6;
         |FINSI;

         #rem01002#3 = 1;            || Bloque
         #rem01002#4 = "024";        || Casilla
         |LEE 040#rem01002.=;
         |SI FSalida = 0;
             #rem00006#21 = #rem01002#6;
         |FINSI;
         |CIERRA #rem01002;
     |SINO;
         |SI #rem00021#15 ] #rem00021#26;
             #rem00006#20 = #rem00021#15 % nParte;
         |SINO;
             #rem00006#20 = #rem00021#26 % nParte;
         |FINSI;
         #rem00006#21 = (#rem00021#22 + #rem00021#23 + #rem00021#29) % nParte;
     |FINSI;

     nAutonomia = 0;
     |SI aTit = "TITULAR";  nAutonomia = #rem00100#11;  |FINSI;
     |SI aTit = "CONYUGE";  nAutonomia = #rem00100#43;  |FINSI;

     |ABRE #retabla3;
     #retabla3#0 = nAutonomia;
     |LEE 000#retabla3.=;
     |SI FSalida ! 0;
         #retabla3#0 = 0;
         |LEE 000#retabla3.=;
         |SI FSalida ! 0;  |DDEFECTO #retabla3;  |FINSI;
     |FINSI;
     |CIERRA #retabla3;

     #rem00006#22 = #rem00006#20 - #rem00006#21;
     |PARA Campo1 = 8;  |SI Campo1 ] 1; |HACIENDO  Campo1 = Campo1 - 1;
           Campo2 = Campo1 + 8;
           Campo3 = Campo1 + 16;
           |SI #rem00006#22 > #retabla3#Campo1#;
               |SI #retabla3#Campo1# ! 0;  |O Campo1 = 1;
                    #rem00006#23 = #retabla3#Campo1#;
                    #rem00006#25 = #retabla3#Campo2#;
                    #rem00006#27 = #retabla3#Campo3#;
                    #rem00006#24 = #rem00006#22 - #rem00006#23;
                    #rem00006#26 = #rem00006#24 % #rem00006#27;
                    #rem00006#28 = #rem00006#25 + #rem00006#26;
                    |SAL;
               |FINSI;
           |FINSI;
     |SIGUE;

     |SI #rem00005#2 = "P";  |PDEFECTO #3, 29, 32;    |FINSI;   || solo patrimonio
|FPRC;

|PROCESO Modelo214;
     |ABRE #100;
     |LEE 000#100p;
     |SI FSalida ! 0;  |DDEFECTO #100;  |FINSI;
     |CIERRA #100;

     nParte  = 0;

     |ABRE #rem00021;
     #rem00021#0 = #rem00002#0;
     #rem00021#1 = #rem00002#1;
     |LEE 000#rem00021.=;
     |SI FSalida ! 0;  |DDEFECTO #rem00021;  |FINSI;
     |CIERRA #rem00021;

     |SI #rem00002#4 = "GANANCIAL ";
         nParte = 50;
         aTit   = "GANANCIAL";
         |HAZ CalcuRenta;

         |SI #0#7 = "S"; #rey00100#1 = #rem00006#31; #rey00100#13 = 1; |FINSI;
         |SI #0#7 = "S"; #rey00100#2 = #rem00006#31; #rey00100#13 = 1; |FINSI;

         aTit = "TITULAR";  |HAZ CalcuPatri;
         |SI #0#8 = "S"; #rey00100#7 = #rem00006#28; #rey00100#14 = 1; |FINSI;

         aTit = "CONYUGE";  |HAZ CalcuPatri;
         |SI #0#8 = "S"; #rey00100#8 = #rem00006#28; #rey00100#14 = 1; |FINSI;

         SwPrint = 1;
     |SINO;
         |SI #rem00002#4 = "VARIOS    ";
             |SI #rem00002#8 ! 0;
                 aTit   = "TITULAR";
                 nParte = #rem00002#8;
                 |HAZ CalcuRenta;
                 |HAZ CalcuPatri;
                 |SI #0#7 = "S"; #rey00100#1 = #rem00006#31; #rey00100#13 = 1; |FINSI;
                 |SI #0#8 = "S"; #rey00100#7 = #rem00006#28; #rey00100#14 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;

             |SI #rem00002#9 ! 0;
                 aTit   = "CONYUGE";
                 nParte = #rem00002#9;
                 |HAZ CalcuRenta;
                 |HAZ CalcuPatri;
                 |SI #0#7 = "S"; #rey00100#2 = #rem00006#31; #rey00100#13 = 1; |FINSI;
                 |SI #0#8 = "S"; #rey00100#8 = #rem00006#28; #rey00100#14 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;

             aTit   = "";
             |SI #rem00002#10 ! 0;
                 nParte = #rem00002#10;
                 |HAZ CalcuRenta;
                 |SI #0#7 = "S"; #rey00100#3 = #rem00005#31; #rey00100#13 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;

             |SI #rem00002#11 ! 0;
                 nParte = #rem00002#11;
                 |HAZ CalcuRenta;
                 |SI #0#7 = "S"; #rey00100#4  = #rem00005#31; #rey00100#13 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;

             |SI #rem00002#12 ! 0;
                 nParte = #rem00002#12;
                 |HAZ CalcuRenta;
                 |SI #0#7 = "S"; #rey00100#5  = #rem00005#31; #rey00100#13 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;

             |SI #rem00002#13 ! 0;
                 nParte = #rem00002#13;
                 |HAZ CalcuRenta;
                 |SI #0#7 = "S"; #rey00100#6  = #rem00005#31; #rey00100#13 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;
         |SINO;
             nParte = 100;

             |SI #rem00002#4 = "CONJUNTA  ";
                 nParte = 50;
                 aTit   = "TITULAR";
                 |HAZ CalcuRenta;
                 |HAZ CalcuPatri;
                 |SI #0#7 = "S"; #rey00100#1 = #rem00006#31; #rey00100#13 = 1; |FINSI;
                 |SI #0#8 = "S"; #rey00100#7 = #rem00006#28; #rey00100#14 = 1; |FINSI;

                 nParte = 50;
                 aTit   = "CONYUGE";
                 |HAZ CalcuRenta;
                 |HAZ CalcuPatri;
                 |SI #0#7 = "S"; #rey00100#2 = #rem00006#31; #rey00100#13 = 1; |FINSI;
                 |SI #0#8 = "S"; #rey00100#8 = #rem00006#28; #rey00100#14 = 1; |FINSI;

                 SwPrint = 1;
             |FINSI;

             nParte = 100;
             |SI #rem00002#4 = "TITULAR   ";
                 aTit   = "TITULAR";
                 |HAZ CalcuRenta;
                 |HAZ CalcuPatri;
                 |SI #0#7 = "S"; #rey00100#1 = #rem00006#31; #rey00100#13 = 1; |FINSI;
                 |SI #0#8 = "S"; #rey00100#7 = #rem00006#28; #rey00100#14 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;

             |SI #rem00002#4 = "CONYUGE   ";
                 aTit   = "CONYUGE";
                 |HAZ CalcuRenta;
                 |HAZ CalcuPatri;
                 |SI #0#7 = "S"; #rey00100#2 = #rem00006#31; #rey00100#13 = 1; |FINSI;
                 |SI #0#8 = "S"; #rey00100#8 = #rem00006#28; #rey00100#14 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;

             |SI #rem00002#4 = "1 FAMILIAR";
                 |HAZ CalcuRenta;
                 |SI #0#7 = "S"; #rey00100#3 = #rem00005#31; #rey00100#13 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;

             |SI #rem00002#4 = "2 FAMILIAR";
                 |HAZ CalcuRenta;
                 |SI #0#7 = "S"; #rey00100#4  = #rem00005#31; #rey00100#13 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;

             |SI #rem00002#4 = "3 FAMILIAR";
                 |HAZ CalcuRenta;
                 |SI #0#7 = "S"; #rey00100#5  = #rem00005#31; #rey00100#13 = 1; |FINSI;
