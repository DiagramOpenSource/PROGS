|PROCESO Tipo3; |TIPO 3;
   |IMPRE 0;
   |SI FSalida ! 0; |ACABA; |FINSI;

   |INFOR "reinfimp";
   |SI FSalida ! 0;
      |FINIMP;
      |ACABA;
   |FINSI;

   |ABRE #reacceso;
   |SI #0#18 = "N";
      DEmpresa = #0#0;
      HEmpresa = #0#1;
      |SI #0#17 = "1"; |BUCLE ContribCod; |FINSI;
      |SI #0#17 = "2"; |BUCLE ContribNom; |FINSI;
   |SINO;
      |PARA Cont = 19; |SI Cont [ 36; |HACIENDO Cont = Cont + 1;
         |SI #0#Cont# ! 0;
            DEmpresa = #0Cont;
            HEmpresa = #0Cont;
            |SI #0#17 = "1"; |BUCLE ContribCod; |FINSI;
            |SI #0#17 = "2"; |BUCLE ContribNom; |FINSI;
         |FINSI;
      |SIGUE;
   |FINSI;

   |PIEINF;
   |FININF;
   |FINIMP;

   |CIERRA #reacceso;
|FPRC;
