|PROCESO Contrib;
    SwPrint = 0;
    |DDEFECTO #rey00100;

    |SI #rem00100#10 = "S";
       |BUCLE ContribNac;
       |SI SwPrint = 1;
          |PRINT;
       |FINSI;
    |FINSI;

    || Luego debo hacer el bucle de los extranjeros para los modelos 210, 211,
    ||   212 y 214
    SwPrint = 0;
    |SI #rem00100#10 = "N";
       |BUCLE ContribExt;
       |SI SwPrint = 1;
          |PRINT;
       |SINO;
          |BUCLE ContribNac;
          |SI SwPrint = 1;
              |PRINT;
          |FINSI;
       |FINSI;
    |FINSI;
|FPRC;

