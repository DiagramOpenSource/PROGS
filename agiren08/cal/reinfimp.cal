             |FINSI;

             |SI #rem00002#4 = "4 FAMILIAR";
                 |HAZ CalcuRenta;
                 |SI #0#7 = "S"; #rey00100#6  = #rem00005#31; #rey00100#13 = 1; |FINSI;
                 SwPrint = 1;
             |FINSI;
         |FINSI;
      |FINSI;
|FPRC;

