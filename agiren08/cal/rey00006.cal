     |SI #1001#2 ! "TITULAR   ";  |Y #1001#2 ! "CONJUNTA  ";
      |Y #1001#2 ! "CONJUNTAE ";  |Y #1001#2 ! "ZPATRITIT ";
      |Y #1001#2 ! "CONYUGE   ";  |Y #1001#2 ! "ZPATRICON ";
