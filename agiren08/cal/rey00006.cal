         |FINSI;
         |SI #0#5 = "S";  |Y #1001#6 = "N";
             |ACABA;
         |FINSI;
     |FINSI;

     |DDEFECTO #552;

     |SI #1001#5 = "100";  |O #1001#5 = "101";
