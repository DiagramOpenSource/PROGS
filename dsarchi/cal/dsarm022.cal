|FICHEROS;
     dsarm022 #0;
|FIN;

|VARIABLES;
     nModo = 0;
     nBoton = 0;
     aLog = "";
     aMensaje = "";
     aAlfa = "";
     aAlfa1 = "";
     aAlfa2 = "";
     aDestino = "";
     aOrigen  = "";
     aIPRemoto = "";

     aBase = "";
     aFicLog = "";
     &eaFicLogVisor = "";
     &eaPathLogVisor = "";
     &eaNomLogVisor = "";
     &eaDesLogVisor = "";
     &enSoyDESPANET = 0;
     &eaUnidadBasico = "";
|FIN;

|PROCESO min;
     #0#0 = 0001;
|FPRC;

|PROCESO max;
     #0#0 = 9999;
|FPRC;

|PROCESO PonBarra; |TIPO 0;
     aAlfa1 = #0Campo;
     |QBF aAlfa1;
     aAlfa2 = aAlfa1 % -101;
     |SI aAlfa2 ! "/";|Y aAlfa2 ! "\";
          #0Campo = aAlfa1 + "/";
          |PINTA #0Campo;
     |FINSI;
|FPRC;

|PROCESO Boton;
     aAlfa = #0#2;
     |QBF aAlfa;
     |SI aAlfa = "";
          aMensaje = "0000Introduzca el nombre del Log.";
          |MENAV aMensaje;
          |ACABA;
     |FINSI;

     aLog = #0#1;
     |QBF aLog;
     |SI aLog = "";
          aMensaje = "0000Introduzca el path del Log.";
          |MENAV aMensaje;
          |ACABA;
     |FINSI;
     aLog = aLog + #0#2;
     |QBF aLog;

     |HAZ LeeUnidadBasico;

     aDestino = eaUnidadBasico + "\DOCUMENTOS";
     |RMKDIR aDestino;
     aDestino = aDestino + "\AGITOOL";
     |RMKDIR aDestino;
     aDestino = aDestino + #0#2;
     |QBF aDestino;

     aOrigen  = aLog;

     aIPRemoto = "";
     |IP_REMOTO aIPRemoto;  |QBT aIPRemoto;

     |SI aIPRemoto ! "";
          |ENVIA_FICHERO aOrigen, aDestino;
     |SINO;
          |COPIA_FICHERO aOrigen, aDestino;
     |FINSI;
     |SI FSalida = 0;
         |RSYSTEM aDestino;
     |SINO;
          aMensaje = "0000Error al copiar el log: " + aOrigen;
          |MENAV aMensaje;
     |FINSI;
|FPRC;

|PROGRAMA;
     |HAZ SoyDESPANET;
     |SI enSoyDESPANET = 1;  |ACABA;  |FINSI;

     |CLS;

     |DBASE aBase;
     |QBF aBase;
     aFicLog = aBase + "logs/";

     eaPathLogVisor = aFicLog
     eaNomLogVisor = "dsreglas.log";
     eaFicLogVisor = eaPathLogVisor + eaNomLogVisor;
     eaDesLogVisor = "Logs Reglas Automaticas. (dsarz030)";
     |HAZ MeteVisorLog;

     eaPathLogVisor = aFicLog
     eaNomLogVisor = "com_auto.log";
     eaFicLogVisor = eaPathLogVisor + eaNomLogVisor;
     eaDesLogVisor = "Logs Comunicaciones automaticas. (dsarz031)";
     |HAZ MeteVisorLog;

     eaPathLogVisor = aFicLog
     eaNomLogVisor = "rec_doc.log";
     eaFicLogVisor = eaPathLogVisor + eaNomLogVisor;
     eaDesLogVisor = "Logs Recepcion Documentos. (dsarz032)";
     |HAZ MeteVisorLog;



     |CONTROL_SIMPLE 0, "BOTON, Visualizar Log", 0662, 0677, Boton;
     nBoton = FSalida;
     |ESTADO_CONTROL nBoton, "ENABLE";

     |ABRE #0;
     |ENTLINEAL #1#0, -1, 4, 20, 1, min, max;
     |CIERRA #0;

     |FIN_CONTROL_WINDOWS nBoton;
|FPRO;
