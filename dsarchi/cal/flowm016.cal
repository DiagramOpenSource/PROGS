|FICHEROS;
     flowm016 #0;

     dsarm001 #1;        ||Parametros
     dsarm005 #5;        ||Registro Documentos
     dsarz010 #10;       ||Permisos Usuarios
     flowm002 #602;      ||Situaciones WORKFLOW
     flowm003 #603;      ||Respuestas  WORKFLOW
     flowm004 #604;      ||Usuarios    WORKFLOW
     flowm005 #605;      ||Indices     WORKFLOW
     flowm006 #606;      ||Movimientos
|FIN;

|VARIABLES;
     nPasada = 0;
     nUsuarios = 0;
     nDocu = 0;
     fFechaHoy = @;
     aHora = "";
     aDUsu = "";
     aHUsu = "";
     nSwHay = 0;
     nLinea = 0;
     &enNumRegFlow = 0;
     &enSituFlow = 0;
     &eaDesSituFlow = "";
     nGrupo = 0;
     nSubGrupo = 0;
     nTipo = 0;
     nSituacion = 0;
     aDTipo = "";
     aHTipo = "";



     aGuarda10 = "";          ||Sirven para guardar los nuevos campos al cambiar de situacion
     aGuarda11 = "";
     aGuarda12 = "";
     aGuarda13 = "";
     aGuarda14 = "";
     aGuarda15 = "";
     aGuarda16 = "";
     aGuarda17 = "";
     aGuarda18 = "";

     nSitu = 0;
     nExiste = 0;
     aMensaje = "";
     &eaPrograma = "";
     &enAprobacion = 0;
     nCampo = 0;
     aUsuario = "";
     aRespuesta = "";
|FIN;

|PROCESO flowm004R;
     |SI nPasada = 0;
         nUsuarios  = nUsuarios + 1;
         |ACABA;
     |FINSI;

     |DDEFECTO #605;

     #605#0  = #604#4;
     #605#1  = #604#0;
     #605#2  = #604#1;
     #605#3  = #604#2;
     #605#4  = #604#3;
     #605#5  = nDocu;
     #605#6  = fFechaHoy;
     #605#7  = aHora;
     #605#8  = "E";
     #605#9  = "0,0,0;0,255,0";

     #605#10 = aGuarda10;
     #605#11 = aGuarda11;
     #605#12 = aGuarda12;
     #605#13 = aGuarda13;
     #605#14 = aGuarda14;
     #605#15 = aGuarda15;
     #605#16 = aGuarda16;
     #605#17 = aGuarda17;
     #605#18 = aGuarda18;

     |SI nUsuarios > 1;
         #605#8  = "C";
         #605#9  = "0,0,0;255,255,0";
     |FINSI;

     |GRABA 020#605n;
     |LIBERA #605;
|FPRC;

|DEFBUCLE flowm004R;
     #604#1;
     ;
     #flowm016#2, #flowm016#3, #flowm016#4, #flowm016#10, aDUsu;
     #flowm016#2, #flowm016#3, #flowm016#4, #flowm016#10, aHUsu;
     ;
     NULL, flowm004R;
|FIN;

|PROCESO flowm005_hay;
     nSwHay = 1;
     |ERROR10;
|FPRC;

|DEFBUCLE flowm005_hay;
 #flowm005#2;
 ;
 #flowm016#1, "          ", #flowm016#2, #flowm016#3, #flowm016#4, #flowm016#8;
 #flowm016#1, "zzzzzzzzzz", #flowm016#2, #flowm016#3, #flowm016#4, #flowm016#8;
 ;
 NULL, flowm005_hay;
|FIN;

|PROCESO flowm005B;
     |BORRA 020#605a;
     |LIBERA #605;
|FPRC;

|DEFBUCLE flowm005B;
     #605#1;
     8;
     "          ", nGrupo, nSubGrupo, nTipo, nSituacion, nDocu, aDTipo;
     "zzzzzzzzzz", nGrupo, nSubGrupo, nTipo, nSituacion, nDocu, aHTipo;
     be;
     NULL, flowm005B;
|FIN;

|PROCESO Principal;
     |SI #flowm016#8 = #flowm016#10;
          aMensaje = "0000Situación Actual igual a situación correcta. Proceso cancelado";
          |MENAV aMensaje;
          |ACABA;
     |FINSI;

     ||LEO el PRIMERO PARA OBTENER LOS CAMPOS.  (Del 10 al 18)
     nSwHay = 0;
     |BUCLE flowm005_hay;

     |SI nSwHay = 0;
          aMensaje = "0000Este registro NO esta en el WorkFlow. Proceso Cancelado";
          |MENAV aMensaje;
          |ACABA;
     |FINSI;

     aUsuario = Usuario % 810;

     aGuarda10 = #605#10;
     aGuarda11 = #605#11;
     aGuarda12 = #605#12;
     aGuarda13 = #605#13;
     aGuarda14 = #605#14;
     aGuarda15 = #605#15;
     aGuarda16 = #605#16;
     aGuarda17 = #605#17;
     aGuarda18 = #605#18;

     |ABRE #606;
     #606#0 = #605#5;
     #606#1 = 99999;
     |LEE 000#606];
     |SI FSalida = 0;
         |LEE 000#606a;
     |SINO;
         |LEE 000#606u;
     |FINSI;

     nLinea = 1;
     |SI FSalida = 0;  |Y #606#0 = #605#5;
         nLinea = #606#1;
     |FINSI;

     #606#0 = #605#5;
     #606#1 = nLinea;
     |LEE 101#606=;
     |SI FSalida = 0;
         #606#5 = aUsuario;
         |GRABA 020#606a;
         |LIBERA #606;
     |FINSI;

     fFechaHoy = ~;
     |HORA aHora;

     |DDEFECTO #606;
     #606#0 = #605#5;
     #606#1 = nLinea + 1;
     #606#2 = #flowm016#10;
     #606#3 = fFechaHoy;
     #606#4 = aHora;
     #606#5 = aUsuario;
     #606#6 = "CAMBIO MANUAL SITUACION";

     enNumRegFlow = #flowm006#0;
     enSituFlow = #flowm006#2;
     |HAZ DimeDesFlow;
     #flowm006#7 = eaDesSituFlow;

     |GRABA 020#606n;
     |LIBERA #606;

     nGrupo     = #605#1;
     nSubGrupo  = #605#2;
     nTipo      = #605#3;
     nSituacion = #605#4;
     nDocu      = #605#5;
     aDTipo     = " ";
     aHTipo     = "z";
     |BUCLE flowm005B;

     aDUsu      = "";
     aHUsu      = "zzzzzzzzzz";

     |ABRE #flowm005;
     nUsuarios = 0;
     nPasada   = 0;  |BUCLE flowm004R;
     nPasada   = 1;  |BUCLE flowm004R;
     |CIERRA #flowm005;
|FPRC;

|PROCESO flowm005
     nSitu = #flowm005#4;
     |ERROR10;
|FPRC;

|DEFBUCLE flowm005;
 #flowm005#2;
 ;
 #flowm016#1, "          ", #flowm016#2, #flowm016#3, #flowm016#4, 000;
 #flowm016#1, "zzzzzzzzzz", #flowm016#2, #flowm016#3, #flowm016#4, 999;
 ;
 NULL, flowm005;
|FIN;

|PROCESO Tipo0C1m016; |TIPO 0;
     |ABRE #dsarm005;
     |DDEFECTO #dsarm005;
     #dsarm005#0 = #flowm016#1;
     |LEE 000#dsarm005.=;
     |SI FSalida ! 0;
          #flowm016#2 = 000;
          #flowm016#3 = 000;
          #flowm016#4 = 000;
          #flowm016#5 = "01.01.0000";
          #flowm016#6 = "";
          #flowm016#7 = "";
          |PINTA #flowm016#2;
          |PINTA #flowm016#3;
          |PINTA #flowm016#4;
          |PINTA #flowm016#5;
          |PINTA #flowm016#6;
          |PINTA #flowm016#7;
     |SINO;
          #flowm016#2 = #dsarm005#1;
          #flowm016#3 = #dsarm005#2;
          #flowm016#4 = #dsarm005#3;
          #flowm016#5 = #dsarm005#4;
          #flowm016#6 = #dsarm005#5;
          #flowm016#7 = #dsarm005#6;
          |PINTA #flowm016#2;
          |PINTA #flowm016#3;
          |PINTA #flowm016#4;
          |PINTA #flowm016#5;
          |PINTA #flowm016#6;
          |PINTA #flowm016#7;
     |FINSI;
     |CIERRA #dsarm005;

     nSitu = 0;
     |BUCLE flowm005;

     |SI nSitu = 0;
          aMensaje = "0000El documento seleccionado NO esta en el WorkFlow";
          |MENAV aMensaje;
          |ERROR;
          #flowm016#8 = 0;
          #flowm016#9 = "";
          |PINTA #flowm016#8;
          |PINTA #flowm016#9;
     |SINO;
          |ABRE #flowm002;
          |DDEFECTO #flowm002;
          #flowm002#0 = #flowm016#2;
          #flowm002#1 = #flowm016#3;
          #flowm002#2 = #flowm016#4;
          #flowm002#3 = nSitu;
          |LEE 000#flowm002.=;
          #flowm016#8 = nSitu;
          #flowm016#9 = #flowm002#4;
          |CIERRA #flowm002;

          |PINTA #flowm016#8;
          |PINTA #flowm016#9;
     |FINSI;
|FPRC;

|PROCESO Baja002;
     #flowm002#0 = #flowm016#2;
     #flowm002#1 = #flowm016#3;
     #flowm002#2 = #flowm016#4;
     #flowm002#3 = 000;
|FPRC;

|PROCESO Alta002;
     #flowm002#0 = #flowm016#2;
     #flowm002#1 = #flowm016#3;
     #flowm002#2 = #flowm016#4;
     #flowm002#3 = 999;
|FPRC;

|PROCESO Tipo66C10; |TIPO 66;
     |ABRE #flowm002;
     |CONSULTA_F_CLAVE #1#flowm002, Baja002, Alta002;
     |SI FSalida = 0;
          #flowm016#10 = #flowm002#3;
          #flowm016#11 = #flowm002#4;
     |SINO;
          #flowm016#10 = 000;
          #flowm016#11 = "";
     |FINSI;
     |PINTA #flowm016#10;
     |PINTA #flowm016#11;
     |CIERRA #flowm002;
|FPRC;

|PROCESO Tipo0C10; |TIPO 0;
     ||ARA
     nSitu = #flowm016#10;
     |SI nSitu ! 0;
          |ABRE #flowm002;
          |DDEFECTO #flowm002;
          #flowm002#0 = #flowm016#2;
          #flowm002#1 = #flowm016#3;
          #flowm002#2 = #flowm016#4;
          #flowm002#3 = nSitu;
          |LEE 000#flowm002.=;
          |SI FSalida ! 0;
               aMensaje = "0000No existe la situación para el Grupo/Subgrupo/Tipo";
               |MENAV aMensaje;
               #flowm016#11 = "";
               |PINTA #flowm016#11;
               |ERROR;
          |SINO;
               #flowm016#11 = #flowm002#4;
               |PINTA #flowm016#11;
          |FINSI;
          |CIERRA #flowm002;
     |SINO;
          aMensaje = "0000Introduzca la nueva situación correcta";
          |MENAV aMensaje;
          #flowm016#11 = "";
          |PINTA #flowm016#11;
          |ERROR;
     |FINSI;
|FPRC;

|PROGRAMA;
     eaPrograma   = "WORKFLOW";
     |HAZ DSARCHI_INI;
     |SI enAprobacion = 0;
         |MENAV "0000No tiene instalado el m¢dulo WORKFLOW.";
         |ACABA;
     |FINSI;

     |ABRE #1;
     |LEE 000#1p;
     |SI FSalida ! 0;  |DDEFECTO #1;  |FINSI;
     |CIERRA #1;

     |ABRE #dsarz010;
     aUsuario = Usuario % 810;
     |DDEFECTO #dsarz010;
     #dsarz010#0 = aUsuario;
     |LEE 000#dsarz010.=;
     |SI FSalida ! 0;
          |PARA nCampo = 3;  |SI nCampo [ 52;  |HACIENDO nCampo = nCampo + 1;
               #dsarz010#nCampo# = "S";
          |SIGUE;
     |FINSI;
     |CIERRA #dsarz010;

     |CLS;
     |ABRE #0;
     |DDEFECTO #0;
     |LEE 101#0p;
     nExiste = FSalida;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          aRespuesta = #flowm016#12;
          #flowm016#12 = "NO";
          |SI nExiste = 0;
               |GRABA 020#0a;
          |SINO;
               |GRABA 020#0n;
          |FINSI;
          |SI aRespuesta = "SI";
               |HAZ Principal;
          |FINSI;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;
