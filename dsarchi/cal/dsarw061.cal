|PROCESO Baja61;
     |SI nSwEstaIntegral = 1;
          #103#0 = "            ";
     |SINO;
          #203#0 = "            ";
     |FINSI;
|FPRC;

|PROCESO Alta61;
     |SI nSwEstaIntegral = 1;
          #103#0 = "zzzzzzzzzzzz";
     |SINO;
          #203#0 = "zzzzzzzzzzzz";
     |FINSI;
|FPRC;

|PROCESO Evento61;
     |SI FSalida = 1;  |O FSalida = 2;
          |SI nSwEstaIntegral = 1;
               |LEE 000#103=;
          |SINO;
               |LEE 000#203=;
          |FINSI;
     |FINSI;

     |SI FSalida = 4;
          |SI nSwEstaIntegral = 1;
          |LEE 101#103=;
          |SI FSalida = 0;
               |ENDATOS #1#103;
               |SI FSalida = 0;
                    |GRABA 020#103.a;
               |FINSI;
               |LIBERA #103;
          |FINSI;
          |SINO;
               |LEE 101#203=;
               |SI FSalida = 0;
                    |ENDATOS #1#203;
                    |SI FSalida = 0;
                         |GRABA 020#203.a;
                    |FINSI;
                    |LIBERA #203;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO BotonNIF1_61;
     |DBASS aBase;
     |QBF aBase;
     aDef = aBase + "integral/def/dsam0103.mas";
     |XABRE "E", aDef, 0;
     |SI FSalida < 0;
          aDef = aBase + "basica/def/agim0003.mas";
          |XABRE "E", aDef, 0;
          |SI FSalida < 0;
               aMensaje = "0000Aplicacion integral/basica NO instaladas";
               |MENAV aMensaje;
               |ACABA;
          |SINO;
               nSwEstaBasica = 1;
          |FINSI;
     |SINO;
          nSwEstaIntegral = 1;
     |FINSI;

     |ESTADO_CONTROL nBotonNIF1, "HIDE";
     |ESTADO_CONTROL nBotonNIF2, "HIDE";

     aQueBoton     = "|X000";
     aID           = #dsarw061#aQueBoton#;
     nBotonAceptar = aID;
     |ESTADO_CONTROL nBotonAceptar, "HIDE";

     aQueBoton      = "|X001";
     aID            = #dsarw061#aQueBoton#;
     nBotonCancelar = aID;
     |ESTADO_CONTROL nBotonCancelar, "HIDE";

     |SI nSwEstaIntegral = 1;

          |ABRE #103;
          nRango = 2 + 4 + 8 + 16 + 32 + 128;

          |LINEAL_SIMPLE #1#103, nRango, 0602, 2698, Baja61, Alta61, Evento61;
          nGrid = FSalida;

          aEsc1  = &255 + "806";
          aEsc2  = &255 + "807";
          aRetu  = &255 + "802";
          |PARA; |SI;  |HACIENDO;
              FSalida = "::Salir," + nGrid;
              |LEETECLA aTecla;
              |SI aTecla = aEsc1; |SAL;  |FINSI;
              |SI aTecla = aEsc2; |SAL;  |FINSI;
              |SI aTecla = aRetu; |SAL;  |FINSI;
          |SIGUE;
          |CIERRA #103;
     |SINO;
          |ABRE #203;
          nRango = 2 + 4 + 8 + 16 + 32 + 128;

          |LINEAL_SIMPLE #1#203, nRango, 0602, 2698, Baja, Alta, Evento;
          nGrid = FSalida;

          aEsc1  = &255 + "806";
          aEsc2  = &255 + "807";
          aRetu  = &255 + "802";
          |PARA; |SI;  |HACIENDO;
               FSalida = "::Salir," + nGrid;
               |LEETECLA aTecla;
               |SI aTecla = aEsc1; |SAL;  |FINSI;
               |SI aTecla = aEsc2; |SAL;  |FINSI;
               |SI aTecla = aRetu; |SAL;  |FINSI;
          |SIGUE;
          |CIERRA #203;
     |FINSI;

     |DESTRUYECONTROL nGrid;

     |ESTADO_CONTROL nBotonNIF1, "SHOW";
     |ESTADO_CONTROL nBotonNIF2, "SHOW";
     |ESTADO_CONTROL nBotonAceptar, "SHOW";
     |ESTADO_CONTROL nBotonCancelar, "SHOW";
|FPRC;

