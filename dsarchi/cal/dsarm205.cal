|| aqui voy a cargar la consulta de documentos desde nomina
|| tengo que cargar el dsarm005@ que es donde metere el resultado de la
|| SQL que me cargara los documentos que despues tendre que presentar

|| esto sera una vesion reducida (reducidisima) del dsarz002, que es
|| donde se cargan los registros necesarios desde la busqueda

|FICHEROS;
     dsarm005;

     dsarm005@;
|FIN;

|VARIABLES;
     nCampo         = 0;
     nSubVent       = 0;

     aAlfa          = "";

     {-1}aVent      = "";
         aVent1     = 0;
         aVent2     = 0;
         aVent3     = 0;
         aVent4     = 0;
         aVent5     = "";

     &eaNifTrab   = "";
     &enNumCampos = 0;
|FIN;

|PROCESO CargaSqlLaboral;
     aAlfa = "SELECT * FROM dsarm005 INTO tm" + Usuario;
     aAlfa = aAlfa + ".def WHERE 10 == " + eaNifTrab;
     aAlfa = aAlfa + " OR 95 == " + eaNifTrab;
     |SQL aAlfa;

     |DBASE aAlfa;
     |QBF aAlfa;
     aAlfa = aAlfa + "def/tm" + Usuario + ".mas";
     |CARGA_DEF aAlfa , dsarm005@;
|FPRC;

|PROCESO DescargaSqlLaboral;
     aAlfa = "tm" + Usuario;
     |NOME_DAT #dsarm005@#aAlfa#;
     |DELINDEX #dsarm005@;
     |DESCARGA_DEF #dsarm005@;
     |DBASE aAlfa;  |QBF aAlfa;
     aAlfa = aAlfa + "def/tm" + Usuario + ".mas";
     |FBORRA aAlfa;
|FPRC;
