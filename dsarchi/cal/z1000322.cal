|FICHEROS;
|FIN;

|VARIABLES;
|FIN;

|INCLUYE z1000var;

|PROCESO Es322;
     |SI nTipoModelo = 1;
         nLinea = 0;
         |PARA enLinea = 1; |SI enLinea [ 100; |HACIENDO enLinea = enLinea + 1;
               |HAZ LeeContenido;

               aAlfa = eaContenido - "DevengoEjercicio";
               |SI FSalida ! 0;
                   nLinea = enLinea;
                   |SAL;
               |FINSI;
         |SIGUE;

         |SI nLinea = 0;  |ACABA;  |FINSI;

         enLinea = nLinea;  |HAZ LeeContenido;

         aAlfa = eaContenido - "NIF";
         |SI FSalida ! 0;
             eaEjercicio = eaContenido % 2004;
         |FINSI;

         aAlfa = eaContenido - "N.I.F.";
         |SI FSalida ! 0;
             eaEjercicio = eaContenido % 2304;
         |FINSI;

         eaContenido = eaContenido % -102; |HAZ CargaPeriodoPdf;
         |SI eaPeriodo = "";  |ACABA;  |FINSI;

         enLinea = nLinea + 1;  |HAZ LeeContenido;  eaDni = eaContenido % -109;

         enLinea = 8;   |HAZ LeeContenido;  eaElect = eaContenido % -116;
         enLinea = 20;  |HAZ LeeContenido;  eaJusti = eaContenido % -113;

         enModelo  = 322;
     |FINSI;

     aAlfa = eaJusti % 103;
     |SI aAlfa ! "322";
         eaJusti   = "";
     |FINSI;

     |SI eaDni  = "";   eaPeriodo = "";  |FINSI;
|FPRC;

|PROCESO MiraSiEs322;
     enLinea = 3;  |HAZ LeeContenido;
     eaContenido = eaContenido - "MODELO322";
     |SI FSalida ! 0;
         nTipoModelo = 1;   |HAZ Es322;  |SI eaPeriodo ! "";  |ACABA;  |FINSI;
     |FINSI;
|FPRC;
