     |FINSI;

     |SI FSalida = 4;
         |LEE 101#trabajo@.=;
         |SI FSalida ! 0;  |ACABA;  |FINSI;

         #trabajo@#1 = 0;
         #trabajo@#2 = 0;
         #trabajo@#3 = 0;
         #trabajo@#4 = 0;
         #trabajo@#6 = "Formato de modelo no reconocido";
         #trabajo@#7 = "";
