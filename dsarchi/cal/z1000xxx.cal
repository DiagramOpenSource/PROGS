|PROCESO agifigen;
     eaAlfa = #agifigen#2 > "";  |QBT eaAlfa;
     |SI eaAlfa = "";
         eaAlfa = #agifigen#1 > "";  |QBT eaAlfa;
     |FINSI;
     |HAZ QuitaPuntos;

     aAlfa  = eaAlfa - aAlfa1;
     |SI FSalida ! 0;
         |SI aAlfa2 = "";
             eaAlfa = #agifigen#2;  |QBF eaAlfa;
             |SI eaAlfa = "";
                 eaAlfa = #agifigen#1;
             |FINSI;

             #trabajo@#1 = #agifigen#0;
             #trabajo@#6 = eaAlfa;
             #trabajo@#9 = #agifigen#13;

             |ERROR10;
             |ACABA;
         |FINSI;

         |SI aAlfa2 ! "";
             aAlfa  = eaAlfa - aAlfa2;
             |SI FSalida ! 0;
                 eaAlfa = #agifigen#2;  |QBF eaAlfa;
                 |SI eaAlfa = "";
                     eaAlfa = #agifigen#1;
                 |FINSI;

                 #trabajo@#1 = #agifigen#0;
                 #trabajo@#6 = eaAlfa;
                 #trabajo@#9 = #agifigen#13;

                 |ERROR10;
             |FINSI;
         |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE agifigen;
     #agifigen#3;
     ;
     aDesde;
     aHasta;
     ;
     NULL, agifigen;
|FIN;

|PROCESO LeeCliDNIz4;
     |QBF eaDni;
     |SI eaDni = "";  |ACABA;  |FINSI;

     |SI #trabajo@#4 = 100;  |O #trabajo@#4 = 102;
         |SI enSwEstaIntegral = 1;
             |SELECT #2#dsam0003;
             #dsam0003#1 = eaDni;
             |LEE 000#dsam0003.=;
             |SI FSalida = 0;
                 #trabajo@#1 = #dsam0003#0;
                 #trabajo@#6 = #dsam0003#5;
                 |ACABA;
             |FINSI;

             |SELECT #3#dsam0003;
             #dsam0003#35 = eaDni;
             |LEE 000#dsam0003.=;
             |SI FSalida = 0;
                 #trabajo@#1 = #dsam0003#0;
                 #trabajo@#6 = #dsam0003#5;
                 |ACABA;
             |FINSI;
         |FINSI;
     |FINSI;

     |ABRE #agifigen;
     |SELECT #4#agifigen;
     #agifigen#13 = eaDni;
     |LEE 000#agifigen.=;
     |SI FSalida ! 0;  |DDEFECTO #agifigen;  #agifigen#0 = 0;  |FINSI;
     |CIERRA #agifigen;
     |SELECT #1#agifigen;

     enIdeCli = #agifigen#0;
     |HAZ LeeCliente;

     #trabajo@#1 = enIdeCli;
     #trabajo@#6 = eaNomCli;
     #trabajo@#9 = eaNifCli;
|FPRC;

