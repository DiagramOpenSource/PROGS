         #trabajo@#9 = "";

         |SI #trabajo@#11 = " ";
             #trabajo@#4 = #dsarw105#0;
             #trabajo@#2 = #dsarw105#2;
             #trabajo@#3 = #dsarw105#1;

             |HAZ IndexaFichero;

             #trabajo@#11 = "*";
         |SINO;
             #trabajo@#11 = " ";
         |FINSI;

         |GRABA 020#trabajo@.a;
         |LIBERA #trabajo@;

         |PINTA #trabajo@#1;
         |PINTA #trabajo@#2;
         |PINTA #trabajo@#3;
         |PINTA #trabajo@#4;
         |PINTA #trabajo@#6;
         |PINTA #trabajo@#7;
         |PINTA #trabajo@#8;
         |PINTA #trabajo@#9;
         |PINTA #trabajo@#10;
         |PINTA #trabajo@#11;

