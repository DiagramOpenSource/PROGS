|FICHEROS;
|FIN;

|VARIABLES;
|FIN;

|INCLUYE z1000var;

|PROCESO Es340;
     enModelo  = 340;
     enChequeo = 1;

     |SI nTipoModelo = 1;
         enLinea = 36;  |HAZ LeeContenido;  eaEjercicio = (eaContenido % 105) - ".";
         enLinea = 38;  |HAZ LeeContenido;
         |HAZ CargaPeriodo;

         |SI eaPeriodo ! "";
             enLinea = 32;  |HAZ LeeContenido;  eaDni   = eaContenido % 209;
             enLinea = 12;  |HAZ LeeContenido;  eaElect = eaContenido % -116;
             enLinea = 25;  |HAZ LeeContenido;  eaJusti = eaContenido % -113;
         |FINSI;
     |FINSI;

     |SI nTipoModelo = 2;
         enLinea = 33;  |HAZ LeeContenido;  eaEjercicio = (eaContenido % -1504);
                                            eaDni       = eaContenido % 209;
         |HAZ CargaPeriodo;

         |SI eaPeriodo ! "";
             enLinea = 36;  |HAZ LeeContenido;  eaElect = eaContenido % -116;
             enLinea = 48;  |HAZ LeeContenido;  eaJusti = eaContenido % -113;
         |FINSI;
     |FINSI;

     |SI nTipoModelo = 3;
         enLinea = 63;  |HAZ LeeContenido;  eaEjercicio = (eaContenido % -1504);
                                            eaDni       = eaContenido % 209;
         |HAZ CargaPeriodo;

         |SI eaPeriodo ! "";
             enLinea = 8;   |HAZ LeeContenido;  eaElect = eaContenido % -116;
             enLinea = 20;  |HAZ LeeContenido;  eaJusti = eaContenido % -113;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO MiraSiEs340;
     enLinea = 3;  |HAZ LeeContenido;
     eaContenido = eaContenido - "MODELO340";
     |SI FSalida ! 0;
         nTipoModelo = 1; |HAZ Es340;
         |SI eaPeriodo ! "";  |ACABA;  |FINSI;

         nTipoModelo = 2; |HAZ Es340;
         |SI eaPeriodo ! "";  |ACABA;  |FINSI;

         nTipoModelo = 3; |HAZ Es340;
         |SI eaPeriodo ! "";  |ACABA;  |FINSI;
     |FINSI;
|FPRC;
