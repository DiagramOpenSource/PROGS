         #dsary003#7  = enGrupoTres;
         #dsary003#8  = enSubGTres;
         #dsary003#9  = enSubGTres;
         #dsary003#10 = enTipoTres;
         #dsary003#11 = enTipoTres;

         |PINDA #0#dsary003;

         |POSICIONATE #dsary003#6;  |PTEC 802;
     |FINSI;
|FPRC;

