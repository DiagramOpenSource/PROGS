

/*
|| YA NO SE UTILIZA, ESTA INTEGRADO EN QuitaRarosXML

|| dsarplb0

|VARIABLES;
     aLon = "";
     aDato = "";
     i = 0;
     nPos = 0;
     aCar = "":
     aCarAnt = "";
     nValor = 0;
     nSwYaUTF = 0;
     &eaTextoConvertir = "";
|FIN;

|PROCESO Ansi2Utf8;
     |QBF eaTextoConvertir;
     aLon = eaTextoConvertir % A0;
     aDato = "";
     |PARA i = 0; |SI i [ aLon; |HACIENDO i = i + 1;
          nPos = (i + 1) * 100 + 1;
          aCar = eaTextoConvertir % nPos;

          nValor = &aCar;
          nSwYaUTF = 0;

          |SI aCar = "Ñ"; aCar = &195 + &145; nSwYaUTF = 1; |FINSI; || en algunos defectos la ¥ ya esta cambiada
          |SI aCar = "¥"; aCar = &195 + &145; nSwYaUTF = 1; |FINSI;

          |SI aCar = "ñ"; aCar = &195 + &177; nSwYaUTF = 1; |FINSI;
          |SI aCar = "¤"; aCar = &195 + &177; nSwYaUTF = 1; |FINSI;

          |SI aCar = "á"; aCar = &195 + &161; nSwYaUTF = 1; |FINSI;
          |SI aCar = " "; aCar = &195 + &161; nSwYaUTF = 1; |FINSI;

          |SI aCar = "é"; aCar = &195 + &169; nSwYaUTF = 1; |FINSI;
          |SI aCar = "‚"; aCar = &195 + &169; nSwYaUTF = 1; |FINSI;

          |SI aCar = "í"; aCar = &195 + &173; nSwYaUTF = 1; |FINSI;
          |SI aCar = "¡"; aCar = &195 + &173; nSwYaUTF = 1; |FINSI;

          |SI aCar = "ó"; aCar = &195 + &179; nSwYaUTF = 1; |FINSI;
          |SI aCar = "¢"; aCar = &195 + &179; nSwYaUTF = 1; |FINSI;

          |SI aCar = "ú"; aCar = &195 + &186; nSwYaUTF = 1; |FINSI;
          |SI aCar = "£"; aCar = &195 + &186; nSwYaUTF = 1; |FINSI;

          |SI aCar = "º"; aCar = &194 + &186; nSwYaUTF = 1; |FINSI;
          |SI aCar = "§"; aCar = &194 + &186; nSwYaUTF = 1; |FINSI;

          |SI aCar = "ª"; aCar = &194 + &170; nSwYaUTF = 1; |FINSI;
          |SI aCar = "¦"; aCar = &194 + &170; nSwYaUTF = 1; |FINSI;

          |SI aCar = "°"; aCar = &226 + &130 + &172; nSwYaUTF = 1; |FINSI;  ||Euro

          |SI nValor > 127; |Y nSwYaUTF = 0;
               |SI nValor = "135"; aCar = &195 + &167; |FINSI;    || c cedilla
               |SI nValor = "128"; aCar = &195 + &135; |FINSI;    || C cedilla

               |SI nValor = "133"; aCar = &195 + &160; |FINSI;    || a acento cerrado
               |SI nValor = "138"; aCar = &195 + &168; |FINSI;    || e acento cerrado
               |SI nValor = "141"; aCar = &195 + &172; |FINSI;    || i acento cerrado
               |SI nValor = "149"; aCar = &195 + &178; |FINSI;    || o acento cerrado
               |SI nValor = "151"; aCar = &195 + &185; |FINSI;    || u acento cerrado

               |SI nValor = "129"; aCar = &195 + &175; |FINSI;    || i con dieresis
               |SI nValor = "139"; aCar = &195 + &188; |FINSI;    || u con dieresis

               |SI nValor = "207"; aCar = &195 + &143; |FINSI;    || I con dieresis
               |SI nValor = "154"; aCar = &195 + &156; |FINSI;    || U con dieresis

               |SI nValor = "165"; aCar = &195 + &145; |FINSI;    || ENYE

               |SI nValor = "160"; aCar = &195 + &161; |FINSI;    || a acento
               |SI nValor = "130"; aCar = &195 + &169; |FINSI;    || e acento

               ||CCC. SOLO Sirve para debugar.
               ||DEJAR COMENTADO
               ||aCar = aCar + "(" + nValor + ")";
          |FINSI;

          |SI aCarAnt ! " "; |O aCar ! " ";
               aDato = aDato + aCar;
          |FINSI;
          aCarAnt = "";
     |SIGUE;
     eaTextoConvertir = aDato;
|FPRC;
*/
