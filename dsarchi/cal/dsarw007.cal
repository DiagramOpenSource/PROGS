|FICHEROS;
     dsarw007 #907;
|FIN;

|VARIABLES;
     aQueBoton      = "";
     aID            = "";
     nBotonAceptar  = 0;
     nBotonCancelar = 0;

     &enSeguir     = 0;
     &enError      = 0;
|FIN;

|PROCESO Tipo12Fw007;  |TIPO 12;
     |SI FSalida = 7;
         enError = 1;
         |ACABA;
     |FINSI;

     |SI enSeguir = 1;
         enError = 0;
         |ACABA;
     |FINSI;

     |ERROR;
|FPRC;

|PROCESO Tipo7C0Fw007;  |TIPO 7;
     aQueBoton     = "|X000";
     aID           = #907#aQueBoton#;
     nBotonAceptar = aID;
     |ESTADO_CONTROL nBotonAceptar, "HIDE";

||     aQueBoton      = "|X001";
||     aID            = #907#aQueBoton#;
||     nBotonCancelar = aID;
||     |ESTADO_CONTROL nBotonCancelar, "HIDE";

     |HAZ Tipo0C4Fw007;
|FPRC;

|PROCESO Tipo0C0Fw007;  |TIPO 0;
/*
     |SI #907#0 = "S";
         |C_M #907#1, 1, "N";  #907#1 = "T";  |PINTA #907#1;
         |C_M #907#2, 1, "N";  #907#2 = 0;    |PINTA #907#2;
     |SINO;
         |C_M #907#1, 1, "S";  |PINTA #907#1;
         |C_M #907#2, 1, "S";  |PINTA #907#2;

         |SI #907#1 = " ";  #907#1 = "T";   |PINTA #907#1;  |FINSI;
         |SI #907#2 = 0;    #907#2 = 1;     |PINTA #907#2;  |FINSI;
     |FINSI;
*/
|FPRC;

|PROCESO Tipo0C3Fw007;  |TIPO 0;
     |SI #907#3 = "S";
          |C_M #907#2, 1, "S";     |PINTA #907#2;
          #907#1 = "P";
     |SINO;
          #907#2 = 1;
          |C_M #907#2, 1, "N";     |PINTA #907#2;
          #907#1 = "T";
     |FINSI;
|FPRC;

|PROCESO Tipo0C4Fw007; |TIPO 0;
     |SI #907#4 = "J";        ||Implica 1 registro por pagina
          #907#5 = "S";
          |C_M #907#5, 1, "N";
          |PINTA #907#5;
     |SINO;
          |SI Contenido = "J";
               #907#5 = "N";
          |FINSI;
          |C_M #907#5, 1, "S";
          |PINTA #907#5;
     |FINSI;
|FPRC;
