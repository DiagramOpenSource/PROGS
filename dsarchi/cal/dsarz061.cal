|PROCESO Tipo0C0w99; |TIPO 0;
     |SI nModo = 1;
          #dsarm069#0 = #dsarw099#0;
          |LEE 000#dsarm069.=;
          |SI FSalida = 0;
               aMensaje = "0000Ya existe una seleccion con este numero";
               |MENAV aMensaje;
               |ERROR;
          |SINO;
               ||Una vez introducido el numero NO dejara modificarlo.
               |C_M #dsarw099#0, 1, "N";
               |PINTA #dsarw099#0;

               |ESTADO_CONTROL nBotonAcota, "ENABLE";
          |FINSI;
     |FINSI;
