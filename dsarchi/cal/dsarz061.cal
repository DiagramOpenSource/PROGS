|PROCESO ModiAlta;
     nIdSalir = 0;
     |HAZ ObtenIdSalir;
     |ESTADO_CONTROL nIdSalir, "HIDE";

     ||VENTANA 0501, 2999, -1, -1, "";
     |VENTANA 0501, 2999, -1, 17, "";
     nVentana = FSalida;

     aCadena ="|NC";
     aAlfa = #69#aCadena#;
     nCampoHasta = aAlfa;
     nCampoHasta = nCampoHasta - 1;

     |SI nModo ! 1;
          |PARA nCampoD = 0;  |SI nCampoD [ nCampoHasta; |HACIENDO nCampoD = nCampoD + 1;
                nCampoH = nCampoD;
                #99nCampoH = #69nCampoD;
          |SIGUE;

          |C_M #99#0, 1, "N";
     |SINO;
          |C_M #99#0, 1, "S";
          nUltimo = 0;
          |HAZ DameUltimo;
          |DDEFECTO #99;
          nUltimo = nUltimo + 1;
          #99#0 = nUltimo;
     |FINSI;

     |PINPA #0#99; nPantaDos = FSalida;
     |PINDA #0#99;

     |CONTROL_SIMPLE nPantaDos, "BOTON,Acotar por Grupo/Sub./Tipo", 1172, 1288, BotonAcota;
     nBotonAcota = FSalida;

     |SI nModo = 1;
          |ESTADO_CONTROL nBotonAcota, "DISABLE";
     |SINO;
          |SI nModo = 2;
               |ESTADO_CONTROL nBotonAcota, "ENABLE";
          |FINSI;
     |FINSI;

     |SI nModo = 1;  |ENDATOS #1#99;  |FINSI;
     |SI nModo = 2;  |ENDATOS #2#99;  |FINSI;

     |SI FSalida = 0;
          |SI aSwGuardarSel = "S";
              |HAZ GrabaDatos;
          |SINO;
               |SI nModo = 1;
                    nBorraSel = #dsarw099#0;
                    |BUCLE dsarm070_borra;
               |FINSI;
          |FINSI;
     |SINO;
          |SI nModo = 1;
               nBorraSel = #dsarw099#0;
               |BUCLE dsarm070_borra;
          |FINSI;
     |FINSI;

     |FINVENTANA nVentana;

     |ESTADO_CONTROL nIdSalir, "SHOW";
|FPRC;

|PROCESO DameUltimo;
     |LEE 000#dsarm069.u;
     |SI FSalida = 0;
          nUltimo = #dsarm069#0;
     |SINO;
          nUltimo = 0;
     |FINSI;
|FPRC;

|PROCESO GrabaDatos;
     #69#0 = #99#0;
     |LEE 101#69=;
     |SI FSalida ! 0;
         |DDEFECTO #69;
         #69#0 = #99#0;
         |GRABA 020#69b;
     |FINSI;

     |PARA nCampoD = 1;  |SI nCampoD [ nCampoHasta;  |HACIENDO nCampoD = nCampoD + 1;
           nCampoH   = nCampoD;
           #69nCampoD = #99nCampoH;
     |SIGUE;

     |GRABA 020#69a;
     |LIBERA #69;
|FPRC;

|PROCESO Tipo80C0; |TIPO 80;
     FSalida = 2999;
|FPRC;

|PROCESO BajaPanta;
     #dsarm069#0 = 0001;
|FPRC;

|PROCESO AltaPanta;
     #dsarm069#0 = 9999;
|FPRC;

|PROCESO EventoPanta;
     |SI FSalida = 1;  |O FSalida = 2;
          |LEE 000#dsarm069.=;
          |SI FSalida = 0;
               |ESTADO_CONTROL nBoton2, "ENABLE";
               |ESTADO_CONTROL nBoton3, "ENABLE";
          |SINO;
               |ESTADO_CONTROL nBoton2, "DISABLE";
               |ESTADO_CONTROL nBoton3, "DISABLE";
          |FINSI;
     |FINSI;

     |SI FSalida = 4;
          |HAZ Boton2;
     |FINSI;
|FPRC;

|PROCESO Boton1;
     nModo = 1;
     |HAZ ModiAlta;
     |REFRESCACONTROL nGrid;
|FPRC;

|PROCESO Boton2;
     |LEE 000#dsarm069.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     nModo = 2;
     |HAZ ModiAlta;
     |REFRESCACONTROL nGrid;
|FPRC;


|PROCESO Boton3;
     |LEE 000#dsarm069.=;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |LEE 101#dsarm069.=;
     |SI FSalida = 0;
          aMensaje = "0000NDesea Borrar la seleccion: " + #69#1;
          |QBF aMensaje;
          aMensaje = aMensaje + "?";
          |CONFI aMensaje;
          |SI FSalida = 0;
               nBorraSel = #dsarm069#0;
               |BUCLE dsarm070_borra;

               |BORRA 020#dsarm069.a;
          |FINSI;
          |LIBERA #dsarm069;
     |FINSI;

     |REFRESCACONTROL nGrid;
|FPRC;

|PROGRAMA;
     |CLS;
     |CABEZA "Asistente Seleccion Envio Correo";

     |ABRE #dsarm001;
     |DDEFECTO #dsarm001;
     |LEE 000#dsarm001.=;
     |CIERRA #dsarm001;

     |ABRE #dsarm069;

     |PINPA #0#dsarz061;
     nPanta = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{193}}Alta ", 2753, 2763, Boton1;
     nBoton1 = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{197}}Modificar ", 2765, 2775, Boton2;
     nBoton2 = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{199}}Baja ", 2777, 2787, Boton3;
     nBoton3 = FSalida;

     nRango = 4 + 8 + 16 + 32;
     nPos1  = 0702;
     nPos2  = 2698;

     |LINEAL_SIMPLE #1#dsarm069, nRango, nPos1, nPos2, BajaPanta, AltaPanta, EventoPanta;
     nGrid = FSalida;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
         FSalida = "::{{001}}Salir," + nGrid;
         |LEETECLA aTecla;
         |SI aTecla = aEsc1; |SAL;  |FINSI;
         |SI aTecla = aEsc2; |SAL;  |FINSI;
         |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;

     |FIN_CONTROL_WINDOWS nGrid;

     |DESTRUYECONTROL nBoton1;
     |DESTRUYECONTROL nBoton2;
     |DESTRUYECONTROL nBoton3;

     |CIERRA #dsarm069;

     |SI nRegSeleccion = 0;
          enSwHaySelAva = 0;
          eaAsuntoSel = "";
          enCodSelAva = 0;
     |SINO;
          enSwHaySelAva = 1;
     |FINSI;
|FPRO;
