|PROCESO BotonAcota;
     nNumSel = #dsarw099#0;

     |VENTANA 0501, 2999, -1, 17, "VERDE - ACTIVADO.  ROJO - DESACTIVADO";
     nVent70 = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVent70;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PTEC 802;  |PAUSA;

     |HAZ GrabaAcotaciones;

     |ABRET #70;
     |CONTROL_SIMPLE 0, "BOTON,{{235}}Activar todo", 0502, 0620, Todo;
     |CONTROL_SIMPLE 0, "BOTON,{{234}}Desactivar todo", 0522, 0640, Ninguno;
     |CONTROL_SIMPLE 0, "LABEL,{{1}}Doble click en la l¡nea para seleccionar o quitar el registro seleccionado", 0542, 0698, NULL;

     nRango  = 2 + 4 + 8 + 16 + 32 + 128;
     |LINEAL_SIMPLE #1#70, nRango, 0701, 2799, Min70, Max70, Evento70;
     nGrid70 = FSalida;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
         FSalida = "::{{001}}Salir," + nGrid70;
         |LEETECLA aTecla;
         |SI aTecla = aEsc1; |SAL;  |FINSI;
         |SI aTecla = aEsc2; |SAL;  |FINSI;
         |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVent70;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |FINVENTANA nVent70;

     |CIERRAT #70;
|FPRC;

|PROCESO dsarm070_borra;
     |BORRA 020#dsarm070.a;
