|PROCESO dsarm005;
     ||Quito las acotaciones.
     ||ARA39

     ||Cliente
     |SI #dsarm005#8 < #dsarw099#2; |O #dsarm005#8 > #dsarw099#3;
          |ACABA;
     |FINSI;

     ||Nif
     |SI #dsarw099#31 = "S";
          aNif = #dsarm005#10;
          |QBF aNif;
          |SI aNif = ""; |ACABA; |FINSI;

          aNifSel1 = #dsarw099#21; |QBF aNifSel1;
          aNifSel2 = #dsarw099#22; |QBF aNifSel2;
          aNifSel3 = #dsarw099#23; |QBF aNifSel3;
          aNifSel4 = #dsarw099#24; |QBF aNifSel4;
          aNifSel5 = #dsarw099#25; |QBF aNifSel5;
          aNifSel6 = #dsarw099#26; |QBF aNifSel6;
          aNifSel7 = #dsarw099#27; |QBF aNifSel7;
          aNifSel8 = #dsarw099#28; |QBF aNifSel8;
          aNifSel9 = #dsarw099#29; |QBF aNifSel9;
          aNifSel10 = #dsarw099#30; |QBF aNifSel10;

          |SI aNif ! aNifSel1; |Y aNif ! aNifSel2; |Y aNif ! aNifSel3;
           |Y aNif ! aNifSel4; |Y aNif ! aNifSel5; |Y aNif ! aNifSel6;
           |Y aNif ! aNifSel7; |Y aNif ! aNifSel7; |Y aNif ! aNifSel9;
           |Y aNif ! aNifSel10;
               |ACABA;
          |FINSI;
     |FINSI;

     ||Grupo/Sub./Tipo
     aCodigo = ((("000" + #dsarm005#1) % -103) + (("000" + #dsarm005#2) % -103) + (("000" + #dsarm005#3) % -103));

     |DDEFECTO #dsarm070;
     #dsarm070#0 = #dsarw099#0;
     #dsarm070#1 = aCodigo;
     |LEE 000#dsarm070.=;
     |SI FSalida = 0;
          |SI #dsarm070#3 = "N";
               |ACABA;
          |FINSI;
     |FINSI;

     ||Trabajador
     |SI #dsarm005#15 < #dsarw099#4; |O #dsarm005#15 > #dsarw099#5;
          |ACABA;
     |FINSI;

     ||Modelo
     |SI #dsarm005#19 < #dsarw099#6; |O #dsarm005#19 > #dsarw099#7;
          |ACABA;
     |FINSI;

     ||Ejercicio
     |SI #dsarm005#17 < #dsarw099#8; |O #dsarm005#17 > #dsarw099#9;
          |ACABA;
     |FINSI;

     ||Periodo
     |SI #dsarm005#18 < #dsarw099#10; |O #dsarm005#18 > #dsarw099#11;
          |ACABA;
     |FINSI;

     ||Complementaria
     |SI #dsarm005#20 < #dsarw099#12; |O #dsarm005#20 > #dsarw099#13;
          |ACABA;
     |FINSI;

     ||Serie Minuta
     |SI #dsarm005#39 < #dsarw099#14; |O #dsarm005#39 > #dsarw099#15;
          |ACABA;
     |FINSI;

     ||Numero Minuta
     |SI #dsarm005#40 < #dsarw099#16; |O #dsarm005#40 > #dsarw099#17;
          |ACABA;
     |FINSI;

     |SALVA_DATOS #dsarm005;
     |REPON_DATOS #referen@;
     |GRABA 020#referen@.n;

     nRegSeleccion = nRegSeleccion + 1;
