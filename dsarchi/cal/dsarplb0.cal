               |SI nCaracter > 127; |O nCaracter = 61; |O nCaracter = 63; |O nCaracter = 32;
                    ||Escapamos el caracter
                    |SI nCaracter = 61; |O nCaracter = 63;
                         |SI nCaracter = 61;
                              aCadena = aCadena + "=3D";
