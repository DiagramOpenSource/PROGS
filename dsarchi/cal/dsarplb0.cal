|PROCESO BorraRegistro;
     |ABRE #1;
     |LEE 000#1p;
     |SI FSalida ! 0;  |DDEFECTO #1;  |FINSI;
     |CIERRA #1;

     |SI eaTipoBorra = "*";
          nTipoD = 0;
          nTipoH = 999;
     |SINO;
          nTipoD = eaTipoBorra;
          nTipoH = eaTipoBorra;
     |FINSI;

     |SI eaSubBorra = "*";
          nSubD = 0;
          nSubH = 999;
     |SINO;
          nSubD = eaSubBorra;
          nSubH = eaSubBorra;
     |FINSI;

     nGrupo = eaGrupoBorra;

