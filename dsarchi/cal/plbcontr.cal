|PROCESO Historico;
     ||Cambia la accion.  (aAccion)

     |HORA aHora;
     aUsuario = Usuario % 810;

     ||Historico
     |ABRE #flowm006;
     #flowm006#0  = #dsarm005#0;
     #flowm006#1  = 99999;
     |LEE 000#flowm006.];
     |SI FSalida = 0;
          |LEE 000#flowm006.a;
     |SINO;
          |LEE 000#flowm006.u;
     |FINSI;
     |SI FSalida = 0; |Y #flowm006#0  = #dsarm005#0;
          nLinea = #flowm006#1 + 1;
          nSitu = #flowm006#2;
     |SINO;
          nSitu = 0;
          nLinea = 1;
     |FINSI;

     |SI aAccion = "BAJA";  |O aAccion = "ARCHIVAR EN EL HISTORICO";
         nSitu = 0;
     |FINSI;

     |SI aAccion = "ENTRADA WORKFLOW";
         nSitu = 1;
     |FINSI;

     |DDEFECTO #flowm006;
     #flowm006#0  = #dsarm005#0;
     #flowm006#1  = nLinea;
     |LEE 101#flowm006.=;
     |SI FSalida ! 0; |GRABA 020#flowm006.b; |FINSI;
     #flowm006#2  = nSitu;
     #flowm006#3  = ~;
     #flowm006#4  = aHora;
     #flowm006#5  = aUsuario;
     #flowm006#6  = aAccion;

     enNumRegFlow = #flowm006#0;
