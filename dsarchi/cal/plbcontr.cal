     |SI FSalida < 0;
          nSwErrorSQL = 1;
          aMenLogSQL = "No puedo abrir " + aFicheroErr;
          |HAZ MeteLogSQL;
     |SINO;
          |PARA; |SI; |HACIENDO;
               |XLEE nHandleErr, 250, aLinea;
               |SI FSalida < 0;
                    |SAL;
               |SINO;
                    |SI FSalida ! 0;
                         |QBF aLinea;
                         |SI aLinea = "OK";
                              nSwErrorSQL = 0;
                              aMenLogSQL = " Ejecucion SQL [" + aNomDes + "]  realizada CORRECTAMENTE";
                              |HAZ MeteLogSQL;
                         |SINO;
                              nSwErrorSQL = 1;
                              aMenLogSQL = " " + aLinea;
                              |HAZ MeteLogSQL;
                         |FINSI;
                    |FINSI;
               |FINSI;
          |SIGUE;
          |XCIERRA nHandleErr;
     |FINSI;
|FPRC;

