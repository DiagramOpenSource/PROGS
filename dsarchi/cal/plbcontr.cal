     |NOME_DAT #dsarm055#aFichero#;
     |PATH_DAT #dsarm055#aDirVistas#;

     aFichero = "vb" + (("000000" + #dsarm008#31) % -106);
     |NOME_DAT #dsarm056#aFichero#;
     |PATH_DAT #dsarm056#aDirVistas#;
|FPRC;

|PROCESO dsarm008Plb;
     |SI #dsarm008#31 = 0;
         |LEE 000#defcont@.u;
         |SI FSalida ! 0;  |DDEFECTO #defcont@;  |FINSI;

         #dsarm008#31 = #defcont@#31 + 1;
         |GRABA 020#dsarm008.a;
     |FINSI;

     |HAZ PreparaFicherosVista;

     |ABRE #dsarm055;
     |ABRE #dsarm056;
     |SI nPasada = 0;
         |SELECT #1#dsarm055;
         #dsarm055#0  = #dsarm005#0;
         |LEE 101#dsarm055.=;
         |SI FSalida = 0;
             |BORRA 020#dsarm055.a;
             |LIBERA #dsarm055;

             nIdentifi = #dsarm055#1;

             |SELECT #2#dsarm055;
             #dsarm055#1 = nIdentifi;
             #dsarm055#0 = 1;
             |LEE 000#dsarm055.];
             |SI FSalida ! 0;  |O #dsarm055#1 ! nIdentifi;
                 |SELECT #2#dsarm056;
                 #dsarm056#5 = nIdentifi;
                 |LEE 101#dsarm056.=;
                 |SI FSalida = 0;
                     |BORRA 020#dsarm056.a;
                     |LIBERA #dsarm056;
                 |FINSI;
             |FINSI;
         |FINSI;
     |SINO;
         |HAZ GrabaVistas;
     |FINSI;
     |CIERRA #dsarm055;
     |CIERRA #dsarm056;

     |LIBERA #dsarm008;

     |PULSATECLA;
|FPRC;

|DEFBUCLE dsarm008Plb;
     #dsarm008#1;
     ;
     aDVista;
     aHVista;
     be;
     NULL, dsarm008Plb;
|FIN;

|PROCESO GrabaVistaTiempoReal;
