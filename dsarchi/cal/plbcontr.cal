|PROCESO EjecutaSQL1;
     ||Sirve para obtener el Nombre del Paciente

     |HORA aHoraAux;
     |QBF aHoraAux;
     aSegundos = aHoraAux % -102;

     aNomDes = Usuario % 108;
     aNomDes = aNomDes + "sql" + aSegundos + "1" + ".txt";

     |HAZ EjecutaSQLNucleo;

     aFicRes = aNomDes + ".res";
     aFicErr = aNomDes + ".err";
     aAlfa  = eaUnidad + ":\DOCUMENTOS\DSARCHI\" + aFicRes;
     aAlfa  = eaUnidad + ":\DOCUMENTOS\DSARCHI\" + aFicErr;

     nSal = 0;
     |PARA; |SI; |HACIENDO;
          aFicheroRes = eaUnidad + ":\DOCUMENTOS\DSARCHI\" + aFicRes;
          |XABRE "EC", aFicheroRes, nHandle;
          |SI FSalida ] 0;
               nSwHayRes = 1;
               |SLEEP 5;      ||Le doy tiempo a que se termine de copiar
               nSal = 1;
          |FINSI;

          aFicheroErr = eaUnidad + ":\DOCUMENTOS\DSARCHI\" + aFicErr;
          |XABRE "EC", aFicheroErr, nHandle;
          |SI FSalida ] 0;
               nSwHayErr = 1;
               |SLEEP 1;      ||Le doy tiempo a que se termine de copiar
               nSal = 1;
          |FINSI;

          |SI nSal = 1;
               |SAL;
          |FINSI;

          |PARA nPausa = 1; |SI nPausa < 10000; |HACIENDO nPausa = nPausa + 1;
          |SIGUE;
          |PULSATECLA;
     |SIGUE;
     |PULSATECLA;

     nSwErrorSQL = 0;
     |SI nSwHayErr = 1;
          |HAZ ProcesaErrSQL;
     |FINSI;

     |SI nSwHayRes = 1; |Y nSwErrorSQL = 0;
          |HAZ ProcesaRes1;
     |FINSI;

     aMenLogSQL = "Final Ejecucion [" + aNomDes + "]";
     |HAZ MeteLogSQL;
     ||Tiene que devolver eaNomPaciente.
|FPRC;

|PROCESO ProcesaRes1;
     ||ARA39
     |XABRE "CU", aFicheroRes, nHandleRes;
     |SI FSalida < 0;
          aMenLogSQL = "No puedo abrir " + aFicheroRes;
          |HAZ MeteLogSQL;
     |SINO;
          |XLEE nHandleRes, 250, aLinea;
          |SI FSalida < 0;
               aMenLogSQL = "No puedo leer " + aFicheroRes;
               |HAZ MeteLogSQL;
          |SINO;
               |QBF aLinea;
               |SI aLinea ! "";
                    ||Obtener Nombre y Apellidos.

                    aLong = aLinea % A0;
                    nLong = aLong;

                    ||ACERA VARIABLES.
                    aCodHis = "";
                    aApel1 = "";
                    aApel2 = "";
                    aNombre = "";
                    aSexo = "";

                    nContador = 0;
                    aTextAux = "";
                    |PARA nVoy = 1; |SI nVoy [ nLong; |HACIENDO nVoy = nVoy + 1;
                         aSaca = nVoy + "01";
                         aLetra = aLinea % aSaca;
                         |SI aLetra = aTab; |O aLetra = aReturnUnix;
                              nContador = nContador + 1;

                              |HAZ PonResSQL1;

                              aTextAux = "";

                              |SI nContador ] 5; |SAL; |FINSI;
                         |SINO;
                              aTextAux = aTextAux + aLetra;
                         |FINSI;
                    |SIGUE;

                    ||eaNomPaciente
                    eaNomPaciente = aApel1;
                    |QBF eaNomPaciente;
                    eaNomPaciente = eaNomPaciente + " " + aApel2;
                    |QBF eaNomPaciente;
                    eaNomPaciente = eaNomPaciente + " " + aNombre;
                    |QBF eaNomPaciente;
                    aLong = eaNomPaciente % A0;
                    nLong1 = aLong;

                    aHistoria = aCodHis;
                    |QBF aHistoria;
                    aLong = aHistoria % A0;
                    nLong2 = aLong;

                    nLongTotal = nLong1 + nLong2 + 1;
                    |SI nLongTotal [ 50;
                         eaNomPaciente = eaNomPaciente + " " + aHistoria;
                         |QBF eaNomPaciente;
                    |SINO;
                         nDif = 50 - nLong2;
                         nSaca = 100 + nDif;
                         aSaca = eaNomPaciente % aSaca;
                         eaNomPaciente = aSaca;
                         eaNomPaciente = eaNomPaciente + aHistoria;
                         |QBF eaNomPaciente;
                    |FINSI;
               |FINSI;
               |XCIERRA nHandleRes;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO PonResSQL1;
     |QBF aTextAux;
     |SI nContador = 1;
          aCodHis = aTextAux;
     |FINSI;
     |SI nContador = 2;
          aApel1 = aTextAux;
     |FINSI;
     |SI nContador = 3;
          aApel2 = aTextAux;
     |FINSI;
     |SI nContador = 4;
          aNombre = aTextAux;
     |FINSI;
     |SI nContador = 5;
          aSexo = aTextAux;
     |FINSI;
|FPRC;

|PROCESO ProcesaErrSQL;
