|FICHEROS;
     dsarm005;
     dsarm046;

     dsarm005@;
|FIN;

|VARIABLES;
     aMas = "";
     &eaFichero = "";
     nContador = 0;

     &eaTipoVincu = "";
     &eaValorVincu1 = "";
     &eaValorVincu2 = "";
|FIN;

|PROCESO dsarm046;
     #dsarm005#0 = #dsarm046#0;
     |LEE 000#dsarm005.=;
     |SI FSalida = 0;
          |DDEFECTO #dsarm005@;
          #dsarm005@#0 = #dsarm005#0;
          |LEE 101#dsarm005@.=;
          |SI FSalida ! 0;
               |PARA nContador = 1; |SI nContador [ 98; |HACIENDO nContador = nContador + 1;
                    #dsarm005@#nContador# = #dsarm005#nContador#;
               |SIGUE;
               |GRABA 020#dsarm005@.n;
          |FINSI;
          |LIBERA #dsarm005@;
     |FINSI;
|FPRC;

|DEFBUCLE dsarm046;
 #dsarm046#2;
 ;
 eaTipoVincu, eaValorVincu1, eaValorVincu2;
 eaTipoVincu, eaValorVincu1, eaValorVincu2;
 ;
 NULL, dsarm046;
|FIN;


|PROCESO CreaTempoVinculacion;
     |DBASE aMas;  |QBF aMas;
     aMas = aMas + "def/dsarm005.mas";
     |CARGA_DEF aMas, dsarm005@;

     |SI FSalida ! 0;
          |MENAV "0000[dsarm005.mas] inaccesible. Consulte con su distribuidor.";
          |ACABA;
     |FINSI;

     eaFichero = ("01" + Usuario + "zzzzzzzz") % 108;
     |NOME_DAT #dsarm005@#eaFichero#;

     |ABRE #dsarm005@;
     |CIERRA #dsarm005@;

     |DELINDEX #dsarm005@;

     |ABRE #dsarm005;
     |ABRE #dsarm005@;
     |BUCLE dsarm046;
     |CIERRA #dsarm005@;
     |CIERRA #dsarm005;

     |DESCARGA_DEF #dsarm005@;
|FPRC;
