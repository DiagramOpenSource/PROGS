|FICHEROS;
     dsarw011 #911;
|FIN;

|VARIABLES;
     aIni             = "";
     aAlfa            = "";

     nIni             = 0;

     {-1}aSerie       = "";

     &eaClaveCorreo   = "";
     &eaClaveEntrante = "";
|FIN;

|PROCESO Tipo12Fw11; |TIPO 12;
|FPRC;

|PROCESO AveriguaINI;
     aSerie = "";
     |ESPECIFICA "SERIALIZACION", aSerie;
     aIni = FSalida;          ||Antes esto lo devolvia en la variable aSerie
     aIni = aIni % 106;       ||Esto es el numero de ORO. Los 6 primeros digitos
     nIni = aIni;
|FPRC;

|PROCESO Tipo7C0Fw11;  |TIPO 7;
     aAlfa = #911#0;  |QBF aAlfa;
     |SI aAlfa = "";
         |HAZ AveriguaINI;
         |SI nIni ] 910000;   |Y nIni [ 910999;
             #911#0 = "xdscorreo.dv";        || IP interna dscorreo DV
         |SINO;
             #911#0 = "xdscorreo.dv.diagram.net";      || IP externa dscorreo DV
         |FINSI;

         |PINTA #911#0;
     |FINSI;

     aAlfa = #911#1;  |QBF aAlfa;
     |SI aAlfa = "";
         #911#1 = "asmtp.diagram.es";   |PINTA #911#1;
     |FINSI;
|FPRC;

|PROCESO Tipo0C3_w011; |TIPO 0;
     |SI #dsarw011#3 = "S";
          |C_M #dsarw011#4, 1, "S";
          |C_M #dsarw011#5, 1, "S";
     |SINO;
          |C_M #dsarw011#4, 1, "N";
          |C_M #dsarw011#5, 1, "N";
     |FINSI;
     |PINTA #dsarw011#4;
     |PINTA #dsarw011#5;
|FPRC;

|PROCESO Tipo7C5_w011; |TIPO 7;
     #dsarw011#5 = eaClaveCorreo;
|FPRC;

|PROCESO Tipo0C5_w011; |TIPO 0;
     eaClaveCorreo = #dsarw011#5;
     #dsarw011#5 = "*********";
     |PINTA #dsarw011#5;
|FPRC;

|PROCESO Tipo7C9_w011; |TIPO 7;
     #dsarw011#9 = eaClaveEntrante;
|FPRC;

|PROCESO Tipo0C9_w011; |TIPO 0;
     eaClaveEntrante = #dsarw011#9;
     #dsarw011#9 = "*********";
     |PINTA #dsarw011#9;
|FPRC;

|PROCESO Tipo0C6w011; |TIPO 0;
     |SI #911#6 = "N";
          |C_M #911#2, 1, "N";
          |C_M #911#7, 1, "N";
          |C_M #911#8, 1, "N";
          |C_M #911#9, 1, "N";
          |PINTA #911#2;
          |PINTA #911#7;
          |PINTA #911#8;
          |PINTA #911#9;
     |SINO;
          |C_M #911#2, 1, "S";
          |C_M #911#7, 1, "S";
          |C_M #911#8, 1, "S";
          |C_M #911#9, 1, "S";
          |PINTA #911#2;
          |PINTA #911#7;
          |PINTA #911#8;
          |PINTA #911#9;
     |FINSI;
|FPRC;
