|FICHEROS;
     dsarm066;
     dsarm067;
     dsarw096;
|FIN;

|VARIABLES;
     aUsuario             = "";
     aEsc1                = "";
     aEsc2                = "";
     aRetu                = "";
     aTecla               = "";
     aAlfa                = "";

     nVentana             = 0;
     nVentTrab            = 0;
     nGrid1               = -1;
     nGrid2               = -1;
     nRango               = -1;
     nSalir               = -1;

     {-1}aVent            = "";
         aVent1           = 0;
         aVent2           = 0;
         aVent3           = 0;
         aVent4           = 0;
         aVent5           = "";
|FIN;

|PROCESO Baja2;
     #dsarm067#0 = #dsarm066#0;
     #dsarm067#1 = #dsarm066#1;
     #dsarm067#2 = 1;
|FPRC;

|PROCESO Alta2;
     #dsarm067#0 = #dsarm066#0;
     #dsarm067#1 = #dsarm066#1;
     #dsarm067#2 = 99999;
|FPRC;

|PROCESO Evento1;
     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#dsarm066.=;

         |REFRESCACONTROL nGrid2;
     |FINSI;
|FPRC;

|PROCESO Evento2;
     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#dsarm067.=;
     |FINSI;

     |SI FSalida = 4;
         |VENTANA 0501, 1580, -1, 16, "Incidencias env¡os FacturaE";
         nVentTrab = FSalida;

         aVent1 = -1;
         aVent2 = -1;
         aVent3 = 0;
         aVent4 = nVentTrab;
         aVent5 = "MODAL";

         #dsarw096#0 = #dsarm067#0;
         #dsarw096#1 = #dsarm067#1;
         #dsarw096#2 = #dsarm067#2;
         #dsarw096#3 = #dsarm067#3;
         #dsarw096#4 = #dsarm067#4;
         #dsarw096#5 = #dsarm067#5;
         #dsarw096#6 = #dsarm067#6;

         |PINPA #0#dsarw096;
         |PINDA #0#dsarw096;

         |PAUSA;

         aVent1 = -1;
         aVent2 = -1;
         aVent3 = 0;
         aVent4 = nVentTrab;
         aVent5 = "MODELESS";
         |ESPECIFICA "ESTADO_VENTANA", aVent;

         |FINVENTANA nVentTrab;
     |FINSI;
|FPRC;


|PROGRAMA;
     |VENTANA 0501, 3399, -1, 16, "Incidencias env¡os FacturaE";
     nVentana = FSalida;


     |PTEC 802;  |PAUSA;

     |CONTROL_SIMPLE 0, "LABEL,{{15}}Doble click en la l¡nea para leer la incidencia", 1602, 1660, NULL;

     |ABRE #dsarm066;
     |ABRE #dsarm067;

     nRango  = 2 + 4 + 8 + 16 + 32 + 128;

     |LINEAL_SIMPLE #1#dsarm066, nRango, 0502, 1498, NULL, NULL, Evento1;
     nGrid1   = FSalida;

     |LINEAL_SIMPLE #1#dsarm067, nRango, 1702, 3098, Baja2, Alta2, Evento2;
     nGrid2   = FSalida;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
          FSalida = "::Salir," + nGrid1;
          |LEETECLA aTecla;

          |SI nSalir = 1;  aTecla = "";  |FINSI;

          |SI aTecla = aEsc1; |SAL;  |FINSI;
          |SI aTecla = aEsc2; |SAL;  |FINSI;
          |SI aTecla = aRetu; |SAL;  |FINSI;

          nSalir = 0;
          |PULSATECLA;
     |SIGUE;

     |CIERRA #dsarm066;
     |CIERRA #dsarm067;

     |DESTRUYECONTROL nGrid1;
     |DESTRUYECONTROL nGrid2;


     |FINVENTANA nVentana;
|FPRO;
