|PROCESO FiltroFecha;
     aAlfa = #Fichero@#nCampo#;

     |SI #Fichero@#Campo# = "01.01.0000";  |ACABA;  |FINSI;

     fHoy = ~;

     |SI #Fichero@#Campo# > fHoy;
         aAlfa = "0000SLa fecha " + #Fichero@#Campo# + " puede ser incorrecta si es de nacimiento. Corregir siglo?";
         |CONFI aAlfa;
         |SI FSalida = 0;
             aAlfa1    = (#Fichero@#Campo# % 106) + "19" + (#Fichero@#Campo# % -102);

             #Fichero@#Campo# = aAlfa1;
             |PINTA #Fichero@#Campo#;
         |FINSI;
     |FINSI;
|FPRC;
