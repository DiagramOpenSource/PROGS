|| Recupera campos del log a un labtraba que se la hecho un sustituir a los
|| bestia

|FICHEROS;
     :laboral/labtraba;
|FIN;

|VARIABLES;
     aAlfa        = "";
     aRegistro    = "";

     nHandle      = 0;
     nCampo       = 0;
|FIN;

|PROCESO Labtraba_15;
|FPRC;

|PROCESO Labtraba_16;
|FPRC;

|PROCESO Labtraba_17;
|FPRC;

|PROGRAMA;
     aAlfa = "/tmp/manolo/";
     |PATH_DAT #labtraba#aAlfa#;
     |ABRE #labtraba;

     |CONSULTA_CLAVE #1#labtraba;

     aAlfa = "/tmp/manolo/zmanolo.txt";
     |XABRE "",  aAlfa,  nHandle;

     |PARA;  |SI;  |HACIENDO;
          |XLEE nHandle, 255, aRegistro;
          |SI FSalida [ 0;  |SAL;  |FINSI;

          aAlfa = aRegistro % 17210;  |QBF aAlfa;
          |SI aAlfa ! "";
              aAlfa = aRegistro % 4505;  #labtraba#0 = aAlfa;
              aAlfa = aRegistro % 5105;  #labtraba#1 = aAlfa;
              |LEE 101#labtraba.=;
              |SI FSalida = 0;
                  aAlfa = aRegistro % 4203;  nCampo = aAlfa;

                  aAlfa = aRegistro % 17210;  |QBF aAlfa;
                  #labtraba#nCampo# = aAlfa;

                  |GRABA 020#labtraba.a;
                  |LIBERA #labtraba;
              |FINSI;
          |FINSI;
     |SIGUE;
     |XCIERRA nHandle;

     |CONSULTA_CLAVE #1#labtraba;

     |CIERRA #labtraba;
|FPRO;
