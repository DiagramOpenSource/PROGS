|FICHEROS;
     rem00006 #0;
     reparame #1;
|FIN;

|VARIABLES;
     &aTipoGente = "";
     &empresa   = 0;
     &nombre    = "";
     &modo      = 0;
     &modelito  = 0;
     &indivi    = "";

     Forma      = 0;
|FIN;

|INCLUYE i_husuar;
|INCLUYE i_activo;
|INCLUYE i_letran;

|PROCESO ConsuGente;
     |SI FSalida ! 13;  |ACABA;  |FINSI;

     aTipoGente = #0Campo;
     |SUB_EJECUTA "rez00003";
     #0Campo = aTipoGente;
     |PINTA #0Campo;
|FPRC;

|RUTINA ClaveBaja;
  #0#0 = empresa;
  #0#1 = 1;
|FRUT;

|RUTINA ClaveAlta;
  #0#0 = empresa;
  #0#1 = 99;
|FRUT;

|PROGRAMA;
  |PINPA #0#0;
  |SI modo = 1;  |ENTLINEAL #1#0, 2, 1, 13, 0, ClaveBaja, ClaveAlta;  |FINSI;
  |SI modo = 2;  |ENTLINEAL #1#0, 2, 2, 13, 0, ClaveBaja, ClaveAlta;  |FINSI;
  |SI modo = 4;  |ENTLINEAL #1#0, 2, 4, 13, 0, ClaveBaja, ClaveAlta;  |FINSI;
|FPRO;

|PROCESO Parametro; |TIPO 0;
  modelito = (FEntrada / 100) ! 0;
  |SI modelito ! 1;  |ACABA;  |FINSI;

  |ABRE #1;
  |LEE 040#1=;
  |SI FSalida ! 0;  |CIERRA #1;  |ACABA;  |FINSI;
  |CIERRA #1;

  |SI #1#6 ! "V"; |Y #1#6 ! "G";
      #0#3 = #1#6;  |PINTA #0#3;
  |FINSI;
|FPRC;

|PROCESO Actualiza; |TIPO 2;
  Forma      = 0 +. 1;
  QueTipo    = (FEntrada / 100) ! 0;
  HContribu  = #0#0;
  HNombre    = nombre;
  HPrograma  = "(rem00006) DERECHOS DE IMAGEN";
  HLinea     = #0#1;
  HPerceptor = #0#4;
  HRdto      = #0#7;

  |SI QueTipo = 1; HTipo = "ALTA FICHA";         |FINSI;
  |SI QueTipo = 2; HTipo = "MODIFICACION FICHA"; |FINSI;
  |SI QueTipo = 3; HTipo = "BAJA FICHA";         |FINSI;

  |SI QueTipo = 3; |Y Forma ! 1; |HAZ AltaHistorico;  |FINSI;
  |SI QueTipo < 3; |Y Forma = 1; |HAZ AltaHistorico;  |FINSI;
|FPRC;

|PROCESO TipoIndi; |TIPO 7;
  |SI indivi = "S";
      #0Campo = "T"; |PINTA #0Campo;
  |FINSI;
|FPRC;

|PROCESO Perceptor; |TIPO 0;
  |SI indivi = "S"; |Y #0#3 ! "T";
      |MENAV "      La renta es individual";
      |ERROR; |ACABA;
  |FINSI;

  npro = 60; |HAZ sigrabo;
  |SI #0#3 = "T";  #0#4 = "TITULAR"; |FINSI;
  |SI #0#3 = "C";  #0#4 = "CONYUGE"; |FINSI;
  |SI #0#3 = "1";  #0#4 = "FAMILIAR"; |FINSI;
  |SI #0#3 = "2";  #0#4 = "FAMILIAR"; |FINSI;
  |SI #0#3 = "3";  #0#4 = "FAMILIAR"; |FINSI;
  |SI #0#3 = "4";  #0#4 = "FAMILIAR"; |FINSI;
  |PINTA #0#4;
|FPRC;
