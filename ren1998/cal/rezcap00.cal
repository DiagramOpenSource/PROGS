|FICHEROS;
     rezcap00 #0;
|FIN;

|VARIABLES;
     nTecla  = 0;

     &DEmpre = 0;
     &HEmpre = 0;
|FIN;

|PROCESO Funcion; |TIPO 11;
     nTecla = FSalida;

     |SI nTecla > 10; |ACABA; |FINSI;
     |SI nTecla < 9;  |ACABA; |FINSI;

     |SI #0#2 = "P"; |Y nTecla = 10; |ACABA; |FINSI;

     |HAZ Cuadro1;

     |SI #0#2 = "P"; |Y nTecla = 9; |Y DEmpre = 1; |Y HEmpre = 99999;
         nTecla = 0;
         |MENAV "     El fichero ya ha sido pasado";
         |CONFI "2400NDesea pasarlo de nuevo : ";
         |SI FSalida = 0; nTecla = 9; |FINSI;
     |FINSI;

     |LEE 140#0=;
     |SI nTecla = 9;  #0#2 = "S"; |FINSI;
     |SI nTecla = 10; #0#2 = "N"; |FINSI;
     |GRABA 140#0a;
     |LIBERA #0;

     |PTEC 809;
|FPRC;

|PROCESO Cuadro1; |TIPO 7;
       |SI #0#0 = 9;  |O #0#0 = 10; |O #0#0 = 11; |O #0#0 = 12;
        |O #0#0 = 13; |O #0#0 = 36; |O #0#0 = 37;
           |PUSHV 0501 2380;
           |CUADRO 1229 1671;
           |ATRI R;
           |PINTA 1330, "  Si los ficheros de actividades se van  ";
           |PINTA 1430, "a obtener mediante la Captura de R.E.O.  ";
           |PINTA 1530, "introducir en PASAR S/N una <N>          ";
           |ATRI 0;
           |PAUSA;
           |POPV;
       |FINSI;
|FPRC;
