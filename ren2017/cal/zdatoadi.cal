|PROCESO LeePersona;
     enError = 0
     |DBASS aBase;   |QBT aBase;
     aBase = aBase + "integral/def/dsam0000.mas";
     |XABRE "E", aBase, nCanal;
     |SI FSalida ] 0;
          enSwDeDonde = 1;
          |REFERENCIA_DEF clientes@, #717;

          |ABRE #791;
          #791#0 = enCodCli;
          |LEE 040#791=;
          |SI FSalida ! 0;
              |MENAV "     No existe Cliente en Mantenimiento y Gestion del Cliente";
              enError = 1;
          |SINO;
              |SI #791#68 = "N";
                  |MENAV "     No tiene el Cliente Activado Datos Renta <S>";
                  enError = 1;
              |FINSI;
          |FINSI;
     |SINO;
          enSwDeDonde = 0;
         |REFERENCIA_DEF clientes@, #716;
     |FINSI;

     |ABRE #715;
     #715#0 = enCodCli;
     |LEE 040#715=;
     |SI FSalida ! 0; |DDEFECTO #715; |FINSI;
     |CIERRA #715;
|FPRC;

|PROCESO SacaPantaEpi;  |TIPO 7;
     nRefer    = 0;
     aPrograma = Control;  |QBF aPrograma;

     |SI aPrograma = "rew00625";
         nRefer = 1;
         |REFERENCIA_DEF program@, #965;
         nCampoEpi1 = 18;              || Tipo Epigrafe
         nCampoEpi2 = 19;              || Codigo Epigrafe
         nCampoEpi3 = 22;              || Nombre Epigrafe
         nCampoEpi4 = 17;              || Tipo de Actividad

     |FINSI;

     |SI aPrograma = "rew00626";
         nRefer = 1;
         |REFERENCIA_DEF program@, #966;
         nCampoEpi1 = 18;              || Tipo Epigrafe
         nCampoEpi2 = 19;              || Codigo Epigrafe
         nCampoEpi3 = 22;              || Nombre Epigrafe
         nCampoEpi4 = 17;              || Tipo de Actividad
     |FINSI;

     |SI aPrograma = "rew00627";
         nRefer = 1;
         |REFERENCIA_DEF program@, #967;
         nCampoEpi1 = 16;              || Tipo Epigrafe
         nCampoEpi2 = 17;              || Codigo Epigrafe
         nCampoEpi3 = 20;              || Nombre Epigrafe
         nCampoEpi4 = -1;              || Tipo de Actividad
     |FINSI;

     |SI aPrograma = "rew00628";
         nRefer = 1;
         |REFERENCIA_DEF program@, #968;
         nCampoEpi1 = 18;              || Tipo Epigrafe
         nCampoEpi2 = 19;              || Codigo Epigrafe
         nCampoEpi3 = 21;              || Nombre Epigrafe
         nCampoEpi4 = -1;              || Tipo de Actividad
     |FINSI;

     |SI nRefer = 0;  |ACABA;  |FINSI;

     eaTipoEpi = #0nCampoEpi1;
     |SI #0nCampoEpi1 ! "E";  |Y #0nCampoEpi1 ! "P";
      |Y #0nCampoEpi1 ! "A";  |Y #0nCampoEpi1 ! " ";
         eaTipoEpi = "O";
     |FINSI;

     enAgrupa = 0;
     |SI #0nCampoEpi1 = "E";
         aAlfa    = #0nCampoEpi2 % 101;
         enAgrupa = aAlfa;
         enAgrupa = enAgrupa * 100;
     |FINSI;

     eaEpigrafe   = #0nCampoEpi2;  |QBT eaEpigrafe;
     aAlfa        = eaEpigrafe % 0;
     |SI aAlfa    = "4";
         eaEpigrafe = (eaEpigrafe % 103) + "." + (eaEpigrafe % 401);
     |FINSI;

     |SI eaTipoEpi ! "O";
         |ABRE #602;
         #602#0 = eaTipoEpi;
         #602#1 = enAgrupa;
         #602#2 = eaEpigrafe;
         |LEE 040#602=;
         |SI FSalida ! 0;  nSwPantalla = 1;  |FINSI;
         |CIERRA #602;

         eaNombEpi = #602#3;
     |SINO;
         eaNombEpi  = #0nCampoEpi3;
         eaEpigrafe = "9999";
     |FINSI;

     |SI nSwPantalla = 1;
         aQueBoton = "|X000";
         aID       = #program@#aQueBoton#;
         nBotonMod = aID;
         |ESTADO_CONTROL nBotonMod, "HIDE";

         aQueBoton = "|X001";
         aID       = #program@#aQueBoton#;
         nBotonCan = aID;
         |ESTADO_CONTROL nBotonCan, "HIDE";

         |SUB_EJECUTA ":basica/agiepix1";

         |PTEC 802;  |PAUSA;
         |PULSATECLA;

         |ESTADO_CONTROL nBotonMod, "SHOW";
         |ESTADO_CONTROL nBotonCan, "SHOW";
     |FINSI;

     |SI nCampoEpi1 ! -1;
         #0nCampoEpi1 = eaTipoEpi;

         |PINTA #0nCampoEpi1;
     |FINSI;

     |SI nCampoEpi2 ! -1;
         #0nCampoEpi2 = eaEpigrafe;

         aAlfa      = eaEpigrafe % 0;
         |SI aAlfa  = "5";
             aAlfa        = (eaEpigrafe % 103) + (eaEpigrafe % 501);
             #0nCampoEpi2 = aAlfa;
         |FINSI;
         |PINTA #0nCampoEpi2;
     |FINSI;

     |SI nCampoEpi3 ! -1;
         #0nCampoEpi3 = eaNombEpi;   |PINTA #0nCampoEpi3;
     |FINSI;

     |SI nCampoEpi4 ! -1;
         #0nCampoEpi4 = "1";

         |SI enAgrupa  = 0;  |Y eaTipoEpi = "E";
             #0nCampoEpi4 = "3";
         |FINSI;

         |SI eaTipoEpi = "P";  #0nCampoEpi4 = "5";  |FINSI;
         |SI eaTipoEpi = "A";  #0nCampoEpi4 = "4";  |FINSI;
         |SI eaTipoEpi = "O";  #0nCampoEpi4 = "2";  |FINSI;

         |PINTA #0nCampoEpi4;
     |FINSI;

     nSwPantalla = 0;
|FPRC;

|PROCESO ExisteEpi;
     |ET Arriba;
         nSwPantalla = 0;
         |SI FSalida = 10;  nSwPantalla = 1;  |FINSI;

         |HAZ SacaPantaEpi;

     |SI eaTipoEpi ! "O";
         |ABRE #602;
         #602#0 = eaTipoEpi;
         #602#1 = enAgrupa;
         #602#2 = eaEpigrafe;
         |LEE 040#602=;
         |SI FSalida ! 0;
             |CIERRA #602;
             |VETE Arriba;
         |FINSI;
         |CIERRA #602;

     |FINSI;
|FPRC;

