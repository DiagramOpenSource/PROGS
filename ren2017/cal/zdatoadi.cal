     |SI eaPrograma = "rew00200";  enSw = 1;  |REFERENCIA_DEF program@, #100;   |FINSI;
     |SI eaPrograma = "rew00625";  enSw = 1;  |REFERENCIA_DEF program@, #965;   |FINSI;
     |SI eaPrograma = "rew00626";  enSw = 1;  |REFERENCIA_DEF program@, #966;   |FINSI;
     |SI eaPrograma = "rew00627";  enSw = 1;  |REFERENCIA_DEF program@, #967;   |FINSI;
     |SI eaPrograma = "rew00628";  enSw = 1;  |REFERENCIA_DEF program@, #968;   |FINSI;
     |SI eaPrograma = "rew00201";  enSw = 1;  |REFERENCIA_DEF program@, #901;   |FINSI;
     |SI eaPrograma = "rew00202";  enSw = 1;  |REFERENCIA_DEF program@, #902;   |FINSI;
     |SI eaPrograma = "rew00204";  enSw = 1;  |REFERENCIA_DEF program@, #904;   |FINSI;

     |PULSATECLA;

     |SI enSw = 0;  |ACABA;  |FINSI;

     |DBASS aBase;   |QBT aBase;
     aBase = aBase + "integral/def/dsam0000.mas";
     |XABRE "E", aBase, nCanal;
     |SI FSalida ] 0;
         ||Desactivado

         |MODO_RELACION #0, 1, "repoblac", "N", "N";
         |MODO_RELACION #0, 1, "rehacien", "N", "N";

         |MODO_RELACION #0, 1, "repoblac", "X", "X";
         |MODO_RELACION #0, 1, "rehacien", "X", "X";

         || Activado

         |MODO_RELACION #0, 0, "dsax0101", "X", "X";
         |MODO_RELACION #0, 0, "dsax0102", "X", "X";

         |MODO_RELACION #0, 1, "dsax0101", "S", "S";
         |MODO_RELACION #0, 1, "dsax0102", "S", "S";

         |REFERENCIA_DEF poblaci@,  #705;
         |REFERENCIA_DEF haciend@,  #708;
     |SINO;
         |MODO_RELACION #0, 1, "dsax0101", "N", "N";
         |MODO_RELACION #0, 1, "dsax0102", "N", "N";

         |MODO_RELACION #0, 1, "dsax0101", "X", "X";
         |MODO_RELACION #0, 1, "dsax0102", "X", "X";

         |MODO_RELACION #0, 0, "repoblac", "X", "X";
         |MODO_RELACION #0, 0, "rehacien", "X", "X";

         |MODO_RELACION #0, 1, "repoblac", "S", "S";
         |MODO_RELACION #0, 1, "rehacien", "S", "S";

         |REFERENCIA_DEF poblaci@,  #704;
         |REFERENCIA_DEF haciend@,  #707;
     |FINSI;
