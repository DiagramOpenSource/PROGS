|PROCESO GuardaDocumento;
     |ABRE #dsam0054;
     #dsam0054#0  = #reacceso#0;
     #dsam0054#1  = 9999;
     |LEE 040#dsam0054.];
     |SI FSalida = 0;
         |LEE 040#dsam0054.a;
     |SINO;
         |LEE 040#dsam0054.u;
     |FINSI;
     |SI FSalida = 0;  |Y #dsam0054#0 = #reacceso#0;
         nLinea = #dsam0054#1 + 1;
     |SINO;
         nLinea = 1;
     |FINSI;

     |DDEFECTO #dsam0054;
     |HORA aAlfa;
     aAlfa1 = aAlfa % 102;
     aAlfa2 = aAlfa % 402;

     #dsam0054#0  = #reacceso#0;
     #dsam0054#1  = nLinea;
     #dsam0054#2  = "CARTA SOLICITANDO NRC PARA EL PAGO DE LA RENTA";
     #dsam0054#3  = "DOC" + (("00000" + #dsam0054#0) % -105) + (("0000" + #dsam0054#1) % -104) + ".DOC";
     #dsam0054#4  = ~;
     #dsam0054#5  = aAlfa1;
     #dsam0054#6  = aAlfa2;
     #dsam0054#7  = ~;
     #dsam0054#8  = aAlfa1;
     #dsam0054#9  = aAlfa2;
     #dsam0054#10 = #reinfnrc#26;
     #dsam0054#11 = "";
     #dsam0054#12 = "";
     |GRABA 020#dsam0054.n;
     |LIBERA #dsam0054;
     |CIERRA #dsam0054;

     aOrigen  = eaDirTrabajo + eaFicheroTra;
     aDestino = eaRutaDocumento + "/" + "DOC" + (("00000" + #reacceso#0) % -105) + (("0000" + nLinea) % -104) + ".DOC";
     |QBT aOrigen;
     |QBT aDestino;

     |RCOPIA_FICHERO aOrigen, aDestino;
     |SI FSalida ! 0;
         |MENAV "     No se ha podido copia el Documento al Cliente.";
     |FINSI;
|FPRC;

|PROCESO CargaCampos;
     |HAZ AbreWord;

     |MENSA "      Asignando Campos, espere un Momento.";

     #rem00100#0 = #reacceso#0;
     |LEE 040#rem00100.=;
     |SI FSalida ! 0;  |DDEFECTO #rem00100;  |FINSI;

     |SI #rem01001#2 = "CONJUNTA  ";  |HAZ Titular;  |FINSI;
     |SI #rem01001#2 = "CONJUNTAE ";  |HAZ Titular;  |FINSI;
     |SI #rem01001#2 = "TITULAR   ";  |HAZ Titular;  |FINSI;
     |SI #rem01001#2 = "CONYUGE   ";  |HAZ Conyuge;  |FINSI;
     |SI #rem01001#2 = "1 FAMILIAR";  |HAZ Familiar; |FINSI;
     |SI #rem01001#2 = "2 FAMILIAR";  |HAZ Familiar; |FINSI;
     |SI #rem01001#2 = "3 FAMILIAR";  |HAZ Familiar; |FINSI;
     |SI #rem01001#2 = "4 FAMILIAR";  |HAZ Familiar; |FINSI;
     |SI #rem01001#2 = "ZPATRITIT ";  |HAZ Titular;  |FINSI;
     |SI #rem01001#2 = "ZPATRICON ";  |HAZ Conyuge;  |FINSI;

     aAlfa1   = #reinfnrc#24 % 402;  |HAZ Mes;
     |QBF aAlfa;

     eaConten = #reinfnrc#24 % 102;
     eaConten = eaConten + " de " + aAlfa + " de ";
     eaConten = eaConten + #reinfnrc#24 % -104;
     eaAsigna = "V#FECHA#";
     |HAZ Asigna;

     eaConten = "2017";
     eaAsigna = "V#EJERCICIO#";
     |HAZ Asigna;

     |SI #rem01001#8 = "N";
         nImporte  = #rem01001#16;
     |SINO;
         nImporte  = #rem01001#16 % 60;
     |FINSI;

     eaImporte = nImporte;  |HAZ Importe;
     eaConten  = eaImporte + " Euros.";
     eaAsigna  = "V#IMPORTE#";
     |HAZ Asigna;

     aAlfa = "[IMPRESORA]#I" + &13 + &10;
     |XGRABA enHandleTxt, aAlfa;

     |XCIERRA enHandleTxt;

     eaParametro1 = eaDirTrabajo + "dsxml";
     eaParametro2 = eaDirTrabajo + eaFicheroTxt;
     eaParametro3 = aPath01 + "basica/plantillas/";
     eaParametro4 = eaDirTrabajo;
     eaParametro5 = eaDirTrabajo;

     |HAZ EjecutaDSXML;

     |SI #reinfnrc#25 = "S";  |HAZ GuardaDocumento;  |FINSI;

     aAlfa = eaDirTrabajo + eaFicheroTra;
     |RFBORRA aAlfa;
|FPRC;

|DEFBUCLE rem01001;
     #rem01001#1;
     5, 6, 9;
     #reacceso#0, 00, "          ", "100", "S", "A";
     #reacceso#0, 99, "zzzzzzzzzz", "100", "S", "A";
     ;
     NULL, CargaCampos;
|FIN;

|PROCESO Emision;
     |SI #reacceso#14 ! "IMPRESO   ";  |ACABA;  |FINSI;

     |BUCLE rem01001;
|FPRC

|DEFBUCLE reacceso;
     #reacceso#1;
     ;
     #reinfnrc#1;
     #reinfnrc#2;
     ;
     NULL, Emision;
|FIN;

|PROCESO T12;  |TIPO 12;
     |HAZ BuscaUbicacion;

     |ABRE #rem00100;
     |SI #reinfnrc#0 = "S";
         |BUCLE reacceso;
     |SINO;
         |PARA nCampo = 3; |SI nCampo [ 23; |HACIENDO nCampo = nCampo + 1;
               |SI #reinfnrc#nCampo# ! 0;
                   #reinfnrc#1 = #reinfnrc#nCampo#;
                   #reinfnrc#2 = #reinfnrc#nCampo#;
                   |BUCLE reacceso;
               |FINSI;
         |SIGUE;
     |FINSI;
     |CIERRA #rem00100;
|FPRC;

|PROGRAMA;
     |HAZ PreparaInformes;
     |HAZ LeeUnidadBasico;

     |CLS;
     |CABEZA "Emision Carta solicitando NRC";

     |DBASS aPath01; |QBF aPath01;

     aPathDoc01 = aPath01 + "basica/word/";
     aAbre = aPathDoc01 + "word0160.doc";
     |XABRE "E", aAbre, nHandle;
     |SI FSalida < 0;
         |SUB_EJECUTA_NP ":basica/wordm000;CREALOS";
     |FINSI;

     aAbre = aPathDoc01 + "word0160.doc";
     |XABRE "E", aAbre, nHandle;
     |SI FSalida < 0;
         |MENAV "     Instale Ficheros patrones (Documentos en Word)";
         |ACABA;
     |FINSI;

     |PINPA #0#reinfnrc;
     |PINDA #0#reinfnrc;
     |ENDATOS #1#reinfnrc;
     |SI FSalida ! 0;  |ACABA;  |FINSI;
|FPRO;

