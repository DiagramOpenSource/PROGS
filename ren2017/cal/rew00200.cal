         aDef2 = "dsam0004";

         |PARA nCmp1 = 2;  |SI nCmp1 [ 15;  |HACIENDO nCmp1 = nCmp1 + 1;
               nCmp2 = nCmp1;

               |SI #rem00101#nCmp1# ! #dsam0004#nCmp2#;
                   |HAZ GrabaDife;
                   nRegraba = 1;
               |FINSI;
         |SIGUE;

         |PARA nCmp1 = 17;  |SI nCmp1 [ 24;  |HACIENDO nCmp1 = nCmp1 + 1;
               nCmp2 = nCmp1 - 1;

               |SI #rem00101#nCmp1# ! #dsam0004#nCmp2#;
                   |HAZ GrabaDife;
                   nRegraba = 1;
               |FINSI;
         |SIGUE;
     |FINSI;

