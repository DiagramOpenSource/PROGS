|FICHEROS;
     reemi102 #0;
     rem01001 #1;
     rem01002 #2;
     rem00100 #3;
     rem00101 #4;
     repoblac #5;
     rew00060 #99,MANTE;
|FIN;

|VARIABLES;
     aComodin = "";
     aFichero = "";

     nCampos  = 0;
     nProvi   = 0;
     nPobla   = 0;
     nFamil   = 0;
     nFamil1  = 0;
     nConta   = 0;
     nSwEstar = 0;
     nDEmpre  = 0;
     nHEmpre  = 0;

     aNom     = "";
     aApel1   = "";
     aApel2   = "";
     aLong    = "";
     aAlfa    = "";
     aAlfa1   = "";
     aAlfa2   = "";

     nInd     = 0;
     nLong    = 0;
     nCalc    = 0;
     nPos     = 0;
     SwSalir  = 0;
