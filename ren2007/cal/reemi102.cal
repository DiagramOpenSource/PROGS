|PROCESO Tipo11w60;  |TIPO 11;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     |LEE 101#99=;
     |SI #99#30 = "N";
         #99#30 = "S";
     |SINO;
         #99#30 = "N";
     |FINSI;
     |PINTA #99#30;
     |GRABA 020#99a;
     |LIBERA #99;
|FPRC;

|PROCESO Seleccion; |TIPO 0;
  |SI #0#0 = "S";
      |CAMPO_MODIFICA #0#01, 1, "S";
      |CAMPO_MODIFICA #0#02, 1, "S";

      |PARA nCampos = 3; |SI nCampos [ 32; |HACIENDO nCampos = nCampos + 1;
            #0nCampos = 0; |PINTA #0nCampos;
            |CAMPO_MODIFICA #0nCampos, 1, "N";
      |SIGUE;
  |SINO;
      #0#1 = 00001; |PINTA #0#1;    |CAMPO_MODIFICA #0#01, 1, "N";
      #0#2 = 99999; |PINTA #0#2;    |CAMPO_MODIFICA #0#02, 1, "N";

      |PARA nCampos = 3; |SI nCampos [ 32; |HACIENDO nCampos = nCampos + 1;
            |CAMPO_MODIFICA #0nCampos, 1, "S";
      |SIGUE;
  |FINSI;
|FPRC;

|PROCESO LeeProvi;
     #5#0 = nProvi;
     #5#1 = nPobla;
     |LEE 040#5=;
     |SI FSalida ! 0;  |DDEFECTO #5;  |FINSI;
|FPRC;

|PROCESO SeparaNombre;
   aNom = ""; aApel1 = ""; aApel2 = "";
   nInd = 1;
   aLong = aAlfa % 0; nLong = aLong;
   aAlfa1 = "";
   |PARA nCalc = 1; |SI nCalc [ nLong; |HACIENDO nCalc = nCalc + 1;
      nPos = (nCalc * 100) + 1;
      aAlfa2 = aAlfa % nPos;
      |SI aAlfa2 = " "; |O aAlfa2 = ",";
         |SI aAlfa1 ! "DE";
          |Y aAlfa1 ! "LA";
          |Y aAlfa1 ! "LAS";
          |Y aAlfa1 ! "LOS";
          |Y aAlfa1 ! "DEL";
            |SI nInd = 1; aApel1 = aApel1 + aAlfa1 + " "; |FINSI;
            |SI nInd = 2; aApel2 = aApel2 + aAlfa1 + " "; |FINSI;
            |SI nInd ] 3; aNom   = aNom   + aAlfa1 + " "; |FINSI;
             nInd = nInd + 1;
         |SINO;
            |SI nInd > 1; nInd = nInd - 1; |FINSI;
            |SI nInd = 1; aApel1 = aApel1 + aAlfa1 + " "; |FINSI;
            |SI nInd = 2; aApel2 = aApel2 + aAlfa1 + " "; |FINSI;
            |SI nInd ] 3; aNom   = aNom   + aAlfa1 + " "; |FINSI;
         |FINSI;
         aAlfa1 = "";
      |SINO;
         aAlfa1 = aAlfa1 + aAlfa2;
      |FINSI;
   |SIGUE;

   |PARA; |SI SwSalir = 0; |HACIENDO;
      SwSalir = 1;
      aAlfa1 = aApel1 % 0101;
      |SI aAlfa1 = " "; aApel1 = aApel1 % 0299; SwSalir = 0; |FINSI;
      aAlfa1 = aApel2 % 0101;
      |SI aAlfa1 = " "; aApel2 = aApel2 % 0299; SwSalir = 0; |FINSI;
      aAlfa1 = aNom   % 0101;
      |SI aAlfa1 = " "; aNom   = aNom   % 0299; SwSalir = 0; |FINSI;
   |SIGUE;
|FPRC;

|PROCESO MiraPago;
   |SI #0#36 = "E";
      #99#22 = "X";
      #99#23 = " ";
      #99#24 = " " * 4;
      #99#25 = " " * 4;
      #99#26 = " " * 2;
      #99#27 = " " * 10;
   |SINO;
      #99#22 = " ";
      #99#23 = "X";
      |SI #99#1 = "TITULAR   "; aAlfa = #3#53; |FINSI;
      |SI #99#1 = "CONYUGE   "; aAlfa = #3#54; |FINSI;
      |SI #99#1 = "CONJUNTA  "; aAlfa = #3#55; |FINSI;
      |SI #99#1 = "CONJUNTAE "; aAlfa = #3#55; |FINSI;
      aAlfa1 = #99#1; |QBF aAlfa1; aAlfa1 = aAlfa1 - "FAMILIAR";
      |SI FSalida ! 0; aAlfa = #4#10; |FINSI;
      |QBF aAlfa;
      |SI aAlfa = "";
         #99#22 = "X";
         #99#23 = " ";
         #99#24 = " " * 4;
         #99#25 = " " * 4;
         #99#26 = " " * 2;
         #99#27 = " " * 10;
      |SINO;
         aAlfa1 = aAlfa % 0104; #99#24 = aAlfa1;
         aAlfa1 = aAlfa % 0504; #99#25 = aAlfa1;
         aAlfa1 = aAlfa % 0902; #99#26 = aAlfa1;
         aAlfa1 = aAlfa % 1110; #99#27 = aAlfa1;
      |FINSI;
   |FINSI;
|FPRC;

|PROCESO GrabaTempo;
     |SI #1#10 = "S";  |ACABA;  |FINSI;

     #2#0 = #1#0;
     #2#1 = #1#1;
     #2#2 = #1#2;
     #2#3 = 31;
     #2#4 = "131";
     |LEE 040#2=;
     |SI FSalida ! 0;  |DDEFECTO #2;  |FINSI;

     |SI #2#6 ! 0;  |ACABA;  |FINSI;

     #2#0 = #1#0;
     #2#1 = #1#1;
     #2#2 = #1#2;
     #2#3 = 39;
     #2#4 = "760";
     |LEE 040#2=;
     |SI FSalida ! 0;  |DDEFECTO #2;  |FINSI;

     |SI #2#6 [ 0;  |ACABA;  |FINSI;

     #3#0 = #1#0;
     |LEE 040#3=;
     |SI FSalida ! 0;  |DDEFECTO #3;  |FINSI;

     |DDEFECTO #99;
     #99#0  = #1#0;
     #99#1  = #1#2;
     #99#2  = #3#71;
     #99#3  = #3#72;
     #99#4  =  (("00" + #3#69) % -102) + (("000" + #3#70) % -103);
     #99#13 = #3#68;

     |SI #99#1 = "CONJUNTAE ";
         |SI #3#91 = "C";
             nProvi = #3#20;
             nPobla = #3#21;
             |HAZ LeeProvi;

             #99#5  = #3#1;
             #99#6  = #3#5;
             #99#7  = #3#14;
             #99#8  = #3#15;
             #99#9  = #3#16;
             #99#10 = #3#17;
             #99#11 = #3#18;
             #99#12 = #3#19;
             #99#14 = #3#22;
             #99#15 = #5#2;
             #99#16 =  (("00" + #3#20) % -102) + (("000" + #3#21) % -103);
             #99#17 = #3#35;
             #99#18 = #3#39;

             |HAZ MiraPago;

             #99#31 = #3#2; #99#34 = #3#36;
             #99#32 = #3#3; #99#35 = #3#37;
             #99#33 = #3#4; #99#36 = #3#38;
         |SINO;
             nProvi = #3#50;
             nPobla = #3#51;
             |HAZ LeeProvi;

             #99#5  = #3#35;
             #99#6  = #3#39;
             #99#7  = #3#44;
             #99#8  = #3#45;
             #99#9  = #3#46;
             #99#10 = #3#47;
             #99#11 = #3#48;
             #99#12 = #3#49;
             #99#14 = #3#52;
             #99#15 = #5#2;
             #99#16 =  (("00" + #3#50) % -102) + (("000" + #3#51) % -103);
             #99#17 = #3#1;
             #99#18 = #3#5;

             |HAZ MiraPago;

             #99#31 = #3#36; #99#34 = #3#2;
             #99#32 = #3#37; #99#35 = #3#3;
             #99#33 = #3#38; #99#36 = #3#4;
         |FINSI;
     |FINSI;

     |SI #99#1 = "TITULAR   ";  |O #99#1 = "CONJUNTA  ";
         nProvi = #3#20;
         nPobla = #3#21;
         |HAZ LeeProvi;

         #99#5  = #3#1;
         #99#6  = #3#5;
         #99#7  = #3#14;
         #99#8  = #3#15;
         #99#9  = #3#16;
         #99#10 = #3#17;
         #99#11 = #3#18;
         #99#12 = #3#19;
         #99#14 = #3#22;
         #99#15 = #5#2;
         #99#16 =  (("00" + #3#20) % -102) + (("000" + #3#21) % -103);
         #99#17 = #3#35;
         #99#18 = #3#39;

         |HAZ MiraPago;

         #99#31 = #3#2; #99#34 = #3#36;
         #99#32 = #3#3; #99#35 = #3#37;
         #99#33 = #3#4; #99#36 = #3#38;
     |FINSI;

     |SI #99#1 = "CONYUGE   ";
         nProvi = #3#50;
         nPobla = #3#51;
         |HAZ LeeProvi;

         #99#5  = #3#35;
         #99#6  = #3#39;
         #99#7  = #3#44;
         #99#8  = #3#45;
         #99#9  = #3#46;
         #99#10 = #3#47;
         #99#11 = #3#48;
         #99#12 = #3#49;
         #99#14 = #3#52;
         #99#15 = #5#2;
         #99#16 =  (("00" + #3#50) % -102) + (("000" + #3#51) % -103);
         #99#17 = #3#1;
         #99#18 = #3#5;

         |HAZ MiraPago;

         #99#31 = #3#36; #99#34 = #3#2;
         #99#32 = #3#37; #99#35 = #3#3;
         #99#33 = #3#38; #99#36 = #3#4;
     |FINSI;

     aComodin = #99#1 - "FAMILIAR";
     |SI FSalida ! 0;
         |QBF aComodin;
         nFamil = aComodin;
         nProvi = #3#20;
         nPobla = #3#21;
         |HAZ LeeProvi;

         nFamil1 = 0;
         |PARA nConta = 1; |SI nConta [ 100;  |HACIENDO nConta = nConta + 1;;
               #4#0 = #3#0;
               #4#1 = nConta;
               |LEE 040#4=;
               |SI FSalida = 0;  |Y #4#13 = "S";
                   nFamil1 = nFamil1 + 1;
               |FINSI;
               |SI nFamil = nFamil1;
                   #99#5  = #4#2;
                   #99#6  = #4#3;
                   |SAL;
               |FINSI;
         |SIGUE;

         #99#7  = #3#14;
         #99#8  = #3#15;
         #99#9  = #3#16;
         #99#10 = #3#17;
         #99#11 = #3#18;
         #99#12 = #3#19;
         #99#14 = #3#22;
         #99#15 = #5#2;
         #99#16 =  (("00" + #3#20) % -102) + (("000" + #3#21) % -103);

         |HAZ MiraPago;

         aAlfa  = #99#6;  |HAZ SeparaNombre;
         #99#31 = aNom;   #99#34 = "";
         #99#32 = aApel1; #99#35 = "";
         #99#33 = aApel2; #99#36 = "";
     |FINSI;

/*
     #99#19   = #2#6;
     #99#20   = #2#6 % 60;
     #99#21   = #2#6 % 40;
*/

     #99#19   = #1#16;
     #99#20   = #1#16 % 60;
     #99#21   = #1#16 % 40;

     #99#28   = #0#33 % 102;
     aComodin = #0#33 % 402;
     |SI aComodin = "01";  #99#29 = "ENERO     ";  |FINSI;
     |SI aComodin = "02";  #99#29 = "FEBRERO   ";  |FINSI;
     |SI aComodin = "03";  #99#29 = "MARZO     ";  |FINSI;
     |SI aComodin = "04";  #99#29 = "ABRIL     ";  |FINSI;
     |SI aComodin = "05";  #99#29 = "MAYO      ";  |FINSI;
     |SI aComodin = "06";  #99#29 = "JUNIO     ";  |FINSI;
     |SI aComodin = "07";  #99#29 = "JULIO     ";  |FINSI;
     |SI aComodin = "08";  #99#29 = "AGOSTO    ";  |FINSI;
     |SI aComodin = "09";  #99#29 = "SEPTIEMBRE";  |FINSI;
     |SI aComodin = "10";  #99#29 = "OCTUBRE   ";  |FINSI;
     |SI aComodin = "11";  #99#29 = "NOVIEMBRE ";  |FINSI;
     |SI aComodin = "12";  #99#29 = "DICIEMBRE ";  |FINSI;

     |SI #0#34 = "N";  #99#30 = "S";  |FINSI;

     |GRABA 020#99n;
     |LIBERA #99;
     nSwEstar = 1;
|FPRC;

|DEFBUCLE rem01001;
     #1#1;
     5, 14, 8, 10;
     nDEmpre, 00, "          ", "100", "N", "S", "N";
     nHEmpre, 00, "zzzzzzzzzz", "101", "N", "S", "S";
     ;
     NULL, GrabaTempo;
|FIN;

|RUTINA ClaveBaja99;
     #99#0 = 1;
     #99#1 = "          ";
|FRUT;

|RUTINA ClaveAlta99;
     #99#0 = 99999;
     #99#1 = "zzzzzzzzzz";
|FRUT;

|PROCESO CargaTempo;
     nSwEstar = 0;

     |ABRE #99;
     |ABRE #2;
     |ABRE #3;
     |ABRE #4;
     |ABRE #5;
     |SI #0#0 = "S";
         nDEmpre = #0#1;   nHEmpre = #0#2;
         |BUCLE rem01001;
     |SINO;
         |PARA nCampos = 3; |SI nCampos [ 32; |HACIENDO nCampos = nCampos + 1;
               |SI #0nCampos ! 0;
                   nDEmpre = #0nCampos;
                   nHEmpre = #0nCampos;
                   |BUCLE rem01001;
               |FINSI;
         |SIGUE;
     |FINSI;
     |CIERRA #2;
     |CIERRA #3;
     |CIERRA #4;
     |CIERRA #5;
     |CIERRA #99;

     |SI nSwEstar = 0; |ACABA; |FINSI;

     |SI #0#34 = "N"; |ACABA; |FINSI;

     |ENTLINEAL #1#99, -1, 4, 22, 1, ClaveBaja99, ClaveAlta99;
|FPRC;

|PROCESO Imprime;
     |SI #0#35 = "H";
         |MENAV "     Coloque hojas Modelo 102 en la Impresora y pulse una tecla cuando este preparado.";
     |FINSI;

     |PRINT;
     |PIEINF;
|FPRC;

|DEFBUCLE rew00060;
     #99#1;
     30;
     00001, "          ", "S";
     99999, "zzzzzzzzzz", "S";
     ;
     NULL, Imprime;
|FIN;

|PROCESO Tipo3;  |TIPO 3;
     |IMPRE 0;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |SI #0#35 = "H";
         |INFOR "m102m001";
     |SINO;
         |INFOR "m102h001";
     |FINSI;

     |SI FSalida ! 0;  |FINIMP;  |ACABA;  |FINSI;

     aFichero = "p2" + Usuario;
     |NOME_DAT #99 aFichero;
     |ABRE #99;  |CIERRA #99;  |DELINDEX #99;

     |HAZ CargaTempo;

     |SI nSwEstar = 1;
         |BUCLE rew00060;
     |FINSI;

     |DELINDEX #99;
     |FININF;
     |FINIMP;
|FPRC;

