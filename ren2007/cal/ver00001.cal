|| ************************************************************************
|| PROCESOS DE CAMBIO DE VERSION
|| ************************************************************************

|PROCESO MiraPatrones;
     nSwHay = 0
     eaPrograma  = "";
     eaExtension = "";

     |ABRE #8;
     #8#0 = 17;
     #8#1 = 902;
     |LEE 040#8=;
     |SI FSalida ! 0;  |O #8#7 ! 260;
         eaPrograma  = "rem00008";
         eaExtension = "r07";
         |CIERRA #8;   |DELINDEX #8;
         nSwHay = 1;
     |SINO;
         |CIERRA #8;
     |FINSI;

     |ABRE #401;
     |LEE 040#401p;
     |SI FSalida ! 0;
         |CIERRA #401;  |DELINDEX #401;
         eaPrograma  = "";
         eaExtension = "";
         nSwHay     = 1;
     |FINSI;
     |CIERRA #401;

     |SI nSwHay = 1;
         |SI eaPrograma = "";
             |MENAV "0000 Han cambiado o no se han instalado los ficheros patrones, por favor instalelos.";
         |FINSI;
         eaRenta = "r07";
         |SUB_EJECUTA_NP ":basica/dspatron";
     |FINSI;

     |ABRE #401;
     |LEE 040#401p;
     |SI FSalida ! 0;
         enError = 1;
         |CIERRA #401;  |DELINDEX #401;
     |SINO;
         |CIERRA #401;
     |FINSI;

     |ABRE #8;
     #8#0 = 17;
     #8#1 = 902;
     |LEE 040#8=;
     |SI FSalida ! 0;  |O #8#7 ! 260;
         |CIERRA #8;   |DELINDEX #8;
         enError = 1;
     |SINO;
         |CIERRA #8;
     |FINSI;
|FPRC;

|PROGRAMA;
     |MENSA "0000Comprobando configuracion patrones.";
     enError = 0;
     |HAZ MiraPatrones;
     |BLIN 05;
|FPRO;
