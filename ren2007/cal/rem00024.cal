     |PINTA #50#5;

     |SI #20#13 = "N";
         #50#8 = 0;  |PINTA #50#8;
         #50#9 = 0;  |PINTA #50#9;
     |FINSI;
|FPRC;

|PROCESO nContenido50;
     nContenido = Contenido;
     |SI #50Campo ! nContenido;
         #50#5  = #50#4 - #50#3;                    |PINTA #50#5;
         #50#7  = 0;
         #50#9  = (#50#5 - #50#7)  % #50#8;         |PINTA #50#9;
     |FINSI;
|FPRC;

|PROCESO Reduccion150;
     nContenido = Contenido;
     |SI #50Campo = nContenido;  |Y #50#7 ! 0;  |ACABA;  |FINSI;

     #50#7 = 0;

     |PINTA #50#7;
|FPRC;

|PROCESO Reduccion250;
     nContenido = Contenido;
     |SI #50Campo = nContenido;  |Y #50#9 ! 0;  |ACABA;  |FINSI;

     #50#9 = (#50#5 - #50#7) % #50#8;

     |PINTA #50#9;
|FPRC;

|PROCESO Totalm020;
     |C_M #20#12, 1, "S";
     |C_M #20#13, 1, "S";
     |SI #20#11 = "S";
         #20#12 = 0;
         #20#13 = "N";
         |C_M #20#12, 1, "N";
         |C_M #20#13, 1, "N";
     |FINSI;

     |C_M #20#16, 1, "S";
     |C_M #20#17, 1, "S";
     |C_M #20#23, 1, "S";
     |C_M #20#24, 1, "S";
     |SI #20#12 ] 1;  |Y #20#12 [ 12;
         |C_M #20#16, 1, "N";
         |C_M #20#17, 1, "N";
         |C_M #20#23, 1, "N";
         |C_M #20#24, 1, "N";
     |SINO;
         |SI #20#12 ] 13;
             #20#13 = "N";
             |C_M #20#13, 1, "N";
         |FINSI;

         |SI #20#13 = "N";
             |C_M #20#23, 1, "N";  #20#23 = 0;
             |C_M #20#24, 1, "N";  #20#24 = 0;
         |FINSI;
     |FINSI;

     #20#18 = #20#17 - #20#16;

     |PINTA #20#16;
     |PINTA #20#17;
     |PINTA #20#18;
     |PINTA #20#19;
     |PINTA #20#20;
     |PINTA #20#23;
     |PINTA #20#24;
|FPRC;

|PROCESO nContenido;
     nContenido = Contenido;
     |SI #20Campo ! nContenido;
         #20#18 = #20#17 - #20#16;                      |PINTA #20#18;
         #20#22 = 0;
         #20#24 = (#20#18 - #20#22) % #20#23;           |PINTA #20#24;
         #20#20 = (#20#18 - #20#22 - #20#24) % #20#19;  |PINTA #20#20;
     |FINSI;
|FPRC;

|PROCESO Retencion;
     nContenido = Contenido;
     |SI #20Campo = nContenido;  |Y #20#20 ! 0;  |ACABA;  |FINSI;

     #20#20 = (#20#18 - #20#22 - #20#24) % #20#19;  |PINTA #20#20;
|FPRC;

|PROCESO Reduccion1;
     nContenido = Contenido;
     |SI #20Campo = nContenido;  |Y #20#22 ! 0;  |ACABA;  |FINSI;

     #20#22 = 0;
|FPRC;

|PROCESO Reduccion2;
     nContenido = Contenido;
     |SI #20Campo = nContenido;  |Y #20#24 ! 0;  |ACABA;  |FINSI;

     #20#24 = (#20#18 - #20#22) % #20#23;
|FPRC;


