|PROCESO Tipo2m20;  |TIPO 2;
     nModo    = (FEntrada / 100) ! 0;

     |SI nModo = 1;  |O nModo = 3;
         |SI nModo = 3;
             nSw = 0;
             |BUCLE rem00050;
         |FINSI;
         enSwRetorno  = 1;

         |SI nModo = 1;
             |HAZ Patrimonio;
         |FINSI;

         |SI nModo = 3;
             eaDeDonde  = "C2";
             eaLinDonde = #20#1;
             eaLetra    = "I1";
             |HAZ BorraPatrimonio;
         |FINSI;

         |ACABA;
     |FINSI;

     nEntrada = 0 +. 1;

     |SI nEntrada ! 1;
         eaDeDonde  = "C2";
         eaLinDonde = #20#1;
         eaLetra    = "I1";
         |HAZ BorraPatrimonio;

         |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
               #99nCampo = #20nCampo;
         |SIGUE;
     |SINO;
         |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
               |SI #99nCampo ! #20nCampo;
                   enSwRetorno  = 1;
               |FINSI;
         |SIGUE;
         |HAZ Patrimonio;
     |FINSI;
|FPRC;

