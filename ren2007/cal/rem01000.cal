|FICHEROS;
     rem01000 #1000;
|FIN;

|VARIABLES;
     aCodigo1   = "";
     aCodigo2   = "";
     aContenido = "";
     aCaracter  = "";
     aLineaMenu = "";
     aAlfa      = "";
     aAlfa1     = "";
     aAlfa2     = "";
     aAbre      = "";
     aPrograma  = "";

     nCodigos   = 0;
     nCodigo1   = 0;
     nCodigo2   = 0;
     nCodigo3   = 0;
     nPos       = 0;
     nPosi      = 0;
     nRango     = 0;
     nBoton1    = 0;
     nHandle    = 0;
     nLong      = 0;
     nCaracter  = 0;
     nLLaves    = 0;
|FIN;

|PROCESO RecogeDatos;
     aAlfa = "/u/dsprofes/ren2007/cogetxt/rem01000.txt";
     |XABRE "AB",  aAlfa,  nHandle;

     |PARA;  |SI;  |HACIENDO;
          |XLEE nHandle, 151, aLineaMenu;
          |SI FSalida [ 0;  |SAL;  |FINSI;

          aAlfa1 = aLineaMenu % 107;
          aAlfa2 = aLineaMenu - aAlfa1;

          aCodigo1 = aAlfa1 % 102;
          aCodigo2 = aAlfa1 % 403;

          |DDEFECTO #1000;
          #1000#0 = aCodigo1;
          #1000#1 = aCodigo2;
          #1000#2 = aAlfa2;
          |GRABA 020#1000n;
          |LIBERA #1000;
     |SIGUE;
     |XCIERRA nHandle;
|FPRC;

|PROCESO rem01000;
     aAlfa1 = (("00" + #1000#0) % -102) + " " + #1000#1 + ":" + #1000#2 + &13 + &10;
     |XGRABA nHandle, aAlfa1;
|FPRC;

|DEFBUCLE rem01000;
     #1000#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, rem01000;
|FIN;

|PROCESO HazElTxt;
     aAbre = "/u/dsprofes/ren2007/cogetxt/rem01000.txt";

     |XABRE "WB", aAbre, nHandle;
     |BUCLE rem01000;
     |XCIERRA nHandle;
|FPRC;

|PROCESO Tipo80Fm1000;  |TIPO 80;
||     |HAZ HazElTxt;

     |ABRET #1000;
     |LEE 000#1000p;
     |SI FSalida ! 0;
         |HAZ RecogeDatos;
     |FINSI;

     FSalida = 2799;
|FPRC;

|PROCESO Tipo90Fm1000;  |TIPO 90;
     |CIERRAT #1000;
|FPRC;

|PROCESO Salir;
     |PTEC 806;
|FPRC;

|PROGRAMA;
     |CABEZA "Liquidacion";

     |PINPA #0#1000;

     |CONTROL_SIMPLE 0, "BOTON,{{001}}Salir", 2785, 2795, Salir;
     nBoton1 = FSalida;

     nRango = 1 + 4 + 8 + 16 + 128;

     |LEE 040#1000p;

     |LINEAL_SIMPLE #1#1000, nRango, 0602, 2698, NULL, NULL, NULL;

     |PULSATECLA;

     |FIN_CONTROL_WINDOWS nBoton1;
|FPRO;
