|VARIABLES;
      aBase          = "";
      aPathFich      = "";
      aMas           = "";
      aFichero       = "";
      aNif           = "";
      aAlfa          = "";
      aAlfa1         = "";
      aAlfa2         = "";
      aIdentificador = "";

      nLinea      = 0;
      nValor      = 0;
      nEmpresa43  = 0;
      nActivid43  = 0;
      nPorce      = 0;
