|PROCESO TraeTxtComprimido;
     aAlfa = aPathRenta + aFichero + ".tar " + aFichero + ".txt";
     |ATAR aAlfa, aPathRenta;

     aAlfa = aPathRenta + aFichero + ".tar";
     |COMPRIME aAlfa;

     aAlfa = aPathRenta + aFichero + ".tar";  |FBORRA aAlfa;
     aAlfa = aPathRenta + aFichero + ".txt";  |FBORRA aAlfa;

     aOrigen  = aPathRenta + aFichero + ".tgz";
     aDestino = "C:\MODELOS\TMP\"  + aFichero + ".tgz";

     |RFBORRA aDestino;
     aIPRemoto = "";
     |IP_REMOTO aIPRemoto;  |QBT aIPRemoto;

     |SI aIPRemoto ! "";
         |ENVIA_FICHERO aOrigen, aDestino;
     |SINO;
         |COPIA_FICHERO aOrigen, aDestino;
     |FINSI;

     Pos    = -1;
     aAlfa  = "C:\MODELOS\TMP\" + aFichero + ".tgz";
     aAlfa1 = "C:\MODELOS\TMP\";
     |RDESCOMPRIME aAlfa;
     |RDETAR aAlfa, aAlfa1;

     aAlfa = aPathRenta + aFichero + ".tgz";         |FBORRA aAlfa;
     aAlfa = "C:\MODELOS\TMP\"  + aFichero + ".tgz";  |RFBORRA aAlfa;
     aAlfa = "C:\MODELOS\TMP\"  + aFichero + ".tar";  |RFBORRA aAlfa;
|FPRC;

|PROCESO CreaFicheros;
     aFichero = "001" + (("00000" + #0#0) % -105);

     |DBASE aPathRenta;  |QBT aPathRenta;
     aPathRenta = aPathRenta + "tmp";  |MKDIR aPathRenta;
     aPathRenta = aPathRenta + "/";

     aOrigen    = aPathRenta + aFichero + ".txt";

     #0#3  = "";
     #0#4  = "";
     #0#5  = "";
     #0#6  = "";
     #0#7  = "";
     #0#8  = "";
     #0#9  = "";
     #0#10 = "";
     #0#11 = "";
     #0#12 = "";

     |XABRE "WB", aOrigen, nHandle2;
     |BUCLE rem00900;
     |XCIERRA nHandle2;

     |HAZ TraeTxtComprimido;

     aDestino = "C:\MODELOS";      |RMKDIR aDestino;
     aDestino = "C:\MODELOS\TMP";  |RMKDIR aDestino;
     aDestino = "C:\MODELOS\TMP\"  + aFichero + ".TXT";
|FPRC;

|PROCESO LanzaPrograma;
     |PULSATECLA;

     aAlfa  = aPrograma % 108;
     aAlfa1 = aPrograma % 902;
     |SI aAlfa1 ! "  ";
         eaLetra = aAlfa1;
     |FINSI;

     |SI aPrograma ! "rezcalcu";
         |SUB_EJECUTA_NP aAlfa;

         |HAZ CreaFicheros;
     |SINO;
         nContinuar = 0;
         |SI enModo ! 4;
             |HAZ ChequeaLiquidacion;

             |SI nContinuar = 0;  |SUB_EJECUTA_NP "rezcalcu";  |FINSI;
         |FINSI;

         |SI nContinuar = 0;
             |SUB_EJECUTA_NP "rem01001";
             aPrograma = "";
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO CreaMenu;
     |HAZ CreaFicheros;

     aPrograma = "";
     aTitulo   = (("00000" + #0#0) % -105) + " " + #0#1;
     |PARA;  |SI;  |HACIENDO;
          aAbre = "C:\MODELOS\TMP\"  + "000" + (("00000" + #0#0) % -105) + ".TXT";
          |XABRE "WCB", aAbre, nHandle;

          aAlfa = ((aDestino + (" " * 99)) % 198) + &13 + &10;
          |XGRABA nHandle, aAlfa;

          aAlfa = ((aTitulo + (" " * 99)) % 198) + &13 + &10;
          |XGRABA nHandle, aAlfa;

          aAlfa = ((aPrograma + (" " * 99)) % 198) + &13 + &10;
          |XGRABA nHandle, aAlfa;
          |XCIERRA nHandle;

          aAlfa    = "|:ren2007/rez99999;" + aAbre;
          |SUB_EJECUTA_NP aAlfa;

          nLinea = 1;
          |XABRE "ABC",  aAbre,  nHandle;
          |PARA;  |SI;  |HACIENDO;
              |XLEE nHandle, 100, aAlfa;
              |SI FSalida [ 0;  |SAL;  |FINSI;

              |SI nLinea = 1;  aAbre     = aAlfa % 198;  |FINSI;
              |SI nLinea = 2;  aTitulo   = aAlfa % 198;  |FINSI;
              |SI nLinea = 3;  aPrograma = aAlfa % 198;  |FINSI;

              nLinea = nLinea + 1;
          |SIGUE;
          |XCIERRA nHandle;

          |QBF aAbre;
          |QBF aTitulo;
          |QBF aPrograma;

          |SI aPrograma = "";  |SAL;  |FINSI;

          |HAZ LanzaPrograma;

          |SI aPrograma = "";
              |HAZ CreaFicheros;
              |SAL;
          |FINSI;
     |SIGUE;

     aAbre = "C:\MODELOS\TMP\"  + "000" + (("00000" + #0#0) % -105) + ".TXT";
     |RFBORRA aAbre;

     aAbre = "C:\MODELOS\TMP\"  + "001" + (("00000" + #0#0) % -105) + ".TXT";
     |RFBORRA aAbre;
|FPRC;

