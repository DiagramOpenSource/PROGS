|FICHEROS;
     dscurm00 #0;
     dscurm02 #2;
     dscurm06 #6;
     dscurm07 #7;
     dscurw08 #908;
|FIN;

|VARIABLES;
     nBoton0_1      = 0;
     nBoton0_2      = 0;
     nBoton0_3      = 0;

     nPanta0        = 0;
     nCampoD        = 0;
     nCampoH        = 0;
     nCampoHasta    = 0;
     nCampo1        = 0;
     nVentana       = 0;
     nGrid1         = 0;
     nGrid2         = 0;
     nRango         = 0;
     nPos1          = 0;
     nPos2          = 0;
     nResp          = 0;
     nCampo         = 0;

     aAlfa          = "";
     aBase          = "";
     aMas           = "";
     aFichero       = "";

     &enCurso       = 0;
     &enModo        = 1;
     &eaAplica      = "";
     &eaPracti      = "";
     &eaEjerc       = "";
|FIN;

|PROCESO BajaPanta0;
     #7#0  = #908#1;
     #7#1  = #908#4;
     #7#2  = #908#6;
     #7#3  = "";
|FPRC;

|PROCESO AltaPanta0;
     #7#0  = #908#1;
     #7#1  = #908#4;
     #7#2  = #908#6;
     #7#3  = "zz";
|FPRC;

|| ------------------------------------------------------
|| Procesos Grabacion del Bloque
|| ------------------------------------------------------

|PROCESO GrabaPractica;
     #6#0 = #908#1;
     #6#1 = #908#4;
     #6#2 = #908#6;
     |LEE 101#6=;
     |SI FSalida ! 0;
         |DDEFECTO #6;
         #6#0 = #908#1;
         #6#1 = #908#4;
         #6#2 = #908#6;
         |GRABA 020#6b;
     |FINSI;

     #6#3 = #908#7;
     |GRABA 020#6a;
     |LIBERA #6;
|FPRC;

|| ------------------------------------------------------
|| Procesos del Boton 0_1
|| ------------------------------------------------------

|PROCESO Boton0_1;
     |HAZ GrabaPractica;

     |VENTANA 0501, 2799, -1, -1, "";
     nVentana = FSalida;

     enCurso   = #908#0;
     eaAplica  = #908#4;
     eaPracti  = #908#6;
     eaEjerc   = "";
     enModo    = 1;
     |SUB_EJECUTA "dscurw09";

     |FINVENTANA nVentana;

     |REFRESCACONTROL nGrid1;
|FPRC;

|| ------------------------------------------------------
|| Procesos del Boton 0_2
|| ------------------------------------------------------

|PROCESO Boton0_2;
     |VENTANA 0501, 2799, -1, -1, "";
     nVentana = FSalida;

     enCurso   = #908#0;
     eaAplica  = #908#4;
     eaPracti  = #908#6;
     eaEjerc   = #7#3;
     enModo    = 2;
     |SUB_EJECUTA "dscurw09";

     |FINVENTANA nVentana;

     |REFRESCACONTROL nGrid1;
|FPRC;

|| ------------------------------------------------------
|| Procesos del Boton 0_3
|| ------------------------------------------------------

|PROCESO Boton0_3;
     aAlfa = "0000NSeguro de dar de baja a " + #7#4;
     |CONFI aAlfa;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |LEE 101#7=;
     |SI FSalida = 0;
         |BORRA 020#7a;
         |LIBERA #7;
     |FINSI;

     #7#0 = #908#1;
     #7#1 = #908#4;
     #7#2 = #908#6;
     #7#3 = "";
     |LEE 000#7];
     |SI FSalida = 0;  |Y  #7#0 = #908#1;  |Y #7#1 = #908#4;  |Y #7#2 = #908#6;
         |ESTADO_CONTROL nBoton0_2, "ENABLE";
         |ESTADO_CONTROL nBoton0_3, "ENABLE";
     |SINO;
         |ESTADO_CONTROL nBoton0_2, "DISABLE";
         |ESTADO_CONTROL nBoton0_3, "DISABLE";
     |FINSI;

     |REFRESCACONTROL nGrid1;
|FPRC;

|| ------------------------------------------------------
|| Procesos del Incio del Programa
|| ------------------------------------------------------

|PROCESO EventoPanta0;
     |SI FSalida = 1;  |O FSalida = 2;
         |ESTADO_CONTROL nBoton0_1, "ENABLE";

         |LEE 000#7=;
         |SI FSalida = 0;  |Y  #7#0 = #908#1;  |Y #7#1 = #908#4;  |Y #7#2 = #908#6;
             |ESTADO_CONTROL nBoton0_2, "ENABLE";
             |ESTADO_CONTROL nBoton0_3, "ENABLE";
         |SINO;
             |ESTADO_CONTROL nBoton0_2, "DISABLE";
             |ESTADO_CONTROL nBoton0_3, "DISABLE";
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO BotonesPanta0;
     |CONTROL_SIMPLE nPanta0, "BOTON,{{193}} Alta", 1277, 1398, Boton0_1;
     nBoton0_1 = FSalida;

     |CONTROL_SIMPLE nPanta0, "BOTON,{{197}} Modificar", 1577, 1698, Boton0_2;
     nBoton0_2 = FSalida;

     |CONTROL_SIMPLE nPanta0, "BOTON,{{199}} Baja ", 1877, 1998, Boton0_3;
     nBoton0_3 = FSalida;

     |ESTADO_CONTROL nBoton0_1, "DISABLE";
     |ESTADO_CONTROL nBoton0_2, "DISABLE";
     |ESTADO_CONTROL nBoton0_3, "DISABLE";
|FPRC;

|PROCESO Practica;  |TIPO 0;
     |SI enModo ! 1;  |ACABA;  |FINSI;

     #6#0 = #908#1;
     #6#1 = #908#4;
     #6#2 = #908#6;
     |LEE 000#6=;
     |SI FSalida = 0;
         |MENAV "0000 La Practica ya existe.";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Tipo12;  |TIPO 12;
|FPRC;

|PROCESO Tipo80;  |TIPO 80;
     |ABRET #7;

     FSalida = 2799;
|FPRC;

|PROCESO Tipo90;  |TIPO 90;
     |CIERRAT #7;
|FPRC;

|PROGRAMA;
     |C_M #908#6, 1, "S";

     |ABRE #0;
     #0#0 = enCurso;
     |LEE 000#0=;
     |SI FSalida ! 0;  |DDEFECTO #0; |FINSI;
     |CIERRA #0;

     |ABRE #2;
     #2#0 = #0#1;
     #2#1 = eaAplica;
     |LEE 000#2=;
     |SI FSalida ! 0;  |DDEFECTO #2; |FINSI;
     |CIERRA #2;

     #908#0 = #0#0;
     #908#1 = #0#1;
     #908#2 = #0#2;
     #908#3 = #0#3;
     #908#4 = #2#1;
     #908#5 = #2#2;

     |ABRE #6;
     |SI enModo ! 1;
         #6#0 = #908#1;
         #6#1 = #908#4;
         #6#2 = eaPracti;
         |LEE 101#6=;
         |SI FSalida ! 0;  |DDEFECTO #6; |FINSI;

         #908#6 = #6#2;
         #908#7 = #6#3;

         |C_M #908#6, 1, "N";
     |FINSI;

     |CABEZA "Bloques";

     |PINPA #0#908;  nPanta0 = FSalida;
     |PINDA #0#908;

     nRango = 4 + 8 + 16 + 32;
     nPos1  = 1202;
     nPos2  = 2675;

     |LINEAL_SIMPLE #1#7, nRango, nPos1, nPos2, BajaPanta0, AltaPanta0, EventoPanta0;
     nGrid1 = FSalida;

     |HAZ BotonesPanta0;

     |SI enModo = 1;  |ENDATOS #1#908;  |FINSI;
     |SI enModo = 2;  |ENDATOS #2#908;  |FINSI;

     |SI FSalida = 0;
         |HAZ GrabaPractica;
     |FINSI;
     |CIERRA #6;

     |FIN_CONTROL_WINDOWS nBoton0_1;
     |FIN_CONTROL_WINDOWS nBoton0_2;
     |FIN_CONTROL_WINDOWS nBoton0_3;

     |DESTRUYECONTROL nGrid1;

     |PULSATECLA;
|FPRO;
