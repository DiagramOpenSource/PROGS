|FICHEROS;
     dsorm000 #0;
     dsorz008 #1;
|FIN;

|VARIABLES;
     aDirDestino    = "";
     aDirOrigen     = "";
     aBase          = "";
     aAlfa          = "";
     aOrigen        = "";
     aDestino       = "";
     aFichero       = "";
|FIN;

|PROCESO Todo;
     |C_M #1#3, 1, "S";
     |C_M #1#4, 1, "S";
     |C_M #1#5, 1, "S";
     |C_M #1#6, 1, "S";
     |C_M #1#7, 1, "S";
     |C_M #1#8, 1, "S";
     |C_M #1#9, 1, "S";

     |SI #1#2 = "S";
         #1#3  = "S";  |C_M #1#3, 1, "N";  |PINTA #1#3;
         #1#4  = "S";  |C_M #1#4, 1, "N";  |PINTA #1#4;
         #1#5  = "S";  |C_M #1#5, 1, "N";  |PINTA #1#5;
         #1#6  = "S";  |C_M #1#6, 1, "N";  |PINTA #1#6;
         #1#7  = "S";  |C_M #1#7, 1, "N";  |PINTA #1#7;
         #1#8  = "S";  |C_M #1#8, 1, "N";  |PINTA #1#8;
         #1#9  = "S";  |C_M #1#9, 1, "N";  |PINTA #1#9;
     |FINSI;
|FPRC;

|PROCESO CopiaFicheros;
     aAlfa = aFichero % -103;
     |SI aAlfa = "txt";
         aOrigen  = aDirOrigen  + aFichero;
         aDestino = aDirDestino + aFichero;
         |COPIA_FICHERO aOrigen, aDestino;
     |SINO;
         aOrigen  = aDirOrigen  + aFichero + ".dat";
         aDestino = aDirDestino + aFichero + ".dat";
         |COPIA_FICHERO aOrigen, aDestino;

         aOrigen  = aDirOrigen  + aFichero + ".ddx";
         aDestino = aDirDestino + aFichero + ".ddx";
         |COPIA_FICHERO aOrigen, aDestino;

         aOrigen  = aDirOrigen  + aFichero + ".ixx";
         aDestino = aDirDestino + aFichero + ".ixx";
         |COPIA_FICHERO aOrigen, aDestino;
     |FINSI;
|FPRC;

|PROCESO Tipo3;  |TIPO 3;
     |DFICE aDirDestino;       |QBF aDirDestino;
     |DBASE aBase;             |QBF aBase;
     aBase = aBase + "fich/";

     aDirOrigen = aBase + (("000000" + #0#0) % -106) + "/";

     |SI #1#3 = "S"; aFichero = "dslisdef.txt"; |HAZ CopiaFicheros; |FINSI;
     |SI #1#4 = "S"; aFichero = "dsorm001";     |HAZ CopiaFicheros; |FINSI;
     |SI #1#5 = "S"; aFichero = "dsorm002";     |HAZ CopiaFicheros; |FINSI;
     |SI #1#6 = "S"; aFichero = "dsorm003";     |HAZ CopiaFicheros; |FINSI;
     |SI #1#7 = "S"; aFichero = "dsorm004";     |HAZ CopiaFicheros; |FINSI;
     |SI #1#8 = "S"; aFichero = "dsorm005";     |HAZ CopiaFicheros; |FINSI;
     |SI #1#9 = "S"; aFichero = "dsorm006";     |HAZ CopiaFicheros; |FINSI;
     |SI #1#9 = "S"; aFichero = "dsorm007";     |HAZ CopiaFicheros; |FINSI;
     |SI #1#9 = "S"; aFichero = "dsorm008";     |HAZ CopiaFicheros; |FINSI;
     |SI #1#9 = "S"; aFichero = "dsorm009";     |HAZ CopiaFicheros; |FINSI;
     |SI #1#9 = "S"; aFichero = "dsorm010";     |HAZ CopiaFicheros; |FINSI;
|FPRC;
