|FICHEROS;
     dsorz006 #0;
|FIN;

|VARIABLES;
     aRuta       = "";
     aPath       = "";
     aGrupo      = "";
     aParametro1 = "";
     aParametro2 = "";
     aOrigen     = "";
     aDestino    = "";
     aIPRemoto   = "";
     aAlfa       = "";
     aAlfa1      = "";
     aLong       = "";
     aParametro  = "";
     aFichGrupo  = "";

     nBoton1     = 0;
     nLong       = 0;
|FIN;

|PROCESO Browse;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     aRuta = #0#0;  |QBF aRuta;
     aRuta = "D" + aRuta;

     |BROWSE aRuta;

     aAlfa  = aRuta % 299;  |QBF aAlfa;
     aAlfa1 = aAlfa % -101;
     |SI aAlfa1 = "\";
         aLong = aAlfa % 0;
         nLong = aLong;
         nLong = nLong - 1;
         nLong = 100 + nLong;
         aAlfa = aAlfa % nLong;
     |FINSI;

     #0#0 = aAlfa;
     #0#0 = #0#0 > #0#0;  |PINTA #0#0;
|FPRC;

|PROCESO Boton1;
     |PTEC 824;
|FPRC;

|PROCESO Tipo3;  |TIPO 3;
     |DBASS aGrupo;  |QBF aGrupo;

     aPath       = aGrupo;  |QBF aPath;
     aParametro1 = aPath + "zdatogru" + " grupos";
     aParametro2 = aPath;
     |ATAR aParametro1, aParametro2;

     aParametro1 = aPath + "zdatogru";
     |COMPRIME aParametro1;

     |FBORRA aParametro1;

     aOrigen   = aGrupo + "zdatogru.gz";
     aDestino  = #0#0 + "\zdatogru.gz";  |QBT aDestino;
     aIPRemoto = "";
     |IP_REMOTO aIPRemoto;
     |SI aIPRemoto = "";
         |COPIA_FICHERO aOrigen, aDestino;
     |SINO;
         |ENVIA_FICHERO aOrigen, aDestino;
     |FINSI;
     |FBORRA aOrigen;
|FPRC;

|PROGRAMA;
     aParametro = PARAMETRO;  |QBF aParametro;

     |SI aParametro = "";
         |CLS;
         |CABEZA "Instalador";
         |PINPA #0#0;
         |PINDA #0#0;

         |CONTROL_SIMPLE 0, "BOTON,...", 1076, 1179, Boton1;
         nBoton1 = FSalida;

         |ENDATOS #1#0;

         |FIN_CONTROL_WINDOWS nBoton1;
         |PULSATECLA;
     |SINO;
         aAlfa = "C:\TMP";   |RMKDIR aAlfa;
         #0#0 = aAlfa;
         |HAZ Tipo3;
     |FINSI;
|FPRO;
