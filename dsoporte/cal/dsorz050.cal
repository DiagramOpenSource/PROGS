|FICHEROS;
     dsorz050 #0;
|FIN;

|VARIABLES;
     aBase      = "";
     aAbre      = "";
     aAlfa      = "";
     aAlfa1     = "";

     nHandle    = 0;
|FIN;

|PROCESO Tipo3;  |TIPO 3;
     |SI #0#3 ! "SI";  |ACABA;  |FINSI;

     |DBASS aBase; |QBF aBase;
     aAbre  = aBase + "dev/usr/ds_sys.smf";
     |FBORRA aAbre;
|FPRC;

|PROGRAMA;
     |DBASS aBase; |QBF aBase;
     aAbre  = aBase + "dev/usr/ds_sys.smf";
     |XABRE "E", aAbre, nHandle;
     |SI FSalida < 0;
         |MENAV "    No hay Bloqueo de Usuarios.";
         |ACABA;
     |FINSI;


     |XABRE "U", aAbre, nHandle;
     |XLEE nHandle, 250, aAlfa;

                             #0#0 = aAlfa % 1108;
     aAlfa1 = aAlfa % 2810;  #0#1 = aAlfa1;
                             #0#2 = aAlfa % 4608;

     |XCIERRA nHandle;

     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
|FPRO;
