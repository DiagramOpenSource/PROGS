|FICHEROS;
     dscurm08 #8;
     dscurm09 #9;

     dscurm08@ #999;
|FIN;

|VARIABLES;
     nCampo  = 0;
     nCampoH = 0;

     fFecha  = @;

     aBase   = "";
     aMas    = "";
|FIN;

|PROCESO NoEntra;
     |SI Campo ! 2;  |Y Campo ! 38;
         |ACABA;
     |FINSI;

     nCampoH = Campo + 8;
     |PARA nCampo = Campo + 1;  |SI nCampo [ nCampoH;  |HACIENDO nCampo = nCampo + 1;
          |SI #8Campo = "N";
              #8nCampo = 00;  |PINTA #8nCampo;
              |C_M #8nCampo, 1, "N";
          |SINO;
              |C_M #8nCampo, 1, "S";
          |FINSI;
     |SIGUE;
|FPRC;

|PROCESO CapaHora;
     |SI #8Campo < 0;  |O #8Campo > 23;
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO CapaMinuto;
     |SI #8Campo < 0;  |O #8Campo > 59;
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO PonDia;  |TIPO 7;
     fFecha = #9#1 % 1;
     #9#2   = (fFecha % 1109) > " ";
     |PINTA #9#2;
|FPRC;

|PROCESO Seccion;  |TIPO 0;
     |DBASE aBase;  |QBF aBase;
     aMas = aBase + "def/dscurm08.mas";
     |CARGA_DEF aMas, dscurm08@;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |ABRE #999;
     |SELECT #2#999;
     #999#66 = #8#66;
     |LEE 000#999=;
     |SI FSalida = 0;  |Y #999#0 ! #8#0;
         |MENAV "0000 La Seccion ya existe en otro Calendario. Solo puede haber un Calendario por Seccion.";
         |ERROR;
     |FINSI;
     |CIERRA #999;

     |DESCARGA_DEF #dscurm08@;
|FPRC;

