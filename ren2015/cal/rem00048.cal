|PROCESO Tipo12Fw648;  |TIPO 12;
     |SI FSalida = 7;  |ACABA;  |FINSI;

     aAlfa = #648#2;  |QBF aAlfa;
     |SI aAlfa = "";
         |MENAV "0000No se puede validar la ficha, no hay contribuyente introducido";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO LetraNif;  |TIPO 6;
     |SI FSalida = 10;
         eaVarDni  = #648Campo;
         enCalcCif = 1;
         |HAZ CalculoDNI;
         #648Campo = eaVarDni;
         |PINTA #648Campo;
         |ERROR;
         |ACABA;
     |FINSI;

     #rew00648#22 = "S";
     eaVarDni     = #648Campo;
     enCalcCif    = 0;
     |HAZ CalculoDNI;
     eaVarDni = (eaVarDni + "                    ") % 120;
     |SI #648Campo ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #648Campo = eaVarDni;  |PINTA #648Campo;
             |ERROR;
             |ACABA;
         |FINSI;

         |CONFI "0000NEs un N£mero de Identificaci¢n de otro pais";
         |SI FSalida = 0;
             #rew00648#22 = "N";
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO TotalFw648;
     |SI #648#16 = "1";  #648#13 = 10.05;  |FINSI;
     |SI #648#16 = "2";
         #648#13 = 20;
         |SI #648#18 < "07.05.2011";
             #648#13 = 10;
         |FINSI;
     |FINSI;

     |PINTA #648#13;

     #648#14 = #648#12 % #648#13;
     |PINTA #648#14;
|FPRC;

