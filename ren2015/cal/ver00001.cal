|FICHEROS;
     ver00001;

     retabla1;
     retabla2;
     rem00500;
     rem00008;
|FIN;

|VARIABLES;
     aDef         = "";
     aMas         = "";
     aBase        = "";
     aPerceptor   = "";
     aAlfa        = "";
     aAlfa1       = "";
     aAlfa2       = "";

     nSwHay       = 0;
     nLimite      = 0;
     nPorce       = 0;
     nCanti1      = 0;
     nCanti2      = 0;
     nCanti3      = 0;
     nCanti4      = 0;
     nDias        = 0;
     nValor       = 0;
     nRecalculo   = 0;

     &enError     = 0;
|FIN;

|| ************************************************************************
|| PROCESOS DE CAMBIO DE VERSION
|| ************************************************************************

|PROCESO MiraPatrones;
     nSwHay = 0

     |ABRE #retabla1;
     |LEE 040#retabla1.p;
     |SI FSalida ! 0;  |O #retabla1#5 = 0;
         |CIERRA #retabla1;  |DELINDEX #retabla1;

         aAlfa = "*:ren2015/zacceso9;" + "00000PATRONES               ";
         |SUB_EJECUTA aAlfa;
     |SINO;
         |CIERRA #retabla1;
     |FINSI;


     |ABRE #rem00500;
     #rem00500#0 = 3;
     |LEE 000#rem00500.=;
     |SI FSalida ! 0;  |O #rem00500#5 ! 0.22;
          |CIERRA #rem00500;  |DELINDEX #rem00500;

          aAlfa = "*:ren2015/zacceso9;" + "00000PATRONES       rem00500";
          |SUB_EJECUTA aAlfa;
     |FINSI;
     |CIERRA #rem00500;

     |ABRE #rem00008;
     #rem00008#0 = 7;
     #rem00008#1 = 821;
     |LEE 000#rem00008.=;
     |SI FSalida ! 0;  |O #rem00008#9 ! 150;
          |CIERRA #rem00008;  |DELINDEX #rem00008;
          aAlfa = "*:ren2015/zacceso9;" + "00000PATRONES       rem00008";
          |SUB_EJECUTA aAlfa;

          |CIERRA #retabla2;  |DELINDEX #retabla2;
          aAlfa = "*:ren2015/zacceso9;" + "00000PATRONES       retabla2";
          |SUB_EJECUTA aAlfa;
     |FINSI;
     |CIERRA #rem00008;

     |ABRE #retabla2;
     #retabla2#0 = 8;
     |LEE 000#retabla2.=;
     |SI FSalida ! 0;  |O #retabla2#5 ! 10;
         |CIERRA #retabla2;  |DELINDEX #retabla2;

         aAlfa = "*:ren2015/zacceso9;" + "00000PATRONES       retabla2";
         |SUB_EJECUTA aAlfa;
     |SINO;
         |CIERRA #retabla2;
     |FINSI;

     |ABRE #retabla1;
     |LEE 040#retabla1.p;
     |SI FSalida ! 0;  |O #retabla1#5 = 0;
         enError = 1;
         |CIERRA #retabla1;  |DELINDEX #retabla1;
     |SINO;
         |CIERRA #retabla1;
     |FINSI;

     |ABRE #rem00500;
     #rem00500#0 = 3;
     |LEE 000#rem00500.=;
     |SI FSalida ! 0;  |O #rem00500#5 ! 0.22;
          |CIERRA #rem00500;  |DELINDEX #rem00500;

          enError = 1;
     |SINO;
          |CIERRA #rem00500;
     |FINSI;

     |ABRE #rem00008;
     #rem00008#0 = 7;
     #rem00008#1 = 821;
     |LEE 000#rem00008.=;
     |SI FSalida ! 0;  |O #rem00008#9 ! 150;
          |CIERRA #rem00008;  |DELINDEX #rem00008;

          enError = 1;
     |SINO;
          |CIERRA #rem00008;
     |FINSI;

     |ABRE #retabla2;
     #retabla2#0 = 8;
     |LEE 000#retabla2.=;
     |SI FSalida ! 0;  |O #retabla2#5 ! 10;
         |CIERRA #retabla2;  |DELINDEX #retabla2;

         enError = 1;
     |SINO;
         |CIERRA #retabla2;
     |FINSI;
|FPRC;

|PROGRAMA;
     |MENSA "0000Comprobando configuracion patrones.";
     enError = 0;
     |HAZ MiraPatrones;
|FPRO;
