     |SI #0#13 = "F"; #1#20 = "X";  |FINSI;
     |SI #1#19 = "X"; #1#20 = " ";  |FINSI;

     |GRABA 020#1n;
     |LIBERA #1;

     #2#0 = #1001#0;
     #2#1 = #1001#1;
     #2#2 = #1001#2;
