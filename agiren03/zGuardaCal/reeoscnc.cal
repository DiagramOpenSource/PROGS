|FICHEROS;
   reeoscnc #0;

   eosconce@ #1;
   eosconc6@ #2;
   agicontr@ #3;
|FIN;

|VARIABLES;
   Comodin  = "";
   Comodin1 = "";
   ComodNum = 0;
   Long     = 0;
   LongAlfa = "";
   FEstado  = 0;
|FIN;

|PROCESO MiraLong;
   |SI FSalida = 10;
      |CONSULTA_CLAVE #1#eosconce@;
      |SI FSalida < 0; |ERROR; |ACABA; |FINSI;
      Comodin = ("----" + ("00" + #eosconce@#0) % -102);
      #0#1 = Comodin;
      #0#2 = #eosconce@#1;
      |PINTA #0#1;
      |PINTA #0#2;
      |ACABA;
   |FINSI;
   |SI FSalida = 11;
      |CONSULTA_CLAVE #1#eosconc6@;
      |SI FSalida < 0; |ERROR; |ACABA; |FINSI;
      #0#1 = #eosconc6@#0;
      #0#2 = #eosconce@#1;
      |PINTA #0#1;
      |PINTA #0#2;
      |ACABA;
   |FINSI;
   Comodin = #0#1; Comodin = Comodin - "----";
   |QBF Comodin;
   LongAlfa = Comodin % 0;
   Long = LongAlfa;
   |SI Long [ 2;
      ComodNum = Comodin;
      #eosconce@#0 = ComodNum;
      |LEE 010#eosconce@.=;
      |SI FSalida ! 0;
         |MENAV "    Concepto inexistente";
         |ERROR;
         |ACABA;
      |FINSI;
      #0#2 = #eosconce@#1;
      |PINTA #0#2;
      Comodin = "----" + Comodin;
      #0#1 = Comodin;
      |PINTA #0#1;
   |SINO;
      #eosconc6@#0 = Comodin;
      |LEE 010#eosconc6@.=;
      |SI FSalida ! 0;
         |MENAV "    Concepto inexistente";
         |ERROR;
         |ACABA;
      |FINSI;
      #0#2 = #eosconc6@#1;
      |PINTA #0#2;
   |FINSI;
|FPRC;

|PROCESO PonTabla; |TIPO 7;
