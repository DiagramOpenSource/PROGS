|PROCESO Hoja17;
   nSwDatos = 0; nCalc1 = 0;
   aTipo = VTipo % 0110;

   Cadena = "<T10017> ";

   TipoDato = "N"; Long = 11; LongDec = 2;
   #rem01001#0 = VEmpresa;
   #rem01001#1 = 0;
   #rem01001#2 = VTipo;
   |LEE 000#rem01001.=;
   |SI FSalida = 0;
      nTotal = 0;
      || Liquidacion : Resultado a ingresar o a devolver
      Bloque = 25;     Casilla = "1  "; |HAZ BuscaCasilla;
      |SI SwError = 1; Casilla = "2  "; |HAZ BuscaCasilla; |FINSI;
      |SI SwError = 1;
         Bloque = 23; Casilla = "116"; |HAZ BuscaCasilla; nTotal = Casilla; SwError = 1;
         nSusp = 0;
      |SINO;
         nSusp = 1;
         nTotal = #rem01001#16;
      |FINSI;

      aAlfa = nTotal; |HAZ Formateo; Cadena = Cadena + aAlfa;
      || Liquidacion : Solicitud de suspension del ingreso conyuge/renuncia a la devol.otro conyuge
      aAlfa = nSusp; Cadena = Cadena + aAlfa;
      || Declaracion complementaria : Resultado de la decl.complementaria
      Bloque = 24;     Casilla = "119"; |HAZ BuscaCasilla; nCalc = Casilla;
      aAlfa = Casilla; |HAZ Formateo;  Cadena = Cadena + aAlfa;
      |SI nCalc ! 0; nTotal = nCalc; |FINSI;
      |SI nTotal > 0;
         || Ingreso : Fracciona o no el pago
         |SI #rem01001#8 = "S"; aAlfa = "01"; |SINO; aAlfa = "10"; |FINSI;
         Cadena = Cadena + aAlfa;
         || Ingreso : Importe del ingreso
         nCalc1 = #rem01001#16;
         |SI #rem01001#8 = "S"; nCalc1 = (nCalc1 % 60) ! 2; |FINSI;
         aAlfa = nCalc1; |HAZ Formateo; Cadena = Cadena + aAlfa;
         || Ingreso : Forma de pago
         |SI #rem01001#9 = "S"; aAlfa = "10"; |SINO; aAlfa = "01"; |FINSI;
         Cadena = Cadena + aAlfa;
         || Ingreso : Codigo cuenta
         |SI #rem01001#9 = "S";
            Cadena = Cadena + ("0" * 20);
         |SINO;
            Cadena = Cadena + aCuenta1;
         |FINSI;
         || Opciones de pago 2§ plazo : Si domicilia o no el 2§ pago
         |SI #rem01001#8 = "S";
            |SI #rem01001#10 = "S"; aAlfa = "01"; |SINO; aAlfa = "10"; |FINSI;
         |SINO;
            aAlfa = "00";
         |FINSI;
         Cadena = Cadena + aAlfa;
         || Opciones de pago 2§ plazo : Importe 2§ plazo
         nCalc1 = 0;
         |SI #rem01001#8 = "S"; |Y #rem01001#10 = "S"; nCalc1 = (#rem01001#16 % 40) ! 2; |FINSI;
         aAlfa = nCalc1; |HAZ Formateo; Cadena = Cadena + aAlfa;
         || Opciones de pago 2§ plazo : Codigo cuenta
         |SI #rem01001#8 = "S"; |Y #rem01001#10 = "S";
            Cadena = Cadena + aCuenta1;
         |SINO;
            Cadena = Cadena + ("0" * 20);
         |FINSI;
      |SINO;
         || Ingreso : Fracciona o no el pago
         Cadena = Cadena + "00";
         || Ingreso : Importe del ingreso
         aAlfa = 0; |HAZ Formateo; Cadena = Cadena + aAlfa;
         || Ingreso : Forma de pago
         Cadena = Cadena + "00";
         || Ingreso : Codigo cuenta
         Cadena = Cadena + ("0" * 20);
         || Opciones de pago 2§ plazo : Si domicilia o no el 2§ pago
         Cadena = Cadena + "00";
         || Opciones de pago 2§ plazo : Importe 2§ plazo
         aAlfa = 0; |HAZ Formateo; Cadena = Cadena + aAlfa;
         || Opciones de pago 2§ plazo : Codigo cuenta
         Cadena = Cadena + ("0" * 20);
      |FINSI;
      |SI nTotal < 0;
         nTotal = 0 - nTotal; aAlfa = "";
         || Devolucion : Renuncia a la devolucion o Devolucion por transferencia
         |SI #rem01001#11 = "X"; Cadena = Cadena + "1"; |SINO; Cadena = Cadena + "0"; |FINSI;
         |SI #rem01001#12 = "X"; Cadena = Cadena + "1"; |SINO; Cadena = Cadena + "0"; |FINSI;
         || Devolucion : Importe de la devolucion
         aAlfa = nTotal; |HAZ Formateo; Cadena = Cadena + aAlfa;
         || Devolucion : Codigo cuenta
         |SI #rem01001#12 = "X";
            Cadena = Cadena + aCuenta1;
         |SINO;
            Cadena = Cadena + ("0" * 20);
         |FINSI;
      |SINO;
         || Devolucion : Renuncia a la devolucion o Devolucion por transferencia
         Cadena = Cadena + "00";
         || Devolucion : Importe de la devolucion
         aAlfa = 0; |HAZ Formateo; Cadena = Cadena + aAlfa;
         || Devolucion : Codigo cuenta
/*
         |SI nSusp = 1; |Y nTotal = 0;
            Cadena = Cadena + aCuenta1;
         |SINO;
*/
            Cadena = Cadena + ("0" * 20);
         ||FINSI;
      |FINSI;
      Cadena = Cadena + "</T10017>" + &13 + &10; |HAZ GrabaCad;
   |FINSI;
|FPRC;

