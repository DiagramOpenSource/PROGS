|PROCESO Seleccion2;
     |SI #0#2 = "GANANCIAL ";                || Si es Ganancial
         nCoefi = 50;  eaContri = "TITULAR   ";   |HAZ Liquida2;
         nCoefi = 50;  eaContri = "CONYUGE   ";   |HAZ Liquida2;
         nCoefi = 100; eaContri = "CONJUNTA  ";   |HAZ Liquida2;

