|PROCESO LiquidaE4;
     enSwAcum  = 1;
     enBloque  = 29;
     eaCasilla = "115";  enImporte = (#0#11 + #0#12 + #0#13 + #0#14) % nCoefi;
     |HAZ GrabaLineal;

     |SI #0#17 = "N";
         enSwAcum  = 1;
         enBloque  = 29;
         eaCasilla = "114";  enImporte = #0#15 % nCoefi;   |HAZ GrabaLineal;
     |FINSI;

     |SI #0#17 = "S";
         #1#0 = #1001#0;
         #1#1 = #1001#1;
         #1#2 = #1001#2;
         |LEE 101#1=;
         |SI FSalida ! 0;
             |DDEFECTO #1;
             #1#0 = #1001#0;
             #1#1 = #1001#1;
             #1#2 = #1001#2;
             |GRABA 020#1b;
         |FINSI;

         #1#20  = (N\ #1#20 + (#0#15 % nCoefi));

         |GRABA 020#1a;
         |LIBERA #1;

         enSwAcum  = 0;
         eaCasilla = "116";  enImporte = #1#20;   |HAZ GrabaLineal;
     |FINSI;
|FPRC;

