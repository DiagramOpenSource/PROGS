|FICHEROS;
     trasm001 #1;
     trasm002 #2;
|FIN;

|VARIABLES;
     aParametro = "";
     aFichero   = "";
|FIN;

|RUTINA ClaveBaja1;
     #1#0 = aParametro;
     #1#1 = 1;
|FRUT;

|RUTINA ClaveAlta1;
     #1#0 = aParametro;
     #1#1 = 999999999;
|FRUT;

|RUTINA ClaveBaja2;
     #2#0 = #1#0;
     #2#1 = #1#1;
     #2#2 = 1;
|FRUT;

|RUTINA ClaveAlta2;
     #2#0 = #1#0;
     #2#1 = #1#1;
     #2#2 = 999;
|FRUT;

|PROCESO Tipo11;  |TIPO 11;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     |ENTLINEAL #1#2, -3, 4, 22, 0, ClaveBaja2, ClaveAlta2;
|FPRC;

|PROCESO Tipo7;  |TIPO 7;
     |ENTLINEAL #1#2, -3, 7, 22, 0, ClaveBaja2, ClaveAlta2;
|FPRC;

|PROGRAMA;
     aParametro = PARAMETRO;  |QBF aParametro;
     aFichero   = (aParametro % 108) < "";
     |QBF aFichero;
     |NOME_DAT #2 aFichero;

     |ENTLINEAL #1#1, -2, 4, 22, 1, ClaveBaja1, ClaveAlta1;
|FPRO;
