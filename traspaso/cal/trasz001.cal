|FICHEROS;
     trasz001 #0;
     trasm001 #1;
|FIN;

|VARIABLES;
     aBase    = "";

     &enError        = 0;
     &enComprobacion = 0;
|FIN;

|PROCESO Tipo7;  |TIPO 7;
     |DBASE aBase;  |QBF aBase;
     #0#1 = aBase + "pasopi/";
     |PINTA #0#1;
|FPRC;

|PROCESO Tipo3;  |TIPO 3;
     enComprobacion = 1;
     |HAZ PasoNominas;     |SI enError = 1;  |ACABA;  |FINSI;
     |HAZ PasoEos;         |SI enError = 1;  |ACABA;  |FINSI;
     |HAZ PasoSociedades;  |SI enError = 1;  |ACABA;  |FINSI;
     |HAZ PasoMinutacion;  |SI enError = 1;  |ACABA;  |FINSI;

     |PINPA #0#0;

     enComprobacion = 0;
     |ABRE #1;  |CIERRA #1;  |DELINDEX #1;

     |ABRE #1;

     |HAZ PasoNominas;
     |SI enError = 1;  |CIERRA #1;  |DELINDEX #1;  |ACABA;  |FINSI;

     |HAZ PasoEos;
     |SI enError = 1;  |CIERRA #1;  |DELINDEX #1;  |ACABA;  |FINSI;

     |HAZ PasoSociedades;
     |SI enError = 1;  |CIERRA #1;  |DELINDEX #1;  |ACABA;  |FINSI;

     |HAZ PasoMinutacion;
     |SI enError = 1;  |CIERRA #1;  |DELINDEX #1;  |ACABA;  |FINSI;

     |CIERRA #1;
     |MENAV "     Pase Realizado";
|FPRC;
