|FICHEROS;
     dsay0009 #0;
     dsam0000 #1;
     dsam0116 #2;
|FIN;

|VARIABLES;
     nCampo  = 0;
     nSwHay  = 0;
     nSigue  = 0;

     nSwSi   = 0;
     nPara1  = 0;
     aDSit   = "";
     aHSit   = "";
     nDTEm   = 0;
     nHTEm   = 0;
|FIN;

|PROCESO Seleccion;
     |SI #0#2 = "D";
         |C_M #0#3, 1, "S";
         |C_M #0#4, 1, "S";
         |PARA nCampo = 5;  |SI nCampo [ 16;  |HACIENDO nCampo = nCampo + 1;
               |C_M #0nCampo, 1, "N";  #0nCampo = 0;  |PINTA #0nCampo;
         |SIGUE;
     |SINO;
         |C_M #0#3, 1, "N";  #0#3 = 0;        |PINTA #0#3;
         |C_M #0#4, 1, "N";  #0#4 = 99999;    |PINTA #0#4;
         |PARA nCampo = 5;  |SI nCampo [ 16;  |HACIENDO nCampo = nCampo + 1;
               |C_M #0nCampo, 1, "S";
         |SIGUE;
     |FINSI;
|FPRC;

|PROCESO NoMasBancos;
     |SI #0Campo = 0;
         |PARA nCampo = Campo + 1;  |SI nCampo [ 16;  |HACIENDO nCampo = nCampo + 1;
               #0Campo = 0;  |C_M #0nCampo, 1, "N";  |PINTA #0Campo;
         |SIGUE;
     |SINO;
         |PARA nCampo = Campo + 1;  |SI nCampo [ 16;  |HACIENDO nCampo = nCampo + 1;
               |C_M #0nCampo, 1, "S"; |PINTA #0Campo;
         |SIGUE;
     |FINSI;
|FPRC;

|PROCESO Seleccion_TE;
     |SI #0#18 = "D";
         |C_M #0#19, 1, "S";
         |C_M #0#20, 1, "S";
         |PARA nCampo = 21;  |SI nCampo [ 32;  |HACIENDO nCampo = nCampo + 1;
               |C_M #0nCampo, 1, "N";  #0nCampo = -1;  |PINTA #0nCampo;
         |SIGUE;
     |SINO;
         |C_M #0#19, 1, "N";  #0#19 = 0;      |PINTA #0#3;
         |C_M #0#20, 1, "N";  #0#20 = 99;     |PINTA #0#4;
         |PARA nCampo = 21;  |SI nCampo [ 22;  |HACIENDO nCampo = nCampo + 1;
               |C_M #0nCampo, 1, "S";
         |SIGUE;
     |FINSI;
|FPRC;

|PROCESO NoMasEmpresas;
     |SI #0Campo = -1;
         |PARA nCampo = Campo + 1;  |SI nCampo [ 32;  |HACIENDO nCampo = nCampo + 1;
               #0Campo = -1;  |C_M #0nCampo, 1, "N";  |PINTA #0Campo;
         |SIGUE;
     |SINO;
         |PARA nCampo = Campo + 1;  |SI nCampo [ 32;  |HACIENDO nCampo = nCampo + 1;
               |C_M #0nCampo, 1, "S"; |PINTA #0Campo;
         |SIGUE;
     |FINSI;
|FPRC;

|| *************************************************************************

|PROCESO Imprimelos;
     |SI #0#18 = "P";
         nSwSi = 0;
         |PARA nPara1 = 21; |SI nPara1 [ 32; |HACIENDO nPara1 = nPara1 + 1;
               |SI #1#9 = #0nPara1;
                   nSwSi = 1;
                   |SAL;
               |FINSI;
         |SIGUE;

         |SI nSwSi = 0; |ACABA; |FINSI;
     |FINSI;

     |SI #0#17 = "N";
         |SI #0#2 = "D";
             |SI #1#88 < #0#3;  |O #1#88 > #0#4;
                 |ACABA;
             |FINSI;
         |SINO;
             nSigue = 0;
             |PARA nCampo = 5;  |SI nCampo [ 16;  |HACIENDO nCampo = nCampo + 1;
                   |SI #0nCampo ! 0;
                       |SI #1#88 = #0nCampo;
                           nSigue = 1;
                       |FINSI;
                   |FINSI;
             |SIGUE;
             |SI nSigue = 0;  |ACABA;  |FINSI;
         |FINSI;
     |FINSI;

     |PRINT;
     nSwHay = 1;
|FPRC;

|DEFBUCLE dsam0000T;
     #1#1;
     9, 85;
     #0#0, nDTEm, aDSit;
     #0#1, nHTEm, aHSit;
     ;
     NULL, Imprimelos;
|FIN;

|DEFBUCLE dsam0000;
     #1#1;
     88, 9, 85;
     #0#0, #2#0, nDTEm, aDSit;
     #0#1, #2#0, nHTEm, aHSit;
     ;
     NULL, Imprimelos;
|FIN;

|PROCESO MiraClientes;
     nSwHay = 0;
     |BUCLE dsam0000;
     |SI nSwHay = 1;  |PIEINF;  |FINSI;
|FPRC;

|DEFBUCLE dsam0116;
     #2#1;
     ;
     #0#3;
     #0#4;
     ;
     NULL, MiraClientes;
|FIN;

|PROCESO Tipo3;  |TIPO 3;

 || ...  Situacion Activa/Inactiva
     |SI #0#33 = "X";
         aDSit = "A"; aHSit = "Z";
     |SINO;
         aDSit = #0#33; aHSit = #0#33;
     |FINSI;

 || ... Tipo Empresa
      |SI #0#18 = "D";
          nDTEm = #0#19; nHTEm = #0#20;
      |SINO;
          nDTEm = 0; nHTEm = 99;
      |FINSI;

     |IMPRE 0;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |SI #0#17 = "S";
         |INFOR "dsay1009";
         |SI FSalida ! 0;  |FINIMP;  |ACABA;  |FINSI;

         |SI #0#2 = "D";
             |BUCLE dsam0116;
         |SINO;
             |PARA nCampo = 5;  |SI nCampo [ 16;  |HACIENDO nCampo = nCampo + 1;
                   |SI #0nCampo ! 0;
                       #0#3 = #0nCampo;
                       #0#4 = #0nCampo;
                       |BUCLE dsam0116;
                   |FINSI;
             |SIGUE;
         |FINSI;
     |SINO;
         |INFOR "dsay2009";
         |SI FSalida ! 0;  |FINIMP;  |ACABA;  |FINSI;

         |BUCLE dsam0000T;
         |SI nSwHay = 1;  |PIEINF;  |FINSI;
     |FINSI;

     |FININF;
     |FINIMP;
|FPRC;
