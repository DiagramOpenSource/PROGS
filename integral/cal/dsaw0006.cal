|FICHEROS;
     :integral/dsam0000;
     :integral/dsaw0006,MANTE;
|FIN;

|VARIABLES;
     &enEmpresa  = 0;
     &enAnyo     = 0;
     &enModo     = 0;
     &enModo1    = 0;
     &enEjerBaja = 0;

     &eaCw61     = "";
     &eaCw62     = "";
     &eaCw63     = "";
|FIN;

|PROCESO Tipo3w06;  |TIPO 3;
     eaCw61  = #dsaw0006#1;
     eaCw62  = #dsaw0006#2;
     eaCw63  = #dsaw0006#3;

     enModo1 = (FEntrada / 100) ! 0;
     |SI enModo1 = 1; |Y enEjerBaja > 0;
         |SI #dsaw0006#0 > enEjerBaja;
             |MENAV "    Atencion. Ejercicio Posterior a la fecha de baja de la Empresa.";
             |ERROR;
             |ACABA;
         |FINSI;
     |FINSI;
     enEmpresa = #dsam0000#0;
     enAnyo    = #dsaw0006#0;
     |SUB_EJECUTA ":basica/agim0033";
     #dsaw0006#1 = eaCw61; |PINTA #dsaw0006#1;
     #dsaw0006#2 = eaCw62; |PINTA #dsaw0006#2;
     #dsaw0006#3 = eaCw63; |PINTA #dsaw0006#3;
|FPRC;

|PROCESO Tipo2w06;  |TIPO 2;
     enModo1 = (FEntrada / 100) ! 0;
     |SI enModo1 ! 3;  |ACABA;  |FINSI;

     enEmpresa = #dsam0000#0;
     enAnyo    = #dsaw0006#0;
     |SUB_EJECUTA ":basica/agim0033";
     enModo1 = 0;
|FPRC;
