
------------------- CALCULO LINEAL DE POBLACIONES ---------------------------

|FICHEROS;
      :/integral/def/dsam0113 #113,MANTE;
|FIN;

|VARIABLES;
      &enCodProv    = 0;
      &enCodPobl    = 0;
      &eaPoblacion  = "";
      &eaPoblaParri = "";
      &enModoPob    = 1;
|FIN;

|PROCESO PulsoIntro;
     |SI enModoPob = 0;  |ACABA;  |FINSI;
     |SI FSalida ! 0;    |ACABA;  |FINSI;

     eaPoblaParri = #113#3;
     |PTEC 807;
|FPRC;

|RUTINA ClaveBaja113_2;
     #113#0 = enCodProv;
     #113#1 = enCodPobl;
     #113#2 = 1;
|FRUT;

|RUTINA ClaveAlta113_2;
     #113#0 = enCodProv;
     #113#1 = enCodPobl;
     #113#2 = 99;
|FRUT;

|RUTINA ClaveBaja113_1;
     |SI FSalida = "POSICION";
         |ABRE #113;
         |SELECT #2#113;
         #113#0 = enCodProv;
         #113#1 = enCodPobl;
         #113#3 = eaPoblacion;
         |LEE 040#113=;
         |SI FSalida ! 0;  |DDEFECTO #113;  |FINSI;
         |CIERRA #113;
         |ACABA;
     |FINSI;

     #113#0 = enCodProv;
     #113#1 = enCodPobl;
     #113#2 = 1;
|FRUT;

|RUTINA ClaveAlta113_1;
     #113#0 = enCodProv;
     #113#1 = enCodPobl;
     #113#2 = 99;
|FRUT;

|PROGRAMA;
     |SI enModoPob = 0;
         |ENTLINEAL #1#113, -3, 1, 22, 1, ClaveBaja113_2, ClaveAlta113_2;
     |SINO;
         eaPoblaParri = "";

         |ABRE #113;
         #113#0 = enCodProv;
         #113#1 = enCodPobl;
         |LEE 040#113];
         |SI FSalida ! 0;  |O #113#0 ! enCodProv;  |O  #113#1 ! enCodPobl;
             |CIERRA #113;
             |ACABA;
         |FINSI;
         |CIERRA #113;

         |ENTLINEAL #1#113, -3, 4, 22, 4, ClaveBaja113_1, ClaveAlta113_1;
     |FINSI;
|FPRO;
