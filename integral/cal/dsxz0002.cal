|FICHEROS;
     dsxz0002;

     :integral/dsam0000;

     trabajo@;
|FIN;

|INCLUYE dsxz002v;

|PROCESO MiraDsArchi;
     enSwDSArchi = 0;
     |DBASS aBass; |QBF aBass;
     aDef = aBass + "dsarchi/def/dsarm001.mas";
     |XABRE "E", aDef, nHandle;
     |SI FSalida < 0;  |ACABA;  |FINSI;

     |CARGA_DEF aDef, trabajo@;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     aAlfa = aBass + "dsarchi/fich/";
     |PATH_DAT #trabajo@#aAlfa#;
     |ABRE #trabajo@;
     |LEE 000#trabajo@.p;
     |SI FSalida ! 0; |DDEFECTO #trabajo@; |FINSI;
     |CIERRA #trabajo@;

     |SI #trabajo@#3 = "S";
         enSwDSArchi = 1;
     |FINSI;

     |DESCARGA_DEF #trabajo@;
|FPRC;

|PROGRAMA;
     eaOpcion1 = PARAMETRO % 105;
     eaOpcion2 = PARAMETRO % 605;
     eaOpcion3 = PARAMETRO % 1101;   enModo = eaOpcion3;
     eaOpcion4 = PARAMETRO % 1299;
     eaOpcion5 = PARAMETRO % 101;

     enSwDSArchi = 0;
     |HAZ MiraDsArchi;

     |CLS;
     |CABEZA eaOpcion4;

     |ABRE #dsam0000;
     #dsam0000#0 = eaOpcion2;
     |LEE 000#dsam0000.=;
     |SI FSalida ! 0;  |DDEFECTO #dsam0000;  |FINSI;
     |CIERRA #dsam0000;

     #dsxz0002#0  = #dsam0000#0;
     #dsxz0002#1  = #dsam0000#1;
     #dsxz0002#2  = #dsam0000#3;

     |PINPA #0#dsxz0002;
     |PINDA #0#dsxz0002;
     |ENDATOS #4#dsxz0002;

     |ATRI I;  |PINTA 0702, eaOpcion4;  |ATRI i;

     enEmpresa = eaOpcion2;

     |PULSATECLA;
     |PULSATECLA;
     |PULSATECLA;
     |PULSATECLA;

     |SI eaOpcion5 = "A";  |SUB_EJECUTA "dsxz002a";  |FINSI;
     |SI eaOpcion5 = "B";  |SUB_EJECUTA "dsxz002b";  |FINSI;
     |SI eaOpcion5 = "C";  |SUB_EJECUTA "dsxz002c";  |FINSI;
     |SI eaOpcion5 = "D";  |SUB_EJECUTA "dsxz002d";  |FINSI;
     |SI eaOpcion5 = "E";  |SUB_EJECUTA "dsxz002d";  |FINSI;
     |SI eaOpcion5 = "F";  |SUB_EJECUTA "dsxz002d";  |FINSI;
     |SI eaOpcion5 = "H";  |SUB_EJECUTA "dsxz002d";  |FINSI;
     |SI eaOpcion5 = "I";  |SUB_EJECUTA "dsxz002j";  |FINSI;
     |SI eaOpcion5 = "J";  |SUB_EJECUTA "dsxz002j";  |FINSI;

     |SUB_EJECUTA "|:modelos/dsmox000.tab";
|FPRO;

|PROCESO Tipo12;  |TIPO 12;
|FPRC;
