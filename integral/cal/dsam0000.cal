|PROCESO PonPtecs;
    aAlfa  = aCadena % 0;
    nLong  = aAlfa;
    |PARA nCaracter = nLong;  |SI nCaracter ] 1;  |HACIENDO nCaracter = nCaracter - 1;
          nPosi  = (100 * nCaracter) + 1;
          aAlfa  = aCadena % nPosi;
          nPtec  = &aAlfa;
          |PTEC nPtec;
    |SIGUE;
|FPRC;

