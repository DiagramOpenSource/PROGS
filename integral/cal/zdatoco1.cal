|| ------------------------------------------------
|PROCESO BFDisolucion;

  enEjerBaja = -1;
  |DBASS aDirInicio;
  aMas  = aDirInicio + "basica/def/agim0019";
  |CARGA_DEF aMas, auxilio@;
  |SI FSalida ! 0;
      |ACABA;
  |FINSI;

 || Lee Ficha Sociedades
    swExiste = 0;
    |ABRE #90;
    #90#0 = enEmpSoc; || Empresa
    #90#1 = 99;
    |LEE 041#90];
    |SI FSalida = 0;
        |SI #90#0 ! enEmpSoc;
            |LEE 041#90a;
            |SI FSalida = 0; |Y #90#0 = enEmpSoc;
                swExiste = 1;
            |FINSI;
        |SINO;
            swExiste = 1;
        |FINSI;
    |FINSI;
    |SI swExiste = 1;
        |SI #90#12 > "01.01.1000";
            aAlfa1 = #90#12; aAlfa1 = aAlfa1 % -104; enEjerBaja = aAlfa1;
        |FINSI;
    |FINSI;
    |CIERRA #90;
    |DESCARGA_DEF #auxilio@;
|FPRC;
