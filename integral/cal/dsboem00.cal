|FICHEROS;
 :/integral/def/dsboem00 #0;
 :/integral/def/dsboem01 #1;
|FIN;

|VARIABLES;
   aAlfa  = "";
   aAlfa1 = "";
|FIN;

|PROCESO NoBaja;
   |ERROR;
|FPRC;

|PROCESO Inf;
   #dsboem01#0 = #dsboem00#0;
   #dsboem01#1 = "                  ";
   #dsboem01#4 = "                ";
|FPRC;

|PROCESO Sup;
   #dsboem01#0 = #dsboem00#0;
   #dsboem01#1 = "zzzzzzzzzzzzzzzzzz";
   #dsboem01#4 = "zzzzzzzzzzzzzzzz";
|FPRC;

|PROCESO Tipo0;
   aAlfa = #dsboem00#0; |QBT aAlfa;
   aAlfa1 = #dsboem00#0; |QBF aAlfa1;
   |SI aAlfa ! aAlfa1;
      #dsboem00#0 = aAlfa;
      |MENAV "    Espacios en la referencia no permitidos";
      |ERROR;
   |FINSI;
|FPRC;

|PROCESO Tipo66; |TIPO 66;
   |CONSULTA_CLAVE #1#dsboem00;
   |PINDA #0#0; |ERROR;
|FPRC;

|PROGRAMA;
   |ABRE #dsboem00;
   |CLS;
   |PINPA #0#0; |PINDA #0#0;

   |PARA; |SI FSalida = 0; |HACIENDO;
      |ENCAMPO #0#0;
      |SI FSalida = 0;
         |LEE 000#dsboem00.=;
         |SI FSalida = 0;
            |PINDA #0#0;

            aAlfa = #dsboem00#6; |QBF aAlfa;
            |SI aAlfa ! ""; |NOME_DAT #dsboem01#aAlfa#; |FINSI;

            |ENTLINEAL #1#dsboem01, -3, 4, 19, 1, Inf, Sup;
         |SINO;
            |MENAV "    Registro inexistente";
            FSalida = 0;
         |FINSI;
      |FINSI;
   |SIGUE;
   |CIERRA #dsboem00;
|FPRO;
