|FICHEROS;
     dsaz0013 #13;
     :/integral/def/dsam0000 #0;
     :/integral/def/dsam0010 #10;
     :/facturar/def/facemp1  #700;
|FIN;

|VARIABLES;
  &eaPathFacturar = "";
  aBase           = "";
  aAlfa           = "";
  nCanal          = 0;
|FIN;


|PROCESO dsam0010;
 |DDEFECTO #0;
 #0#0 = #10#0;
 |LEE 041#0=;

 |DDEFECTO #700;
 #700#0 = #10#1;
 |LEE 041#700=;

 #10#2  = #700#1;
 #10#18 = #0#1;
 |GRABA 020#10a;
 |LIBERA #10;
|FPRC;

|DEFBUCLE dsam0010;
 #10#1;
 ;
 #13#0, 00;
 #13#1, 99;
 be;
 NULL, dsam0010;
|FIN;


|PROCESO Tipo3;  |TIPO 3;
     |SI #13#2 = "SI";
         |ABRE #0;
         |ABRE #700;
         |BUCLE dsam0010;
         |CIERRA #700;
         |CIERRA #0;
     |FINSI;
|FPRC;


|PROGRAMA;

     eaPathFacturar = "";
     |DBASS aBase;   |QBF aBase;
     aAlfa = aBase + "facturar/def/asclient.mas";
     |XABRE "E", aAlfa, nCanal;
     |SI FSalida ] 0;
         eaPathFacturar  = aBase + "facturar/";
     |FINSI;
     |SI eaPathFacturar = "";
         |MENAV "    No esta instalada la aplicacion de Facturacion";
         |ACABA;
     |FINSI;

     |CLS;
     |CABEZA "Recuperar Nombre en Bancos Facturacion";
     |PINPA #0#13;
     |PINDA #0#13;
     |ENDATOS #1#13;
|FPRO;
