|FICHEROS;
     dsam0022;
|FIN;

|VARIABLES;

     aAlfa    = "";
     nPara1   = 0;

     nDModelo = 0;
     nHModelo = 0;

     &eaM22Prog  = "";
     &enM22Clie  = 0;
     &enM22MoFa  = 0;
     &enM22Inkey = 0;
|FIN;

|RUTINA ClaveBaja0;
     #dsam0022#26 = eaM22Prog;
     #dsam0022#27 = enM22Clie;
     #dsam0022#29 = nDModelo;
     #dsam0022#31 = "01.01.0000";
     #dsam0022#32 = "     ";
|FRUT;

|RUTINA ClaveAlta0;
     #dsam0022#26 = eaM22Prog;
     #dsam0022#27 = enM22Clie;
     #dsam0022#29 = nHModelo;
     #dsam0022#31 = "31.12.2999";
     #dsam0022#32 = "99999";
|FRUT;

|PROGRAMA;

     |C_V #dsam0022#29,1,"N";
     |C_V #dsam0022#34,1,"N";

     |SI eaM22Prog = "dsam0000";
         nDModelo = 000;
         nHModelo = 999;
     |FINSI;

     |SI eaM22Prog = "dsam0009";
         |C_V #dsam0022#29,1,"S";
         nDModelo = enM22MoFa;
         nHModelo = enM22MoFa;
         |SI enM22Inkey = 11;
             nDModelo = 000;
             nHModelo = 999;
             |C_V #dsam0022#34,1,"S";
         |FINSI;
         |PARA nPara1 = 7; |SI nPara1 [16; |HACIENDO nPara1 = nPara1 + 1;
              |C_V #dsam0022#nPara1#,1,"N";
         |SIGUE;
         |C_V #dsam0022#18,1,"N";
         |C_V #dsam0022#19,1,"N";
         |C_V #dsam0022#23,1,"N";
         |C_V #dsam0022#24,1,"N";
         |C_V #dsam0022#25,1,"N";
     |FINSI;

     |SI eaM22Prog = "dsam0010";
         |C_V #dsam0022#29,1,"S";
         nDModelo = enM22MoFa;
         nHModelo = enM22MoFa;
         |SI enM22Inkey = 11;
             nDModelo = 00;
             nHModelo = 99;
             |C_V #dsam0022#34,1,"S";
         |FINSI;
         |C_V #dsam0022#6, 1,"N";
         |C_V #dsam0022#14,1,"N";
         |C_V #dsam0022#15,1,"N";
     |FINSI;

     |ABRE #dsam0022;
     |PINPA #0#dsam0022;
     |CONSULTA_F_CLAVE #2#dsam0022, ClaveBaja0, ClaveAlta0;
     |CIERRA #dsam0022;
|FPRO;
