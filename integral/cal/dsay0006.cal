|FICHEROS;
     dsay0006 #0;
     dsam0000 #1;
|FIN;

|VARIABLES;
     nCampo = 0;
     nSwHay = 0;
     aDSit  = "";
     aHSit  = "";

     &eaTitulo = "";
|FIN;

|PROCESO Imprimelos;
     |PRINT;
     nSwHay = 1;
|FPRC;

|DEFBUCLE Contabilidad;
     #1#1;
     65, 85;
     #0#0, "S", aDSit;
     #0#1, "S", aHSit;
     ;
     NULL, Imprimelos;
|FIN;

|DEFBUCLE Fiscal;
     #1#1;
     66, 85;
     #0#0, "S", aDSit;
     #0#1, "S", aHSit;
     ;
     NULL, Imprimelos;
|FIN;

|DEFBUCLE Sociedad;
     #1#1;
     67,85;
     #0#0, "S", aDSit;
     #0#1, "S", aHSit;
     ;
     NULL, Imprimelos;
|FIN;

|DEFBUCLE Renta;
     #1#1;
     68,85;
     #0#0, "S", aDSit;
     #0#1, "S", aHSit;
     ;
     NULL, Imprimelos;
|FIN;

|DEFBUCLE Laboral;
     #1#1;
     69,85;
     #0#0, "S", aDSit;
     #0#1, "S", aHSit;
     ;
     NULL, Imprimelos;
|FIN;

|PROCESO Tipo3;  |TIPO 3;
     |IMPRE 0;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |INFOR "dsay0006";
     |SI FSalida ! 0;  |FINIMP;  |ACABA;  |FINSI;

 || ...  Situacion Activa/Inactiva
     |SI #0#7 = "X";
         aDSit = "A"; aHSit = "Z";
     |SINO;
         aDSit = #0#7; aHSit = #0#7;
     |FINSI;

     |SI #0#2 = "S";
         nSwHay = 0;
         eaTitulo = "CONTABILIDAD";
         |BUCLE Contabilidad;
         |SI nSwHay = 1;  |PIEINF;  |FINSI;
     |FINSI;

     |SI #0#3 = "S";
         nSwHay = 0;
         eaTitulo = "FISCAL";
         |BUCLE Fiscal;
         |SI nSwHay = 1;  |PIEINF;  |FINSI;
     |FINSI;

     |SI #0#4 = "S";
         nSwHay = 0;
         eaTitulo = "SOCIEDADES";
         |BUCLE Sociedad;
         |SI nSwHay = 1;  |PIEINF;  |FINSI;
     |FINSI;

     |SI #0#5 = "S";
         nSwHay = 0;
         eaTitulo = "RENTA";
         |BUCLE Renta;
         |SI nSwHay = 1;  |PIEINF;  |FINSI;
     |FINSI;

     |SI #0#6 = "S";
         nSwHay = 0;
         eaTitulo = "LABORAL";
         |BUCLE Laboral;
         |SI nSwHay = 1;  |PIEINF;  |FINSI;
     |FINSI;

     |FININF;
     |FINIMP;
|FPRC;
