|FICHEROS;
     av060w00;
     av060w02;
|FIN;

|VARIABLES;
     nLong      = 0;

     aAlfa      = "";
     aPass1     = "";
     aPass2     = "";

     &eaPass    = "";
     &eaPassEnc = "";
|FIN;

|PROCESO Tipo12Fw02;   |TIPO 12;
     |SI FSalida = 7;
         |ACABA;
     |FINSI;

     |SI aPass1 = "";  |O aPass2 = "";
         |MENAV "0000Introduzca las contrase바s";
         |ERROR;
         |ACABA;
     |FINSI;

     |SI aPass1 ! aPass2;
         |MENAV "0000Las contrase바s no son identicas";
         |ERROR;

         #av060w02#1 = "";  |PINTA #av060w02#1;
         #av060w02#2 = "";  |PINTA #av060w02#2;

         aPass1      = "";
         aPass2      = "";

         |POSICIONATE #av060w02#1;

         |ACABA;
     |FINSI;

     #av060w02#1 = aPass1;
|FPRC;

|PROCESO Tipo7C0Fw02;  |TIPO 7;
     #av060w02#0 = "";  |PINTA #av060w02#0;
     #av060w02#1 = "";  |PINTA #av060w02#1;
     #av060w02#2 = "";  |PINTA #av060w02#2;

     aPass1      = "";
     aPass2      = "";
|FPRC;

|PROCESO Tipo0C0Fw02;  |TIPO 0;
     |C_M #av060w02#0, 0, aAlfa;
     |SI aAlfa = "N";  |ACABA;  |FINSI;

     eaPass = #av060w02#0;  |QBF eaPass;
     |SI eaPass = "";
         |MENAV "0000Introduzca la contrase바 del certificado";
         |ERROR;
         |ACABA;
     |FINSI;

     |HAZ EncriptaPass;
     #av060w02#0 = eaPassEnc;

     |SI #av060w02#0 ! #av060w00#6;
         |MENAV "0000Contrase바 incorrecta.";

         #av060w02#0 = "";
         |PINTA #av060w02#0;
         |ERROR;
     |FINSI;

     aAlfa = #av060w02#0;  |QBF aAlfa;
     aAlfa = aAlfa % 0;
     nLong = aAlfa;

     #av060w02#0 = "*" * nLong;
     |PINTA #av060w02#0;
|FPRC;

|PROCESO Tipo0C1Fw02;  |TIPO 0;
     eaPass = #av060w02#1;  |QBF eaPass;
     |HAZ EncriptaPass;

     aPass1 = eaPassEnc;

     aAlfa = #av060w02#1;  |QBF aAlfa;
     aAlfa = aAlfa % 0;
     nLong = aAlfa;

     #av060w02#1 = "*" * nLong;
     |PINTA #av060w02#1;
|FPRC;

|PROCESO Tipo0C2Fw02;  |TIPO 0;
     eaPass = #av060w02#2;  |QBF eaPass;
     |HAZ EncriptaPass;

     aPass2 = eaPassEnc;

     aAlfa = #av060w02#2;  |QBF aAlfa;
     aAlfa = aAlfa % 0;
     nLong = aAlfa;

     #av060w02#2 = "*" * nLong;
     |PINTA #av060w02#2;
|FPRC;
