|FIN;

|PROCESO LetraDnim110;  |TIPO 6;
     |SI FSalida = 10;
         eaVarDni  = #dsam0110#Campo#;
         enCalcCif = 1;
         |HAZ CalculoDNI;
         #dsam0110#Campo# = eaVarDni;
         |PINTA #dsam0110#Campo#;
         |ERROR;
         |ACABA;
     |FINSI;

     eaVarDni  = #dsam0110#Campo#;
     enCalcCif = 0;
     |HAZ CalculoDNI;
     eaVarDni = (eaVarDni + "             ") % 112;
     |SI #dsam0110#Campo# ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #dsam0110#Campo# = eaVarDni;  |PINTA #dsam0110#Campo#;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;
