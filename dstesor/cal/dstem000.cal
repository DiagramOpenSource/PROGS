|FICHEROS;
     dstem000 #0;

     :/contagen/def/canempre #1;
     :/dscomer9/def/agifa041 #2;
|FIN;

|VARIABLES;
     aBase         = "";
     aBase1        = "";
     aBase2        = "";
     aAlfa         = "";
     aPath         = "";

     nModoEntrada  = 0;
|FIN;

|PROCESO Tipo80;  |TIPO 80;
     |DBASS aBase;  |QBF aBase;

     aPath = aBase + "contagen/fich/";
     |PATH_DAT #1 aPath;

     aPath = aBase + "dscomer9/fich/";
     |PATH_DAT #2 aPath;
|FPRC;

|PROCESO CreaDir; |TIPO 3;
     nModoEntrada = (FEntrada / 100) ! 0;
     |SI nModoEntrada ! 1; |ACABA; |FINSI;

     |DBASE aBase; |QBF aBase;
     aAlfa = aBase + "fich/" + (("00000" + #0#0) % -105) + "/";
     |MKDIR aAlfa;
|FPRC;

|PROCESO BorraDir; |TIPO 2;
     nModoEntrada = (FEntrada / 100) ! 0;

     |SI nModoEntrada ! 3;  |ACABA;  |FINSI;

     |DBASS aBase1; |QBF aBase1;
     |DBASE aBase2; |QBF aBase2;

     aAlfa = aBase2 + "fich/" + (("00000" + #0#0) % -105);
     aAlfa = aBase1 + "bin/agirm -r " + aAlfa;
     |SYSTEM aAlfa;
|FPRC;
