  Letra2 = CampoDni;
  |QBT Letra2;

      Caracter = Letra2 % 101;
  |SI Caracter!"A";|Y Caracter!"B";|Y Caracter!"C";|Y Caracter!"D";
       |Y Caracter!"E";|Y Caracter!"F";
       |Y Caracter!"G";|Y Caracter!"P";|Y Caracter!"Q";|Y Caracter!"S";|Y Caracter!"X";
