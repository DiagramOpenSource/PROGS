|PROCESO copiando;
  temporal1 = ficheros + traba1 + ".ddd";
  temporal2 = ficheros + traba1 + ".dat";
  |COPIA_FICHERO temporal1, temporal2;
  |SI FSalida ! 0;  |ACABA;  |FINSI;

  temporal1 = ficheros + traba1 + ".ixx";
  |FBORRA temporal1;

  temporal1 = ficheros + traba1 + ".dxx";
  |FBORRA temporal1;
|FPRC;
