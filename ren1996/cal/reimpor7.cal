|FICHEROS;
    retotpat #06;
    zetotpat #07;

    retotrea #08;
    zetotrea #09;

    retotren #10;
    zetotren #11;

    reliquid #12;
    zeliquid #13;

    repagret #14;
    zepagret #15;

    reempagr #16;
    zeempagr #17;

    reempag1 #18;
    zeempag1 #19;
|FIN;

|VARIABLES;
    campos_1 =  47;  || nro. de campos de cada uno
    campos_2 = 250;
    campos_3 = 228;
    campos_4 =  43;
    campos_5 =  9;
    campos_6 =  36;
    puntero = 0;
|FIN;

|INCLUYE i_impor1;
|INCLUYE i_impor2;

|PROGRAMA;
|HAZ imp_1;
|HAZ imp_2;
|HAZ imp_3;
|HAZ imp_4;
|HAZ imp_6;
|HAZ imp_7;
|CORRE "reimpor1.run";
|FPRO;

|PROCESO imp_6;
|SI #20#51 = 1;
    |ABRE #15;
    |PATH_DAT #14 path;
        error = -1;
    |PARA; |SI error ! 0; |HACIENDO;
        |ABRE #14;
            error = FSalida;
    |SIGUE;
        #14#0 = codi;
        #14#1 = 1;             || bucle de lectura de lineas
    |LEE 001#14=;
    |SI FSalida ! 0;  |LEE 001#14];  |FINSI;
    |PARA; |SI FSalida = 0;|Y #14#0 = codi; |HACIENDO;
        |SI borra = "S";  |BORRA 020#14a; |FINSI;
        |PARA nume1 = 0; |SI nume1 [ campos_1; |HACIENDO nume1 = nume1 + 1;
            #15nume1 = #14nume1;
        |SIGUE;
        |GRABA 020#15n;
        |LEE 001#14s;
    |SIGUE;
    |CIERRA #14;
    |CIERRA #15;
|FINSI;
|FPRC;

|PROCESO imp_7;
|SI #20#52 = 1;
    |ABRE #17;
    |PATH_DAT #16 path;
        error = -1;
    |PARA; |SI error ! 0; |HACIENDO;
        |ABRE #16;
            error = FSalida;
    |SIGUE;
        #16#0 = codi;
        #16#1 = 1;             || bucle de lectura de lineas
    |LEE 001#16=;
    |SI FSalida ! 0;  |LEE 001#16];  |FINSI;
    |PARA; |SI FSalida = 0;|Y #16#0 = codi; |HACIENDO;
        |SI borra = "S";  |BORRA 020#16a; |FINSI;
        |PARA nume1 = 0; |SI nume1 [ 36; |HACIENDO nume1 = nume1 + 1;
            #17nume1 = #16nume1;
        |SIGUE;
        |GRABA 020#17n;
        |LEE 001#16s;
    |SIGUE;
    |CIERRA #16;
    |CIERRA #17;

    |ABRE #19;
    |PATH_DAT #18 path;
        error = -1;
    |PARA; |SI error ! 0; |HACIENDO;
        |ABRE #18;
            error = FSalida;
    |SIGUE;
        #18#0 = codi;
        #18#1 = 1;             || bucle de lectura de lineas
        #18#2 = 1;
    |LEE 001#18=;
    |SI FSalida ! 0;  |LEE 001#18];  |FINSI;
    |PARA; |SI FSalida = 0;|Y #18#0 = codi; |HACIENDO;
        |SI borra = "S";  |BORRA 020#18a; |FINSI;
        |PARA nume1 = 0; |SI nume1 [ 9; |HACIENDO nume1 = nume1 + 1;
            #19nume1 = #18nume1;
        |SIGUE;
        |GRABA 020#19n;
        |LEE 001#18s;
    |SIGUE;
    |CIERRA #18;
    |CIERRA #19;
|FINSI;
|FPRC;
