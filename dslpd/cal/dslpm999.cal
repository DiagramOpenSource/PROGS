|FICHEROS;
     dslpm999 #999;
     dslpm511 #511;
|FIN;

|VARIABLES;
     aEsc1         = "";
     aEsc2         = "";
     aRetu         = "";
     aTecla        = "";
     aUsuario      = "";

     nPanta0       = 0;
     nRango        = 0;
     nPos1         = 0;
     nPos2         = 0;
     nGrid         = 0;
     nPermiso      = 0;
|FIN;

|| ------------------------------------------------------
|| Procesos del Incio del Programa
|| ------------------------------------------------------

|PROCESO Tipo80;  |TIPO 80;
     |ABRET #999;

     nPermiso = 0;
     aUsuario = Usuario % 810;

     |ABRE #511;
     #511#0 = aUsuario;
     |LEE 000#511=;
     |SI #511#2 = "N";
         || |MENAV "0000 Usted no tiene permiso para entrar en esta opcion";
         || nPermiso = 1;
     |FINSI;
     |CIERRA #511;

     FSalida = 2899;
|FPRC;

|PROCESO Tipo90;  |TIPO 90;
     |CIERRAT #999;
|FPRC;

|PROGRAMA;
     |SI nPermiso = 1;  |ACABA;  |FINSI;

     |CABEZA "Permisos Usuarios";

     |PINPA #0#999;  nPanta0 = FSalida;

     nRango = 4 + 8 + 16 + 32;
     nPos1  = 0902;
     nPos2  = 2698;

     |LEE 000#999p;

     |LINEAL_SIMPLE #1#999, nRango, nPos1, nPos2, NULL, NULL, NULL;
     nGrid = FSalida;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
         FSalida = "::Salir";
         |LEETECLA aTecla;
         |SI aTecla = aEsc1; |SAL;  |FINSI;
         |SI aTecla = aEsc2; |SAL;  |FINSI;
         |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;

     |DESTRUYECONTROL nGrid;

     |PULSATECLA;
|FPRO;
