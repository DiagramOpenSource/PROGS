|FICHEROS;
     dslpw004 #904;
     dslpw033 #933;
     dslpw025 #925;
|FIN;

|VARIABLES;
     aAlfa      = "";

     &enValidar   = 0;
     &enPlantilla = 0;
|FIN;

|PROCESO Fuentew25;
     |C_M #925#3, 1, "S";
     |C_M #925#4, 1, "S";
     |C_M #925#5, 1, "S";
     |C_M #925#6, 1, "S";
     |C_M #925#7, 1, "S";

     |SI #925#2 = "N";
         |C_M #925#3, 1, "N";  #925#3 = "N";
         |C_M #925#4, 1, "N";  #925#4 = "N";
         |C_M #925#5, 1, "N";  #925#5 = "N";
         |C_M #925#6, 1, "N";  #925#6 = "N";
         |C_M #925#7, 1, "N";  #925#7 = "N";
     |FINSI;

     |PINTA #925#3;
     |PINTA #925#4;
     |PINTA #925#5;
     |PINTA #925#6;
     |PINTA #925#7;
|FPRC;

|PROCESO Tipo12w25;  |TIPO 12;
     enValidar = 0;

     |SI enPlantilla = 0;
         #904#211  = "S";

         |SI #925#0 = "N";  |Y #925#1  = "N";  |Y #925#2  = "N";  |Y #925#3 = "N";
          |Y #925#4 = "N";  |Y #925#5  = "N";  |Y #925#6  = "N";  |Y #925#7 = "N";
          |Y #925#8 = "N";  |Y #925#9  = "N";  |Y #925#10 = "N";
             #904#211 = "N";
         |FINSI;

         |PINTA #904#211;
     |SINO;
         #933#191  = "S";

         |SI #925#0 = "N";  |Y #925#1  = "N";  |Y #925#2  = "N";  |Y #925#3 = "N";
          |Y #925#4 = "N";  |Y #925#5  = "N";  |Y #925#6  = "N";  |Y #925#7 = "N";
          |Y #925#8 = "N";  |Y #925#9  = "N";  |Y #925#10 = "N";
             #933#191 = "N";
         |FINSI;

         |PINTA #933#191;
     |FINSI;

     |SI #925#2 = "S";
         |SI #925#3 = "N";  |Y #925#4  = "N";  |Y #925#5  = "N";
          |Y #925#7 = "N";
             |MENAV "0000 Debe seleccionar un tipo de fuente accesible al publico";
             enValidar = 1;
         |FINSI;
     |FINSI;

     |SI enValidar = 0;
         |MENAV "0000 El apartado esta cumplimentado de forma correcta.";
     |FINSI;
|FPRC;
