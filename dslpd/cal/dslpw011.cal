|FICHEROS;
     dslpw004 #904;
     dslpw033 #933;
     dslpw011 #911;
|FIN;

|VARIABLES;
     aAlfa      = "";

     &enValidar   = 0;
     &enPlantilla = 0;
|FIN;

|PROCESO Otrosw11;  |TIPO 0;
     |C_M #911#5, 1, "S";
     |C_M #911#6, 1, "S";

     |SI #911#4 = "N";
         |C_M #911#5, 1, "N";  #911#5 = "";
         |C_M #911#6, 1, "N";  #911#6 = "";
     |FINSI;
|FPRC;

|PROCESO Tipo12w11;  |TIPO 12;
     enValidar = 0;

     |SI enPlantilla = 0;
         #904#31   = "S";

         |SI #911#0 = "N";  |Y #911#1 = "N";  |Y #911#2 = "N";
          |Y #911#3 = "N";  |Y #911#4 = "N";
             #904#31 = "N";
         |FINSI;

         |PINTA #904#31;
     |SINO;
         #933#11   = "S";

         |SI #911#0 = "N";  |Y #911#1 = "N";  |Y #911#2 = "N";
          |Y #911#3 = "N";  |Y #911#4 = "N";
             #933#11 = "N";
         |FINSI;

         |PINTA #933#11;
     |FINSI;

     |SI #911#4 = "S";
          aAlfa = #911#5 + #911#6;  |QBF aAlfa;
          |SI aAlfa = "";
             |MENAV "0000 Debe rellenar el campo Descripcion";
             enValidar = 1;
             |ACABA;
          |FINSI;
     |FINSI;

     |SI enValidar = 0;
         |MENAV "0000 El apartado esta cumplimentado de forma correcta.";
     |FINSI;
|FPRC;
