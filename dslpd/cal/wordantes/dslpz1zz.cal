         |SINO;
             |COPIA_FICHERO aOrigen, aDestino;
         |FINSI;
     |FINSI;

     aAlfa = aDestino;  |QBF aAlfa;
     |SI aAlfa ! "";
         aAlfa = "@" + aAlfa;
         |WORD_CARGA_TEXTO enWord1, "!<<MEMBRETECLI>>", aAlfa;
         |WORD_CARGA_TEXTO enWord1, "!!@<<MEMBRETECLI>>", aAlfa;
     |SINO;
         eaAlfa   = "";
