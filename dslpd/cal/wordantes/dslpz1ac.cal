     efFecha  = ~;
     |HAZ SacaFecha;
     eaAlfa   = eaFecha;
     eaAsigna = "#FECHA#";  |HAZ AsignaWord1;

     |HAZ HojaValoracionO;

     enPresenta = 1;
     |HAZ FinDocumentoWord;
