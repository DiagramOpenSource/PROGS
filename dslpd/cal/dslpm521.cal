|FICHEROS;
     dslpm521 #521;
     dslpm511 #511;
|FIN;

|VARIABLES;
     aOpcion     = "";

     nModo       = 0;
|FIN;

|PROCESO Tipo80m521;  |TIPO 80;
     |ABRE #511;
     #511#0 = Usuario % 810;
     |LEE 000#511=;
     |SI FSalida ! 0;  |DDEFECTO #511;  |FINSI;
     |CIERRA #511;
|FPRC;

|PROCESO Tipo7C0F521;  |TIPO 7;
     aOpcion = #511#2;

     |C_M #521#1, 1, aOpcion;
     |C_M #521#2, 1, aOpcion;
     |C_M #521#4, 1, aOpcion;
     |C_M #521#5, 1, aOpcion;

     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 4;  |ACABA;  |FINSI;

     |SI #511#2 ! "S";
         |SI nModo = 1;
             |MENAV "0000 El Articulo no existe, No tiene permiso para CREAR Articulos";
         |FINSI;
         |SI nModo = 2;
             |MENAV "0000 No tiene permiso para MODIFICAR Articulos";
         |FINSI;
         |SI nModo = 3;
             |MENAV "0000 No tiene permiso para dar de BAJA Articulos";
         |FINSI;
         |PTEC 807;
     |FINSI;
|FPRC;
