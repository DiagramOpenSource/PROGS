|FICHEROS;
     dslpw004 #904;
     dslpw033 #933;
     dslpw006 #906;
|FIN;

|VARIABLES;
     aAlfa      = "";

     &enValidar   = 0;
     &enPlantilla = 0;
|FIN;

|PROCESO Pregunta2SN;  |TIPO 0;
     |SI Campo = 3;
         |SI #906#3 = "S";  |Y #906#9 = "S";
             #906#3 = "S";  |PINTA #906#3;
             #906#9 = "N";  |PINTA #906#9;
         |FINSI;
     |FINSI;

     |SI Campo = 9;
         |SI #906#3 = "S";  |Y #906#9 = "S";
             #906#3 = "N";  |PINTA #906#3;
             #906#9 = "S";  |PINTA #906#9;
         |FINSI;
     |FINSI;

     |SI Campo = 4;
         |SI #906#4 = "S";  |Y #906#10 = "S";
             #906#4  = "S";  |PINTA #906#4;
             #906#10 = "N";  |PINTA #906#10;
         |FINSI;
     |FINSI;

     |SI Campo = 10;
         |SI #906#4 = "S";  |Y #906#10 = "S";
             #906#4  = "N";  |PINTA #906#4;
             #906#10 = "S";  |PINTA #906#10;
         |FINSI;
     |FINSI;

     |C_M #906#5, 1, "S";
     |C_M #906#6, 1, "S";
     |C_M #906#7, 1, "S";
     |C_M #906#8, 1, "S";

     |SI #906#4 = "N";
         |C_M #906#5, 1, "N";  #906#5 = "";
         |C_M #906#6, 1, "N";  #906#6 = "";
         |C_M #906#7, 1, "N";  #906#7 = "";
         |C_M #906#8, 1, "N";  #906#8 = "";
     |FINSI;

     |PINTA #906#5;
     |PINTA #906#6;
|FPRC;

|PROCESO Tipo12w06;  |TIPO 12;
     enValidar = 0;

     |SI #906#0 = "N";  |Y #906#1 = "N";  |Y #906#2 = "N";
         |SI #906#3 = "S";  |O #906#4 = "S";  |O #906#9 = "S";  |O #906#10 = "S";
             |MENAV "0000 Debe marcar alguna tipologia";
             enValidar = 1;
             |ACABA;
         |FINSI;
     |FINSI;

     |SI #906#0 = "S";  |O #906#1 = "S";  |O #906#2 = "S";
         |SI #906#3 = "N";  |Y #906#4 = "N";  |Y #906#9 = "N";  |Y #906#10 = "N";
             enValidar = 1;
             |MENAV "0000 Indique al menos un supuesto que permita tratar este tipo de datos";
             |ACABA;
         |FINSI;

         |SI #906#3 = "N";  |Y #906#9 = "N";
             enValidar = 1;
             |MENAV "0000 Indique al menos un supuesto que permita tratar este tipo de datos";
             |ACABA;
         |FINSI;

         |SI #906#4 = "N";  |Y #906#10 = "N";
             enValidar = 1;
             |MENAV "0000 Indique al menos un supuesto que permita tratar este tipo de datos";
             |ACABA;
         |FINSI;
     |FINSI;

     |SI #906#4 = "S";
          |SI #906#5 = "    ";
             |MENAV "0000 Debe rellenar el campo Numero de Ley";
             enValidar = 1;
             |ACABA;
          |FINSI;

          |SI #906#6 = "    ";
             |MENAV "0000 Debe rellenar el campo A¤o";
             enValidar = 1;
             |ACABA;
          |FINSI;

          aAlfa = #906#7 + #906#8;  |QBF aAlfa;
          |SI aAlfa = "";
             |MENAV "0000 Debe rellenar el campo Descripcion";
             enValidar = 1;
             |ACABA;
          |FINSI;
     |FINSI;

     |SI enValidar = 0;
         |MENAV "0000 El apartado esta cumplimentado de forma correcta.";
     |FINSI;

     |SI enPlantilla = 0;
         #904#26 = "S";
         |SI #906#0 = "N";  |Y #906#1 = "N";  |Y #906#2 = "N";
             #904#26 = "N";
         |FINSI;

         |PINTA #904#26;
     |SINO;
         #933#6 = "S";
         |SI #906#0 = "N";  |Y #906#1 = "N";  |Y #906#2 = "N";
             #933#6 = "N";
         |FINSI;

         |PINTA #933#6;
     |FINSI;
|FPRC;
