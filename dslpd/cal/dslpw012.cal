|FICHEROS;
     dslpw004 #904;
     dslpw033 #933;
     dslpw012 #912;
|FIN;

|VARIABLES;
     aAlfa      = "";

     &enValidar   = 0;
     &enPlantilla = 0;
|FIN;

|PROCESO Otrosw12;  |TIPO 0;
     |C_M #912#5, 1, "S";
     |C_M #912#6, 1, "S";

     |SI #912#4 = "N";
         |C_M #912#5, 1, "N";  #912#5 = "";
         |C_M #912#6, 1, "N";  #912#6 = "";
     |FINSI;
|FPRC;

|PROCESO Tipo12w12;  |TIPO 12;
     enValidar = 0;

     |SI enPlantilla = 0;
         #904#32   = "S";

         |SI #912#0 = "N";  |Y #912#1 = "N";  |Y #912#2 = "N";
          |Y #912#3 = "N";  |Y #912#4 = "N";
             #904#32 = "N";
         |FINSI;

         |PINTA #904#32;
     |SINO;
         #933#12   = "S";

         |SI #912#0 = "N";  |Y #912#1 = "N";  |Y #912#2 = "N";
          |Y #912#3 = "N";  |Y #912#4 = "N";
             #933#12 = "N";
         |FINSI;

         |PINTA #933#12;
     |FINSI;

     |SI #912#4 = "S";
          aAlfa = #912#5 + #912#6;  |QBF aAlfa;
          |SI aAlfa = "";
             |MENAV "0000 Debe rellenar el campo Descripcion";
             enValidar = 1;
             |ACABA;
          |FINSI;
     |FINSI;

     |SI enValidar = 0;
         |MENAV "0000 El apartado esta cumplimentado de forma correcta.";
     |FINSI;
|FPRC;
