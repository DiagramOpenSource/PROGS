|FICHEROS;
     dslpw039 #939;
|FIN;

|VARIABLES;
     aTecla        = "";

     &enAuditoria  = 0;
|FIN;

|PROCESO NuevaSituacion;
     |SI enAuditoria ! 0;  |ACABA;  |FINSI;

     |SI #939#4 > 40;
         |MENAV "0000 La Nueva situacion debe ser inferior o igual a la Situacion 40.";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Tipo12;  |TIPO 12;
     |SI FSalida = 7;  |ACABA;  |FINSI;

     |LEETECLA aTecla;

     |CONFI "0000S Conforme con la nueva situacion";
     |SI FSalida ! 0;
         #939#4 = 0;
     |FINSI;
|FPRC;
