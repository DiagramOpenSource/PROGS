|FICHEROS;
     dslpm522 #522;
     dslpm511 #511;
|FIN;

|VARIABLES;
     aOpcion     = "";

     nModo       = 0;
|FIN;

|PROCESO Tipo80m522;  |TIPO 80;
     |ABRE #511;
     #511#0 = Usuario % 810;
     |LEE 000#511=;
     |SI FSalida ! 0;  |DDEFECTO #511;  |FINSI;
     |CIERRA #511;
|FPRC;

|PROCESO Tipo7C0F522;  |TIPO 7;
     aOpcion = #511#2;

     |C_M #522#1, 1, aOpcion;
     |C_M #522#2, 1, aOpcion;

     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 4;  |ACABA;  |FINSI;

     |SI #511#2 ! "S";
         |SI nModo = 1;
             |MENAV "0000 El Tipo de IVA no existe, No tiene permiso para CREAR Tipos de IVA";
         |FINSI;
         |SI nModo = 2;
             |MENAV "0000 No tiene permiso para MODIFICAR Tipos de IVA";
         |FINSI;
         |SI nModo = 3;
             |MENAV "0000 No tiene permiso para dar de BAJA Tipos de IVA";
         |FINSI;
         |PTEC 807;
     |FINSI;
|FPRC;

|PROCESO Tipo0;  |TIPO 7;
     |C_M #522#2, 1, "S";
     |SI #522#0 = 0;
         |C_M #522#2, 1, "N";  #522#2 = 0;  |PINTA #522#2;
     |FINSI;
|FPRC;


|PROCESO TipoIVA;  |TIPO 0;
     |SI #522#0 ! 0;  |Y #522#2 = 0;
         |MENAV "0000 Introduzca porcentaje";
         |ERROR;
     |FINSI;
|FPRC;
