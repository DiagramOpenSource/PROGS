|PROCESO LeeFicheros;
   |ABRE #reremesa;
   |BUCLE Contrib;

   |ABRE #rem00100;
   #rem00100#0 = #rez00050#3;
   |LEE 000#rem00100.=;
   |SI FSalida ! 0; |DDEFECTO #rem00100; |FINSI;
   |CIERRA #reremesa;
   #rem01001#0 = #rez00050#3;
   |CIERRA #rem00100;
|FPRC;

|PROCESO Funcion1; |TIPO 11;
   |SI FSalida = 10;
      Comodin = #rew00004#8 % 0118; |QBF Comodin;
      |SI Comodin = "APUNTE YA REMESADO";
         |MENAV "    Apunte ya remesado. No se puede volver a remesar";
         |ACABA;
      |FINSI;
      |SI #rew00004#7 = "S";
         #rew00004#7 = "N";
      |SINO;
         #rew00004#7 = "S";
      |FINSI;
      |PINTA #rew00004#7;
      |GRABA 020#rew00004.a;
   |FINSI;
|FPRC;

|PROCESO Inferior;
     #rew00004#0 = 1;
|FPRC;

|PROCESO Superior;
     #rew00004#0 = Linea - 1;
|FPRC;

|PROCESO PintaLineal;
     |PTEC 806;
     |ENTLINEAL #1#rew00004, 1, 4, 23, 0, Inferior, Superior;
|FPRC;

|PROCESO CargaTempo;
     comienza = "50";
     |HAZ fichero;
     |NOME_DAT #rew00004#fichero#;
     |ABRE #rew00004; |CIERRA #rew00004; |DELINDEX #rew00004;

     |ABRE #rew00004;

     Linea = 1;
     |HAZ LeeFicheros;

     |SI Linea = 1;
         |MENAV "0000Selecci¢n vac¡a.";
         |CIERRA #rew00004;
         |DELINDEX #rew00004;
         |HAZ PintaLineal;
         |ACABA;
     |FINSI;

     |ENTLINEAL #1#rew00004, 1, 4, 23, 0, Inferior, Superior;
     |CONFI "2400SConforme con lo seleccionado : ";
     |SI FSalida ! 0;
         |CIERRA #rew00004;  |DELINDEX #rew00004;
         |HAZ PintaLineal;
         |ACABA;
     |FINSI;

     |ABRE #reremesa;
     |BUCLE Volcado;
     |CIERRA #reremesa;

     |CIERRA #rew00004;
     |DELINDEX #rew00004;

     |HAZ PintaLineal;
|FPRC;

|PROGRAMA;
     |CLS;
     |CABEZA "Adjudicacion de Remesas";

     |PARA; |SI FSestado = 0; |HACIENDO;
            #rez00050#3 = 0;

            |PINPA #0#rez00050;
            |PINDA #0#rez00050;
            |ENDATOS #1#rez00050;

            |C_M #rez00050#0, 1, "N"; |C_M #rez00050#2, 1, "N";

            FSestado = FSalida;
            |SI FSalida = 0; |HAZ CargaTempo; |FINSI;
     |SIGUE;
|FPRO;

