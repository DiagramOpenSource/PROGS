|FICHEROS;
     ver00001;
     retabla1;
     reeleact;
     rem00500;
|FIN;

|VARIABLES;
     aDef         = "";
     aMas         = "";
     aBase        = "";
     aPerceptor   = "";
     aAlfa        = "";
     aAlfa1       = "";
     aAlfa2       = "";

     nSwHay       = 0;
     nLimite      = 0;
     nPorce       = 0;
     nCanti1      = 0;
     nCanti2      = 0;
     nCanti3      = 0;
     nCanti4      = 0;
     nDias        = 0;
     nValor       = 0;
     nRecalculo   = 0;

     &enError     = 0;
     &eaRenta     = "";
|FIN;

|| ************************************************************************
|| PROCESOS DE CAMBIO DE VERSION
|| ************************************************************************

|PROCESO MiraPatrones;
     nSwHay     = 0;

     |ABRE #retabla1;
     |LEE 040#retabla1.p;
     |SI FSalida ! 0;
         |CIERRA #retabla1;  |DELINDEX #retabla1;

         |SUB_EJECUTA_NP "rez00001;AUTOMATICO";
     |SINO;
         |CIERRA #retabla1;
     |FINSI;

     |ABRE #rem00500;
     #rem00500#0 = 116;
     |LEE 000#rem00500.=;
     |SI FSalida ! 0;  |O #rem00500#5 ! 0.18;
          |CIERRA #rem00500;  |DELINDEX #rem00500;

          |SUB_EJECUTA_NP "rez00001;AUTOMATICO";
     |FINSI;
     |CIERRA #rem00500;

     |ABRE #retabla1;
     |LEE 040#retabla1.p;
     |SI FSalida ! 0;
         enError = 1;
         |CIERRA #retabla1;  |DELINDEX #retabla1;
         |ACABA;
     |SINO;
         |CIERRA #retabla1;
     |FINSI;

     |ABRE #rem00500;
     #rem00500#0 = 116;
     |LEE 000#rem00500.=;
     |SI FSalida ! 0;  |O #rem00500#5 ! 0.18;
          |CIERRA #rem00500;  |DELINDEX #rem00500;

          enError = 1;
     |SINO;
          |CIERRA #rem00500;
     |FINSI;

     |ABRE #reeleact;
     |LEE 000#reeleact.p;
     |SI FSalida ! 0;  |DDEFECTO #reeleact;  |FINSI;
     |CIERRA #reeleact;

     aAlfa = #reeleact#8 - "/";
     |SI FSalida ! 0;
         |DELINDEX #reeleact;

         |SUB_EJECUTA_NP "rez00001;AUTOMATICO";
     |FINSI;
|FPRC;

|PROGRAMA;
     |MENSA "0000Comprobando configuracion patrones.";

     enError = 0;
     |HAZ MiraPatrones;
|FPRO;
