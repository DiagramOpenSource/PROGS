|PROCESO BotonCalc;
     |SI enModoReg = 4;  |ACABA;  |FINSI;

     |DDEFECTO #rew00005;

     |SI FSalida = nBotonCalc1; nCampo = 15;  |FINSI;
     |SI FSalida = nBotonCalc2; nCampo = 20;  |FINSI;
     |SI FSalida = nBotonCalc3; nCampo = 22;  |FINSI;
     |SI FSalida = nBotonCalc4; nCampo = 23;  |FINSI;
     |SI FSalida = nBotonCalc5; nCampo = 32;  |FINSI;

     #rew00005#1  = #rew00623#nCampo#;
     #rew00005#20 = #rew00623#nCampo#;

     |HAZ PantaREW00005;

     #rew00623#nCampo# = #rew00005#20;  |PINTA #rew00623#nCampo#;

     aAlfa = "|C" + (("000" + nCampo) % -103) + "WID";
     aID   = #rew00623#aAlfa#;
     nID   = aID;
     |FOCOTECLADO nID;
|FPRC;

|PROCESO Rendimientos;
     |SI enModoReg ! 4;  |ACABA;  |FINSI;

     |HAZ QuitaBtnSalir;

     |SUB_EJECUTA_NP "rew00551";

     |HAZ PonBtnSalir;

     |FOCOTECLADO nGrid;
|FPRC;

|PROCESO PintaDatosPantalla;
     |DDEFECTO #rew00623;
     |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
           #rew00623#nCampo# = #rem00023#nCampo#;

           |C_V #rew00623#nCampo#, 0, aAlfa;
           |SI aAlfa = "S";  |PINTA #rew00623#nCampo#;  |FINSI;

           |C_M #rew00623#nCampo#, 1, "N";
     |SIGUE;
|FPRC;

|PROCESO Evento20;
     |SI enModoReg ! 4;  |ACABA;  |FINSI;

     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#rem00023.=;

         |SI enModo = 4;
             |ESTADO_CONTROL nBotonAlta, "DISABLE";
             |ESTADO_CONTROL nBotonModi, "DISABLE";
             |ESTADO_CONTROL nBotonBaja, "DISABLE";
         |SINO;
             |ESTADO_CONTROL nBotonModi, "ENABLE";
             |ESTADO_CONTROL nBotonBaja, "ENABLE";
         |FINSI;

         |PULSATECLA;

         nEntrar   = 1;
         enModoReg = 4;
         |HAZ PintaDatosPantalla;
     |FINSI;

     |SI FSalida = 4;
         |SI enModo = 4;  |ACABA;  |FINSI;

         |HAZ Modificar;
     |FINSI;
|FPRC;

|PROCESO BotonesPanta;
     |CONTROL_SIMPLE nPanta, "BOTON,{{193}}&Alta ", 2264, 2274, Alta;
     nBotonAlta = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{197}}&Modificar ", 2276, 2286, Modificar;
     nBotonModi = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{199}}&Borrar", 2288, 2298, Baja;
     nBotonBaja = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{197}}&Rendimientos", 0580, 0598, Rendimientos;
     nBotonRdto = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{197}} &Y", 2440, 2444, BotonCntr;
     nBotonCntr = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,&1", 2749, 2750, BotonCalc;
     nBotonCalc1 = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,&2", 3215, 3216, BotonCalc;
     nBotonCalc2 = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,&3", 3255, 3256, BotonCalc;
     nBotonCalc3 = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,&4", 3297, 3298, BotonCalc;
     nBotonCalc4 = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,&5", 3597, 3598, BotonCalc;
     nBotonCalc5 = FSalida;

     nEntrar = 0;

     |ESTADO_CONTROL nBotonModi,   "DISABLE";
     |ESTADO_CONTROL nBotonBaja,   "DISABLE";
     |ESTADO_CONTROL nBotonCalc1,  "DISABLE";
     |ESTADO_CONTROL nBotonCalc2,  "DISABLE";
     |ESTADO_CONTROL nBotonCalc3,  "DISABLE";
     |ESTADO_CONTROL nBotonCalc4,  "DISABLE";
     |ESTADO_CONTROL nBotonCalc5,  "DISABLE";

     |SI enModo = 4;
         |ESTADO_CONTROL nBotonAlta, "DISABLE";
     |FINSI;
|FPRC;

|PROCESO Baja20;
     #rem00023#0 = enEmpresa;
     #rem00023#1 = 1;
|FPRC;

|PROCESO Alta20;
     #rem00023#0 = enEmpresa;
     #rem00023#1 = 99;
|FPRC;

|PROGRAMA;
     |HAZ CargaColores;

     enSwRetorno = 0;
     aQueQuiero  = "|NC";
     aNumCampos  = #rem00023#aQueQuiero#;
     nNumCampos  = aNumCampos;
     nNumCampos  = nNumCampos - 1;
     nRango      = 4 + 8 + 16 + 32 + 128;
     enModoReg   = 4;

     |PINPA #0#rew00623;  nPanta = FSalida;

     |HAZ BotonesPanta;

     |DDEFECTO #rem00023;
     |HAZ PintaDatosPantalla;

     |ABRET #rem00023;

     |LINEAL_SIMPLE #1#rem00023, nRango, 0602, 2198, Baja20, Alta20, Evento20;
     nGrid = FSalida;

     |CONTROL_SIMPLE 0, "BOTON,{{001}}sa&Lir", 3488, 3498, Ptec806;
     enBtnSalir = FSalida;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
          FSalida = "::," + nGrid;
          |LEETECLA aTecla;
          |SI aTecla = aEsc1; |SAL;  |FINSI;
          |SI aTecla = aEsc2; |SAL;  |FINSI;

          |SI aTecla = aRetu;
              |HAZ Modificar;
          |FINSI;
     |SIGUE;

     |CIERRAT #rem00023;

     eaPrograma   = "rem00023";
     eaPunto      = "  ";
     |HAZ ProgramaActivo;
|FPRO;
