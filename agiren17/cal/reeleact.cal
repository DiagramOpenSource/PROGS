|FICHEROS;
     reeleact #0;
     relement #1;
|FINSI;

|VARIABLES;
     nCampo = 0;
     nLabel = -1;
     nPanta = -1;
|FIN;

|PROCESO elemento; |TIPO 0;
    |SI #0Campo ! "  ";
       |ABRE #1;
       nCampo = Campo + 1;
       #1#0 = #0Campo;
       |LEE 040#1=;
       |SI FSalida = 0;
           #0nCampo = #1#1;
           |PINTA #0nCampo;
       |SINO;
           |MENAV "    No existe Elemento";
           |ERROR; |ACABA;
       |FINSI;
       |CIERRA #1;
    |SINO;
       nCampo = Campo + 1;
       #0nCampo = "";  |PINTA #0nCampo;
       nCampo = Campo + 2;
       #0nCampo = 0;  |PINTA #0nCampo;
    |FINSI;
|FPRC;

|PROCESO Tipo14;  |TIPO 14;
     |SI nPanta = -1;
         |PINPA #0#0;
         nPanta = FSalida;
     |FINSI;

     |SI nLabel ! -1;
         |FIN_CONTROL_WINDOWS nLabel;
         nLabel = -1;
     |FINSI;

     |SI #0#1 ! "--- ";
         |CONTROL_SIMPLE nPanta, "LABEL,{{7}}Quioscos", 0877, 0887, NULL;
     |SINO;
         |CONTROL_SIMPLE nPanta, "LABEL,{{7}}Batea", 0877, 0887, NULL;
     |FINSI;

     nLabel = FSalida;
|FPRC;

|PROCESO Tipo80; |TIPO 80;
     FSalida = 2599;
|FPRC;
