|FICHEROS;
     rem01030;
|FIN;

|VARIABLES;
      nCampo           = 0;
      nRango           = 0;
      nNumCampos       = 0;
      nGrid            = 0;

      aAlfa            = "";
      aQueQuiero       = "";
      aNumCampos       = "";
      aEsc1            = "";
      aEsc2            = "";
      aRetu            = "";
      aTecla           = "";

     &enEmpresa        = 0;
     &enComplementaria = 0;
     &eaContri         = "";
|FIN;

|PROCESO Baja1030;
     #rem01030#0 = enEmpresa;
     #rem01030#1 = enComplementaria;
     #rem01030#2 = eaContri;
     #rem01030#3 = 1;
|FPRC;

|PROCESO Alta1030;
     #rem01030#0 = enEmpresa;
     #rem01030#1 = enComplementaria;
     #rem01030#2 = eaContri;
     #rem01030#3 = 999;
|FPRC;

|PROGRAMA;
     |ABRET #rem01030;

     |PINPA #0#rem01030;

     nRango      = 4 + 8 + 16 + 32 + 128;

     |LINEAL_SIMPLE #1#rem01030, nRango, 0602, 3498, Baja1030, Alta1030, NULL;
     nGrid = FSalida;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
          FSalida = "::{{001}}Salir," + nGrid;
          |LEETECLA aTecla;
          |SI aTecla = aEsc1; |SAL;  |FINSI;
          |SI aTecla = aEsc2; |SAL;  |FINSI;
          |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;

     |CIERRAT #rem01030;
|FPRO;
