     |HAZ Hoja24;
     |HAZ Hoja25;
     |HAZ Hoja26;
     |HAZ Hoja27;
     |HAZ Hoja28;
     |HAZ Hoja29;
     |HAZ Hoja30;
     |HAZ Hoja31;
     |HAZ Hoja32;

     eaCadena = "</T100020170A0000>" + eaCRLF;
     |HAZ GrabaCadena; eaCadena = "";

     |CIERRA #rem01002; |CIERRA #rem00101; |CIERRA #rem01001;

     |XCIERRA enHandle;
     enSwDatos = 1;
|FPRC;
