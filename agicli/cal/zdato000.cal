|FICHEROS;
  agim0056;
|FIN;


|VARIABLES;
  &eaPathAgicont = "";
  &eaPathSocie   = "";
  aBase          = "";
  aAlfa          = "";
  nCanal         = 0;
  nSw            = 0;
|FIN;

|PROCESO BusquedaApli;
     eaPathAgicont  = "";
     eaPathSocie    = "";

     |DBASS aBase;   |QBF aBase;

     aAlfa = aBase + "agicont/def/canempre.mas";
     |XABRE "E", aAlfa, nCanal;
     |SI FSalida ] 0;
         eaPathAgicont = aBase + "agicont/";
     |FINSI;

     aAlfa = aBase + "sociedad/def/maempr10.mas";
     |XABRE "E", aAlfa, nCanal;
     |SI FSalida ] 0;
         eaPathSocie = aBase + "sociedad/";
     |FINSI;
|FPRC;

|PROCESO eVerRegMer;
     nSw = 0;
     |ABRE #agim0056;
     |LEE 040#agim0056.p;
     |SI FSalida ! 0;
         nSw = 1;
     |SINO;
         #agim0056#0 = "02010";
         |LEE 041#agim0056.=;
         |SI FSalida ! 0;
             nSw = 1;
         |SINO;
             aAlfa = #agim0056#1; |QBF aAlfa;
             |SI aAlfa ! "ALBACETE";
                 nSw = 1;
             |FINSI;
         |FINSI;
     |FINSI;
     |CIERRA #agim0056;

     |SI nSw = 1;
         |SUB_EJECUTA_NP "instalar;agim0056";
     |FINSI;
|FPRC;
