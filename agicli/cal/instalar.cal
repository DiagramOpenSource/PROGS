|FICHEROS;
   instalar #0;

   agim0035 #35;
   agim0135 #135;
   agim0056 #56;

   referen@ #998;
   refere1@ #999;
|FIN;

|VARIABLES;
   XPara     = 0;
   ficheros  = "";
   traba1    = "";
   temporal1 = "";
   temporal2 = "";
   nDCod     = 0;
   nHCod     = 0;
   aParametro = "";
   aDef       = "";
   aPathDef   = "";
   nPara1     = 0;
   nPara2     = 0;
   alfa1      = "";
   nNumCampos = 0;
   nEstado    = 0;
|FIN;

|PROCESO mayor;
  #0Campo = #0Campo > " ";
  |PINTA #0Campo;
|FPRC;

|PROCESO instala; |TIPO 3;

  |SI #0#0 ! "SI";  |ACABA;  |FINSI;
  |DFICE ficheros;
  |SI #0#1 = "SI";
      |ATRI I; |PINTA 2320, "Instalando CNAE 1993-2008";
      traba1 = "agim0035";
      |HAZ copiando;
  |FINSI;

  |SI #0#2 = "SI";
      |ATRI I; |PINTA 2320, "Instalando CNAE 2009 ";
      traba1 = "agim0135";
      |HAZ copiando;
  |FINSI;

  |SI #0#3 = "SI";
      |ATRI I; |PINTA 2320, "Instalando Numero Registro Mercantil";
      traba1 = "agim0056";
      |HAZ copiando;
  |FINSI;


  |ATRI 0;

  |ABRE #035;  |CIERRA #035;
  |ABRE #135;  |CIERRA #135;
  |ABRE #56;   |CIERRA #56;
|FPRC;

|PROCESO copiando;
  temporal1 = ficheros + traba1 + ".ddd";
  temporal2 = ficheros + traba1 + ".dat";
  |COPIA_FICHERO temporal1,temporal2;
  temporal1 = ficheros + traba1 + ".ddx";
  |FBORRA temporal1;

  temporal1 = ficheros + traba1 + ".ixx";
  |FBORRA temporal1;
|FPRC;

|PROGRAMA;
     aParametro = PARAMETRO;   |QBF aParametro;
     |SI aParametro ! "";
         |DDEFECTO #0;
         #0#0  = "SI";
         |SI aParametro = "agim0035";
             #0#1  = "SI";
         |FINSI;
         |SI aParametro = "agim0135";
             #0#2 = "SI";
         |FINSI;
         |SI aParametro = "agim0056";
             #0#3 = "SI";
         |FINSI;
         |HAZ instala;
     |SINO;
         |MANTE #0;
     |FINSI;
|FPRO;
