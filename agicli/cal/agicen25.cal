|FICHEROS;
     agicen25 #25;
|FIN;

|VARIABLES;
      nCampo     = 0;

      aAlfa1     = "";
      aAlfa2     = "";

     &enEmpresa  = 0;
     &enRegistro = 0;
     &eaTipo     = "";
     &enOpcion   = 0;

     &eaVarDni   = "";
     &enCalcCif  = 0;

     &eaTabla    = "";
     &eaDestino  = "";
     &eaAlfa     = "";
     &enError    = 0;
     &enComp     = 0;
     &enPosicion = 0;
|FIN;

|PROCESO LetraDniCen25;  |TIPO 6;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     eaVarDni  = #25Campo;
     enCalcCif = 1;
     |HAZ CalculoDNI;
     #25Campo = eaVarDni;
     |PINTA #25Campo;
|FPRC;

|PROCESO SacaSgCen25;  |TIPO 7;
     eaTabla = "txtm0001.txt";
     |HAZ CargaTabla;
|FPRC;

|PROCESO CompruebaSgCen25;  |TIPO 0;
     |SI FSalida = 10;
         enPosicion = 55;
         |HAZ SacaTabla;
         #25Campo = eaDestino % 102;
         |PINTA #25Campo;
     |FINSI;

     enComp = 2;
     eaAlfa = #25Campo;
     |HAZ CompruebaTabla;

     |SI enError = 1;
         |MENAV "0000 Contenido Incorrecto. Consulta la Tabla con F2";
         |ERROR;
         |ACABA;
     |FINSI;

     |HAZ FinTabla;
|FPRC;

|PROCESO SacaPaisCen25;  |TIPO 7;
     eaTabla = "txtm0006.txt";
     |HAZ CargaTabla;
|FPRC;

|PROCESO CompruebaPaisCen25;  |TIPO 0;
     nCampo   = Campo + 1;
     |SI FSalida = 10;
         enPosicion = 55;
         |HAZ SacaTabla;

         aAlfa1   = eaDestino % 103;
         #25Campo  = aAlfa1;
         |PINTA #25Campo;
     |FINSI;

     enComp = 3;
     eaAlfa = #25Campo;
     |HAZ CompruebaTabla;

     |SI enError = 1;
         |MENAV "0000 Contenido Incorrecto. Consulta la Tabla con F2";
         |ERROR;
         |ACABA;
     |FINSI;

     aAlfa1   = eaDestino % 103;
     aAlfa2   = eaDestino % 520;
     #25Campo  = aAlfa1;
     #25nCampo = aAlfa2;

     |PINTA #25Campo;
     |PINTA #25nCampo;

     |HAZ FinTabla;
|FPRC;

|RUTINA ClaveBaja25;
     #25#0 = enEmpresa;
     #25#1 = enRegistro;
     #25#2 = 1;
|FRUT;

|RUTINA ClaveAlta25;
     #25#0 = enEmpresa;
     #25#1 = enRegistro;
     #25#2 = 99;
|FRUT;

|PROGRAMA;
     |ENTLINEAL #1#25, -3, 2, 22, 1, ClaveBaja25, ClaveAlta25;
|FPRO;
