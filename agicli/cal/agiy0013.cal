|FICHEROS;
     agiy0013 #0;
     agitelef #1;
|FIN;

|VARIABLES;
     SwImpre      = 0;

     ElDesde      = "";
     ElHasta      = "";
     PrimeraLetra = "";

     &ComparaLetra = "";
|FIN;

|PROCESO Imprime;
PrimeraLetra = #1#0 % 101;

|SI ComparaLetra ! PrimeraLetra; |Y SwImpre = 1;
    |PIEINF;
|FINSI;

SwImpre      = 1;
ComparaLetra = PrimeraLetra;
|PRINT;
|FPRC;

|DEFBUCLE agitelef;
#1#1;
;
ElDesde;
ElHasta;
;
NULL, Imprime;
|FIN;

|PROCESO Tipo3; |TIPO 3;
|IMPRE 0;
|SI FSalida ! 0; |ACABA; |FINSI;

|INFOR "agiy0013";
|SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;

ElDesde = #0#0 + "                                       ";
ElHasta = #0#1 + "zzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzzz";

|BUCLE agitelef;

|SI SwImpre = 1; |PIEINF; |FINSI;
|FININF;
|FINIMP;
|FPRC;
