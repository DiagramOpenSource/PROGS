|FICHEROS;
     agiy0009 #0;   || Este
     agim0006 #1;
     agicen00 #2;
     agicen01 #3;
|FIN;

|VARIABLES;
     &pobfech = "";
|FIN;

|PROCESO llena; |TIPO 0;
     |ABRE #1;
     |DDEFECTO #1;
     |LEE 040#1=;
     #0#18 = #1#0;  || Nombre
     #0#19 = #1#1;  || NIF
     |CIERRA #1;
     |PINTA #0#18;
     |PINTA #0#19;
     ||-------------------
     |ABRE #2; |ABRE #3;
     #2#0 = #0#0;
     |LEE 040#2=;
     |SI FSalida = 0;
          #3#0 = #0#0;
          #3#1 = 1;
          |LEE 040#3=;
          |SI FSalida = 0;
               #0#10 =  #3#4; || Actividad
               #0#11 =  #3#2; || Tipo Epigrafe
               #0#12 =  #3#3; || Epigrafe
               |PINTA #0#10;
               |PINTA #0#11;
               |PINTA #0#12;
          |FINSI;
     |FINSI;
     |CIERRA #2; |CIERRA #3;
|FPRC;


|PROCESO imprime; |TIPO 3;
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR "agiy0009";
          |SI FSalida = 0;
               pobfech = #0#9;
               |QBF pobfech;
               pobfech = pobfech + " a " + #0#20 + ".";
               |PRINT;
               |PIEINF;
               |FININF;
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRC;
