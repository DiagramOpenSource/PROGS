|FICHEROS;
     agiy0018 #0;        || Este Def.
     agim0009 #1;        || Llamadas
|FIN;

|VARIABLES;
     Fecha     = @;
     SwImprime = 0;
     &Vacio    = "                                                  ";
|FIN;

|PROCESO Impresion;
  |SI SwImprime = 0; SwImprime = 1;  Fecha = #1#0;  |FINSI;
  |SI Fecha ! #1#0;  |PIEINF;        Fecha = #1#0;  |FINSI;

  |PRINT;
|FPRC;

|DEFBUCLE agim0009;
  #1#1;
  ;
  #0#0, 00, 00;
  #0#1, 99, 99;
  ;
  NULL, Impresion;
|FIN;

|PROCESO Tipo3; |TIPO 3;
  |IMPRE 0;
  |SI FSalida ! 0; |ACABA; |FINSI;

  |INFOR "agiy0018";
  |SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;

  |BUCLE agim0009;
  |SI SwImprime = 1; |PIEINF;  |FINSI;

  |FININF;
  |FINIMP;
|FPRC;
