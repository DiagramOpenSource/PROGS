|FICHEROS;
     agicen49 #0;
|FIN;

|VARIABLES;
     nCampo = 0;
     nCamp1 = 0;
     fFecha = @;
|FIN;

|PROCESO Fecha49;
     |SI Campo ] 4; |Y Campo [ 11;
          nCampo = Campo + 1;
     |FINSI;
     |SI Campo ] 12; |Y Campo [ 84;
          nCampo = Campo + 2;
     |FINSI;
     |SI Campo = 79; |O Campo = 122;
          nCampo = Campo + 1;
     |FINSI;
     |SI Campo = 43;
          nCampo = Campo + 4;
     |FINSI;

     |SI Campo ] 102; |Y Campo ! 122;
          nCampo = Campo + 2;
     |FINSI;

     |CAMPO_MODIFICA #0nCampo, 1, "S";
     |SI #0Campo = " ";
          |CAMPO_MODIFICA #0nCampo, 1, "N";
          #0nCampo = "01.01.0000";
          |PINTA #0nCampo;
     |FINSI;

     |SI Campo ] 12; |Y Campo [ 76; |Y Campo ! 43;
          nCampo = Campo + 1;
          |CAMPO_MODIFICA #0nCampo, 1, "S";
          |SI #0Campo = " ";
               |CAMPO_MODIFICA #0nCampo, 1, "N";
               #0nCampo = "";
               |PINTA #0nCampo;
          |FINSI;
     |FINSI;
     |SI Campo ] 102;  |Y Campo ! 122;
          nCampo = Campo + 1;
          |CAMPO_MODIFICA #0nCampo, 1, "S";
          |SI #0Campo = " ";
               |CAMPO_MODIFICA #0nCampo, 1, "N";
               #0nCampo = "";
               |PINTA #0nCampo;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Desactiva49;
     |PARA nCampo = 90; |SI nCampo [ 101; |HACIENDO nCampo = nCampo + 1;
          |CAMPO_MODIFICA #0nCampo, 1, "S";
     |SIGUE;
     |SI #0#89 = "S";
          |PARA nCampo = 90; |SI nCampo [ 101; |HACIENDO nCampo = nCampo + 1;
               |CAMPO_MODIFICA #0nCampo, 1, "N";
          |SIGUE;
     |FINSI;
|FPRC;

|PROCESO IvaTipo7Impo; |TIPO 7;
     |SI #0Campo = "N";
         #0Campo = ""; |PINTA #0Campo;
     |FINSI;
|FPRC;

|PROCESO IvaImpo;
     |SI Campo = 118; nCampo = 119; |FINSI;
     |SI Campo = 119; nCampo = 118; |FINSI;
     |SI Campo = 124; nCampo = 125; |FINSI;
     |SI Campo = 125; nCampo = 124; |FINSI;
     |SI Campo = 127; nCampo = 128; |FINSI;
     |SI Campo = 128; nCampo = 127; |FINSI;
     |SI #0Campo = "S";
         #0nCampo = ""; |PINTA #0nCampo;
     |FINSI;
     |SI Campo = 119; |HAZ IvaFecha; |FINSI;
     |SI Campo = 125; |HAZ IvaFecha; |FINSI;
     |SI Campo = 128; |HAZ IvaFecha; |FINSI;
|FPRC;

|PROCESO IvaTipo7Fecha; |TIPO 7;
     |SI Campo = 121;
         |SI #0#118 = "S"; |O #0#119 = "S";
             |SI #0#121 [ "01.01.1900";
                 fFecha = ~;
                 #0#121 = fFecha; |PINTA #0#121;
             |FINSI;
         |FINSI;
     |FINSI;
     |SI Campo = 126;
         |SI #0#124 = "S"; |O #0#125 = "S";
             |SI #0#126 [ "01.01.1900";
                 fFecha = ~;
                 #0#126 = fFecha; |PINTA #0#126;
             |FINSI;
         |FINSI;
     |FINSI;
     |SI Campo = 129;
         |SI #0#127 = "S"; |O #0#128 = "S";
             |SI #0#129 [ "01.01.1900";
                 fFecha = ~;
                 #0#129 = fFecha; |PINTA #0#129;
             |FINSI;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO IvaFecha;
     |SI #0#118 = " "; |Y #0#119 = " ";
         #0#121 = "01.01.0000"; |PINTA #0#121;
     |FINSI;
     |SI #0#124 = " "; |Y #0#125 = " ";
         #0#126 = "01.01.0000"; |PINTA #0#126;
     |FINSI;
     |SI #0#127 = " "; |Y #0#128 = " ";
         #0#129 = "01.01.0000"; |PINTA #0#129;
     |FINSI;
|FPRC;
