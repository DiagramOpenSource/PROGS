|FICHEROS;
    agicen19 #19;
    agifigen #1;
|FIN;

|VARIABLES;
     nForma        = 0;

     aParametro    = "";
     aAlfa         = "";

     &enCodCli     = 0;
     &enEmpresa    = 0;
     &enModo       = 0;
     &eaNombreEmp  = "";
|FIN;

|PROCESO Tipo2;  |TIPO 2;
     enModo      = (FEntrada / 100) ! 0;
     enEmpresa   = #19#1;
     |SI enModo = 3;
         |CONFI "0000NSeguro de dar de baja la Empresa y todos sus registros.";
         |SI FSalida = 0;
             |SI #19#0 = "036";  |SUB_EJECUTA_NP "agicen20";  |FINSI;
             |SI #19#0 = "840";  |SUB_EJECUTA_NP "agiw0032";  |FINSI;
         |SINO;
             |ERROR;
         |FINSI;
         |ACABA;
     |FINSI;

     nForma = 0 +. 1;

     |SI nForma ! 1;  |ACABA;  |FINSI;

     enEmpresa   = #19#1;
     eaNombreEmp = #19#2;
     enModo      = (FEntrada / 100) ! 0;

     |SI #19#0 = "036";  |SUB_EJECUTA_NP "agicen20";  |FINSI;
     |SI #19#0 = "840";  |SUB_EJECUTA_NP "agiw0032";  |FINSI;
|FPRC;

|RUTINA ClaveBaja19;
     #19#0 = aParametro;
     #19#1 = 1;
|FRUT;

|RUTINA ClaveAlta19;
     #19#0 = aParametro;
     #19#1 = 99999;
|FRUT;

|PROGRAMA;
     aParametro = PARAMETRO;  |QBF aParametro;

     |SI aParametro = "036";
         |CABEZA "Acceso Modelo 036 y 037";

         |PINPA #0#19;
         |ATRI S;
         |PINTA 0516, "Modelo 036 y 037";
         |ATRI s;
     |FINSI;

     |SI aParametro = "840";
         |CABEZA "Acceso Modelo 840";

         |PINPA #0#19;
         |ATRI S;
         |PINTA 0516, "Modelo 840";
         |ATRI s;
     |FINSI;

     |ENTLINEAL #1#19, -2, 2, 22, 0, ClaveBaja19, ClaveAlta19;
|FPRO;
