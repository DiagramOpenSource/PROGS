|FICHEROS;
     agim0022 #0;
     agim0023 #1,MANTE;
|FIN;

|VARIABLES;
     &Cliente   = 0;
     &Registro  = 0;
     &Modo      = 0;

     Modo1      = 0;
     Forma      = 0;
     SwSalir    = 0;
|FIN;

|RUTINA ClaveBaja0;
  #0#0 = Cliente;
  #0#1 = Registro;
  #0#2 = 1;
|FRUT;

|RUTINA ClaveAlta0;
  #0#0 = Cliente;
  #0#1 = Registro;
  #0#2 = 99;
|FRUT;

|PROGRAMA;
  |SI Modo ! 4;
      |ENTLINEAL #1#0, -3, 2, 15, 1, ClaveBaja0, ClaveAlta0;
  |SINO;
      |ENTLINEAL #1#0, -3, 4, 15, 1, ClaveBaja0, ClaveAlta0;
  |FINSI;
|FPRO;

|RUTINA ClaveBaja1;
  #1#0 = #0#0;
  #1#1 = #0#1;
  #1#2 = #0#2;
  #1#3 = 1;
|FRUT;

|RUTINA ClaveAlta1;
  #1#0 = #0#0;
  #1#1 = #0#1;
  #1#2 = #0#2;
  #1#3 = 99;
|FRUT;

|PROCESO Borra01;
  |BORRA 140#1a;
  |LIBERA #1;
|FPRC;

|DEFBUCLE agim0023;
  #1#1;
  ;
  #0#0, #0#1, #0#2, 00;
  #0#0, #0#1, #0#2, 99;
  be;
  NULL, Borra01;
|FIN;

|PROCESO VeteAl13;  |TIPO 2;
  Modo1  = (FEntrada / 100) ! 0;
  |SI Modo1 = 3;
      |BUCLE agim0023;
      |ACABA;
  |FINSI;

  Forma = 0 +. 1;
  |SI Forma ! 1;  |ACABA;  |FINSI;

  |ENTLINEAL #1#1, -4, 2, 21, 0, ClaveBaja1, ClaveAlta1;
|FPRC;

|PROCESO Tipo7F0; |TIPO 7;
  SwSalir = 1;
  |ENTLINEAL #1#1, -4, 4, 21, 0, ClaveBaja1, ClaveAlta1;
  SwSalir = 0;
|FPRC;

|PROCESO Tipo2F1;  |TIPO 2;
  #0#6 = #0#6 +. #1#7;
|FPRC;

|PROCESO Tipo7F1;  |TIPO 7;
  |SI SwSalir = 0;  |ACABA;  |FINSI;

  |PTEC 807;
|FPRC;
