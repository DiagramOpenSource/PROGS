|FICHEROS;
    reparame #0;
|FIN;

|VARIABLES;
    cab = "Parametros Renta";
    vacio = "                              ";
|FIN;

|PROCESO Mayus;
     #0Campo = #0Campo > #0Campo;
     |PINTA #0Campo;
|FPRC;

|PROCESO PintaDerecha;
     |ATRI R;
     |SI #0#2  = "I";  |PINTA 1039, "DECLARACIONES INDIVIDUALES              ";  |FINSI;
     |SI #0#2  = "C";  |PINTA 1039, "DECLARACIONES CONJUNTAS                 ";  |FINSI;
     |SI #0#2  = "S";  |PINTA 1039, "DECLARACIONES SEPARADAS                 ";  |FINSI;
     |SI #0#3  = "G";  |PINTA 1139, "REGIMEN GANANCIAL                       ";  |FINSI;
     |SI #0#3  = "S";  |PINTA 1139, "REGIMEN SEPARACION DE BIENES            ";  |FINSI;
     |SI #0#3  = "O";  |PINTA 1139, "OTROS REGIMENES                         ";  |FINSI;
     |SI #0#8  = "M";  |PINTA 1339, "CONTADOR MANUAL                         ";  |FINSI;
     |SI #0#8  = "A";  |PINTA 1339, "CONTADOR AUTOMATICO                     ";  |FINSI;
     |SI #0#13 = 1;    |PINTA 1639, "EL 20% + 15% SOBRE EL RENDIMIENTO.      ";  |FINSI;
     |SI #0#13 = 2;    |PINTA 1639, "EL 20% + 15% EN CASCADA.                ";  |FINSI;
     |SI #0#15 = "S";  |PINTA 1839, "EL MODELO 214 RECOGE EL TOTAL CALCULADO ";  |FINSI;
     |SI #0#15 = "N";  |PINTA 1839, "EL MODELO 214 NO RECOGE EL TOTAL        ";  |FINSI;
|FPRC;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |CABEZA cab;
     |ABRE #0;
     |DDEFECTO #0;
     |LEE 101#0p;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     |PINDA #0#0;
     |HAZ PintaDerecha;
     |ENDATOS #1#0;
     |SI FSalida = 0; |GRABA 020#0a; |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;
