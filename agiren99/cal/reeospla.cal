|CALCULO lee_concep;
     |DBASS alfa1;  |QBF alfa1;
     |PATH_DAT #20 alfa1;

     |ABRE #20;
     |LEE 040#20p;
     |SI FSalida ! 0; |DDEFECTO #20; |FINSI;
     |CIERRA #20;

     agieos = #20#3; |QBF agieos;
     Comodin = agieos;
     FSalida = 0;
     |PARA; |SI FSalida = 0; |HACIENDO;
        Caracter = Comodin % -101;
        |SI Caracter = "/";
           |SI Primero = 0;
              Primero = 1;
           |SINO;
              |SAL;
           |FINSI;
        |FINSI;
        LongAlfa = Comodin % 0;
        Long = LongAlfa;
        Long = Long - 1;
        Calc = 100 + Long;
        Comodin = Comodin % Calc;
     |SIGUE;


     Comodin = Comodin - "fich/";
     Comodin = Comodin + "def/eosconpl.mas";
     |CARGA_DEF Comodin, concpla@;
     |SI FSalida < 0;
        |MENAV "    Problemas al abrir la referencia ";
        |ERROR;
        |ACABA;
     |FINSI;

     |PATH_DAT #1 agieos;

     |ABRE #concpla@;
     #concpla@#0 = 99;
     |LEE 001#concpla@.=;
     |SI FSalida ! 0;
         |MENAV "    Conceptos de Gastos no definidos en R.E.O. ...";
         |PTEC 807;
     |FINSI;
|FCAL;

|CALCULO defecto;
     |PARA xxx = 1; |SI xxx [ 20; |HACIENDO xxx = xxx + 1;
           #0xxx = #1xxx;
     |SIGUE;

     |PARA xxx = 61; |SI xxx [ 80; |HACIENDO xxx = xxx + 1;
           yyy = xxx - 40;
           #0xxx = #1yyy;
     |SIGUE;
|FCAL;

