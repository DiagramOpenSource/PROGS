|FICHEROS;
   rexvalor #0;
   reyvalor #1;
   reyvalon #2;
|FIN;

|VARIABLES;
   &empresa    = 0;
   &valorcillo = 0;
   lin         = 0;
   fich        = "";
   traba1      = "";
   traba2      = "";
   temporal1   = "";
   temporal2   = "";
|FIN;

|PROGRAMA;
|PINPA #0#0;
|HAZ base;
|HAZ impresio;
|DELINDEX #1;
|DELINDEX #2;
|FPRO;

|PROCESO base;
  |DFICE fich;
  |SI valorcillo = 1;
      traba1 = "revalore";  traba2 = "reyvalor";  |HAZ copiando;
  |SINO;
      traba1 = "revalono";  traba2 = "reyvalon";  |HAZ copiando;
  |FINSI;
|FPRC;

|PROCESO copiando;
    temporal1 = fich + traba1 + ".ixx";
    temporal2 = fich + traba2 + ".ixx";
    |COPIA_FICHERO temporal1,temporal2;
    |SI FSalida ! 0;  |ACABA;  |FINSI;
    temporal1 = fich + traba1 +  ".dat";
    temporal2 = fich + traba2 + ".dat";
    |COPIA_FICHERO temporal1,temporal2;
    |SI FSalida ! 0;
        temporal2 = fich + traba2 + ".ixx";
        |FBORRA temporal2;
        |ACABA;
    |FINSI;
    temporal1 = fich + traba1 + ".ddx";
    temporal2 = fich + traba2 + ".ddx";
    |COPIA_FICHERO temporal1,temporal2;
|FPRC;
|| *************************************************************************
||                        Impresion
|| *************************************************************************
|PROCESO pintael1;
#0#0 = #1#13;
#0#1 = #1#14;
#0#2 = #1#1;
#0#3 = #1#18;
#0#4 = #1#16;
#0#5 = #1#17;

Pos = lin;
|PINDA #0#0;
Pos = -1;

lin = lin + 100;
|SI lin > 1000;
    lin = 100;
    |PAUSA;
    |PINPA #0#0;
|FINSI;
|FPRC;

|PROCESO pintael2;
#0#0 = #2#13;
#0#1 = #2#14;
#0#2 = #2#1;
#0#3 = #2#18;
#0#4 = #2#16;
#0#5 = #2#17;

Pos = lin;
|PINDA #0#0;
Pos = -1;

lin = lin + 100;
|SI lin > 1000;
    lin = 100;
    |PAUSA;
    |PINPA #0#0;
|FINSI;
|FPRC;

|DEFBUCLE valores;
#1#1;
;
empresa,01;
empresa,99;
e;
NULL,NULL,NULL,NULL,NULL,pintael1;
#1#13;
|FIN;

|DEFBUCLE novalores;
#2#1;
;
empresa,01;
empresa,99;
e;
NULL,NULL,NULL,NULL,NULL,pintael2;
#2#13;
|FIN;

|PROCESO impresio;
lin = 100;
|SI valorcillo = 1;
    |BUCLE valores;
|SINO;
    |BUCLE novalores;
|FINSI;
|PAUSA;
|FPRC;
