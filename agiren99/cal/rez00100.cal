|PROCESO MiraEmpresas;
     nRegistro   = 0;
     aSesion     = #87#0;  |QBF aSesion;
     aDirSesion  = "/MODELOS/" + eaTipoSec + "/";
     aFichero    = "/MODELOS/" + eaTipoSec + "/*.txt";
     |R_PDIR aFichero;
     |PARA; |SI; |HACIENDO;
            |SI FSalida ! 0; |SAL; |FINSI;

            aError = aFichero - aDirSesion;
            aAlfa  = aError % 405;
            #1#0   = aAlfa;
            |LEE 040#1=;
            |SI FSalida = 0;
                nHePasado = 1;
                nRegistro = 1;
                #88#0 = #87#0;
                #88#1 = #1#0;
                #88#2 = #1#2;
                |GRABA 020#88n;
                |LIBERA #88;
            |SINO;
                aAlfa  = aError % 102;
                aAlfa  = aAlfa > aAlfa;
                |SI aAlfa = "RT";
                    nHePasado = 1;
                    nRegistro = 1;
                    #88#0 = #87#0;
                    #88#1 = 1;
                    #88#2 = "SOPORTE " + aError;
                    |GRABA 020#88n;
                    |LIBERA #88;
                |FINSI;
            |FINSI;
            |R_SDIR aFichero;
     |SIGUE;

     |SI nRegistro = 0;
         |BORRA 020#87a;
         |LIBERA #87;
     |FINSI;
|FPRC;

