|FICHEROS;
      rezcalcu #99;
      rem00100 #100;

     &refere1@ #1001;
     &refere2@ #1002;
|FIN;

|VARIABLES;
     nDias             = 0;

     aPathRenta        = "";
     aDef              = "";
     aFichero          = "";
     aAlfa             = "";
     fFecha            = @;

     &enEmpresa        = 0;
     &enComplementaria = 0;
     &eaPathDef        = "";
     &enSwPresenta     = 1;
     &enNomeDat        = 0;
     &eaMuerto         = "";
|FIN;

|PROGRAMA;
     |PINPA #0#99;
     eaMuerto = "";

     |ABRE #100;
     #100#0 = enEmpresa;
     |LEE 040#100=;
     |SI FSalida ! 0;  |DDEFECTO #100; |FINSI;
     |SI #100#90 = "S";  eaMuerto = #100#91;  |QBT eaMuerto;  |FINSI;
     |CIERRA #100;

     enSwPresenta = 1;
     |HAZ SacaDeclarantes;
     enSwPresenta = 0;

     |DBASE aPathRenta;  |QBT aPathRenta;
     eaPathDef = aPathRenta + "def/";

     |SI enNomeDat = 0;  |HAZ BorraLiquidacion;  |FINSI;

     aDef = eaPathDef + "rem01001.mas";
     |CARGA_DEF aDef, refere1@;
     |SI FSalida ! 0;
         aAlfa = "     Error en cargar el Def " + aFichero + ".mas";
         |MENAV aAlfa;
         |ACABA;
     |FINSI;

     |SI enNomeDat = 1;  |NOME_DAT #1001 "zdat1001";  |FINSI;

     aDef = eaPathDef + "rem01002.mas";
     |CARGA_DEF aDef, refere2@;
     |SI FSalida ! 0;
         aAlfa = "     Error en cargar el Def " + aFichero + ".mas";
         |MENAV aAlfa;
         |ACABA;
     |FINSI;

     |SI enNomeDat = 1;  |NOME_DAT #1002 "zdat1002";  |FINSI;

     |ABRET #1001;
     |ABRET #1002;

     |HAZ Bloque1;
     |HAZ Bloque2;
     |HAZ Bloque3;
     |HAZ Bloque4;
     |HAZ Bloque5;
     |HAZ Bloque6;
     |HAZ Bloque7;

     |CIERRAT #1001;
     |CIERRAT #1002;

     |DESCARGA_DEF #refere2@;
     |DESCARGA_DEF #refere1@;
|FPRO;
