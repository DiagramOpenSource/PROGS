     aAlfa = aAlfa % 180;
     |IMPRIME aAlfa;

     aAlfa = (" " * 60) + " N.I.F.:" + #1#2;
     |IMPRIME aAlfa;

     aAlfa = aRaya * 80;
     |IMPRIME aAlfa;
|FPRC;

|PROCESO Pie;
     |SI #0#49 = "S"; |ACABA; |FINSI;

     |PARA i = nLinea; |SI i < #0#38; |HACIENDO i = i + 1;
          |IMPRIME "";
     |SIGUE;
     |SI #0#40 = "S"; |IMPRIME aSalto; |FINSI;
