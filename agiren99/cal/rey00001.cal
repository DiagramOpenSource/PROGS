|PROCESO ImpreWord; |TIPO 0;
     aAlfa = #0#49;
     |C_M #0#50, 1 , aAlfa;
     |C_M #0#52, 1 , aAlfa;
     |SI aAlfa = "S";
          #0#48 = "S";
          |PINTA #0#48;
     |FINSI;
|FPRC;

|PROCESO seleccion; |TIPO 0;
     |SI #0#0 = "S";
