|PROCESO facturas;
    serie = #0#20;
    factura = #0#21;
    fech = #0#22;
|HAZ desconfec;
|IMPRE 0;
|SI FSalida = 0;
    |ABRE #3;
        #3#0 = 1;        || lee datos empresa (siempre 1)
    |LEE 001#3=;
    |SI FSalida ! 0; |DDEFECTO #3; |FINSI;
    |CIERRA #3;
        informe = #3#8;
    |INFOR informe;
    |SI FSalida = 0;
            dto = #0#24;
            retsino = #0#12;
        |HAZ linbuclf;
        |FININF;
        |HAZ anula;
    |SINO;
        |MENAV "     No existe informe.";
    |FINSI;
    |FINIMP;
|FINSI;
|FPRC;

