|| ***************** CALCULOS DE IMPRESION RECIBOS ********************
|| *********************************************************************

|PROCESO imprelir;
|SI indihono < 6;
        Ides = des1 <-;
        Ides = Ides + indihono;
        des = #2#6;
        Iimp = imp1 <-;
        Iimp = Iimp + indihono;
        imp = #2#13;
        indihono = indihono + 1;
|FINSI;
|SI #2#10 = "H"; hono  = hono  + #2#13; |FINSI;
|SI #2#10 = "S"; supli = supli + #2#13; |FINSI;
|SI #2#10 = "A"; acue  = acue  + #2#13; |FINSI;
|FPRC;

|PROCESO escribe;
     |ABRE #7;
     #7#0 = #0#1;
     |LEE 001#7=;
     |SI FSalida ! 0; |DDEFECTO #7; |FINSI;
     |CIERRA #7;

     |PRINT;
     |HAZ borra;
     indihono = 0;
|FPRC;

|PROCESO linbuclr;
|BUCLELIN 0imprelir#0;
|HAZ calcula;
|HAZ escribe;
|HAZ anula;
|PIEINF;
|FPRC;

|PROCESO recibos;
    serie = #0#20;
    factura = #0#21;
    fech = #0#22;
|HAZ desconfec;
|IMPRE 0;
|SI FSalida = 0;
    |ABRE #3;
        #3#0 = 1;
    |LEE 040#3=;
    |SI FSalida ! 0; |DDEFECTO #3; |FINSI;
    |CIERRA #3;
        informe = #3#9;
    |INFOR informe;
    |SI FSalida = 0;
            dto = #0#24;
        |HAZ linbuclr;
        |FININF;
    |SINO;
        |MENAV "    Error no existe informe.";
    |FINSI;
    |FINIMP;
|SINO;
    |MENAV "      Impresion Cancelada.";
|FINSI;
|FPRC;

|PROCESO calcula;
    tdto = hono % dto;
    base = hono - tdto;
    tiva = base % ivabo;
|SI #0#12 = "S";|O #0#12 = "s";
        tret = base % ret;
|SINO;
        tret = 0;
|FINSI;
    tfac = base + supli;
    tfac = tfac + tiva;
    tpagar = (tfac - acue) - tret;
    rlecan = tpagar ! 0;
|HAZ lletres;
|FPRC;

|PROCESO anula;
    hono = 0;
    supli = 0;
    acue = 0;
    tret = 0;
    tdto = 0;
    base = 0;
    acum = 0;
|FPRC;

|PROCESO borra;
    Ides = des1 <-;
    Iimp = imp1 <-;
|PARA i = 0;|SI i < 6;|HACIENDO i = i + 1;
        des = vacio;
        imp = cero;
        Ides = Ides + 1;
        Iimp = Iimp + 1;
|SIGUE;
|FPRC;

