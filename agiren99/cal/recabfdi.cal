|PROCESO lineas;
|SI #2#10 = "H";
        hono = #2#13;
|SINO;
    |SI #2#10 = "S";
            supli = #2#13;
    |SINO;
            acue = #2#13;
    |FINSI;
|FINSI;
    thono = thono + hono;
    tsupli = tsupli  + supli;
    tacum = tacum + acue;
|HAZ fechabien;

|| **********************  Graba Conceptos **********************

    #4#0 = #2#4;
    #4#1 = #2#5;
|LEE 101#4=;
|SI FSalida = 0;
        #4#6 = #4#6 + #2#13;
        #4#7 = #4#7 + #2#13;
    |GRABA 020#4a;
|FINSI;
    hono = 0;
    supli = 0;
    acue = 0;
|LIBERA #4;

|| **********************  Graba Historico Renta **********************

    concepto = concepto + 1;       || solo graba un honorario y un suplido!!!
|SI concepto = 1;
    |SI #4#3 = "H";|Y carga_h = 0;      || honorarios
            #20#03 = #2#4;
            #20#04 = #2#5;
            #20#07 = #2#13;
            #20#09 = #20#09 + (#2#13 % ivabo);
            carga_h = 1;
    |FINSI;
    |SI #4#3 = "S";|Y carga_s = 0;      || suplidos
            #20#05 = #2#4;
            #20#06 = #2#5;
            #20#08 = #2#13;
            carga_s = 1;
    |FINSI;
|FINSI;
|SI concepto = 2;
    |SI #4#3 = "H";|Y carga_h = 0;      || honorarios
            #20#03 = #2#4;
            #20#04 = #2#5;
            #20#07 = #2#13;
            #20#09 = #20#09 + (#2#13 % ivabo);
            carga_h = 1;
    |FINSI;
    |SI #4#3 = "S";|Y carga_s = 0;      || suplidos
            #20#05 = #2#4;
            #20#06 = #2#5;
            #20#08 = #2#13;
            carga_s = 1;
    |FINSI;
|FINSI;
|FPRC;

|PROCESO remata;          || graba el historico
    #20#00 = #0#1;        || codigo
    #20#01 = #7#96;       || tarifa
    #20#02 = fecha;       || fecha
    #20#10 = "D";         || directa (D)
    |GRABA 020#20n;
|FPRC;

|PROCESO borrahis;
    #20#0 = #0#1;
|LEE 001#20=;
|SI FSalida = 0;
    |BORRA 020#20a;
|FINSI;
|DDEFECTO #20;
    concepto = 0;
|FPRC;

|PROCESO actuali;
|HAZ anula;
|ABRE #4;
|ABRE #20;
|HAZ borrahis;
    dto = #0#24;
|BUCLELIN 0lineas#0;
|HAZ remata;
|CIERRA #4;
|CIERRA #20;
    tdto = thono % dto;
    base = thono - tdto;
    timp = base % ivabo;
    tfac = base + tsupli + timp;
|SI #0#12 ! "S";
        ret = 0;
        tret = 0;
|SINO;
        tret = base % ret;
|FINSI;
    total = tfac - tret;
    tpagar = total - tacum;
|FPRC;

|PROCESO fechabien;
    feclin = "0" + #2#3;
    fecsys = ~;
    anyoc = fecsys % 704;
    mesc = feclin % -102;
    numero = mesc % A0;
|SI numero = 1;
        mesc = "0" + mesc;
|FINSI;
    diac = feclin % -302;
    fecha = diac + "." + mesc + "." + anyoc;
|FPRC;

|PROCESO lletres;
     || ======== RUTINA DE NUMEROS A LETRAS ===========
     || VARIABLE DE ENTRADA     RLECAN# = IMPORT(CENTIM)
     || =======  RLEMAX=longitud de le primera impresion.
     || ======= FG$(49,25) tabla de variables =============
     || === variables a dimensionar ===== RLSALI$(70),RLSALA$(70)********
     ||====== la linea de impresion segunda se devuelve en RLSALI$ preguntar===
  rlemax  = 50;
  r9 = 0;
  jkl = 0;
  rlsali = " ";
  rlsala = "";
  r7 = (rlecan*100)+100000000000;
  r1 = rlemax+1000;
  rctraba = r7;
  rc = rctraba % 211;
  rctraba = rc % 103;
  |SI rctraba = "000";
      |VETE  en1560;
  |FINSI;
  fg1 = "UN ";
  fg2 = "DOS ";
  fg30= "DOS-CENTS ";
  fg31= "TRES-CENTS ";
  fg32= "QUA-TRE-CENTS ";
  fg33= "CINC-CENTS ";
  fg34= "SIS-CENTS ";
  fg35= "SET-CENTS ";
  fg36= "VUIT-CENTS ";
  fg37= "NOU-CENTS ";
  |HAZ en1760;
  |SI r1 = 1;
      r2 = 40;
  |SINO;
      r2 = 41;
  |FINSI;
  |HAZ en1960;
  r2 = 47;
  rctraba = rc % 406;
  |SI rctraba = "000000";
      |HAZ en1960;
  |FINSI;
|ET en1560;
  rf = rc % 408;
  rc = rf;
  fg1 = "UNA ";
  fg2 = "DUES ";
  fg30= "DUES-CEN-TES ";
  fg31= "TRES-CEN-TES ";
  fg32= "QUA-TRE-CEN-TES ";
  fg33= "CINC-CEN-TES ";
  fg34= "SIS-CEN-TES ";
  fg35= "SET-CEN-TES ";
  fg36= "VUIT-CEN-TES ";
  fg37= "NOU-CEN-TES ";
  rctraba = rc % 103;
  |SI rctraba > "001";
      |HAZ en1760;
  |FINSI;
  r2 = 39;
  rctraba = rc % 103;
  |SI rctraba ! "000";
      |HAZ en1960;
  |FINSI;
  rf = rc % 405;
  rc = rf;
  rctraba = rc % 103;
  |SI rctraba ! "000";
      |HAZ en1760;
  |FINSI;
  |SI rlecan ! 1;
      r2 = 43;
  |SINO;
      r2 = 42;
  |FINSI;
  |HAZ en1960;
  rctraba = rc % 402;
  rf = "0" + rctraba;
  rc = rf;
  rctraba = rc % 103;
  |SI rctraba = "000";
      |VETE en1730;
  |FINSI;
  r2 = 48;
  |HAZ en1960;
  fg1 = "UN ";
  |HAZ en1760;
  rctraba = rc % 302;
  |SI rctraba = "01";
      r2 = 44;
  |SINO;
      r2 = 45;
  |FINSI;
  |HAZ en1960;
|ET en1730;
  |SI rlemax ! 999;
      rprim = rlsali;
      rlsali = " ";
  |FINSI;
  rsegu = rlsali;
|FPRC;

|PROCESO en1760;
  rctraba = rc % 101;
  r4 = rctraba;
  rctraba = rc % 201;
  r5 = rctraba;
  rctraba = rc % 301;
  r6 = rctraba;
  rctraba = rc % 202;
  r8 = rctraba;
  rctraba = rc % 103;
  r1 = rctraba;
  |SI r4 = 0;
      |VETE en1820;
  |FINSI;
  |SI r1 = 100;
      r2 = 27;
  |SINO;
      r2 = 28;
  |FINSI;
  r2 = r2 + r4;
  |HAZ en1960;
|ET en1820;
  |SI r5 > 2;
      |VETE en1880;
  |FINSI;
  |SI r8 < 21;
      |VETE en1930;
  |FINSI;
  r2 = 38;
  |HAZ en1960;
  r8 = r6;
  |VETE en1930;
|ET en1880;
  r2 = r5 + 18;
  |HAZ en1960;
  r8 = r6;
|ET en1930;
  r2 = r8;
  |SI r2 ! 0;
      |HAZ en1960;
  |FINSI;
|FPRC;

|PROCESO en1960;
  Ifg = fg1 <-;
  Ifg = Ifg + r2 - 1;
  rd = fg;
  rctraba = rd % 0;
  aa = rctraba;
  |PARA r3 = 1; |SI r3 [ aa; |HACIENDO r3 = r3 + 1;
       bb = (100 * r3) + 1;
       ra = rd % bb;
       |SI ra = "-"; |O ra = " ";
           |VETE en2030;
       |FINSI;
       |SI ra = ".";
           ra = " ";
       |FINSI;
       rlsala = rlsali + ra;
       ra = ".";
       r9 = r9 + 1;
       rlsali = rlsala;
       |VETE en2090;
|ET en2030;
       jj = rlemax - 4;
       |SI jj > r9;
           |VETE en2090;
       |FINSI;
       rlsala = rlsali + ra;
       rlsali = rlsala
       r9 = 0;
       rlemax = 999;
       rprim = rlsali;
       jkl = 1;
       rlsali = "";
|ET en2090;
       |SI ra = " ";
           r3 = 999;
       |FINSI;
 |SIGUE;
 r9 = r9 + 1;
 rlsala = rlsali + ra;
 rlsali = rlsala;
|FPRC;

