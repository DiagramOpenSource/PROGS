|FICHEROS;
   recappla #0;
   reeospla #1;
   activid@ #2;
   cliente@ #3;
   mode130@ #4;
   mode131@ #5;
   planill@ #6;
   comulin@ #7;
   rem00025 #8;
   rem00026 #9;
   contrab@ #10;
   rem00031 #11;
|FIN;

|VARIABLES;
   Comodin    = "";
   Comodin1   = "";
   Path       = "";
   Concepto   = 0;
   Calc1      = 0;
   Calc2      = 0;
   Linea1     = 0;
   nImpCompra = 0;
   nImpVenta  = 0;
   nCampo     = 0;
   ExistIni   = 0;
   ExistFin   = 0;

   nActivid   = 0;
   nActi      = 0;
   Calc       = 0;

   Trim      = 0;
   SimpNorm  = 0;
   FEstado = 0;

   Pasada  = 0;
   Tipo    = "";
   Cont    = 0;
   TipoNum = 0;

   &eaPrograma = "";
   &eaPunto    = "";
   &enEmpresa   = 0;

   &enEmprEos  = 0;
   &enActiEos  = 0;
   &enEmprRen  = 0;
   &eaCaracter = "";
   &eaTitCony  = "";
|FIN;

|PROCESO Comunidad;
   #activid@#0 = #recappla#0;
   #activid@#1 = #recappla#1;
   #activid@#2 = 99;
   |LEE 010#activid@.=;
   |SI FSalida = 0;
      |SI #activid@#46 = "S";
         |C_M #recappla#5,1,"S";
         |C_M #recappla#6,1,"S";
      |SINO;
         |C_M #recappla#5,1,"N";
         |C_M #recappla#6,1,"N";
      |FINSI;
   |SINO;
      |C_M #recappla#5,1,"N";
      |C_M #recappla#6,1,"N";
   |FINSI;
|FPRC;

|PROCESO PagosACuenta;

   |SI #activid@#22 ! "A"; |Y #activid@#42 = "N";
      nActivid = 9;
   |SINO;
      nActivid = 0;
   |FINSI;

   Linea1 = 1;
   #rem00031#0 = #recappla#2;
   #rem00031#1 = 1;
   |LEE 010#rem00031.];
   |PARA; |SI FSalida = 0; |Y #rem00031#0 = #recappla#2; |HACIENDO;
      Comodin = #rem00031#2 % 0101;
      |SI nActivid = 0;
         nActivid = #recappla#1;
      |FINSI;
      |SI #rem00031#16 = nActivid; |Y #recappla#3 = Comodin;
         Linea1 = #rem00031#1;
         |BORRA 020#rem00031.a;
         |SAL;
      |SINO;
         Linea1 = #rem00031#1 + 1;
         |LEE 010#rem00031.s;
      |FINSI;
   |SIGUE;

   |DDEFECTO #rem00031;
   #rem00031#0 = #recappla#2;
   #rem00031#1 = Linea1;
   #rem00031#16 = nActivid;
   |SI #recappla#3 = "T";
      #rem00031#2 = "TITULAR";
   |SINO;
      #rem00031#2 = "CONYUGE";
   |FINSI;

   |PARA Cont = 1; |SI Cont [ 4; |HACIENDO Cont = Cont + 1;
      || Leo y guardo los datos del modelo 130
      |SI #activid@#22 ! "A";
         |SI #activid@#46 ! "S";
            #mode130@#0 = #recappla#0;
            #mode130@#1 = #recappla#1;
         |SINO;
            #mode130@#0 = #recappla#5;
            #mode130@#1 = nActivid;
         |FINSI;
         #mode130@#2 = Cont;
         #mode130@#3 = 99;
         |LEE 010#mode130@.=;
         |SI FSalida = 0;
            nActi = #recappla#1;
            |SI #mode130@#3 ! "          ";
               Calc = Cont + 10;
               #rem00031#Calc# = #rem00031#Calc# + #mode130@#29;
            |FINSI;
         |SINO;
            #mode130@#0 = #recappla#0;
            #mode130@#1 = 9;
            #mode130@#2 = Cont;
            #mode130@#3 = 99;
            |LEE 010#mode130@.=;
            |SI FSalida = 0;
               nActi = 9;
               |SI #mode130@#3 ! "          ";
                  Calc = Cont + 10;
                  #rem00031#Calc# = #rem00031#Calc# + #mode130@#29;
               |FINSI;
            |FINSI;
         |FINSI;
      |SINO;
         || Leo y guardo los datos del modelo 131
         #mode131@#0 = #recappla#0;
         #mode131@#1 = 99
         #mode131@#2 = Cont;
         |LEE 010#mode131@.=;
         |SI FSalida = 0; |Y #mode131@#4 ! "01.01.0000";
            Calc = Cont + 10;
            #rem00031#Calc# = #rem00031#Calc# + #mode131@#47;
         |FINSI;
      |FINSI;
   |SIGUE;
   |GRABA 020#rem00031.n;
   |LEE 010#rem00031.=;

   enEmpresa    = #recappla#2;
   eaPrograma   = "rem00031";
   eaPunto      = "  ";
   |HAZ ProgramaActivo;

   |LIBERA #rem00031;
|FPRC;

|PROCESO Recalcula;
   |SI #activid@#22 = "C";
        #rem00025#29 = #rem00025#26 + #rem00025#27 + #rem00025#28;
        #rem00025#44 = 0;
        |PARA nCampo = 30;  |SI nCampo [ 43;  |HACIENDO nCampo = nCampo + 1;
           #rem00025#44 = #rem00025#44 + #rem00025#nCampo#;
        |SIGUE;

        #rem00025#45 = #rem00025#29 - #rem00025#44;
        #rem00025#47 = 0;
        |SI #rem00025#46 = "S";  #rem00025#47 = #rem00025#45 % 30;  |FINSI;
        |SI #rem00025#47 < 0;    #rem00025#47 = 0;              |FINSI;
        #rem00025#48 = #rem00025#45 - #rem00025#47;
  |SINO;
        #rem00026#29 = #rem00026#26 + #rem00026#27 + #rem00026#28;
        #rem00026#42= 0;
        |PARA nCampo = 30;  |SI nCampo [ 41;  |HACIENDO nCampo = nCampo + 1;
           #rem00026#42 = #rem00026#42 + #rem00026#nCampo#;
        |SIGUE;

        #rem00026#43 = #rem00026#29 - #rem00026#42;

        #rem00026#44 = 0;
        |SI #rem00026#43 > 0;  #rem00026#44 = #rem00026#43 % 5;  |FINSI;

        #rem00026#45 = #rem00026#43 - #rem00026#44;
        #rem00026#47 = 0;
        |SI #rem00026#46 = "S";  #rem00026#47 = #rem00026#45 % 30;  |FINSI;
        |SI #rem00026#47 < 0;    #rem00026#47 = 0;              |FINSI;
        #rem00026#48 = #rem00026#45 - #rem00026#47;
  |FINSI;
|FPRC;

|PROCESO Clasifica;
   |SI #activid@#22 = "B";

      |SI #activid@#14 = "S";
         #rem00026#14 = "S";
         #comulin@#3 = #recappla#0;
         #comulin@#5 = #recappla#1;
         #comulin@#7 = 99;
         |LEE 010#comulin@.=;
         |SI FSalida = 0;
            #rem00026#15 = #comulin@#6;
         |FINSI;
      |SINO;
         #rem00026#14 = "N";
         #rem00026#15 = 0;
      |FINSI;
      #rem00026#17 = Tipo;

      #rem00026#26 = nImpVenta;

      |SI TipoNum = 1;  #rem00026#30 = #rem00026#30 + nImpCompra; |FINSI;
      |SI TipoNum = 2;  #rem00026#31 = #rem00026#31 + nImpCompra; |FINSI;
      |SI TipoNum = 3;  #rem00026#32 = #rem00026#32 + nImpCompra; |FINSI;
      |SI TipoNum = 4;  #rem00026#33 = #rem00026#33 + nImpCompra; |FINSI;
      |SI TipoNum = 5;  #rem00026#34 = #rem00026#34 + nImpCompra; |FINSI;
      |SI TipoNum = 6;  #rem00026#35 = #rem00026#35 + nImpCompra; |FINSI;
      |SI TipoNum = 7;  #rem00026#36 = #rem00026#36 + nImpCompra; |FINSI;
      |SI TipoNum = 8;  #rem00026#37 = #rem00026#37 + nImpCompra; |FINSI;
      |SI TipoNum = 9;  #rem00026#38 = #rem00026#38 + nImpCompra; |FINSI;
      |SI TipoNum = 10; #rem00026#39 = #rem00026#39 + nImpCompra; |FINSI;
      |SI TipoNum = 11; #rem00026#40 = #rem00026#40 + nImpCompra; |FINSI;
      |SI TipoNum = 12; #rem00026#41 = #rem00026#41 + nImpCompra; |FINSI;
   |SINO;
      |SI #activid@#14 = "S";
         #rem00025#14 = "S";
         #comulin@#3 = #recappla#0;
         #comulin@#5 = #recappla#1;
         #comulin@#7 = 99;
         |LEE 010#comulin@.=;
         |SI FSalida = 0;
            #rem00025#15 = #comulin@#6;
         |FINSI;
      |SINO;
         #rem00025#14 = "N";
         #rem00025#15 = 0;
      |FINSI;
      #rem00025#17 = Tipo;

      #rem00025#26 = nImpVenta;

      |SI TipoNum = 1;  #rem00025#30 = #rem00025#30 + nImpCompra; |FINSI;
      |SI TipoNum = 2;  #rem00025#31 = #rem00025#31 + nImpCompra; |FINSI;
      |SI TipoNum = 3;  #rem00025#32 = #rem00025#32 + nImpCompra; |FINSI;
      |SI TipoNum = 4;  #rem00025#33 = #rem00025#33 + nImpCompra; |FINSI;
      |SI TipoNum = 5;  #rem00025#34 = #rem00025#34 + nImpCompra; |FINSI;
      |SI TipoNum = 6;  #rem00025#35 = #rem00025#35 + nImpCompra; |FINSI;
      |SI TipoNum = 7;  #rem00025#36 = #rem00025#36 + nImpCompra; |FINSI;
      |SI TipoNum = 8;  #rem00025#37 = #rem00025#37 + nImpCompra; |FINSI;
      |SI TipoNum = 9;  #rem00025#38 = #rem00025#38 + nImpCompra; |FINSI;
      |SI TipoNum = 10; #rem00025#39 = #rem00025#39 + nImpCompra; |FINSI;
      |SI TipoNum = 11; #rem00025#40 = #rem00025#40 + nImpCompra; |FINSI;
      |SI TipoNum = 12; #rem00025#41 = #rem00025#41 + nImpCompra; |FINSI;
      |SI TipoNum = 13; #rem00025#42 = #rem00025#42 + nImpCompra; |FINSI;
      |SI TipoNum = 14; #rem00025#43 = #rem00025#43 + nImpCompra; |FINSI;
   |FINSI;
|FPRC;

|PROCESO PasaPlanilla;
   #planill@#0  = #recappla#0;
   #planill@#83 = #recappla#1;
   #planill@#1  = 99;
   #planill@#2  = 1;
   |LEE 010#planill@.];
   |SI FSalida = 0; |Y #planill@#0 = #recappla#0; |Y #planill@#83 = #recappla#1; |Y #planill@#1 = 99;
      |PARA; |SI FSalida = 0; |Y #planill@#0 = #recappla#0; |Y #planill@#83 = #recappla#1; |Y #planill@#1 = 99; |HACIENDO;
         Trim = #planill@#2;
         |LEE 010#planill@.s;
      |SIGUE;
   |SINO;
      |MENAV "    No hay planilla con datos";
      |ACABA;
   |FINSI;

   #planill@#0  = #recappla#0;
   #planill@#83 = #recappla#1;
   #planill@#1  = 99;
   #planill@#2  = Trim;
   |LEE 010#planill@.=;

   |SI #activid@#22 = "B";
      SimpNorm = 0;
      Linea1 = 1;
      #rem00026#0 = #recappla#2;
      #rem00026#1 = 1;
      |LEE 010#rem00026.];
      |PARA; |SI FSalida = 0; |Y #rem00026#0 = #recappla#2; |HACIENDO;
          Linea1 = #rem00026#1 + 1;
          |LEE 010#rem00026.s;
      |SIGUE;

      |DDEFECTO #rem00026;
      #rem00026#0 = #recappla#2;
      #rem00026#1 = Linea1;
      |SI #recappla#3 = "T";
         #rem00026#2 = "TITULAR";
      |SINO;
         #rem00026#2 = "CONYUGE";
      |FINSI;
/*
      |GRABA 020#rem00026.n;
      |LEE 010#rem00026.=;
*/
      #rem00026#18 = #activid@#4;
      #rem00026#19 = #activid@#5;
      #rem00026#20 = #activid@#41;
      #rem00026#21 = #cliente@#1;
      #rem00026#22 = #activid@#6;
      #rem00026#23 = #activid@#11;
      #rem00026#24 = #activid@#12;
      #rem00026#25 = #activid@#9;
   |SINO;
      SimpNorm = 60;
      Linea1 = 1;
      #rem00025#0 = #recappla#2;
      #rem00025#1 = 1;
      |LEE 010#rem00025.];
      |PARA; |SI FSalida = 0; |Y #rem00025#0 = #recappla#2; |HACIENDO;
          Linea1 = #rem00025#1 + 1;
          |LEE 010#rem00025.s;
      |SIGUE;

      |DDEFECTO #rem00025;
      #rem00025#0 = #recappla#2;
      #rem00025#1 = Linea1;
      |SI #recappla#3 = "T";
         #rem00025#2 = "TITULAR";
      |SINO;
         #rem00025#2 = "CONYUGE";
      |FINSI;
/*
      |GRABA 020#rem00025.n;
      |LEE 010#rem00025.a;
*/
      #rem00025#18 = #activid@#4;
      #rem00025#19 = #activid@#5;
      #rem00025#20 = #activid@#41;
      #rem00025#21 = #cliente@#1;
      #rem00025#22 = #activid@#6;
      #rem00025#23 = #activid@#11;
      #rem00025#24 = #activid@#12;
      #rem00025#25 = #activid@#9;
   |FINSI;

   nImpVenta  = #planill@#92; ExistIni = #planill@#48; ExistFin = #planill@#49;
   |PARA Cont = 1; |SI Cont [ 20; |HACIENDO Cont = Cont + 1;
      Concepto = Cont;
      |SI Cont [ 9;
         Calc1 = Cont + 68;
      |SINO;
         Calc1 = Cont + 109;
      |FINSI;
      nImpCompra = #planill@#Calc1#;
      |SI #activid@#23 = "P";
         Calc2 = Concepto + 20 + SimpNorm;
         Tipo = "P";
      |SINO;
         Calc2 = Concepto + 40 + SimpNorm;
         |SI #recappla#4 = "M";
            Tipo = "M";
         |SINO;
            Tipo = "N";
         |FINSI;
      |FINSI;
      TipoNum = #reeospla#Calc2#;
      |HAZ Clasifica;
   |SIGUE;

   |SI #activid@#22 = "B";
      |SI ExistFin ] ExistIni;
         #rem00026#30 = #rem00026#30 - (ExistFin - ExistIni);
      |SINO;
         #rem00026#30 = #rem00026#30 + (ExistIni - ExistFin);
      |FINSI;
   |SINO;
      |SI ExistFin ] ExistIni;
         #rem00025#30 = #rem00025#30 - (ExistFin - ExistIni);
      |SINO;
         #rem00025#30 = #rem00025#30 + (ExistIni - ExistFin);
      |FINSI;
   |FINSI;

   |HAZ Recalcula;

   |SI #activid@#22 = "B";
      |SI #activid@#46 = "S";
         #comulin@#3 = #recappla#5;
         #comulin@#5 = #recappla#6;
         #comulin@#7 = 99;
         |LEE 010#comulin@.=;
         |SI FSalida = 0;
            #rem00026#14 = "S";
            #rem00026#15 = #comulin@#6;
         |FINSI;
      |FINSI;
      |GRABA 020#rem00026.n;
      enEmpresa    = #rem00026#0;
      eaPrograma   = "rem00026";
      eaPunto      = "  ";
      |HAZ ProgramaActivo;
   |SINO;
      |SI #activid@#46 = "S";
         #comulin@#3 = #recappla#5;
         #comulin@#5 = #recappla#6;
         #comulin@#7 = 99;
         |LEE 010#comulin@.=;
         |SI FSalida = 0;
            #rem00025#14 = "S";
            #rem00025#15 = #comulin@#6;
         |FINSI;
      |FINSI;
      |GRABA 020#rem00025.n;
      enEmpresa    = #rem00025#0;
      eaPrograma   = "rem00025";
      eaPunto      = "  ";
      |HAZ ProgramaActivo;
   |FINSI;

|FPRC;

|PROGRAMA;
   |HAZ QueVersion;

   || Revisar Paths de Agicli

   |DBASS Comodin; |QBF Comodin;
   Comodin1 = Comodin + "agicli/def/agicontr.mas";
   |CARGA_DEF Comodin1,contrab@;
   |PATH_DAT #10 Comodin;

   |ABRE #contrab@;
   |LEE 010#contrab@.p;
   |SI FSalida = 0;
      Path = #contrab@#3;
      |QBF Path;
      |SI Path = "*"; |Y Path = "";
         |DBASS Comodin; |QBF Comodin;
         Path = Comodin + "agieos/fich/";
      |FINSI;
   |SINO;
      |DBASS Comodin; |QBF Comodin;
      Path = Comodin + "agieos/fich/";
   |FINSI;
   |CIERRA #contrab@;
   |DESCARGA_DEF #contrab@;
   Comodin = Comodin + "agieos/def/";

   |SI enEmpresa ! 0;
      #0#0 = enEmprEos;
      #0#1 = enActiEos;
      #0#2 = enEmprRen;
      #0#4 = eaCaracter;
      #0#3 = eaTitCony;
      FSalida = 0;
   |SINO;
      |CLS;
      |CABEZA "Captura datos de planillas de REO a renta"
      |PINPA #0#0;
      |PINDA #0#0;

      Comodin1 = Comodin + "eosactiv.mas";
      |CARGA_DEF Comodin1,activid@;
      |PATH_DAT #2  Path;
      |ABRE #activid@;

      |ENDATOS #1#0;
      FEstado = FSalida;

      |CIERRA #activid@;
      |DESCARGA_DEF #activid@;
      FSalida = FEstado;
   |FINSI;
   |SI FSalida = 0;
      Comodin1 = Comodin + "eosplan1.mas";
      |CARGA_DEF Comodin1,planill@;
      Comodin1 = Comodin + "eosclien.mas";
      |CARGA_DEF Comodin1,cliente@;
      Comodin1 = Comodin + "eosactiv.mas";
      |CARGA_DEF Comodin1,activid@;
      Comodin1 = Comodin + "eosm0020.mas";
      |CARGA_DEF Comodin1,mode131@;
      Comodin1 = Comodin + "eosm3130.mas";
      |CARGA_DEF Comodin1,mode130@;
      Comodin1 = Comodin + "eoscomul.mas";
      |CARGA_DEF Comodin1,comulin@;

      |PATH_DAT #2  Path;
      |PATH_DAT #3  Path;
      |PATH_DAT #4  Path;
      |PATH_DAT #5  Path;
      |PATH_DAT #6  Path;
      |PATH_DAT #7  Path;

      |ABRE #rem00031;
      |ABRE #rem00025;
      |ABRE #rem00026;
      |ABRE #reeospla;
      |ABRE #activid@;
      |ABRE #cliente@;
      |ABRE #mode130@;
      |ABRE #mode131@;
      |ABRE #planill@;
      |ABRE #comulin@;
      |SELECT #2#comulin@;

      #activid@#0 = #recappla#0;
      #activid@#1 = #recappla#1;
      #activid@#2 = 99;
      |LEE 010#activid@.=;
      |SI FSalida = 0;
         |LEE 010#reeospla.p;
         |SI FSalida = 0;
            |HAZ PasaPlanilla;
            |HAZ PagosACuenta;
         |SINO;
            |MENAV "    No se ha definido el control de planillas";
         |FINSI;
      |SINO;
         |MENAV "    No se han encontrado datos de dicha actividad";
      |FINSI;

      |SELECT #1#comulin@;
      |CIERRA #comulin@;
      |CIERRA #rem00031;
      |CIERRA #rem00025;
      |CIERRA #rem00026;
      |CIERRA #reeospla;
      |CIERRA #planill@;
      |CIERRA #mode130@;
      |CIERRA #mode131@;
      |CIERRA #activid@;
      |CIERRA #cliente@;

      |DESCARGA_DEF #comulin@;
      |DESCARGA_DEF #planill@;
      |DESCARGA_DEF #cliente@;
      |DESCARGA_DEF #activid@;
      |DESCARGA_DEF #mode130@;
      |DESCARGA_DEF #mode131@;

   |FINSI;
|FPRO;
