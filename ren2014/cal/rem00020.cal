     |C_M #620#27, 1, "S";
     |C_M #620#17, 1, "S";
     |SI #620#28 = 0;
         |C_M #620#27, 1, "N";  #620#27 = 0;  |PINTA #620#27;
         |C_M #620#17, 1, "N";  #620#17 = 0;  |PINTA #620#17;
