|PROCESO Tipo12Fw620;  |TIPO 12;
     |SI FSalida = 7;  |ACABA;  |FINSI;

     aAlfa = #620#2;  |QBF aAlfa;
     |SI aAlfa = "";
         |MENAV "0000No se puede validar la ficha, no hay contribuyente introducido";
         |ERROR;
     |FINSI;

     |SI #rew00620#29 = "N";
         |SI enMinusvalia ] 33;
             |MENAV "0000Contribuyente con minusval¡a y no tiene activado el campo TRABAJADOR ACTIVO DISCAPACITADO";
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo0C7w620;
     |SI #620#6 > #620#7;
          |MENAV "0000 ATENCION. Importe de retencion superior a la retribucion dineraria";
     |FINSI;
|FPRC;

|PROCESO Tipo0C1Fw620;  |TIPO 0;
     |C_M #620#1, 0, aAlfa;
     |SI aAlfa = "N";  |ACABA;  |FINSI;

     #20#0 = #620#0;
     #20#1 = #620#1;
     |LEE 000#20=;
     |SI FSalida = 0;
         |MENAV "0000El registro ya existe.";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Totalw620;
     #620#12 = #620#9  + #620#10 - #620#11;

     nCampo28 = #620#28;
     |SI nCampo28 > 1000000;
         nCampo28 = 0;
     |SINO;
         |SI nCampo28 > 700000;
             nCampo28 = 300000 - (#620#28 - 700000);
         |SINO;
             |SI nCampo28 > 300000;
                 nCampo28 = 300000;
             |FINSI;
         |FINSI;
     |FINSI;

     |SI #620#17 ! -1;
         |C_M #620#18, 1, "N";
         #620#18 = nCampo28 % #620#17;
     |SINO;
         |C_M #620#18, 1, "S";
     |FINSI;

     |SI #620#27 ! -1;
         nLimite  = (#401#1 * #620#27) % 40;
         |SI #620#18 > nLimite;  #620#18 = nLimite;  |FINSI;
     |FINSI;

     |SI #620#21 > #401#2;  #620#21 = #401#2;  |FINSI;
     |SI #620#22 > #401#3;  #620#22 = #401#3;  |FINSI;

     #620#23 = #620#19 + #620#20 + #620#21 + #620#22;
     #620#24 = #620#7  + #620#12 + #620#34 + #620#16 + #620#28 - #620#18 - #620#23;
     #620#37 = #620#24;

     |PINTA #620#12;
     |PINTA #620#16;
     |PINTA #620#18;
     |PINTA #620#21;
     |PINTA #620#22;
     |PINTA #620#23;
     |PINTA #620#25;
     |PINTA #620#26;
     |PINTA #620#37;
|FPRC;

|PROCESO TraePensiones;
     |SI nSw = 0;
         #620#13 = #53#11;
         #620#35 = #53#29;
         #620#16 = #620#16 + #53#15;

         |SI #53#13 = "S";
             #620#16 = #620#16 + #53#18;
         |FINSI;

         #53#3 = #20#3;
         |SI #53#3 = "A";  #53#3 = "R";  |FINSI;
         |SI #53#3 = "P";  #53#3 = "R";  |FINSI;

                            #53#28 = "";
         |SI #53#21 = "S";  #53#28 = eaAzul;  |FINSI;
         |SI #53#3  = "X";  #53#28 = eaRojo;  |FINSI;

         |GRABA 020#53a;
         |LIBERA #53;
     |SINO;
         |BORRA 020#53a;
         |LIBERA #53;

         eaPrograma   = "rem00053";
         eaPunto      = "  ";
         |HAZ ProgramaActivo;
     |FINSI;
|FPRC;

|DEFBUCLE rem00053;
     #53#1;
     21, 22;
     #20#0, 01, "S", #20#1;
     #20#0, 99, "S", #20#1;
     be;
     NULL, TraePensiones;
|FIN;

|PROCESO Pensiones;
     |SI FSalida = 10;
         |VENTANA 0501, 3499, -1, 16, "Pensiones";
         nVent = FSalida;

         aVent1 = -1;
         aVent2 = -1;
         aVent3 = 0;
         aVent4 = nVent;
         aVent5 = "MODAL";
         |ESPECIFICA "ESTADO_VENTANA", aVent;

         |PTEC 802;  |PAUSA;

         enLinTrabajo = #620#1;
         eaDeclaTraba = #620#2;
         |SUB_EJECUTA_NP "rem00053";

         aVent1 = -1;
         aVent2 = -1;
         aVent3 = 0;
         aVent4 = nVent;
         aVent5 = "MODELESS";
         |ESPECIFICA "ESTADO_VENTANA", aVent;

         |FINVENTANA nVent;
     |FINSI;

     #620#16 = 0;
     nSw      = 0;   |BUCLE rem00053;

     |PINTA #620#13;
     |PINTA #620#16;
|FPRC;

|PROCESO Irregular;
