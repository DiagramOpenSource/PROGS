|PROCESO BotonCalc;
     |SI enModoReg = 4;  |ACABA;  |FINSI;

     |SI FSalida = nBotonCalc1;
         |DDEFECTO #rew00005;

         #rew00005#0  = "Seguridad Social";
         #rew00005#2  = "Mutualidades ";
         #rew00005#4  = "Colegios de Huerfanos";
         #rew00005#6  = "Montepios ";

         #rew00005#1  = #620#19;
         #rew00005#20 = #620#19;

         |HAZ PantaREW00005;

         #620#19   = #rew00005#20;  |PINTA #620#19;

         aAlfa = "|C019WID";
         aID   = #620aAlfa;
         nID   = aID;
         |FOCOTECLADO nID;

         |ACABA;
     |FINSI;

     |DDEFECTO #rew00005;

     |SI FSalida = nBotonCalc2; nCampo = 20;  |FINSI;
     |SI FSalida = nBotonCalc3; nCampo = 21;  |FINSI;
     |SI FSalida = nBotonCalc4; nCampo = 22;  |FINSI;

     #rew00005#1  = #620nCampo;
     #rew00005#20 = #620nCampo;

     |HAZ PantaREW00005;

     #620nCampo = #rew00005#20;  |PINTA #620nCampo;

     aAlfa = "|C" + (("000" + nCampo) % -103) + "WID";
     aID   = #620aAlfa;
     nID   = aID;
     |FOCOTECLADO nID;
|FPRC;

|PROCESO PintaDatosPantalla;
     |DDEFECTO #620;
     |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
           #620nCampo = #20nCampo;

           |C_V #620nCampo, 0, aAlfa;
           |SI aAlfa = "S";  |PINTA #620nCampo;  |FINSI;

           |C_M #620nCampo, 1, "N";
     |SIGUE;
|FPRC;

|PROCESO CargaFantasma;
     |C_M #620#5,  1, "S";
     |C_M #620#6,  1, "S";
     |C_M #620#7,  1, "S";
     |C_M #620#8,  1, "S";
     |C_M #620#9,  1, "S";
     |C_M #620#10, 1, "S";
     |C_M #620#11, 1, "S";
     |C_M #620#13, 1, "S";
     |C_M #620#16, 1, "S";
     |C_M #620#17, 1, "S";
     |C_M #620#19, 1, "S";
     |C_M #620#20, 1, "S";
     |C_M #620#21, 1, "S";
     |C_M #620#22, 1, "S";
     |C_M #620#27, 1, "S";
     |C_M #620#28, 1, "S";
     |C_M #620#29, 1, "S";
     |C_M #620#30, 1, "S";
     |C_M #620#34, 1, "S";

     |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
           #620nCampo = #20nCampo;

           |C_V #620nCampo, 0, aAlfa;
           |SI aAlfa = "S";  |PINTA #620nCampo;  |FINSI;
     |SIGUE;
|FPRC;

|PROCESO DescargaFantasma;
     |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
           |SI enSwLiquida = 1;
               |SI #20nCampo = #620nCampo;
                   enSwRetorno = 1;
               |FINSI;
           |FINSI;

           #20nCampo = #620nCampo;
     |SIGUE;

                        #20#38 = "";
     |SI #20#31 = "S";  #20#38 = eaAzul;  |FINSI;
     |SI #20#3  = "X";  #20#38 = eaRojo;  |FINSI;

     |HAZ PintaDatosPantalla;
|FPRC;

|PROCESO Alta;
     |ESTADO_CONTROL nBotonAlta, "DISABLE";
     |ESTADO_CONTROL nBotonModi, "DISABLE";
     |ESTADO_CONTROL nBotonBaja, "DISABLE";
     |ESTADO_CONTROL nBotonCalc1, "ENABLE";
     |ESTADO_CONTROL nBotonCalc2, "ENABLE";
     |ESTADO_CONTROL nBotonCalc3, "ENABLE";
     |ESTADO_CONTROL nBotonCalc4, "ENABLE";

     |HAZ QuitaBtnSalir;

     |ESTADO_CONTROL nGrid, "DISABLE";

     |PULSATECLA;

     #20#0 = enEmpresa;
     #20#1 = 99;
     |LEE 000#20];
     |SI FSalida = 0;
         |LEE 000#20a;
     |SINO;
         |LEE 000#20u;
     |FINSI;

     |DDEFECTO #620;
     #620#0 = enEmpresa;
     #620#1 = 1;
     |SI FSalida = 0;  |Y  #20#0 = enEmpresa;
         #620#1 = #20#1 + 1;
     |FINSI;

     |DDEFECTO #20;

     #20#0 = #620#0;
     #20#1 = #620#1;

     enModoReg = 1;
     |HAZ CargaFantasma;

     |C_M #620#1, 1, "S";
     |ENDATOS #1#620;
     |SI FSalida = 0;
         |HAZ DescargaFantasma;

         |GRABA 020#20n;
     |FINSI;
     |LIBERA #20;

     |SI enModo ! 4;
         |ESTADO_CONTROL nBotonAlta, "ENABLE";
     |FINSI;

     |ESTADO_CONTROL nBotonCalc1, "DISABLE";
     |ESTADO_CONTROL nBotonCalc2, "DISABLE";
     |ESTADO_CONTROL nBotonCalc3, "DISABLE";
     |ESTADO_CONTROL nBotonCalc4, "DISABLE";

     |HAZ PonBtnSalir;

     |ESTADO_CONTROL nGrid, "ENABLE";
     |REFRESCACONTROL nGrid;

     FSalida = 1;  |HAZ Evento20;

     |FOCOTECLADO nGrid;
|FPRC;

|PROCESO Modificar;
     |SI nEntrar = 0;  |ACABA;  |FINSI;

     |LEE 101#20=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |ESTADO_CONTROL nBotonAlta,  "DISABLE";
     |ESTADO_CONTROL nBotonModi,  "DISABLE";
     |ESTADO_CONTROL nBotonBaja,  "DISABLE";
     |ESTADO_CONTROL nBotonCalc1, "ENABLE";
     |ESTADO_CONTROL nBotonCalc2, "ENABLE";
     |ESTADO_CONTROL nBotonCalc3, "ENABLE";
     |ESTADO_CONTROL nBotonCalc4, "ENABLE";

     |HAZ QuitaBtnSalir;

     |ESTADO_CONTROL nGrid, "DISABLE";

     enModoReg = 2;
     |C_M #620#1, 1, "N";

     |HAZ CargaFantasma;
     |ENDATOS #2#620;
     |SI FSalida = 0;
         |HAZ DescargaFantasma;

         |GRABA 020#20a;

         nSw = 0;
         |BUCLE rem00053;
     |FINSI;
     |LIBERA #20;

     |SI enModo ! 4;
         |ESTADO_CONTROL nBotonAlta, "ENABLE";
         |ESTADO_CONTROL nBotonModi, "ENABLE";
         |ESTADO_CONTROL nBotonBaja, "ENABLE";
     |FINSI;

     |ESTADO_CONTROL nBotonCalc1, "DISABLE";
     |ESTADO_CONTROL nBotonCalc2, "DISABLE";
     |ESTADO_CONTROL nBotonCalc3, "DISABLE";
     |ESTADO_CONTROL nBotonCalc4, "DISABLE";

     |HAZ PonBtnSalir;

     |ESTADO_CONTROL nGrid, "ENABLE";
     |REFRESCACONTROL nGrid;
     |FOCOTECLADO nGrid;
|FPRC;

|PROCESO Baja;
     |SI nEntrar = 0;  |ACABA;  |FINSI;

     |CONFI "0000 Confirma la baja del registro seleccionado";
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |LEE 101#20=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |BORRA 020#20a;
     |LIBERA #20;

     nEntrar     = 0;
     enSwRetorno = 1;
     nSw         = 1;   |BUCLE rem00053;

     |ESTADO_CONTROL nBotonModi,  "DISABLE";
     |ESTADO_CONTROL nBotonBaja,  "DISABLE";

     |REFRESCACONTROL nGrid;
     |FOCOTECLADO nGrid;
|FPRC;

|PROCESO Evento20;
     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#20=;

         |SI enModo = 4;
             |ESTADO_CONTROL nBotonAlta, "DISABLE";
             |ESTADO_CONTROL nBotonModi, "DISABLE";
             |ESTADO_CONTROL nBotonBaja, "DISABLE";
         |SINO;
             |ESTADO_CONTROL nBotonModi, "ENABLE";
             |ESTADO_CONTROL nBotonBaja, "ENABLE";
         |FINSI;

         |PULSATECLA;

         nEntrar   = 1;
         enModoReg = 4;
         |HAZ PintaDatosPantalla;
     |FINSI;

     |SI FSalida = 4;
         |SI enModo = 4;  |ACABA;  |FINSI;

         |HAZ Modificar;
     |FINSI;
|FPRC;

|PROCESO BotonesPanta;
