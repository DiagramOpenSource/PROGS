     aAlfa        = "";
     aAlfa1       = "";
     aAlfa2       = "";
     aAlfa3       = "";
     aComa        = "";
|FIN;

|PROCESO Tipo12Fw201;  |TIPO 12;
     |HAZ Tipo7C5Fw201;

     |SI #201#18 = "N";
         aAlfa = #201#20;
     |SINO;
