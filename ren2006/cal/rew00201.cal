         |MENAV aAlfa;
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Tipo0C8Fw201;
     nCampo1 = Campo + 1;
     |C_M #201nCampo1, 1, "S";
     |SI #201Campo = 0;
         |C_M #201nCampo1, 1, "N";
         #201nCampo1 = "N";  |PINTA #201nCampo1;
     |FINSI;
|FPRC;
