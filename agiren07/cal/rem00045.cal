|| ************************************************************************
|| PROCESOS DE CONSULTA DE PANTALLAS NORMALES
|| ************************************************************************

|PROCESO Totalm020;
     |SI #20#17 = 0;
         #20#19 = #20#18;
     |SINO;
         #20#19 = #20#18 % #20#17;
     |FINSI;
     |PINTA #20#19;

     |C_M #20#20, 1, "N";
     |SI #20#11 = 7;  |C_M #20#20, 1, "S";  |FINSI;
|FPRC;

|PROCESO Tipo11m20;  |TIPO 11;
     |SI FSalida = 11;
         |SI #20#2 ! "VARIOS    ";  |ACABA;  |FINSI;

         #900#0 = #20#5;
         #900#1 = #20#6;
         #900#2 = #20#7;
         #900#3 = #20#8;
         #900#4 = #20#9;
         #900#5 = #20#10;

         |PUSHV 0501 2380;
         |PINPA #0#900;
         |PINDA #0#900;
         |LEETECLA aTecla;
         |POPV;
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Varios;  |TIPO 0;
     |SI #20#2 ! "VARIOS    ";
         #20#5  = 0;
         #20#6  = 0;
         #20#7  = 0;
         #20#8  = 0;
         #20#9  = 0;
         #20#10 = 0;
         |ACABA;
     |FINSI;

     |C_M #900#0,  1, "N";
     |C_M #900#1,  1, "N";
     |C_M #900#2,  1, "N";
     |C_M #900#3,  1, "N";
     |C_M #900#4,  1, "N";
     |C_M #900#5,  1, "N";

     aAlfa = eaSwVarios;
     aLong = aAlfa % 0;
     nLong = aLong;
     |PARA nCaracter = 1;  |SI nCaracter [ nLong;  |HACIENDO nCaracter = nCaracter + 1;
           nPosi = (100 * nCaracter) + 1;
           aAlfa1 = aAlfa % nPosi;
           |SI aAlfa1 = "T";  |C_M #900#0,  1, "S";  |FINSI;
           |SI aAlfa1 = "C";  |C_M #900#1,  1, "S";  |FINSI;
           |SI aAlfa1 = "1";  |C_M #900#2,  1, "S";  |FINSI;
           |SI aAlfa1 = "2";  |C_M #900#3,  1, "S";  |FINSI;
           |SI aAlfa1 = "3";  |C_M #900#4,  1, "S";  |FINSI;
           |SI aAlfa1 = "4";  |C_M #900#5,  1, "S";  |FINSI;
     |SIGUE;

     #900#0 = #20#5;
     #900#1 = #20#6;
     #900#2 = #20#7;
     #900#3 = #20#8;
     #900#4 = #20#9;
     #900#5 = #20#10;

     |PUSHV 0501 2380;

     |PINPA #0#900;
     |PINDA #0#900;
     |ENDATOS #1#900;
     |SI FSalida = 0;
         #20#5  = #900#0;
         #20#6  = #900#1;
         #20#7  = #900#2;
         #20#8  = #900#3;
         #20#9  = #900#4;
         #20#10 = #900#5;
     |FINSI;

     |POPV;
|FPRC;

|RUTINA ClaveBaja20;
     #20#0 = enEmpresa;
     #20#1 = 1;
|FRUT;

|RUTINA ClaveAlta20;
     #20#0 = enEmpresa;
     #20#1 = 99;
|FRUT;

|PROCESO Agrupacion;  |TIPO 6;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     |PUSHV 0501 2380;
     |ABRE #1;
     #1#0 = #20#11;
     |LEE 040#1];
     |CONSULTA_CLAVE #1#1;
     |CIERRA #1;
     |POPV;

     #20#11 = #1#0;  |PINTA #20#11;
     #20#12 = #1#1;  |PINTA #20#12;
|FPRC;

|RUTINA ClaveBaja0;
     #0#0 = #20#11;
     #0#1 = 1;
|FRUT;

|RUTINA ClaveAlta0;
     #0#0 = #20#11;
     #0#1 = 99;
|FRUT;

|PROCESO Concepto;  |TIPO 6;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     |ABRE #0;
     #0#0 = #20#11;
     #0#1 = 1;
     |LEE 040#0];
     |SI FSalida = 0;  |Y #0#0 = #20#11;
         |PUSHV 0501 2380;
         |CONSULTA_F_CLAVE #1#0, ClaveBaja0, ClaveAlta0;
         |POPV;

         #20#13 = #0#1;  |PINTA #20#13;
         #20#14 = #0#3;  |PINTA #20#14;
         #20#15 = #0#4;  |PINTA #20#15;
         #20#16 = #0#5;  |PINTA #20#16;
         #20#17 = #0#6;  |PINTA #20#17;
     |FINSI;
     |CIERRA #0;
|FPRC;

|PROCESO PonDeclarante;  |TIPO 7;
     eaDeclarante = #20#2;
     enComunes    = 1;
     |HAZ SacaDeclarantes;
     #20#2  = eaDeclarante;
     |PINTA #20#2;
|FPRC;

|PROCESO Fallece;  |TIPO 7;
     |SI eaFallece = "S";
         |C_M #20#4, 1, "S";
     |SINO;
         |C_M #20#4, 1, "N";  #20#4 = 0;  |PINTA #20#4;
     |FINSI;
|FPRC;

|PROCESO TipoX;
     |SI #20Campo = "X";
         |PTEC 806;
     |FINSI;
|FPRC;

|PROCESO Tipo2m20;  |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;  |O nModo = 3;
         enSwRetorno  = 1;
         |ACABA;
     |FINSI;

     nEntrada = 0 +. 1;
     |SI nEntrada ! 1;
         |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
               #99nCampo = #20nCampo;
         |SIGUE;
     |SINO;
         |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
               |SI #99nCampo ! #20nCampo;
                   enSwRetorno  = 1;
               |FINSI;
         |SIGUE;
     |FINSI;
|FPRC;

|PROCESO PonLimite;
     nSwPregunta = 1;
     |SI nLimite1 = 0;
         nLimite1 = #1#3;
         nLimite3 = #0#5;
     |SINO;
         |SI nLimite1 = #1#3;  |ACABA;  |FINSI;

         |SI nLimite2 = 0;
             nLimite2 = #1#3;
             nLimite4 = #0#5;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO BuscaLimite;
     #1#0 = #20#11;
     |LEE 040#1=;
     |SI FSalida ! 0;  |DDEFECTO #1;  |FINSI;

     #0#0 = #20#11;
     #0#1 = #20#13;
     |LEE 040#0=;
     |SI FSalida ! 0;  |DDEFECTO #0;  |FINSI;

     |SI #1#2 = "N";  |ACABA;  |FINSI;

     |SI #1#4 = #20#11;  |Y #1#5 = #20#13;
         |HAZ PonLimite;
         |ACABA;
     |FINSI;

     |SI #1#6 = #20#11;  |Y #1#7 = #20#13;
         |HAZ PonLimite;
         |ACABA;
     |FINSI;

     |SI #1#8 = #20#11;  |Y #1#9 = #20#13;
         |HAZ PonLimite;
         |ACABA;
     |FINSI;

     |SI #1#10 = #20#11;  |Y #1#11 = #20#13;
         |HAZ PonLimite;
         |ACABA;
     |FINSI;

     |SI #1#12 = #20#11;  |Y #1#13 = #20#13;
         |HAZ PonLimite;
         |ACABA;
     |FINSI;
|FPRC;

|DEFBUCLE rem00045;
     #20#1;
     ;
     enEmpresa, 01;
     enEmpresa, 99;
     ;
     NULL, BuscaLimite;
|FIN;

|PROCESO GrabaLimite;
     #1#0 = #20#11;
     |LEE 040#1=;
     |SI FSalida ! 0;  |DDEFECTO #1;  |FINSI;

     |SI #1#2 = "N";  |ACABA;  |FINSI;

     |SI nEstoy = 1;
         #0#0 = #20#11;
         #0#1 = #20#13;
         |LEE 040#0=;
         |SI FSalida = 0;  |Y #0#5 = nLimite3;
             #20#16 = #0#5;
             |GRABA 020#20a;
             |LIBERA #20;
             enSwRetorno  = 1;
         |FINSI;
         |ACABA;
     |FINSI;

     |SI nEstoy = 2;
         #0#0 = #20#11;
         #0#1 = #20#13;
         |LEE 040#0=;
         |SI FSalida = 0;  |Y #0#5 = nLimite4;
             #20#16 = #0#5;
             |GRABA 020#20a;
             |LIBERA #20;
             enSwRetorno  = 1;
         |FINSI;
         |ACABA;
     |FINSI;

     |SI #1#3 ! nLimite;  |ACABA;  |FINSI;

     #20#16 = nLimite;
     |GRABA 020#20a;
     |LIBERA #20;
     enSwRetorno  = 1;
|FPRC;

|DEFBUCLE rem00045R;
     #20#1;
     ;
     enEmpresa, 01;
     enEmpresa, 99;
     be;
     NULL, GrabaLimite;
|FIN;

|PROGRAMA;
     aQueQuiero = "|NP";
     aNumPanta  = #20#aQueQuiero#;
     nNumPanta  = aNumPanta;
     nNumPanta  = nNumPanta - 1;

     aQueQuiero = "|NC";
     aNumCampos = #20#aQueQuiero#;
     nNumCampos = aNumCampos;
     nNumCampos = nNumCampos - 1;

     |DBASE aPathRenta;  |QBT aPathRenta;
     aDef = aPathRenta + "def/rem00045.mas";
     |CARGA_DEF aDef, espejo@;
     |SI FSalida ! 0;
         aAlfa = "     Error en cargar el Def rem00045.mas";
         |MENAV aAlfa;
         |ERROR;
     |FINSI;

     |ENTLINEAL #1#20, -2, enModo, 22, 1, ClaveBaja20, ClaveAlta20;

     nSwPregunta = 0;
     nLimite1    = 0;
     nLimite2    = 0;

     |ABRE #1;
     |ABRE #0;
     |BUCLE rem00045;

     |SI nSwPregunta = 1;
         |SI nLimite1 ! 0;
             aConfi = "0000NDetectado Deduccion que puede activar otro limite del " + nLimite1 + "% ¨Procede este limite? ";
             |CONFI aConfi;
             |SI FSalida = 0;
                 nEstoy   = 0;
                 nLimite  = nLimite1;
                 nLimite3 = 0;
                 |BUCLE rem00045R;
             |SINO;
                 nEstoy   = 1;
                 nLimite  = nLimite3;
                 |BUCLE rem00045R;
             |FINSI;
         |FINSI;

         |SI nLimite2 ! 0;
             aConfi = "0000NDetectado Deduccion que puede activar otro limite del " + nLimite2 + "% ¨Procede este limite? ";
             |CONFI aConfi;
             |SI FSalida = 0;
                 nEstoy   = 0;
                 nLimite  = nLimite2;
                 nLimite4 = 0;
                 |BUCLE rem00045R;
             |SINO;
                 nEstoy   = 2;
                 nLimite  = nLimite4;
                 |BUCLE rem00045R;
             |FINSI;
         |FINSI;
     |FINSI;
     |CIERRA #0;
     |CIERRA #1;

     |DESCARGA_DEF #espejo@;

     eaPrograma   = "rem00045";
     eaPunto      = "  ";
     |HAZ ProgramaActivo;
|FPRO;
