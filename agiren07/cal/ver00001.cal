|FICHEROS;
     ver00001 #100;
     retabla1 #401;
     rem00008 #8;
|FIN;

|VARIABLES;
     &enError    = 0;
     &eaRenta    = "";

      nSwHay     = 0;
      nLimite    = 0;
      nPorce     = 0;
      nCanti1    = 0;
      nCanti2    = 0;
      nCanti3    = 0;
      nCanti4    = 0;
      nDias      = 0;
      nValor     = 0;
      nRecalculo = 0;

      aAlfa   = "";
|FIN;

|| ************************************************************************
|| PROCESOS DE CAMBIO DE VERSION
|| ************************************************************************

|PROCESO MiraPatrones;
     nSwHay = 0;

     |ABRE #8;
     #8#0 = 17;
     #8#1 = 902;
     |LEE 040#8=;
     |SI FSalida ! 0;  |O #8#7 ! 260;
         |CIERRA #8;   |DELINDEX #8;
         nSwHay = 2;
     |SINO;
         |CIERRA #8;
     |FINSI;

     |ABRE #401;
     |LEE 040#401p;
     |SI FSalida ! 0;
         |CIERRA #401;  |DELINDEX #401;
         nSwHay = 1;
     |FINSI;
     |CIERRA #401;

     |SI nSwHay = 1;
         |MENAV "0000 Han cambiado o no se han instalado los ficheros patrones, por favor instalelos.";
         |SUB_EJECUTA_NP "rez00001";
     |FINSI;

     |SI nSwHay = 2;
         |SUB_EJECUTA_NP "rez00001;AUTOMATICO";
     |FINSI;

     |ABRE #401;
     |LEE 040#401p;
     |SI FSalida ! 0;
         enError = 1;
         |CIERRA #401;  |DELINDEX #401;
     |SINO;
         |CIERRA #401;
     |FINSI;

     |ABRE #8;
     #8#0 = 17;
     #8#1 = 902;
     |LEE 040#8=;
     |SI FSalida ! 0;  |O #8#7 ! 260;
         |CIERRA #8;   |DELINDEX #8;
         enError = 1;
     |SINO;
         |CIERRA #8;
     |FINSI;
|FPRC;

|PROGRAMA;
     |MENSA "0000Comprobando configuracion patrones.";
     enError = 0;
     |HAZ MiraPatrones;
     |BLIN 05;
|FPRO;
