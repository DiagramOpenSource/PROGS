|PROCESO Tipo2m20;  |TIPO 2;
     nModo = (FEntrada / 100) ! 0;

     nEntrada = 0 +. 1;
     |SI nEntrada ! 1;
         eaDeDonde  = "C3";
         eaLinDonde = #20#1;
         eaLetra    = #20#11;
         |HAZ BorraPatrimonio;
     |FINSI;

     |SI nModo = 3;  enSwRetorno  = 1;  |ACABA;  |FINSI;
     |SI nModo = 1;  enSwRetorno  = 1;           |FINSI;

     |SI nEntrada ! 1;
         |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
               #99nCampo = #20nCampo;
         |SIGUE;
     |SINO;
         |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
               |SI #99nCampo ! #20nCampo;
                   enSwRetorno  = 1;
               |FINSI;
         |SIGUE;

         |HAZ Patrimonio;
     |FINSI;
|FPRC;

|PROGRAMA;
