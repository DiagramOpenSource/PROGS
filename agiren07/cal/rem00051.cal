     |FINSI;
|FPRC;

|PROCESO nContenido;
     nContenido = Contenido;
     |SI #20Campo ! nContenido;
         ||#20#29 = (#20#23 + #20#24 - #20#26) % #20#28;
         #20#29 = #20#26 % #20#28;
         |PINTA #20#29;
     |FINSI;
|FPRC;

|PROCESO Retencion;
     nContenido = Contenido;
     |SI #20Campo = nContenido;  |Y #20#29 ! 0;  |ACABA;  |FINSI;

     ||#20#29 = (#20#23 + #20#24 - #20#26) % #20#28;
     #20#29 = #20#26 % #20#28;
     |PINTA #20#29;
|FPRC;

|PROCESO Totalm020;
     #20#23 = #20#21 - #20#22;

     |SI #20#25 ! -1;
         |C_M #20#26, 1, "N";

         #20#25 = 0;
         |SI #20#19 = "V";
             |SI #20#20 [ 39;                   #20#25 = 40;  |FINSI;
             |SI #20#20 ] 40;  |Y #20#20 [ 49;  #20#25 = 35;  |FINSI;
             |SI #20#20 ] 50;  |Y #20#20 [ 59;  #20#25 = 28;  |FINSI;
             |SI #20#20 ] 60;  |Y #20#20 [ 65;  #20#25 = 24;  |FINSI;
             |SI #20#20 ] 66;  |Y #20#20 [ 69;  #20#25 = 20;  |FINSI;
             |SI #20#20 ] 70;                   #20#25 = 8;   |FINSI;
         |SINO;
             |SI #20#20 [ 5;                    #20#25 = 12;  |FINSI;
             |SI #20#20 ] 6;   |Y #20#20 [ 10;  #20#25 = 16;  |FINSI;
             |SI #20#20 ] 11;  |Y #20#20 [ 15;  #20#25 = 20;  |FINSI;
             |SI #20#20 ] 16;                   #20#25 = 25;  |FINSI;
         |FINSI;

         #20#26 = #20#24 % #20#25;
     |SINO;
         |C_M #20#26, 1, "S";
     |FINSI;

     |PINTA #20#23;
     |PINTA #20#25;
     |PINTA #20#26;
|FPRC;

|PROCESO Usufructuario;
     |C_M #20#14, 1, "S";
     |SI #20#13 ! "N";
         |C_M #20#14, 1, "N";  #20#14 = 0;  |PINTA #20#14;
     |FINSI;
|FPRC;

|PROCESO Varios;  |TIPO 0;
     |SI #20#2 ! "VARIOS    ";
         #20#5  = 0;
         #20#6  = 0;
         #20#7  = 0;
         #20#8  = 0;
         #20#9  = 0;
         #20#10 = 0;
         |ACABA;
     |FINSI;

     |C_M #900#0,  1, "N";
     |C_M #900#1,  1, "N";
     |C_M #900#2,  1, "N";
     |C_M #900#3,  1, "N";
     |C_M #900#4,  1, "N";
     |C_M #900#5,  1, "N";

     aAlfa = eaSwVarios;
     aLong = aAlfa % 0;
     nLong = aLong;
     |PARA nCaracter = 1;  |SI nCaracter [ nLong;  |HACIENDO nCaracter = nCaracter + 1;
           nPosi = (100 * nCaracter) + 1;
           aAlfa1 = aAlfa % nPosi;
           |SI aAlfa1 = "T";  |C_M #900#0,  1, "S";  |FINSI;
           |SI aAlfa1 = "C";  |C_M #900#1,  1, "S";  |FINSI;
           |SI aAlfa1 = "1";  |C_M #900#2,  1, "S";  |FINSI;
           |SI aAlfa1 = "2";  |C_M #900#3,  1, "S";  |FINSI;
           |SI aAlfa1 = "3";  |C_M #900#4,  1, "S";  |FINSI;
           |SI aAlfa1 = "4";  |C_M #900#5,  1, "S";  |FINSI;
     |SIGUE;

     #900#0 = #20#5;
     #900#1 = #20#6;
     #900#2 = #20#7;
     #900#3 = #20#8;
     #900#4 = #20#9;
     #900#5 = #20#10;

     |PUSHV 0501 2380;

     |PINPA #0#900;
     |PINDA #0#900;
     |ENDATOS #1#900;
     |SI FSalida = 0;
         #20#5  = #900#0;
         #20#6  = #900#1;
         #20#7  = #900#2;
         #20#8  = #900#3;
         #20#9  = #900#4;
         #20#10 = #900#5;
     |FINSI;

     |POPV;
|FPRC;

