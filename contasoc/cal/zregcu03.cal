|FICHEROS;
     zregcu03;
|FIN;

|VARIABLES;
     nGrabo            = 0;
     nCampo           = 0;
     nCampo1          = 0;
     nCampo2          = 0;
     nCampo3          = 0;
     nVentana         = 0;
     nSubPanta        = 0;
     nNume            = 0;

     aAlfa1           = "";
     aAlfa2           = "";

     {-1}aVent        = "";
         aVent1       = 0;
         aVent2       = 0;
         aVent3       = 0;
         aVent4       = 0;
         aVent5       = "";

     &enEmpresa       = 0;
     &enEjerCuadro    = 0;
|FIN;

|PROCESO Tipo12Cuadros;  |TIPO 12;
|FPRC;

|PROCESO Totalcu03;
     |PARA nCampo3 = 24;  |SI nCampo3 [ 34;  |HACIENDO nCampo3 = nCampo3 + 1;
           nCampo1   = nCampo3 - 22;
           nCampo2   = nCampo3 - 11;
           #zregcu03#nCampo3# = #zregcu03#nCampo1# + #zregcu03#nCampo2#;
           |PINTA #zregcu03#nCampo3#;
     |SIGUE;

     |PARA nCampo3 = 57;  |SI nCampo3 [ 67;  |HACIENDO nCampo3 = nCampo3 + 1;
           nCampo1   = nCampo3 - 22;
           nCampo2   = nCampo3 - 11;
           #zregcu03#nCampo3# = #zregcu03#nCampo1# + #zregcu03#nCampo2#;
           |PINTA #zregcu03#nCampo3#;
     |SIGUE;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 3099, -1, 16, "Correcciones por deterioro de valor de activos a largo y corto plazo";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #zregcu03;
     #zregcu03#0 = enEmpresa;
     #zregcu03#1 = enEjerCuadro;
     |LEE 101#zregcu03.=;
     |SI FSalida ! 0;
         |DDEFECTO #zregcu03;
         #zregcu03#0 = enEmpresa;
         #zregcu03#1 = enEjerCuadro;
         |GRABA 020#zregcu03.b;
     |FINSI;

     |PINPA #0#zregcu03;
     nSubPanta = FSalida;

     nNume  = enEjerCuadro - 1;
     aAlfa1 = enEjerCuadro;
     aAlfa2 = nNume;

     aAlfa1 = "LABEL,{{0}}" + aAlfa1;
     aAlfa2 = "LABEL,{{0}}" + aAlfa2;

     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0733, 0737, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0833, 0837, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0933, 0937, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 1033, 1037, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 1133, 1137, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 1233, 1237, NULL;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 1333, 1337, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 1433, 1437, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 1533, 1537, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 1633, 1637, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 1733, 1737, NULL;

     |CONTROL_SIMPLE nSubPanta, aAlfa2, 2033, 2037, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 2133, 2137, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 2233, 2237, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 2333, 2337, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 2433, 2437, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 2533, 2537, NULL;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 2633, 2637, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 2733, 2737, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 2833, 2837, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 2933, 2937, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 3033, 3037, NULL;

     |PINDA #0#zregcu03;
     |ENDATOS #1#zregcu03;
     |SI FSalida = 0;
         nGrabo = 0;
         |PARA nCampo = 2;  |SI nCampo [ 67;  |HACIENDO nCampo = nCampo + 1;
               |SI #zregcu03#nCampo# ! 0;
                   nGrabo = 1;  |SAL;
               |FINSI;
         |SIGUE;

         |SI nGrabo ! 0;
             |GRABA 020#zregcu03.a;
         |SINO;
             |BORRA 020#zregcu03.a;
         |FINSI;
     |FINSI;
     |LIBERA #zregcu03;
     |CIERRA #zregcu03;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
