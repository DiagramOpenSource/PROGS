|FICHEROS;
     isow0024;
|FIN;

|VARIABLES;
     nVal1   = 0;
     nVal2   = 0;
|FIN;

|PROCESO Tipo12Fw24;  |TIPO 12;
     |SI FSalida = 7;
         |ACABA;
     |FINSI;

     nVal1 = (#isow0024#2 + #isow0024#3) ! 2;
     nVal2 = #isow0024#1 ! 2;
     |SI nVal1 > nVal2;
         |MENAV "0000La suma de las casillas 1516 + 1585 no pueden superior a la 1515";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO TotalFw24;  |TIPO 0;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     #isow0024#4 = #isow0024#1 - #isow0024#2 - #isow0024#3;

     |PINTA #isow0024#4;

     |SI Campo = 6;
         |POSICIONATE #isow0024#0;
     |FINSI;
|FPRC;
