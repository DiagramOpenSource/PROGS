|FICHEROS;
   isocie15 #0;
|FIN;

|VARIABLES;
   &empresa   = 0;
   &anyo      = 0;
   &modo      = 0;
   &modelito  = 0;
   &numerica  = 0;
   &tecla     = 0;
|FIN;

|RUTINA inferior;
  #0#0 = empresa;
  #0#1 = anyo;
  #0#2 = 1;
|FRUT;

|RUTINA superior;
  #0#0 = empresa;
  #0#1 = anyo;
  #0#2 = 99;
|FRUT;

|PROGRAMA;
|SI tecla ! 9; |HAZ numerica; |ACABA; |FINSI;

|PINPA #0#0;

|ATRI R;
|SI anyo = 1992; |PINTA 849, "707"; |FINSI;
|SI anyo = 1993; |PINTA 849, "708"; |FINSI;
|ATRI 0;

|SI modo = 1;
    |ENTLINEAL #1#0,3,1,14,0,inferior,superior;
|FINSI;
|SI modo = 2;
    |ENTLINEAL #1#0,3,2,14,0,inferior,superior;
|FINSI;
|SI modo = 4;
    |ENTLINEAL #1#0,3,4,14,0,inferior,superior;
|FINSI;
|HAZ numerica;
|FPRO;

|PROCESO clase; |TIPO 0;
|SI #0#3 = "I";
    |CAMPO_MODIFICA #0#6, 1, "S";
    |CAMPO_MODIFICA #0#4, 1, "N";
    |CAMPO_MODIFICA #0#7, 1, "N";
    |CAMPO_MODIFICA #0#8, 1, "N";
    |CAMPO_MODIFICA #0#9, 1, "N";
    #0#4 = 0; |PINTA #0#4;
    #0#7 = 0; |PINTA #0#7;
    #0#8 = 0; |PINTA #0#8;
    #0#9 = 0; |PINTA #0#9;
|SINO;
    |CAMPO_MODIFICA #0#6, 1, "N";
    |CAMPO_MODIFICA #0#4, 1, "S";
    |CAMPO_MODIFICA #0#7, 1, "S";
    |CAMPO_MODIFICA #0#8, 1, "S";
    |CAMPO_MODIFICA #0#9, 1, "S";
    #0#6 = 0; |PINTA #0#6;
|FINSI;
|FPRC;

|PROCESO calculo; |TIPO 0;
|SI #0#3 = "V";
    #0#6 = #0#8 - #0#9;
    |PINTA #0#6;
|FINSI;
|FPRC;

|PROCESO sumar;
numerica = numerica + #0#6;
|FPRC;

|DEFBUCLE lineas;
#0#1;
;
empresa, anyo, 01;
empresa, anyo, 99;
e;
NULL, sumar;
|FIN;

|PROCESO numerica;
numerica = 0;
|BUCLE lineas;
|FPRC;
