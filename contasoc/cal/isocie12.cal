|FINSI;
|SI modo = 4;
    |ENTLINEAL #1#0,4,4,14,0,inferior,superior;
|FINSI;
|HAZ numerica;
|FPRO;

|PROCESO sumar;
|SI clave1 = "A"; |O clave1 = "C";
    numerica = numerica + (#0#7 - #0#8);
|SINO;
    numerica = numerica + (#0#8 - #0#7);
|FINSI;
|FPRC;

|DEFBUCLE lineas;
#0#1;
;
empresa, anyo, clave1, 01;
empresa, anyo, clave1, 99;
e;
NULL, sumar;
|FIN;

|PROCESO numerica;
