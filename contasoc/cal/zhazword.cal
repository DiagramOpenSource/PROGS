|PROCESO Creacion;
     |IP_LOCAL aAlfa;
     |NOMBRE_IP aAlfa, aAlfa1;    |QBT aAlfa1;

     aAutoReturn = "0";

     |SI aAlfa1 ! "";
         |HAZ LeeUnidadBasico;

         aAlfa = eaUnidadBasico + "\ds\ds.ini";
         |XABRE "C", aAlfa, nCanal;

         |PARA;  |SI  FSalida ] 0;  |HACIENDO;
                 |XLEE nCanal, 254, aAlfa;
                 |SI FSalida < 0;  |SAL;  |FINSI;
                 aAlfa  = aAlfa > aAlfa;
                 aAlfa1 = aAlfa - "AUTORETURN";
                 |SI aAlfa1 ! aAlfa;
                     aAutoReturn = aAlfa % 1201;
                     |SAL;
                 |FINSI;
         |SIGUE;
         |XCIERRA  nCanal;
     |FINSI;

     |SI aAutoReturn ! "0";
         aAutoReturn = "1";
     |FINSI;

     |HAZ MiraApuntes;
     |HAZ MiraCuentas;
     |HAZ MiraEmpMemoria;
|FPRC;

