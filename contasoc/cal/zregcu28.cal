         aVent2       = 0;
         aVent3       = 0;
         aVent4       = 0;
         aVent5       = "";

     &enEmpresa       = 0;
     &enEjerCuadro    = 0;
|FIN;

|PROCESO Tipo12Cuadros;  |TIPO 12;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 1399, -1, 16, "Reservas de revalorizaci¢n";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #zregcu28;
     #zregcu28#0 = enEmpresa;
     #zregcu28#1 = enEjerCuadro;
     |LEE 101#zregcu28.=;
     |SI FSalida ! 0;
         |DDEFECTO #zregcu28;
         #zregcu28#0 = enEmpresa;
         #zregcu28#1 = enEjerCuadro;
         |GRABA 020#zregcu28.b;
     |FINSI;

     |PINPA #0#zregcu28;
