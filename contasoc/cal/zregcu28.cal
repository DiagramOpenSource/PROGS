     nNume  = enEjerCuadro - 1;
     aAlfa1 = enEjerCuadro;
     aAlfa2 = nNume;

     aAlfa1 = "LABEL,{{2}}" + aAlfa1;
     aAlfa2 = "LABEL,{{2}}" + aAlfa2;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0575, 0779, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0591, 0995, NULL;

     |PINDA #0#zregcu28;
     |ENDATOS #1#zregcu28;
     |SI FSalida = 0;
         nGrabo = 0;
         |PARA nCampo = 2;  |SI nCampo [ 9;  |HACIENDO nCampo = nCampo + 1;
               |SI #zregcu28#nCampo# ! 0;
                   nGrabo = 1;  |SAL;
               |FINSI;
         |SIGUE;

         |SI nGrabo ! 0;
             |GRABA 020#zregcu28.a;
         |SINO;
             |BORRA 020#zregcu28.a;
         |FINSI;
     |FINSI;
     |LIBERA #zregcu28;
     |CIERRA #zregcu28;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
