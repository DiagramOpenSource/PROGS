     |SI nBase1 > nBase;
         nBase1 = nBase1 - #isom0027#5;
         #isom0027#5 = nBase - nBase1;
         nBase1 = nBase1 + #isom0027#5;

         |SI #isom0027#7 > #isom0027#5;
