         nBase     = #isom0001#5;
         nBaseApli = -#isom0001#5;
     |FINSI;

     |SI nBaseApli < 0;
         nBaseApli = 0;
     |FINSI;

     |SELECT #1#isom0001;
     |CIERRA #isom0001;

     nBase = (nBase % 10) ! 2;
     |SI nBase > 1000000; nBase = 1000000;  |FINSI;
|FPRC;

