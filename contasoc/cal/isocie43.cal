|SI hasta    < #0campo1; |ACABA;      |FINSI;

|SI #0campo1 ] resto;
    #0campo2 = resto;
    #0campo3 = #0campo1 - #0campo2;
    acum2    = acum2 + #0campo2;
|SINO;
    #0campo2 = #0campo1;
    #0campo3 = #0campo1 - #0campo2;
    acum2    = acum2 + #0campo2;
|FINSI;

hasta = hasta - #0campo2;
|SI hasta < 0; hasta = 0; |FINSI;
|FPRC;
