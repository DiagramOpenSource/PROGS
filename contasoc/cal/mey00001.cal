|FICHEROS;
    mey00001 #0;
    me97acta #1;
    meempmem #2;
|FIN;

|VARIABLES;
     XCampos     = 0;

     &acabalo    = 0;
     &eImpresora = "";
|FIN;

|PROCESO Seleccion; |TIPO 0;
  #0Campo = #0Campo > #0Campo;  |PINTA #0Campo;

  |SI #0Campo = "S";
      |C_M #0#1, 1, "S";
      |C_M #0#2, 1, "S";
      |PARA XCampos = 3;  |SI XCampos [ 22;  |HACIENDO XCampos = XCampos + 1;
             |C_M #0XCampos, 1, "N";  #0XCampos = 0;  |PINTA #0XCampos;
      |SIGUE;
  |SINO;
      |C_M #0#1, 1, "N";
      |C_M #0#2, 1, "N";
      |PARA XCampos = 3;  |SI XCampos [ 22;  |HACIENDO XCampos = XCampos + 1;
             |C_M #0XCampos, 1, "S";
      |SIGUE;
  |FINSI;
|FPRC;

|PROCESO Imprime;
  #2#0 = #1#0;
  |LEE 040#2=;
  |SI FSalida ! 0; |DDEFECTO #2; |FINSI;

  |SI #1#2 = 1; |Y #1#3 = "G"; |INFOR "actasm1g"; |FINSI;
  |SI #1#2 = 1; |Y #1#3 = "P"; |INFOR "actasm1p"; |FINSI;
  |SI #1#2 = 1; |Y #1#3 = "0"; |INFOR "actasm10"; |FINSI;
  |SI #1#2 = 2; |Y #1#3 = "G"; |INFOR "actasm2g"; |FINSI;
  |SI #1#2 = 2; |Y #1#3 = "P"; |INFOR "actasm2p"; |FINSI;
  |SI #1#2 = 2; |Y #1#3 = "0"; |INFOR "actasm20"; |FINSI;
  |SI #1#2 = 3; |Y #1#3 = "G"; |INFOR "actasm3g"; |FINSI;
  |SI #1#2 = 3; |Y #1#3 = "P"; |INFOR "actasm3p"; |FINSI;
  |SI #1#2 = 3; |Y #1#3 = "0"; |INFOR "actasm30"; |FINSI;
  |SI #1#2 = 4; |Y #1#3 = "G"; |INFOR "actasm4g"; |FINSI;
  |SI #1#2 = 4; |Y #1#3 = "P"; |INFOR "actasm4p"; |FINSI;
  |SI #1#2 = 4; |Y #1#3 = "0"; |INFOR "actasm40"; |FINSI;
  |SI #1#2 = 5; |Y #1#3 = "G"; |INFOR "actasm5g"; |FINSI;
  |SI #1#2 = 5; |Y #1#3 = "P"; |INFOR "actasm5p"; |FINSI;
  |SI #1#2 = 5; |Y #1#3 = "0"; |INFOR "actasm50"; |FINSI;
  |SI #1#2 = 6; |Y #1#3 = "G"; |INFOR "actasm6g"; |FINSI;
  |SI #1#2 = 6; |Y #1#3 = "P"; |INFOR "actasm6p"; |FINSI;
  |SI #1#2 = 6; |Y #1#3 = "0"; |INFOR "actasm60"; |FINSI;
  |SI #1#2 = 7; |Y #1#3 = "G"; |INFOR "actasm7g"; |FINSI;
  |SI #1#2 = 7; |Y #1#3 = "P"; |INFOR "actasm7p"; |FINSI;
  |SI #1#2 = 7; |Y #1#3 = "0"; |INFOR "actasm70"; |FINSI;

  |SI FSalida = 0;  |PRINT;  |PIEINF;  |FININF;  |FINSI;
|FPRC;

|DEFBUCLE me97actas;
  #1#1;
  ;
  #0#1, #0#23;
  #0#2, #0#23;
  ;
  NULL, Imprime;
|FIN;

|PROCESO Tipo3;  |TIPO 3;
  |SI acabalo = 1;
      Impresora = eImpresora;
      |IMPRE -1;
  |SINO;
      |IMPRE 0;
  |FINSI;
  |SI FSalida ! 0;  |ACABA;  |FINSI;

  |ABRE #2;
  |SI #0#0 = "S";
      |BUCLE me97actas;
  |SINO;
      |PARA XCampos = 3;  |SI XCampos [ 22;  |HACIENDO XCampos = XCampos + 1;
            |SI #0XCampos ! 0;
                #0#1 = #0XCampos;
                #0#2 = #0XCampos;
                |BUCLE me97actas;
            |FINSI;
      |SIGUE;
  |FINSI;
  |CIERRA #2;

  |FINIMP;
|FPRC;
