         aAbre = aAlfa + "/201" + (("00000" + #99#0) % -105) + ".TXT";
     |FINSI;

     |SI #0#40 = "L";
         aAlfa      = "/MODELOS";                         |RMKDIR aAlfa;
         aAlfa      = "/MODELOS/CONTASOC";                |RMKDIR aAlfa;
         aAlfa      = "/MODELOS/CONTASOC/201";            |RMKDIR aAlfa;
         aAlfa      = "/MODELOS/CONTASOC/201/" + #0#36;   |RMKDIR aAlfa;

         aFichero   = "201" + (("00000" + #99#0) % -105);
         aAbre      = aAlfa + "/" + aFichero;

         aAlfa = "WAIT 20106PDF /E:" + aFichero + " /R:" + aFichero + ".TXT ";
         aAlfa = aAlfa + "/I:" + eaImpresora + " /C:" + eaCopia + " /H:" + eaTipoCola + &13 + &10;
