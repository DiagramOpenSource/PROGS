|FICHEROS;
     maempr12 #0;
     maempr13 #1,MANTE;
|FIN;

|VARIABLES;
     &empresa   = 0;
     &anyo      = 0;
     &modo      = 0;

     Modo       = 0;
     Forma      = 0;
     SwSalir    = 0;
|FIN;

|RUTINA ClaveBaja0;
  #0#0 = empresa;
  #0#1 = anyo;
  #0#2 = 1;
|FRUT;

|RUTINA ClaveAlta0;
  #0#0 = empresa;
  #0#1 = anyo;
  #0#2 = 99;
|FRUT;

|PROGRAMA;
  |ENTLINEAL #1#0, -3, 1, 14, modo, ClaveBaja0, ClaveAlta0;
|FPRO;

|RUTINA ClaveBaja1;
  #1#0 = #0#0;
  #1#1 = #0#1;
  #1#2 = #0#2;
  #1#3 = 1;
|FRUT;

|RUTINA ClaveAlta1;
  #1#0 = #0#0;
  #1#1 = #0#1;
  #1#2 = #0#2;
  #1#3 = 99;
|FRUT;

|PROCESO Borra01;
  |BORRA 140#1a;
  |LIBERA #1;
|FPRC;

|DEFBUCLE maempr13;
  #1#1;
  ;
  #0#0, #0#1, #0#2, 00;
  #0#0, #0#1, #0#2, 99;
  be;
  NULL, Borra01;
|FIN;

|PROCESO VeteAl13;  |TIPO 2;
  Modo  = (FEntrada / 100) ! 0;
  |SI Modo = 3;
      |BUCLE maempr13;
      |ACABA;
  |FINSI;

  Forma = 0 +. 1;
  |SI Forma ! 1;  |ACABA;  |FINSI;

  |ENTLINEAL #1#1, -4, 2, 21, 0, ClaveBaja1, ClaveAlta1;
|FPRC;

|PROCESO Tipo7F0; |TIPO 7;
  SwSalir = 1;
  |ENTLINEAL #1#1, -4, 4, 21, 0, ClaveBaja1, ClaveAlta1;
  SwSalir = 0;
|FPRC;

|PROCESO Tipo2F1;  |TIPO 2;
  #0#6 = #0#6 +. #1#7;
|FPRC;

|PROCESO Tipo7F1;  |TIPO 7;
  |SI SwSalir = 0;  |ACABA;  |FINSI;

  |PTEC 807;
|FPRC;
