|FICHEROS;
     soplanlc #0;
|FIN;

|VARIABLES;
     aAlfa      = "";
     aBase      = "";
     aOrigen    = "";
     aDestino   = "";
     aIPRemoto  = "";

     &eaUnidadBasico = "";
|FIN;

|PROCESO AyudaExtensa;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     |DBASE aBase;  |QBF aBase;
     aOrigen  = aBase + "planilla/cooperativasextensas.txt";
     aDestino = eaUnidadBasico + "\MODELOS";  |RMKDIR aDestino;
     aDestino = eaUnidadBasico + "\MODELOS\cooperativasextensas.txt";

     aIPRemoto = "";
     |IP_REMOTO aIPRemoto;
     |SI aIPRemoto ! "";
         |ENVIA_FICHERO aOrigen, aDestino;
     |SINO;
         |COPIA_FICHERO aOrigen, aDestino;
     |FINSI;

     aAlfa = "cmd " + &34 + "/c START /WAIT " + aDestino + &34;
     |RSYSTEM aAlfa;

     |RFBORRA aDestino;
|FPRC;

|PROCESO AyudaAbreviado;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     |DBASE aBase;  |QBF aBase;
     aOrigen  = aBase + "planilla/cooperativasabreviadas.txt";
     aDestino = eaUnidadBasico + "\MODELOS";  |RMKDIR aDestino;
     aDestino = eaUnidadBasico + "\MODELOS\cooperativasabreviadas.txt";

     aIPRemoto = "";
     |IP_REMOTO aIPRemoto;
     |SI aIPRemoto ! "";
         |ENVIA_FICHERO aOrigen, aDestino;
     |SINO;
         |COPIA_FICHERO aOrigen, aDestino;
     |FINSI;

     aAlfa = "cmd " + &34 + "/c START /WAIT " + aDestino + &34;
     |RSYSTEM aAlfa;

     |RFBORRA aDestino;
|FPRC;


|RUTINA ClaveBaja0;
     #0#0 = 1;
|FRUT;

|RUTINA ClaveAlta0;
     #0#0 = 999;
|FRUT;

|PROGRAMA;
     |HAZ LeeUnidadBasico;

      |CLS;
      |CABEZA "Cuentas Especiales Cooperativas";
      |ENTLINEAL #1#0, -1, 2, 22, 1, ClaveBaja0, ClaveAlta0;
|FPRO;
