         |PARA nPara = 38; |SI nPara [ 54; |HACIENDO nPara = nPara + 1;
               |C_M #0nPara, 1, "N";  #0nPara = "S";  |PINTA #0nPara;
         |SIGUE;

         |PARA nPara = 59; |SI nPara [ 61; |HACIENDO nPara = nPara + 1;
               |C_M #0nPara, 1, "N";  #0nPara = "S";  |PINTA #0nPara;
         |SIGUE;
     |FINSI;

     |SI #0#34 ! "C";
         #0#58 = "N";  |C_M #0#58, 1, "N";  |PINTA #0#58;
     |FINSI;
|FPRC;

