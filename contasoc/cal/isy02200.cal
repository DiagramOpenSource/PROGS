|PROCESO TipoImpresion;
     |C_M #0#37, 1, "S";
     |C_M #0#58, 1, "S";
     |PARA nPara = 38; |SI nPara [ 54; |HACIENDO nPara = nPara + 1;
           |C_M #0nPara, 1, "S";
     |SIGUE;

     |PARA nPara = 59; |SI nPara [ 61; |HACIENDO nPara = nPara + 1;
           |C_M #0nPara, 1, "S";
     |SIGUE;

     |SI #0#34 ! "H";
