|FICHEROS;
     isow9003;
|FIN;

|VARIABLES;
     aBase           = "";
     aMas            = "";
     aAlfa           = "";
     aID             = "";

     nID             = 0;
     nPer            = 0;
     nNume           = 0;
     nVentana        = -1;
     nValor          = 0;
     nPorce1         = 0;
     nPorce2         = 0;

     {10}aPopup      = "";

     &enBtnAnexo     = 0;
|FIN;

|PROCESO Tipo12Fw9003;  |TIPO 12;
|FPRC;

|PROCESO Tipo66C43F9003;  |TIPO 66;
     aAlfa = "|C043WID";
     aID   = #isow9003#aAlfa#;
     nID   = aID;

     aPopup1  = nID;

     |SI #isow9003#42 = "N";
         aPopup2  = "6";
         aPopup3  = "01";
         aPopup4  = "10";
         aPopup5  = "19";
         aPopup6  = "25";
         aPopup7  = "30";
         aPopup8  = "35";
     |FINSI;

     |SI #isow9003#41 = "S"; |O #isow9003#42 = "S";  |O #isow9003#39 = "S";
         aPopup2  = "5";
         aPopup3  = "20/25";
         aPopup4  = "20/30";
         aPopup5  = "25/30";
         aPopup6  = "04/30";
         aPopup7  = "35/30";
         aPopup8  = "";
     |FINSI;

     IaPopup  = aPopup1  <-;
     |ESPECIFICA "TRACKPOPUP", aPopup;
     |SI FSalida ! 0;
         IaPopup  = aPopup3  <-;
         IaPopup  = IaPopup + (FSalida - 1);

         #isow9003#43 = aPopup;

         |PINTA #isow9003#43;
     |FINSI;
|FPRC;

|PROCESO Tipo0C38F9003;  |TIPO 0;
/*
     |ESTADO_CONTROL enBtnAnexo, "DISABLE";
     |SI #isow9003#38 = "3";
         |ESTADO_CONTROL enBtnAnexo, "ENABLE";
     |FINSI;
*/
|FPRC;

|PROCESO Tipo0C6F9003;  |TIPO 0;
     |C_M #isow9003#38, 1, "S";
     |C_M #isow9003#40, 1, "S";
     |C_M #isow9003#42, 1, "S";
     |C_M #isow9003#44, 1, "S";

     |SI #isow9003#6 = "1";
         |C_M #isow9003#38, 1, "N";   #isow9003#38 = "0";
         |C_M #isow9003#40, 1, "N";   #isow9003#40 = "N";
         |C_M #isow9003#42, 1, "N";   #isow9003#42 = "N";
         |C_M #isow9003#44, 1, "N";   #isow9003#44 = "N";
     |FINSI;

     |SI #isow9003#6 = "2";
         |C_M #isow9003#42, 1, "N";   #isow9003#42 = "N";
     |FINSI;

     |PINTA #isow9003#38;
     |PINTA #isow9003#40;
     |PINTA #isow9003#42;
     |PINTA #isow9003#44;
|FPRC;

|PROCESO Tipo0C40F9003;  |TIPO 0;
     |C_M #isow9003#38, 1, "S";
     |SI #isow9003#40 = "N";
         |C_M #isow9003#38, 1, "N";   #isow9003#38 = "0";
     |FINSI;

     |PINTA #isow9003#38;
|FPRC;

|PROCESO Tipo0C44F9003;  |TIPO 0;
     |POSICIONATE #isow9003#43;
|FPRC;
