|FICHEROS;
     zregcu01;
|FIN;

|VARIABLES;
     nVentana         = 0;
     nCampo           = 0;
     nGrabo            = 0;

     {-1}aVent        = "";
         aVent1       = 0;
         aVent2       = 0;
         aVent3       = 0;
         aVent4       = 0;
         aVent5       = "";

     &enEmpresa       = 0;
     &enEjerCuadro    = 0;
|FIN;

|PROCESO Tipo12Cuadros;  |TIPO 12;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 2799, -1, 16, "Inmovilizado intangible, material e inversiones inmobiliarias";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #zregcu01;
     #zregcu01#0 = enEmpresa;
     #zregcu01#1 = enEjerCuadro;
     |LEE 101#zregcu01.=;
     |SI FSalida ! 0;
         |DDEFECTO #zregcu01;
         #zregcu01#0 = enEmpresa;
         #zregcu01#1 = enEjerCuadro;
         |GRABA 020#zregcu01.b;
     |FINSI;

     |PINPA #0#zregcu01;
     |PINDA #0#zregcu01;
     |ENDATOS #1#zregcu01;
     |SI FSalida = 0;
         nGrabo = 0;
         |PARA nCampo = 2;  |SI nCampo [ 49;  |HACIENDO nCampo = nCampo + 1;
               |SI #zregcu01#nCampo# ! 0;
                   nGrabo = 1;  |SAL;
               |FINSI;
         |SIGUE;

         |SI nGrabo ! 0;
             |GRABA 020#zregcu01.a;
         |SINO;
             |BORRA 020#zregcu01.a;
         |FINSI;
     |FINSI;
     |LIBERA #zregcu01;
     |CIERRA #zregcu01;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
