|FICHEROS;
   isocie03 #0;
|FIN;

|VARIABLES;
   &empresa   = 0;
   &anyo      = 0;
   &clave     = 0;
   &modo      = 0;
   &modelito  = 0;
   &numerica  = 0;
   &tecla     = 0;
|FIN;

|RUTINA inferior;
  #0#0 = empresa;
  #0#1 = anyo;
  #0#2 = clave;
  #0#3 = 1;
|FRUT;

|RUTINA superior;
  #0#0 = empresa;
  #0#1 = anyo;
  #0#2 = clave;
  #0#3 = 99;
|FRUT;

|PROCESO cuenta;
numerica = numerica + #0#5;
|FPRC;

|DEFBUCLE numerica;
#0#1;
;
empresa, anyo, clave, 01;
empresa, anyo, clave, 99;
e;
NULL, cuenta;
|FIN;

|PROGRAMA;
numerica = 0;

|SI tecla ! 9; |BUCLE numerica; |ACABA; |FINSI;

|ABRE #0;
|PINPA #0#0;

|SI modo = 1;
    |ENTLINEAL #1#0,-4,1,22,0,inferior,superior;
|FINSI;
|SI modo = 2;
    |ENTLINEAL #1#0,-4,2,22,0,inferior,superior;
|FINSI;
|SI modo = 4;
    |ENTLINEAL #1#0,-4,4,22,0,inferior,superior;
|FINSI;

|BUCLE numerica;
|FPRO;
