|DEFBUCLE isacceso;
#1#1;
4;
#0#33, d_empre, "A";
#0#33, h_empre, "A";
e;
NULL, alataque;
|FIN;

|PROCESO impre; |TIPO 3;
|HAZ PreparaInformes;

|IMPRE 0;
|SI FSalida ! 0; |ACABA; |FINSI;

|HAZ fecha;
|SI #0#0 = "N";
    |PARA c_empre = 3; |SI c_empre [ 32; |HACIENDO c_empre = c_empre + 1;
          |SI #0c_empre ! 000;
              d_empre = #0c_empre;
              h_empre = #0c_empre;
              |BUCLE isacceso;
          |FINSI;
    |SIGUE;
|SINO;
    d_empre = #0#1;
    h_empre = #0#2;
    |BUCLE isacceso;
|FINSI;
|FINIMP;
|FPRC;

