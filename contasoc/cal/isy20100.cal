|| **************************************************************************

|PROCESO seleccion; |TIPO 0;
|SI #0#0 = "S";
    |C_M #0#01, 1, "S";
    |C_M #0#02, 1, "S";
    |PARA c_empre = 3; |SI c_empre [ 32; |HACIENDO c_empre = c_empre + 1;
          #0c_empre = 0; |PINTA #0c_empre;
          |C_M #0c_empre, 1, "N";
    |SIGUE;
|SINO;
    #0#1 = 00001; |PINTA #0#1;
    #0#2 = 99999; |PINTA #0#2;
    |C_M #0#01, 1, "N";
    |C_M #0#02, 1, "N";

    |PARA c_empre = 3; |SI c_empre [ 32; |HACIENDO c_empre = c_empre + 1;
          |C_M #0c_empre, 1, "S";
    |SIGUE;
|FINSI;
|FPRC;

