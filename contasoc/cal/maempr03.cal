|FICHEROS;
     maempr03;
|FIN;

|VARIABLES;
     &enCalcCif = 0;
     &eaVarDni  = "";
|FIN;

|PROCESO LetraNif;  |TIPO 6;
     |SI FSalida = 10;
         enCalcCif = 1;
         eaVarDni  =  #maempr03#Campo#;
         |HAZ CalculoDNI;
         #maempr03#Campo# = eaVarDni;
         |PINTA #maempr03#Campo#;
         |ERROR;
         |ACABA;
     |FINSI;

     eaVarDni  = #maempr03#Campo#;
     enCalcCif = 0;
     |HAZ CalculoDNI;
     eaVarDni = (eaVarDni + "            ") % 112;
     |SI #maempr03#Campo# ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #maempr03#Campo# = eaVarDni;  |PINTA #maempr03#Campo#;
             |ERROR;
             |ACABA;
         |FINSI;
     |FINSI;
|FPRC;
