|FICHEROS;
     maempr02;
|FIN;

|VARIABLES;
     &enCalcCif = 0;
     &eaVarDni  = "";
|FIN;

|PROCESO Mayus;
     #maempr02#Campo# = #maempr02#Campo# > #maempr02#Campo#;
     |PINTA #maempr02#Campo#;
|FPRC;

|PROCESO LetraNif;  |TIPO 6;
     |SI FSalida = 10;
         enCalcCif = 1;
         eaVarDni  =  #maempr02#Campo#;
         |HAZ CalculoDNI;
         #maempr02#Campo# = eaVarDni;
         |PINTA #maempr02#Campo#;
         |ERROR;
         |ACABA;
     |FINSI;

     eaVarDni  = #maempr02#Campo#;
     enCalcCif = 0;
     |HAZ CalculoDNI;
     eaVarDni = (eaVarDni + "            ") % 112;
     |SI #maempr02#Campo# ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #maempr02#Campo# = eaVarDni;  |PINTA #maempr02#Campo#;
             |ERROR;
             |ACABA;
         |FINSI;
     |FINSI;
|FPRC;
