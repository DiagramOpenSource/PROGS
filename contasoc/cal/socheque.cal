|FICHEROS;
    socheque #0;
    sodacoli #1;
    soplanli #2;
|FIN;

|VARIABLES;
   fecha = @;
   que_anyo = "";

   cuentad  = "";
   cuentah  = "";
   vari     = "";
   aste     = "";
   long     = "";
   numero   = "";
   longi    = 0;
   sw_print = 0;
   sw_pie   = 0;

   &estruct = 0;
|FIN;

|PROCESO que_anyo; |TIPO 7;
fecha    = ~;
que_anyo = fecha % -104;
#0#2     = que_anyo;
#0#2     = #0#2 - 1;
|PINTA #0#2;
|FPRC;

|PROCESO al_rollo;
vari   = #2#2;  |QBF vari;
aste   = vari % -101;
long   = vari % 0;
longi  = long - 1;
longi  = 100 + longi;
numero = vari % longi;
|SI aste = "*";
    cuentad = numero + "            ";
    cuentad = cuentad % 112;
    cuentah = numero + "zzzzzzzzzzzz";
    cuentah = cuentah % 112;
|SINO;
    cuentad = #2#2;
    cuentah = #2#2;
|FINSI;

|SI #1#3 ] cuentad; |Y #1#3 [ cuentah;
    sw_print = 0;
    |ERROR10;
|FINSI;
|FPRC;

|DEFBUCLE soplanli;
#2#1;
;
estruct, 000;
estruct, 999;
e;
NULL, al_rollo;
|FIN;

|PROCESO chequear;
|PINTA 2256, "                   ";

|PINTA 2256, #1#0;
|PINTA 2260, #1#1;
|PINTA 2265, #1#3;

sw_print = 1; estruct = #0#3; |BUCLE soplanli;
|SI sw_print = 1;
    |PRINT;
    sw_pie = 1;
|FINSI;

sw_print = 1; estruct = #0#5; |BUCLE soplanli;
|SI sw_print = 1;
    |PRINT;
    sw_pie = 1;
|FINSI;
|FPRC;

|DEFBUCLE sodacoli;
#1#1;
;
#0#0, #0#2, 0000;
#0#1, #0#2, 9999;
e;
NULL, chequear;
|FIN;

|PROCESO tipo_3; |TIPO 3;
|IMPRE 0;
|SI FSalida ! 0; |ACABA; |FINSI;

|INFOR "socheque";
|SI FSalida ! 0; |FININF; |ACABA; |FINSI;

|BUCLE sodacoli;
|SI sw_pie = 1; |PIEINF; |FINSI;

|FININF;
|FINIMP;

|FPRC;
