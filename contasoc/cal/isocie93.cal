|FICHEROS;
     isocie91 #91;
     isocie93 #93,MANTE;
|FIN;

|VARIABLES;
     &enCalcCif = 0;
     &eaVarDni  = "";
|FIN;

|PROCESO LetraNifm93;  |TIPO 11;
     |SI FSalida = 10;
         enCalcCif = 1;
         eaVarDni  = #93#2;
         |HAZ CalculoDNI;
         #93#2 = eaVarDni;
         |PINTA #93#2;
         |ERROR;
         |ACABA;
     |FINSI;
|FPRC;

|PROCESO MiraNifm93;  |TIPO 6;
     eaVarDni  = #93Campo;
     enCalcCif = 0;
     |HAZ CalculoDNI;
     eaVarDni = (eaVarDni + "         ") % 109;
     |SI #93Campo ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #93Campo = eaVarDni;  |PINTA #93Campo;
             |ERROR;
             |ACABA;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo2m93;  |TIPO 2;
     #91#37 = #91#37 +. #93#3;
     #91#38 = #91#38 +. #93#4;
     #91#39 = #91#39 +. #93#5;
     #91#40 = #91#36 + #91#37 + #91#38 + #91#39;

     |PINTA 2020, #91#37;
     |PINTA 2043, #91#38;
     |PINTA 2066, #91#39;
     |PINTA 2266, #91#40;
|FPRC;
