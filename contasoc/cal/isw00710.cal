|FICHEROS;
     isw00710 #780;
|FIN;

|VARIABLES;
     nCampo     = 0;

     &enForales = 0;
|FIN;

|PROCESO Tipo12w710;  |TIPO 12;
|FPRC;

|PROCESO Tipo7w710;  |TIPO 7;
     |C_M #780#4,  1, "S";
     |C_M #780#6,  1, "S";
     |C_M #780#8,  1, "S";
     |C_M #780#10, 1, "S";
     |C_M #780#12, 1, "S";
     |C_M #780#16, 1, "S";
     |C_M #780#19, 1, "S";
     |C_M #780#21, 1, "S";

     |SI enForales = 0;
         |C_M #780#4,  1, "N";
         |C_M #780#6,  1, "N";
         |C_M #780#8,  1, "N";
         |C_M #780#10, 1, "N";
         |C_M #780#12, 1, "N";
         |C_M #780#16, 1, "N";
         |C_M #780#19, 1, "N";
         |C_M #780#21, 1, "N";
     |FINSI;
|FPRC;

|PROCESO Totalw710;
     #780#13 = #780#1 - #780#3 - #780#5 - #780#7 - #780#9  - #780#11;
     #780#14 = #780#2 - #780#4 - #780#6 - #780#8 - #780#10 - #780#12;

     |SI #780#17 ! -1;
         #780#18 = (#780#13 + #780#15) % #780#17;
         #780#19 = (#780#14 + #780#16) % #780#17;
     |FINSI;

     #780#22 = #780#13 + #780#15 + #780#18 + #780#20;
     #780#23 = #780#14 + #780#16 + #780#19 + #780#21;

     |PINTA #780#13;
     |PINTA #780#14;
     |PINTA #780#18;
     |PINTA #780#19;
     |PINTA #780#22;
     |PINTA #780#23;
|FPRC;

|PROCESO AplicAut;
   || Si no , peta el proceso que hay enganchado en isocie62 para calcular la base
   ||    imponible desde el isocie59
|FPRC;

|PROCESO SaldoAnt;
   || Si no , peta el proceso que hay enganchado en isocie62 para calcular la base
   ||    imponible desde el isocie59
|FPRC;
