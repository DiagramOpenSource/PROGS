|FICHEROS;
     zregcu15;
|FIN;

|VARIABLES;
     nGrabo            = 0;
     nCampo           = 0;
     nVentana         = 0;
     nSubPanta        = 0;
     nNume            = 0;

     aAlfa1           = "";
     aAlfa2           = "";

     {-1}aVent        = "";
         aVent1       = 0;
         aVent2       = 0;
         aVent3       = 0;
         aVent4       = 0;
         aVent5       = "";

     &enEmpresa       = 0;
     &enEjerCuadro    = 0;
|FIN;

|PROCESO Tipo12Cuadros;  |TIPO 12;
|FPRC;

|PROCESO Totalcu15;
     #zregcu15#4  = #zregcu15#02 + #zregcu15#03;
     #zregcu15#6  = (#zregcu15#02 * 100) / #zregcu15#4;
     #zregcu15#7  = 0;
     |SI #zregcu15#3 ! 0;
         #zregcu15#7  = 100 - #zregcu15#6;
     |FINSI;
     #zregcu15#8  = #zregcu15#6 + #zregcu15#7;

     #zregcu15#11 = #zregcu15#09 + #zregcu15#10;
     #zregcu15#13 = (#zregcu15#09 * 100) / #zregcu15#11;
     #zregcu15#14 = 0;
     |SI #zregcu15#10 ! 0;
         #zregcu15#14 = 100 - #zregcu15#13;
     |FINSI;
     #zregcu15#15 = #zregcu15#13 + #zregcu15#14;

     |PINTA #zregcu15#4;
     |PINTA #zregcu15#6;
     |PINTA #zregcu15#7;
     |PINTA #zregcu15#8;
     |PINTA #zregcu15#11;
     |PINTA #zregcu15#13;
     |PINTA #zregcu15#14;
     |PINTA #zregcu15#15;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 1599, -1, 16, "Informaci¢n sobre aplazamientos de pago efectuados a proveedores";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #zregcu15;
     #zregcu15#0 = enEmpresa;
     #zregcu15#1 = enEjerCuadro;
     |LEE 101#zregcu15.=;
     |SI FSalida ! 0;
         |DDEFECTO #zregcu15;
         #zregcu15#0 = enEmpresa;
         #zregcu15#1 = enEjerCuadro;
         |GRABA 020#zregcu15.b;
     |FINSI;

     |PINPA #0#zregcu15;
     nSubPanta = FSalida;

     nNume  = enEjerCuadro - 1;
     aAlfa1 = enEjerCuadro;
     aAlfa2 = nNume;

     aAlfa1 = "LABEL,{{2}}" + aAlfa1;
     aAlfa2 = "LABEL,{{2}}" + aAlfa2;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0657, 0661, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0687, 0691, NULL;

     |PINDA #0#zregcu15;
     |ENDATOS #1#zregcu15;
     |SI FSalida = 0;
         nGrabo = 0;
         |PARA nCampo = 2;  |SI nCampo [ 15;  |HACIENDO nCampo = nCampo + 1;
               |SI #zregcu15#nCampo# ! 0;
                   nGrabo = 1;  |SAL;
               |FINSI;
         |SIGUE;

         |SI nGrabo ! 0;
             |GRABA 020#zregcu15.a;
         |SINO;
             |BORRA 020#zregcu15.a;
         |FINSI;
     |FINSI;
     |LIBERA #zregcu15;
     |CIERRA #zregcu15;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
