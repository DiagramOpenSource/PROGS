|FICHEROS;
   ispant23 #0;
|FIN;

|VARIABLES;
   &anyo        = 0;

   &numerica1   = 0;  &numerica2  = 0; &numerica3 = 0;
   &numerica4   = 0;  &numerica5  = 0; &numerica6 = 0;
   &numerica7   = 0;  &numerica8  = 0;

   &alfanume1   = "";

   suma         = 0;
|FIN;

|PROGRAMA;
|ET arriba;
|PINPA #0#0;

|ATRI R;
|SI anyo ] 1994;
    |PINTA 1232, "739";
    |PINTA 1364, "734";
    |PINTA 1464, "735";
    |PINTA 1564, "736";
    |PINTA 1864, "737";
    |PINTA 1964, "738";
|SINO;
    |PINTA 1232, "736";
    |PINTA 1364, "731";
    |PINTA 1464, "732";
    |PINTA 1564, "733";
    |PINTA 1864, "734";
    |PINTA 1964, "735";
|FINSI;
|ATRI r;

|HAZ cargavar;
|PINDA #0#0;
|ENDATOS #1#0;
|SI FSalida = 0;
   |HAZ descarga;
   |SI #0#1 ! 0; |O #0#2 ! 0;
       suma = #0#1 + #0#2;
       |SI suma ! #0#0;
           |MENAV "      No puede ser diferente la 732 + 733 a la 731";
           |VETE arriba;
       |FINSI;
   |FINSI;
|FINSI;
|FPRO;

|PROCESO cargavar;
#0#0  = numerica1;
#0#1  = numerica2;
#0#2  = numerica3;
#0#3  = numerica4;
#0#4  = numerica5;
#0#5  = numerica6;
#0#6  = numerica7;
#0#7  = numerica8;
#0#8  = alfanume1;
|FPRC;

|PROCESO descarga;
numerica1  = #0#0;
numerica2  = #0#1;
numerica3  = #0#2;
numerica4  = #0#3;
numerica5  = #0#4;
numerica6  = #0#5;
numerica7  = #0#6;
numerica8  = #0#7;
alfanume1  = #0#8;
|FPRC;

|PROCESO calculo; |TIPO 0;
|SI #0#1 ! 0; |O #0#2 ! 0;
    |CAMPO_MODIFICA #0#4, 1, "S";
    |CAMPO_MODIFICA #0#5, 1, "S";
    |SI #0#1 = 0; |CAMPO_MODIFICA #0#4, 1, "N"; #0#4 = 0; |PINTA #0#4; |FINSI;
    |SI #0#2 = 0; |CAMPO_MODIFICA #0#5, 1, "N"; #0#5 = 0; |PINTA #0#5; |FINSI;

    suma = #0#1 + #0#2;
    #0#6 = #0#1 % #0#4;
    ||SI #0#6 < 0; #0#6 = 0; |FINSI;
    |PINTA #0#6;

    #0#7 = #0#2 % #0#5;
    ||SI #0#7 < 0; #0#7 = 0; |FINSI;
    |PINTA #0#7;

    |SI #0#1 ! 0;
        #0#3 = #0#4;        |PINTA #0#3;
    |SINO;
        #0#3 = #0#5;        |PINTA #0#3;
    |FINSI;
|SINO;
    |CAMPO_MODIFICA #0#4, 1, "N";
    #0#4 = 0;           |PINTA #0#4;
    #0#6 = 0;           |PINTA #0#6;
    #0#7 = #0#0 % #0#5;
    ||SI #0#7 < 0; #0#7 = 0; |FINSI;
    |PINTA #0#7;
    #0#3 = #0#5;        |PINTA #0#3;
|FINSI;
|FPRC;
