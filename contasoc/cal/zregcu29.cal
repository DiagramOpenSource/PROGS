|FICHEROS;
     zregcu29;
|FIN;

|VARIABLES;
     nGrabo            = 0;
     nCampo           = 0;
     nVentana         = 0;
     nSubPanta        = 0;
     nNume            = 0;

     aAlfa1           = "";
     aAlfa2           = "";

     {-1}aVent        = "";
         aVent1       = 0;
         aVent2       = 0;
         aVent3       = 0;
         aVent4       = 0;
         aVent5       = "";

     &enEmpresa       = 0;
     &enEjerCuadro    = 0;
|FIN;

|PROCESO Tipo12Cuadros;  |TIPO 12;
|FPRC;

|PROCESO Totalcu29;
|FPRC;


|PROGRAMA;

     |VENTANA 0501, 1599, -1, 16, "Informaci¢n sobre aplazamientos de pago efectuados a proveedores";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #zregcu29;
     #zregcu29#0 = enEmpresa;
     #zregcu29#1 = enEjerCuadro;
     |LEE 101#zregcu29.=;
     |SI FSalida ! 0;
         |DDEFECTO #zregcu29;
         #zregcu29#0 = enEmpresa;
         #zregcu29#1 = enEjerCuadro;
         |GRABA 020#zregcu29.b;
     |FINSI;

     |PINPA #0#zregcu29;
     nSubPanta = FSalida;

     nNume  = enEjerCuadro - 1;
     aAlfa1 = enEjerCuadro;
     aAlfa2 = nNume;

     aAlfa1 = "LABEL,{{2}}" + aAlfa1;
     aAlfa2 = "LABEL,{{2}}" + aAlfa2;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0659, 0663, NULL;
     |SI enEjerCuadro > 2015;
         |CONTROL_SIMPLE nSubPanta, aAlfa2, 0679, 0683, NULL;
     |SINO;
         aAlfa2 = "LABEL,{{2}}" + "               ";
         |CONTROL_SIMPLE nSubPanta, aAlfa2, 0775, 0789, NULL;
         |C_M #zregcu29#3, 1, "N";
         |C_V #zregcu29#3, 1, "N";
     |FINSI;

     |PINDA #0#zregcu29;
     |ENDATOS #1#zregcu29;
     |SI FSalida = 0;
         nGrabo = 0;
         |PARA nCampo = 2;  |SI nCampo [ 3;  |HACIENDO nCampo = nCampo + 1;
               |SI #zregcu29#nCampo# ! 0;
                   nGrabo = 1;  |SAL;
               |FINSI;
         |SIGUE;

         |SI nGrabo ! 0;
             |GRABA 020#zregcu29.a;
         |SINO;
             |BORRA 020#zregcu29.a;
         |FINSI;
     |FINSI;
     |LIBERA #zregcu29;
     |CIERRA #zregcu29;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
