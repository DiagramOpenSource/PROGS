|FICHEROS;
     zregcu11;
|FIN;

|VARIABLES;
     nCampo           = 0;
     nGrabo            = 0;
     nVentana         = 0;
     nSubPanta        = 0;
     nNume            = 0;

     aAlfa1           = "";
     aAlfa2           = "";

     {-1}aVent        = "";
         aVent1       = 0;
         aVent2       = 0;
         aVent3       = 0;
         aVent4       = 0;
         aVent5       = "";

     &enEmpresa       = 0;
     &enEjerCuadro    = 0;
|FIN;

|PROCESO Tipo12Cuadros;  |TIPO 12;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 1999, -1, 16, "Sueldos, dietas y remuneraciones";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #zregcu11;
     #zregcu11#0 = enEmpresa;
     #zregcu11#1 = enEjerCuadro;
     |LEE 101#zregcu11.=;
     |SI FSalida ! 0;
         |DDEFECTO #zregcu11;
         #zregcu11#0 = enEmpresa;
         #zregcu11#1 = enEjerCuadro;
         |GRABA 020#zregcu11.b;
     |FINSI;

     |PINPA #0#zregcu11;
     nSubPanta = FSalida;

     nNume  = enEjerCuadro - 1;
     aAlfa1 = enEjerCuadro;
     aAlfa2 = nNume;

     aAlfa1 = "LABEL,{{2}}" + aAlfa1;
     aAlfa2 = "LABEL,{{2}}" + aAlfa2;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0645, 0649, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0661, 0665, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0676, 0680, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0691, 0695, NULL;

     |PINDA #0#zregcu11;
     |ENDATOS #1#zregcu11;
     |SI FSalida = 0;
         nGrabo = 0;
         |PARA nCampo = 2;  |SI nCampo [ 25;  |HACIENDO nCampo = nCampo + 1;
               |SI #zregcu11#nCampo# ! 0;
                   nGrabo = 1;  |SAL;
               |FINSI;
         |SIGUE;

         |SI nGrabo ! 0;
             |GRABA 020#zregcu11.a;
         |SINO;
             |BORRA 020#zregcu11.a;
         |FINSI;
     |FINSI;
     |LIBERA #zregcu11;
     |CIERRA #zregcu11;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
