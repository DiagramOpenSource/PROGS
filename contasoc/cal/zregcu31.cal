|FICHEROS;
     zregcu31;
|FIN;

|VARIABLES;
     nCampo           = 0;
     nGrabo            = 0;
     nVentana         = 0;
     nSubPanta        = 0;
     nNume            = 0;

     aAlfa1           = "";
     aAlfa2           = "";

     {-1}aVent        = "";
         aVent1       = 0;
         aVent2       = 0;
         aVent3       = 0;
         aVent4       = 0;
         aVent5       = "";

     &enEmpresa       = 0;
     &enEjerCuadro    = 0;
|FIN;

|PROCESO Tipo12Cuadros;  |TIPO 12;
|FPRC;

|PROCESO Tipo0Cu31; |TIPO 0;
    #zregcu31#10 = #zregcu31#2 + #zregcu31#3; |PINTA #zregcu31#10;
    #zregcu31#11 = #zregcu31#4 + #zregcu31#5; |PINTA #zregcu31#11;
    #zregcu31#12 = #zregcu31#6 + #zregcu31#7; |PINTA #zregcu31#12;
    #zregcu31#13 = #zregcu31#8 + #zregcu31#9; |PINTA #zregcu31#13;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 1999, -1, 16, "Importes recibidos personal alta direccion y organos administracion";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #zregcu31;
     #zregcu31#0 = enEmpresa;
     #zregcu31#1 = enEjerCuadro;
     |LEE 101#zregcu31.=;
     |SI FSalida ! 0;
         |DDEFECTO #zregcu31;
         #zregcu31#0 = enEmpresa;
         #zregcu31#1 = enEjerCuadro;
         |GRABA 020#zregcu31.b;
     |FINSI;

     |PINPA #0#zregcu31;
     nSubPanta = FSalida;

     nNume  = enEjerCuadro - 1;
     aAlfa1 = enEjerCuadro;
     aAlfa2 = nNume;

     aAlfa1 = "LABEL,{{2}}" + aAlfa1;
     aAlfa2 = "LABEL,{{2}}" + aAlfa2;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0845, 0849, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0861, 0865, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0876, 0880, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0891, 0895, NULL;

     |PINDA #0#zregcu31;
     |ENDATOS #1#zregcu31;
     |SI FSalida = 0;
         nGrabo = 0;
         |PARA nCampo = 2;  |SI nCampo [ 13;  |HACIENDO nCampo = nCampo + 1;
               |SI #zregcu31#nCampo# ! 0;
                   nGrabo = 1;  |SAL;
               |FINSI;
         |SIGUE;

         |SI nGrabo ! 0;
             |GRABA 020#zregcu31.a;
         |SINO;
             |BORRA 020#zregcu31.a;
         |FINSI;
     |FINSI;
     |LIBERA #zregcu31;
     |CIERRA #zregcu31;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
