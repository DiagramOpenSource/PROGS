|FICHEROS;
     isom0006;
     isom0017;
|FIN;

|VARIABLES;
     nVentana       = 0;
     nRango         = 0;
     nGrid          = 0;
     nImporte       = 0;

     aAlfa          = "";
     aEsc1          = "";
     aEsc2          = "";
     aRetu          = "";
     aTecla         = "";

     {-1}aVent      = "";
         aVent1     = 0;
         aVent2     = 0;
         aVent3     = 0;
         aVent4     = 0;
         aVent5     = "";

     &enEmpresa      = 0;
     &enAnyo         = 0;
     &enModoConsulta = 0;
     &enSaltaError   = 0;
|FIN;

|PROCESO Tipo0c2;  |TIPO 0;
     |SI #isom0017#2 > 50;
         |MENAV "0000Solo se permiten 50 registros.";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Baja117;
     #isom0017#0  = enEmpresa;
     #isom0017#1  = enAnyo;
     #isom0017#2  = 1;
|FPRC;

|PROCESO Alta117;
     #isom0017#0  = enEmpresa;
     #isom0017#1  = enAnyo;
     #isom0017#2  = 99;
|FPRC;

|PROCESO isom0017B;
     |BORRA 020#isom0017.a;
     |LIBERA #isom0017;
|FPRC;

|DEFBUCLE isom0017B;
     #isom0017#1;
     ;
     enEmpresa, enAnyo, 01;
     enEmpresa, enAnyo, 99;
     be;
     NULL, isom0017B;
|FIN;

|PROCESO isom0017C;
     nImporte = nImporte + #isom0017#4;
|FPRC;

|DEFBUCLE isom0017C;
     #isom0017#1;
     ;
     enEmpresa, enAnyo, 01;
     enEmpresa, enAnyo, 99;
     ;
     NULL, isom0017C;
|FIN;

|PROGRAMA;
     |HAZ SaltaError;

     |ABRE #isom0006;
     |SELECT #3#isom0006;
     #isom0006#0 = enEmpresa;
     #isom0006#1 = enAnyo;
     #isom0006#6 = 414;
     |LEE 000#isom0006.=;
     |SI FSalida ! 0;  |DDEFECTO #isom0006;  |FINSI;
     |CIERRA #isom0006;

     |SI #isom0006#7 [ 50000;
         |MENAV "0000La casilla 414 de la liquidaci¢n no supera los 50.000 Euros";

         |BUCLE isom0017B;

         |SI enSaltaError = 0;  |ACABA;  |FINSI;
     |FINSI;

     aAlfa = "";
     |SI enModoConsulta = 1;
         aAlfa = "(MODO CONSULTA)   ";
     |FINSI;

     aAlfa = aAlfa + "Otras correcciones a la cuenta de PyG";
     |VENTANA 0501, 2099, -1, 16, aAlfa;
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #isom0017;

     |SI enModoConsulta = 0;
         nRango = 1 + 2 + 4 + 8 + 16 + 32;
     |SINO;
         nRango = 4 + 8 + 16 + 32;
     |FINSI;

     |LINEAL_SIMPLE #1#isom0017, nRango, 0602, 1898, Baja117, Alta117, NULL;
     nGrid = FSalida;

     |PULSATECLA;
     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
          FSalida = "::{{001}}Salir," + nGrid;
          |LEETECLA aTecla;
          |SI aTecla = aEsc1; |SAL;  |FINSI;
          |SI aTecla = aEsc2; |SAL;  |FINSI;
          |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;

     |CIERRA #isom0017;

     |DESTRUYECONTROL nGrid;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
