         |CAMPO_MODIFICA #0#02, 1, "S";

         |PARA xxx = 3; |SI xxx [ 32; |HACIENDO xxx = xxx + 1;
               #0xxx = 0; |PINTA #0xxx;
               |CAMPO_MODIFICA #0xxx, 1, "N";
         |SIGUE;
     |SINO;
         #0#1 = 00001; |PINTA #0#1;    |CAMPO_MODIFICA #0#01, 1, "N";
