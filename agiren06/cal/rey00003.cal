     |IMPRIME aAlfa;

     aAlfa = aRaya * 80;
     |IMPRIME aAlfa;
|FPRC;

|PROCESO Pie;
     |SI #0#49 = "S"; |ACABA; |FINSI;

     |PARA i = nLinea; |SI i < #0#38; |HACIENDO i = i + 1;
          |IMPRIME "";
     |SIGUE;
     |SI #0#40 = "S"; |IMPRIME aSalto; |FINSI;
     nLinea = 0;
|FPRC;

|PROCESO Titulo;
     |SI #0#49 = "S"; |ACABA; |FINSI;

     nLinea = nLinea + 1;
     |IMPRIME aAlfa;
     |SI nLinea ] #0#38; |HAZ Pie; |FINSI;
|FPRC;
