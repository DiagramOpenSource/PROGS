|DEFBUCLE reacceso;
     #1#1;
     ;
     nDcodi;
     nHcodi;
     ;
     NULL, reacceso;
|FIN;

|PROCESO CargaTmp;
     |SI #0#33 = "S";
          |CONFI "0000SMarcar todos: ";
          nSwTodos = FSalida;
     |SINO;
          nSwTodos = 0;
     |FINSI;

     aAlfa = Usuario; |QBF aAlfa;
     |NOME_DAT #500 aAlfa;
     |DELINDEX #500;
     |ABRE #500;
     |SI #0#0 = "S";
          nDcodi = #0#1;
          nHcodi = #0#2;
          |BUCLE reacceso;
     |SINO;
          |PARA nCampo = 3; |SI i [ 32; |HACIENDO; i = i + 1;
               nDcodi = #0nCampo;
               nHcodi = #0nCampo;
               |BUCLE reacceso;
          |SIGUE;
     |FINSI;
     |CIERRA #500;

     |SI nSwAlguno = 0;
          |MENAV "0000Seleccion vacia.";
          |ACABA;
     |FINSI;

     |SI #0#33 = "S";         || Puntear
          |C_V #500#4, 1, "N";
          |PUSHV 0501 2380;
          |BLANCO 0701 2380;
          |PINPA #0#500;
          |PINTA 0967, "      ";
          |ATRI R;
          |PINTA 0912, "    N.I.F.   ";
          |ATRI 0;
          |ENTLINEAL #1#500, -1, 4, 18, 0, Min, Max;
          |POPV;
     |FINSI;
|FPRC;
ÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
|PROCESO EnviaFichero;

     |IP_REMOTO aAlfa1;  |QBT aAlfa1;
     |SI aAlfa1 ! "";
         |ENVIA_FICHERO aFicDes, aFicEnvio;
     |SINO;
         |COPIA_FICHERO aFicDes, aFicEnvio;
     |FINSI;
|FPRC;

|PROCESO QuitaRaros;
     |XLEE nHandle, 250, aAlfa;
     nSalida = FSalida;

     |QBF aAlfa;
     aCadena = "";
     aLen    = aAlfa % 0;
     nLen    = aLen;
     |PARA nCampo = 1;  |SI nCampo [ nLen;  |HACIENDO nCampo = nCampo + 1;
           nPos = (100 * nCampo) + 1;
           aCaracter = aAlfa % nPos;
           sw = 0;
           |SI aCaracter = "¥";  aCaracter = &209; sw = 1; |FINSI;
           |SI aCaracter = "¤";  aCaracter = &241; sw = 1; |FINSI;
           |SI aCaracter = " ";  aCaracter = &225; sw = 1; |FINSI;
           |SI aCaracter = "‚";  aCaracter = &233; sw = 1; |FINSI;
           |SI aCaracter = "¡";  aCaracter = &237; sw = 1; |FINSI;
           |SI aCaracter = "¢";  aCaracter = &243; sw = 1; |FINSI;
           |SI aCaracter = "£";  aCaracter = &250; sw = 1; |FINSI;
           |SI aCaracter = "§";  aCaracter = &176; sw = 1; |FINSI;
           |SI aCaracter = "¦";  aCaracter = &170; sw = 1; |FINSI;
           |SI aCaracter > aMax; |Y aCaracter < aMin; |Y sw = 0;
               |SI aCaracter = aRaya;
                    aCaracter = "_";
               |SINO;
                    aCaracter = " ";
               |FINSI;
           |FINSI;
           aCadena = aCadena + aCaracter;
     |SIGUE;
     aAlfa = aCadena;

     |XGRABA nHandle1, aAlfa;
|FPRC;

|PROCESO Word;
     aFicEnvio = "";
     aNomImp  = #0#52; |QBF aNomImp;
     aNomImp = aNomImp + ".prn";

     aAlfa1 = #0#50; |QBF aAlfa1;
     aAlfa1 = aAlfa1 + "dev/imp/" + aNomImp;
     |QBF aAlfa1;
     |XABRE "C", aAlfa1, nHandle;
     |SI FSalida ] 0;
          |XLEE nHandle, 250, aAlfa;
          |XLEE nHandle, 250, aAlfa;
          |SI FSalida > 0;
               aFicEnvio = aAlfa;
          |SINO;
               aAlfa4 = "0000[" + aAlfa1 + "] incompatible.";
               |MENAV aAlfa4;
          |FINSI;
          |XCIERRA nHandle;
     |SINO;
          aAlfa4 = "0000[" + aAlfa1 + "] inaccesible.";
          |MENAV aAlfa4;
     |FINSI;

     |SI aFicEnvio ! "";
          |XABRE "C", aFicEnvio, nHandle;
          |SI FSalida ] 0;
               |DIRECTORIO aFicDes; |QBF aFicDes;
               aFicDes = aFicDes + "word.txt";

               |XABRE "W", aFicDes, nHandle1;
               |SI FSalida ] 0;
                    |PARA nSalida = 1; |SI nSalida ] 0; |HACIENDO;
                         |HAZ QuitaRaros;
                    |SIGUE;
                    |XCIERRA nHandle1;
                    |HAZ EnviaFichero;
               |SINO;
                    |MENAV "0000Error al convertir el fichero...";
               |FINSI;
               |XCIERRA nHandle;

               |WORD_ABRE nHandle, aFicEnvio;
               |WORD_PROPIEDADES nHandle, "Visible", "Si";
          |SINO;
               aAlfa1 = "0000[" + (aAlfa1 % 5) + "] inaccesible";
               |MENAV aAlfa1;
          |FINSI;
     |FINSI;
|FPRC;
ÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ

|PROCESO tipo_3; |TIPO 3;
     aSalto = &12;
     |DDEF aBase;

     |HAZ CargaTmp;

     |ABRE #500;
     |SI #0#49 = "S";
          aAlfa = #0#52; |QBF aAlfa;
          Impresora = aAlfa;
          |IMPRE -1;
     |SINO;
          |IMPRE 0;
     |FINSI;
     |SI FSalida = 0;
          |ABRE #1;
          |BUCLE Temporal;
          |CIERRA #1;
          |FINIMP;
          |SI #0#49 = "S"; |HAZ Word; |FINSI;
     |FINSI;
     |CIERRA #500;
     |DELINDEX #500;
|FPRC;

|PROGRAMA;
     aMax = &175;
     aMin = &223;
     aRaya = &196;

     |CLS;
     |ABRE #0;
     |LEE 000#0p;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     |PDEFECTO #0, 1,50;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |GRABA 020#0a;
     |FINSI;
     |CIERRA #0;
|FPRO;
