|FICHEROS;
   dsm18405 #0;

   dsm18401 #10;
|FIN;

|VARIABLES;
   &enEmpresa = 0; &enEjer     = 0; &eaFichero = ""; &enCampo = 0;
   &eaTabla = "";  &enPosicion = 0; &eaDestino = ""; &enComp = 0;
   &eaAlfa  = "";  &enError    = 0; &enTotalCas = 0; &eaTitulo = "";

   nLCod  = 0;
   nLDes  = 0;
   nCampo = 0;
   nCalc  = 0;

   aAlfa  = "";
   aAlfa1 = "";
   aAlfa2 = "";

   nFlag  = 0;
|FIN;

|PROCESO SacaPais05; |TIPO 7;
   eaTabla = "ztabla05.txt";  nLCod = 2; nLDes = 30; |HAZ CargaTabla;
   |MENSA "    <F2> Consulta paises ";
|FPRC;

|PROCESO Reducc05;
   |SI #dsm18405#4 = "S";
      #dsm18405#5  = "";  |PINTA #dsm18405#5;
      #dsm18405#11 = "";  |PINTA #dsm18405#11;
      #dsm18405#12 = "N"; |PINTA #dsm18405#12;

      #dsm18405#6 = 0;  |PINTA #dsm18405#6;
      #dsm18405#7 = 0;  |PINTA #dsm18405#7;
      #dsm18405#8 = 0;  |PINTA #dsm18405#8;
      |C_M #dsm18405#5,1,"N"; |C_M #dsm18405#6,1,"N";
      |C_M #dsm18405#7,1,"N";
      |C_M #dsm18405#12,1,"N";
      |C_M #dsm18405#9,1,"S"; |C_M #dsm18405#10,1,"S";
   |SINO;
      #dsm18405#9 = 0;  |PINTA #dsm18405#9;
      #dsm18405#10 = 0; |PINTA #dsm18405#10;
      |C_M #dsm18405#5,1,"S"; |C_M #dsm18405#6,1,"S";
      |C_M #dsm18405#7,1,"S"; |C_M #dsm18405#12,1,"S";
      |C_M #dsm18405#9,1,"N"; |C_M #dsm18405#10,1,"N";
   |FINSI;
|FPRC;

|PROCESO CompPais05;
     |SI #dsm18405#4 = "S"; |ACABA; |FINSI;
     |SI #dsm18405#5 = "  ";
        #dsm18405#5 = "ES"; #dsm18405#11 = "ESPA¥A";
     |FINSI;
     |MENSA "     ";
     nCampo   = Campo + 1;

     |SI FSalida = 10;
         enPosicion = 20;
         |HAZ SacaTabla;
         nCalc = 100 + nLCod;
         aAlfa1   = eaDestino % nCalc;
         #0Campo  = aAlfa1; |PINTA #0Campo;
     |FINSI;

     enComp = nLCod;
     aAlfa = "|C" + (("000" + Campo) % -103) + "TIPO"; aAlfa1 = #0aAlfa;
     aAlfa = #0Campo; |QBF aAlfa;
     |SI aAlfa1 = "N"; nCalc = - (100 + nLCod); aAlfa = (("0" * enComp) + aAlfa) % nCalc; |FINSI;
     |SI aAlfa1 ! "N"; nCalc = 100 + nLCod;     aAlfa = (aAlfa + (" " * enComp)) % nCalc; |FINSI;
     eaAlfa = aAlfa;
     |HAZ CompruebaTabla;

     |SI enError = 1;
         |MENAV "0000 Contenido Incorrecto. Consulta la Tabla con F2";
         |ERROR;
         |PULSATECLA;
         |ACABA;
     |FINSI;

     nCalc = 100 + nLCod;                 aAlfa1   = eaDestino % nCalc;
     nCalc = ((nLCod + 2) * 100) + nLDes; aAlfa2   = eaDestino % nCalc;
     #0Campo  = aAlfa1;
     |C_P #0Campo,0,nCalc; nCalc = nCalc + nLCod + 1;
     |PINTA #0Campo;
     #dsm18405#11 = aAlfa2; |PINTA #dsm18405#11;

     |HAZ FinTabla;
|FPRC;

|PROCESO Calcula05;
   #dsm18405#8 = #dsm18405#6 - #dsm18405#7; |PINTA #dsm18405#8;
|FPRC;

|VARIABLES;
   aPais  = "";
   nLinea = 0;
   nSwError = 0;
|FIN;

|PROCESO Pais05;
   |SI #dsm18405#5 = "ES";
      |C_M #dsm18405#12,1,"N";
      #dsm18405#12 = "S"; |PINTA #dsm18405#12;
   |SINO;
      |SI #dsm18405#4 = "S";
         |C_M #dsm18405#12,1,"N";
      |SINO;
         |C_M #dsm18405#12,1,"S";
      |FINSI;
   |FINSI;

   aPais = #dsm18405#5; nLinea = #dsm18405#3;

   nSwError = 0;
   |SALVA_FICHA #dsm18405;
   #dsm18405#0 = #dsm18401#0;
   #dsm18405#1 = #dsm18401#1;
   #dsm18405#2 = #dsm18401#3;
   #dsm18405#3 = 1;
   |LEE 000#dsm18405.];
   |PARA; |SI FSalida = 0; |Y #dsm18405#0 = #dsm18401#0; |Y #dsm18405#1 = #dsm18401#1;
           |Y #dsm18405#2 = #dsm18401#3; |HACIENDO;
      aAlfa = #dsm18405#5;
      |SI aAlfa = aPais; |Y #dsm18405#3 ! nLinea; nSwError = 1; |SAL; |FINSI;
      |LEE 000#dsm18405.s;
   |SIGUE;
   |REPON_FICHA #dsm18405;

   |SI nSwError ! 0;
      |MENAV "    Ya existe una linea de rdtos del capital inmobiliario con ese Pais";
      |ERROR;
      |HAZ SacaPais05;
   |FINSI;
|FPRC;
