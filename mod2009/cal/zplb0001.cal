|PROCESO eLeeLaProrrata;  || IMPORTE DE PRORRATA

     |HAZ Pongo0;

     |SI enSwProrr = 2;
         eaProrrat = "N";
         nProrrata = 100;
         |ACABA;
     |FINSI;

     |SI enEjer < enEjerPro;
         |HAZ LeeAnterior;
     |SINO;
         |HAZ epLeeEl80;

         |SI eaProrrat ! "N";  || no procede prorrata
             |HAZ epLeeEl82;
             |SI nSwEx82 = 1; |O #82#61 = "S"; |O #82#2 = 12;
                 |SI enTipoEntrada ! 3; |Y enTipoEntrada ! 97; |Y enTipoEntrada ! 99;
                     enSinPantalla = 1;
                     |SUB_EJECUTA_NP "dsmozp81";
                     enSinPantalla = 0;
                     |HAZ epLeeEl82;
                 |FINSI;
             |FINSI;
             |SI nSwEx82 = 0;
                 nProrrata = #82#4;
                 |SI #82#30 = "N";
                     nProrrata = 100;
                 |FINSI;
             |FINSI;
         |FINSI;
     |FINSI;

     |SI nProrrata < 0;    nProrrata = 100;  |FINSI;
     |SI nProrrata > 100;  nProrrata = 100;  |FINSI;

|FPRC;

|PROCESO eLeeLaPro390;  || IMPORTE DE PRORRATA para 390

