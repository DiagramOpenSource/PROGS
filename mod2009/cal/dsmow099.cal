|FICHEROS;
     dsmow098 #956,MANTE;
     dsmow099 #957,MANTE;
     dsmow100 #958,MANTE;
     dsmow097 #999,MANTE;

     &regisnif@ #709;
|FIN;

|VARIABLES;
 &nCambio = 0;
|FIN;

|INCLUYE i_variar;

|PROCESO CalcDni;  |TIPO 6;
     |SI FSalida = 10;
         eaVarDni  = #957Campo;
         enCalcCif = 1;
         |HAZ CalculameDNI;
         #957Campo = eaVarDni;
         |PINTA #957Campo;
         |ERROR;
         |ACABA;
      |FINSI;

      eaVarDni  = #957Campo;
      enCalcCif = 0;
      |HAZ CalculameDNI;
      eaVarDni = (eaVarDni + "             ") % 112;
      |SI #957Campo ! eaVarDni;
          |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
          |SI FSalida = 0;
              #957Campo = eaVarDni;  |PINTA #957Campo;
              |ERROR;
          |FINSI;
      |FINSI;
|FPRC;

|PROCESO PonDatosNif;  |TIPO 0;
     eaCodNif  = #957Campo;
     |HAZ LeeNifs;

     #957Campo = #709#0; |PINTA #957Campo;
     #957#13   = #709#1;  |PINTA #957#13;
|FPRC;

|PROCESO Tipo02w56;  |TIPO 0;
 |SI Contenido ! #Fichero@#Campo#;
     nCambio = 1;
 |FINSI;
|FPRC;

|PROCESO Tipo12w56;  |TIPO 12;
|FPRC;
