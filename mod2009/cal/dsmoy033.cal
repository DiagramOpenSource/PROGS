  #52#7 = #52#5 * #51#24;
  #52#8 = #52#5 * nCapzac;

  #52#9 = #52#6; |SI #52#9 < #52#7; #52#9 = #52#7; |FINSI;
                 |SI #52#9 < #52#8; #52#9 = #52#8; |FINSI;


  #0#42  = #52#2;
