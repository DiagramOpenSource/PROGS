  #0#48 = #125#10;                       || ... Provincia
  #0#49 = #125#18;                       || ... Fecha Alta
  #0#50 = #125#19;                       || ... Fecha Baja

  #0#51 = #125#14;                       || ... Participacion
  #0#52 = #125#13;                       || ... % Participacion

  |PRINT;
  SwHayDatos = 1;
|FPRC;

