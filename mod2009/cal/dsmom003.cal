|FICHEROS;
     dsmom003 #3;
     dsmom004 #4;
|FIN;

|VARIABLES;
     nBoton2 = 0;
     aTecla  = "";
     aF2     = "";
     aEsc    = "";
     nTecla  = 0;
|FIN;

|INCLUYE i_variar;

|RUTINA ClaveBaja4;
     #4#0 = #3#0;
     #4#1 = #3#1;
     #4#2 = #3#2;
     #4#3 = 000;
     #4#4 = 00;
|FRUT;

|RUTINA ClaveAlta4;
     #4#0 = #3#0;
     #4#1 = #3#1;
     #4#2 = #3#2;
     #4#3 = 999;
     #4#4 = 99;
|FRUT;

|PROCESO Tipo0m003;  |TIPO 0;
     |FIN_CONTROL_WINDOWS nBoton2;
     |PULSATECLA;
|FPRC;

|PROCESO Tipo11m003;  |TIPO 11;
     nTecla = FSalida;

     |FIN_CONTROL_WINDOWS nBoton2;
     |PULSATECLA;

     |SI nTecla = 10;
         |SI enSwAviso = 1;
             enEmpresa = #3#0;
             enEjer    = #3#1;
             |HAZ inicial;
         |FINSI;
         |ENTLINEAL #1#4, -4, 4, 22, 0, ClaveBaja4, ClaveAlta4;
     |FINSI;
|FPRC;

|PROCESO Boton2;
     |PTEC 824;
|FPRC;

|PROCESO Tipo7m003;  |TIPO 7;
     |ENTLINEAL #1#4, -4, 7, 22, 0, ClaveBaja4, ClaveAlta4;

     |BLIN 04;
     |CONTROL_SIMPLE 0, "LBOTON,F2 Entrada al Lineal", 0460, 0480, Boton2;
     nBoton2 = FSalida;
|FPRC;

|RUTINA ClaveBaja3;
     #3#0 = enEmpresa;
     #3#1 = 9999;
     #3#2 = 99;
|FRUT;

|RUTINA ClaveAlta3;
     #3#0 = enEmpresa;
     #3#1 = 0000;
     #3#2 = 00;
|FRUT;

|PROGRAMA;
     |HAZ LeeCtrlLibro;
     |ENTLINEAL #1#3, -3, 4, 22, 1, ClaveBaja3, ClaveAlta3;
|FPRO;
