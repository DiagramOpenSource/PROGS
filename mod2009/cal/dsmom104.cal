|FICHEROS;
     dsmom104 #104;
|FIN;

|VARIABLES;
     aParametro = "";
     nDModelo   = 0;
     nHModelo   = 0;
|FIN;

|RUTINA ClaveBaja104;
     #104#0 = nDModelo;
     #104#1 = 00001;
     #104#2 = 0000;
     #104#3 = 01;
|FRUT;

|RUTINA ClaveAlta104;
     #104#0 = nHModelo;
     #104#1 = 99999;
     #104#2 = 2999;
     #104#3 = 12;
|FRUT;

|PROGRAMA;
     aParametro = PARAMETRO;  |QBT aParametro;
     nDModelo   = 0;
     nHModelo   = 999;
     |SI aParametro ! "";
         nDModelo = aParametro;
         nHModelo = aParametro;
     |FINSI;

     |ENTLINEAL #1#104, -1, 2, 22, 1, ClaveBaja104, ClaveAlta104;
|FPRO;
