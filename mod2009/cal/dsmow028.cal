|FICHEROS;
     dsmoz009 #0;
     dsmom024 #24;
     dsmow028 #999,MANTE;
|FIN;

|VARIABLES;
     nBoton1 = 0;
     nTecla  = 0;
|FIN;

|INCLUYE i_variar;

|RUTINA ClaveBaja4;
     #24#0 = #999#0;
     #24#1 = #999#1;
     #24#2 = 00;
     #24#3 = #999#2;
     #24#4 = 00;
|FRUT;

|RUTINA ClaveAlta4;
     #24#0 = #999#0;
     #24#1 = #999#1;
     #24#2 = 99;
     #24#3 = #999#2;
     #24#4 = 99;
|FRUT;

|PROCESO Boton1;
     |PTEC 824;
|FPRC;

|PROCESO Tipo0;  |TIPO 0;
     |FIN_CONTROL_WINDOWS nBoton1;
     |PULSATECLA;
|FPRC;

|PROCESO Tipo7;  |TIPO 7;
     |ENTLINEAL #1#24, -3, 7, 20, 0, ClaveBaja4, ClaveAlta4;

     |BLIN 04;
     |CONTROL_SIMPLE 0, "BOTON,F2 Entrada al Lineal", 0460, 0480, Boton1;
     nBoton1 = FSalida;
|FPRC;

|PROCESO Tipo11;  |TIPO 11;
     nTecla = FSalida;

     |FIN_CONTROL_WINDOWS nBoton1;
     |PULSATECLA;

     |SI nTecla = 10;
         |ENTLINEAL #1#24, -3, 4, 20, 0, ClaveBaja4, ClaveAlta4;
     |FINSI;
|FPRC;
