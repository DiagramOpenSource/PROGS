      nCanti = ((nParteImp * nNume3) / nDias) ! EURODB;
  |SINO;
      nCanti = nParteImp;  || todo
  |FINSI;


  |SI nTip = 1;         || desde el pago
      nImporte = nCanti;
  |FINSI;
  |SI nTip = 2;         || hasta el pago
      nImporte = nParteImp  - nCanti;
  |FINSI;
