      nDias = 366;
  |FINSI;


  nNume1 = fFecMenahor;
  nNume2 = #42#9;
  nDias = nNume2 - nNume1;

  |SI #41#37 ] fFecMenahor;
      nNume1 = #41#37;  nNume2 = #42#9;
      nNume3 = nNume2 - nNume1;
      |SI nNume3 < 0;
