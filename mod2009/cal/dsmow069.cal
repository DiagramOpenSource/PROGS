|RUTINA ClaveBaja309;
     #309#0 = #999#0;
     #309#1 = #999#1;
     #309#2 = #999#2;
     #309#3 = 01;
     #309#4 = 00;
     #309#5 = 01;
|FRUT;

|RUTINA ClaveAlta309;
     #309#0 = #999#0;
     #309#1 = #999#1;
     #309#2 = #999#2;
     #309#3 = 99;
     #309#4 = 99;
     #309#5 = 99;
|FRUT;

|PROCESO Tipo7;  |TIPO 7;
     |ENTLINEAL #1#309, -4, 7, 22, 0, ClaveBaja309, ClaveAlta309;
|FPRC;

|PROCESO Tipo11;  |TIPO 11;
     |SI FSalida = 10;
         |ESTADO_CONTROL enBoton1, "HIDE";
         |ESTADO_CONTROL enBoton2, "HIDE";
         |ENTLINEAL #1#309, -4, 2, 22, 0, ClaveBaja309, ClaveAlta309;
         |ESTADO_CONTROL enBoton1, "SHOW";
         |ESTADO_CONTROL enBoton2, "SHOW";
     |FINSI;

     |SI FSalida = 11;
         ||HAZ EmisionLinea;
     |FINSI;

     |SI FSalida = 12;
         ||HAZ EmisionTodo;
     |FINSI;
|FPRC;
