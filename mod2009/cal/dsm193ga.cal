|FICHEROS;
     dsm193ca #193;
     dsm193ga #195;

     &regisnif@ #709;
|FIN;

|VARIABLES;
     nCampo      = 0;
     nPtec       = 0;
     nCampoD     = 0;
     nCampoO     = 0;
     nForma      = 0;

     aMenav      = "";
     aPathModelo = "";
     aDef        = "";
     aAlfa       = "";
     aTecla      = "";

     &enError    = 0;
|FIN;

|INCLUYE i_variar;

|PROCESO PonDatosNifga;  |TIPO 7;
     eaCodNif  = #195#6;
     |HAZ LeeNifs;

     #195#8 = #709#1;   |PINTA #195#8;
|FPRC;

|PROCESO Tipo2m193ga;  |TIPO 2;
     #193#6  = #193#6  +. 1;
     #193#12 = #193#12 +. #195#9;
|FPRC;

|PROCESO Tipo11m193ga;  |TIPO 11;
     |SI FSalida = 10;
         eaVarDni  = #195Campo;
         enCalcCif = 1;
         |HAZ CalculameDNI;
         #195Campo = eaVarDni;
         |PINTA #195Campo;
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO MiraDni193ga;  |TIPO 6;
     eaVarDni  = #195Campo;
     enCalcCif = 0;
     |HAZ CalculameDNI;
     eaVarDni = (eaVarDni + "             ") % 112;
     |SI #195Campo ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #195Campo = eaVarDni;  |PINTA #195Campo;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;
