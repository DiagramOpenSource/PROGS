|FICHEROS;
     dsmom047 #0;
|FIN;

|VARIABLES;
     aTecla      = "";
     aAlfa       = "";
     aNomPeriodo = "";
|FIN;

|INCLUYE i_variar;

|PROCESO Sumar;
  #0#14 = #0#6 + #0#7 + #0#8 + #0#9 + #0#10 + #0#11 + #0#12 + #0#13;
  |PINTA #0#14;
|FPRC;

|PROCESO Pantalla310;

  |C_M #0#6,1,"N";   |C_V #0#6,1,"N";
  |C_M #0#7,1,"N";   |C_V #0#7,1,"N";
  |C_M #0#8,1,"N";   |C_V #0#8,1,"N";
  |C_M #0#9,1,"N";   |C_V #0#9,1,"N";
  |C_M #0#10,1,"N";  |C_V #0#10,1,"N";
  |C_M #0#11,1,"N";  |C_V #0#11,1,"N";
  |C_M #0#12,1,"N";  |C_V #0#12,1,"N";
  |C_M #0#13,1,"S";  |C_V #0#13,1,"S";

  aAlfa = " " * 46;
  |PINTA 1003, aAlfa;  |PINTA 1103, aAlfa;
  |PINTA 1203, aAlfa;  |PINTA 1303, aAlfa;
  |PINTA 1403, aAlfa;  |PINTA 1503, aAlfa;
  |PINTA 1603, aAlfa;  |PINTA 1703, aAlfa;
  |PINTA 1803, aAlfa;  |PINTA 1903, aAlfa;
  |PINTA 1103, "Adquisicion o importacion de activos fijos .";

|FPRC;

|PROCESO NomPeriodo;
     aNomPeriodo = "          ";
     |SI #0#2 = 1;   aNomPeriodo = "ENERO     ";  |FINSI;
     |SI #0#2 = 2;   aNomPeriodo = "FEBRERO   ";  |FINSI;
     |SI #0#2 = 3;   aNomPeriodo = "MARZO     ";  |FINSI;
     |SI #0#2 = 4;   aNomPeriodo = "ABRIL     ";  |FINSI;
     |SI #0#2 = 5;   aNomPeriodo = "MAYO      ";  |FINSI;
     |SI #0#2 = 6;   aNomPeriodo = "JUNIO     ";  |FINSI;
     |SI #0#2 = 7;   aNomPeriodo = "JULIO     ";  |FINSI;
     |SI #0#2 = 8;   aNomPeriodo = "AGOSTO    ";  |FINSI;
     |SI #0#2 = 9;   aNomPeriodo = "SEPTIEMBRE";  |FINSI;
     |SI #0#2 = 10;  aNomPeriodo = "OCTUBRE   ";  |FINSI;
     |SI #0#2 = 11;  aNomPeriodo = "NOVIEMBRE ";  |FINSI;
     |SI #0#2 = 12;  aNomPeriodo = "DICIEMBRE ";  |FINSI;
     |SI #0#2 = 99;  aNomPeriodo = "ANUAL     ";  |FINSI;
     |ATRI R; |PINTA 0769, aNomPeriodo; |ATRI 0;
|FPRC;


|PROGRAMA;
     eaNomDef = "dsmom047";

|| \\\\\\ Consulta del Modelo ////////

     |SI enTipoEntrada = 4;              || Consulta del Modelo
         |CABEZA "Regularizacion Manual";

         |ABRE #0;
         #0#0 = enEmpresa;
         #0#1 = enEjer;
         #0#2 = enPeriodo;
         #0#3 = enModelo;
         #0#4 = enComplem;
         #0#5 = enActividad;
         |LEE 101#0=;
         |SI FSalida ! 0;
             |DDEFECTO #0;
             #0#0 = enEmpresa;
             #0#1 = enEjer;
             #0#2 = enPeriodo;
             #0#3 = enModelo;
             #0#4 = enComplem;
             #0#5 = enActividad;
             |GRABA 020#0b;
         |FINSI;

         |PINPA #0#0;
         |HAZ NomPeriodo;
         |SI enModelo = 310;
             |HAZ Pantalla310;
         |FINSI;
         |PINDA #0#0;
         |ENDATOS #1#0;
         |SI FSalida = 0;
             |GRABA 020#0a;
         |FINSI;
         |LIBERA #0;
         |CIERRA #0;
     |FINSI;
|FPRO;
