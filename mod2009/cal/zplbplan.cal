|FICHEROS;
     dsmom014;
     dsmom060;
     dsmom061;
     dsmom050;
     dsmom062;
|FIN;

|VARIABLES;
     &eaPrograma  = "";
     &eaExtension = "";
     &eaPlanilla  = "";
     &enEjer      = 0;
     &enPlan      = 0;
     &enSignos    = 0;
|FIN;

|PROCESO dsmom014Plan;
     |ABRE #dsmom014;
     #dsmom014#10 = enPlan;
     #dsmom014#0  = "";
     |LEE 000#dsmom014.];
     |SI FSalida ! 0;  |O #dsmom014#10 ! enPlan;
         |CIERRA #dsmom014;

         eaPrograma  = "dsmom014";
         eaExtension = "mod";
         |SUB_EJECUTA_NP ":basica/dspatron";

         |ABRE #dsmom014;
     |FINSI;

     #dsmom014#10 = enPlan;
     #dsmom014#0  = "6060";   ||cambio de Planilla Asefeco 135_1731 (carloscl)
     |LEE 000#dsmom014.=;
     |SI FSalida ! 0; |O #dsmom014#2 = "I";
         |CIERRA #dsmom014;

         enSignos    = 1;
         eaPrograma  = "dsmom014";
         eaExtension = "mod";
         |SUB_EJECUTA_NP ":basica/dspatron";

         |ABRE #dsmom014;
         enSignos    = 0;
     |FINSI;

     #dsmom014#10 = enPlan;
     #dsmom014#0  = "";
     |LEE 000#dsmom014.];
     |SI FSalida ! 0;  |O #dsmom014#10 ! enPlan;
         |MENAV "0000No existe la planilla de las cuentas contable de Ingresos, Gastos y Consumos para el Plan del 2008";
         |MENAV "0000Consulte con su administrador";
     |FINSI;
     |CIERRA #dsmom014;
|FPRC;

|PROCESO dsmom060Plan;
     |ABRE #dsmom060;
     #dsmom060#6  = enPlan;
     #dsmom060#0  = "";
     #dsmom060#1  = "";
     #dsmom060#2  = 0;
     |LEE 000#dsmom060.];
     |SI FSalida ! 0;  |O #dsmom060#6 ! enPlan;
         |CIERRA #dsmom060;

         eaPrograma  = "dsmom060";
         eaExtension = "mod";
         |SUB_EJECUTA_NP ":basica/dspatron";

         eaPrograma  = "dsmom061";
         eaExtension = "mod";
         |SUB_EJECUTA_NP ":basica/dspatron";

         |ABRE #dsmom060;
     |FINSI;

     |ABRE #dsmom061;
     #dsmom061#6  = enPlan;
     #dsmom061#0  = "N";
     #dsmom061#1  = "G";
     #dsmom061#2  = 1;
     #dsmom061#3  = 7;
     |LEE 000#dsmom061.=;
     |SI FSalida ! 0;
         |CIERRA #dsmom061;
         |CIERRA #dsmom060;

         eaPrograma  = "dsmom060";
         eaExtension = "mod";
         |SUB_EJECUTA_NP ":basica/dspatron";

         eaPrograma  = "dsmom061";
         eaExtension = "mod";
         |SUB_EJECUTA_NP ":basica/dspatron";

         |ABRE #dsmom060;
         |ABRE #dsmom061;
     |FINSI;
     |CIERRA #dsmom061;

     #dsmom060#6  = enPlan;
     #dsmom060#0  = "";
     #dsmom060#1  = "";
     #dsmom060#2  = 0;
     |LEE 000#dsmom060.];
     |SI FSalida ! 0;  |O #dsmom060#6 ! enPlan;
         |MENAV "0000No existe la planilla de las cuentas para Rendimientos de Actividades Economicas para el Plan del 2008";
         |MENAV "0000Consulte con su administrador";
     |FINSI;
     |CIERRA #dsmom060;
|FPRC;

|PROCESO dsmom050Plan;
     |ABRE #dsmom050;
     #dsmom050#3  = enPlan;
     #dsmom050#0  = "";
     |LEE 000#dsmom050.];
     |SI FSalida ! 0;  |O #dsmom050#3 ! enPlan;
         |CIERRA #dsmom050;

