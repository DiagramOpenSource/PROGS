             eaFDestino = eaPathLanza + "zpr05180.exe";
             |HAZ CopiaFichero;
         |FINSI;
     |FINSI;

     aAlfa = eaPathLanza + "IMPRIME";

     |SI nTecla = 1;
         |RSYSTEM aAlfa;
         eaFichError  = "rtc";
         eaFichDatos  = "rta";
         eaFichAuxil  = "rtb";
