     #180#3  = #999#5;
     #180#4  = #999#7;
     #180#5  = 0;
     |LEE 040#180=;
     |SI FSalida = 0;
         |SI nTecla = 1;
             |SI #0#6 ] 2006;
                 |SI #180#6 > 15;
                     aAlfa = "0000 Empresa " + (("00000" + #180#0) % -105) + " mas de 15 Perceptores, emitir por Internet";
                     |MENAV aAlfa;
                     |ACABA;
                 |FINSI;
             |FINSI;

             aAbre = eaPathEmision + "RTA" + (("00000" + #999#0) % -105) + ".TXT";
             |XABRE "BW", aAbre, nHandle2;

             aAbre = eaPathEmision + "RTB" + (("00000" + #999#0) % -105) + ".TXT";
             |XABRE "BW", aAbre, nHandle3;
         |FINSI;

         |HAZ Registro180_1;

         |BUCLE dsm180li;

         |HAZ Registro180_Auxiliar1;

         |SI nTecla = 1;
             |XCIERRA nHandle2;
             |XCIERRA nHandle3;

             eaFOrigen  = eaPathEmision + "RTA" + (("00000" + #999#0) % -105) + ".TXT";
             eaFDestino = eaPathLanza + "RTA" + (("00000" + #999#0) % -105) + ".TXT";
             |HAZ CopiaFichero;

             |FBORRA eaFOrigen;

             eaFOrigen  = eaPathEmision + "RTB" + (("00000" + #999#0) % -105) + ".TXT";
             eaFDestino = eaPathLanza + "RTB" + (("00000" + #999#0) % -105) + ".TXT";
             |HAZ CopiaFichero;

             |FBORRA eaFOrigen;

             |SI #0#6 [ 2005;
