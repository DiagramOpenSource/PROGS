|FICHEROS;
     dsmom060 #0;
     dsmom061 #1;
     dsmoz052 #10;
|FIN;

|VARIABLES;
  aParam = "";
  inkey = 0;
  aAlfa1 = "";
|FIN;

|INCLUYE i_variar;
|INCLUYE i_varcon;

|| ******************************************** Calculos Pantalla
|CALCULO wTipo12; |TIPO 12;
|FCAL;

|CALCULO descri;  |TIPO 0;
  inkey = FSalida;
  |SI inkey = 1;
      inkey =  -1;
  |FINSI;
  |SI #10#1 = "I"; #10#3 = "INGRESOS";  |FINSI;
  |SI #10#1 = "G"; #10#3 = "GASTOS "; |FINSI;
  |PINTA #10#3;
|FCAL;

|| /////////////////////// PROCESOS DE TRABAJO \\\\\\\\\\\\\\\\\\\\
|RUTINA min;
  #0#6 = enPlan;
  #0#0 = #10#0;
  #0#1 = #10#1;
  #0#2 = 01;
|FRUT;

|RUTINA max;
  #0#6 = enPlan;
  #0#0 = #10#0;
  #0#1 = #10#1;
  #0#2 = 99;
|FRUT;

|PROCESO Tipo3;  |TIPO 3;
  |ENTLINEAL #1#0, -4, 2, 22, 0, min, max;
|FPRC;

|PROGRAMA;
   aParam = PARAMETRO; |QBF aParam;

   |SI aParam = "";
       aParam = "N";
   |FINSI;

   |DDEFECTO #10;

   #10#0 = aParam;
   |SI #10#0 = "N"; #10#2 = "Rendimientos de Actividades Economicas en Regimen de E.D.Normal y Sim."; |FINSI;
   |SI #10#0 = "S"; #10#2 = "Rendimientos procedentes de las Ventas del Inmovilizado Empresarial";  |FINSI;

   |CLS;
   |CABEZA "Imputacion Cuentas";
   |PINPA #0#10;
   |SI #10#0 = "S";
       aAlfa1 = "Ä"; aAlfa1 = aAlfa1 * 22;
       |PINTA 0739, aAlfa1;
       |PINTA 0839, "                       ";
       |C_M #0#4, 1, "N"; |C_V #0#4, 1, "N";
       |C_M #0#5, 1, "N"; |C_V #0#5, 1, "N";
       |CAMPO_ANCHO #0#3, 1, 55;
   |FINSI;

   |PINDA #0#10;

   FSalida = 1;
   |PARA; |SI 0 < FSalida; |HACIENDO;
          |ENDATOS #1#10;
          |SI 0 [ FSalida;
              |SI inkey = -1; |SAL; |FINSI;
              |HAZ Tipo3;
              FSalida = 1;
          |FINSI;
   |SIGUE;
|FPRO;
