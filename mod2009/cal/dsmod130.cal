|FICHEROS;
     dsmod130 #0;
|FIN;

|VARIABLES;
     aEscape   = "";
     aAlfa     = "";
|FIN;

|INCLUYE i_variar;

|PROCESO SacaPantallas2007;
     |PINPA #0#0;

     |C_V #0#19, 1, "N";
     |C_V #0#20, 1, "N";
     |C_V #0#21, 1, "N";
     |C_V #0#22, 1, "N";

     |BLANCO 1902 2264;

     |ATRI 0;
     |PINTA 1903, "Suma de pagos fraccionados ...................................";
     |PINTA 2003, "A deducir (dec.complem.) Resultados ant. mismo ejer. y per. ..";
     |PINTA 2203, "Total Liquidacion Cuota a Ingresar ...........................";

     |ATRI S;
     |PINTA 1966, "12";
     |PINTA 2066, "13";
     |PINTA 2266, "14";
     |ATRI 0;

     |C_P #0#17, 1, 1969;

     #0#23 = #0#17 - #0#18;
     |PINDA #0#0;

     aEscape    = &255 + "806";
     |PARA;  |SI;  |HACIENDO;
          |LEETECLA aAlfa;
          |SI aAlfa = aEscape;  |SAL;  |FINSI;
     |SIGUE;
|FPRC;

|PROCESO SacaPantallas2008;
     |PINPA #0#0;
     |PINDA #0#0;

     aEscape    = &255 + "806";
     |PARA;  |SI;  |HACIENDO;
          |LEETECLA aAlfa;
          |SI aAlfa = aEscape;  |SAL;  |FINSI;
     |SIGUE;
|FPRC;

|PROGRAMA;
     eaNomDef = "dsmod130";

|| \\\\\\ Consulta del Modelo ////////

     |SI enTipoEntrada = 4;              || Consulta del Modelo
         |CABEZA "Modelo 130 Impresion";

         |ABRE #0;
         #0#0 = enEmpresa;
         #0#1 = enEjer;
         #0#2 = enPeriodo;
         #0#3 = enModelo;
         #0#4 = enComplem;
         #0#5 = enActividad;
         |LEE 000#0=;
         |SI FSalida ! 0;
             |MENAV "     No existe Registro.";
             |CIERRA #0;
             |ACABA;
         |FINSI;
         |CIERRA #0;

         |SI enEjer [ 2007;
             |HAZ SacaPantallas2007;
         |FINSI;

         |SI enEjer ] 2008;
             |SI enPeriodo [ 3;
                 |HAZ SacaPantallas2007;
             |SINO;
                 |HAZ SacaPantallas2008;
             |FINSI;
         |FINSI;
         |ACABA;
     |FINSI;

|| \\\\\\ Comprobacion Existencia del Modelo

     |SI enTipoEntrada = 99;              || Existencia del Modelo ????
         |ABRE #0;
         #0#0 = enEmpresa;
         #0#1 = enEjer;
         #0#2 = enPeriodo;
         #0#3 = enModelo;
         #0#4 = enComplem;
         #0#5 = enActividad;
         |LEE 040#0=;
         |SI FSalida ! 0;  enExistencia = 1;  |DDEFECTO #0;  |FINSI;
         |SI #0#1 [ 2007;
             |SI #0#4 ! 0;
                 enResultado = #0#17 - #0#18;
                 |SI #0#17 < #0#18; enResultado = 0; |FINSI;
             |SINO;
                 enResultado = #0#17;
             |FINSI;
         |SINO;
             |SI #0#2 ] 4;
                 enResultado = #0#23;
             |SINO;
                 |SI #0#4 ! 0;
                     enResultado = #0#17 - #0#18;
                     |SI #0#17 < #0#18; enResultado = 0; |FINSI;
                 |SINO;
                     enResultado = #0#17;
                 |FINSI;
             |FINSI;
         |FINSI;
         |CIERRA #0;
     |FINSI;
|FPRO;
