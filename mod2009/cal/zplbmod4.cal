     aAlfa = "";
|FIN;

|INCLUYE i_variar;

/******************** PROCESOS CONSULTA DE EMPRESAS LABORAL *****************/

|RUTINA ClaveBaja1_1001;
     |SI FSalida = "POSICION";
         #1001#0 = #0nCampoCliente;
         |ACABA;
     |FINSI;

     #1001#0 = 1;
|FRUT;

|RUTINA ClaveAlta1_1001;
     #1001#0 = 99999;
|FRUT;

|RUTINA ClaveBaja2_1001;
     #1001#2 = "!                                       ";
|FRUT;

|RUTINA ClaveAlta2_1001;
     #1001#2 = "본본본본본본본본본본본본본본본본본본본본";
|FRUT;

|PROCESO ConsuCliente_Lab; |TIPO 0;
     |SI eaRutaFichLab ! "";
          |PATH_DAT #1001 eaRutaFichLab;
     |FINSI;

     |HAZ CargoRef_1;

     |ATRI R; |BLIN 04; |ATRI 0;

     |SI FSalida ! 10; |Y FSalida ! 11; |ACABA; |FINSI;

     |PUSHV 0501 2380;
     nCampoCliente  = Campo;

     |SI FSalida = 10;
         |C_M #1001#0, 1, "S";
         |C_M #1001#2, 1, "N";
         |C_M #1001#10, 1, "N";
         |ENTLINEAL #1#1001, -1, 4, 22, 1, ClaveBaja1_1001, ClaveAlta1_1001;
     |FINSI;

     |SI FSalida = 11;
         |C_M #1001#0, 1, "N";
         |C_M #1001#2, 1, "S";
         |C_M #1001#10, 1, "N";
         |ENTLINEAL #2#1001, -1, 4, 22, 1, ClaveBaja2_1001, ClaveAlta2_1001;
     |FINSI;

     |POPV;
     |PINTA #0nCampoCliente;
|FPRC;

|PROCESO RecogeCod_Lab;
     |SI FSalida ! 10;  |ACABA;  |FINSI;
     #0nCampoCliente = #1001#0;
     |PTEC 807;
|FPRC;

/**************** FIN PROCESOS CONSULTA DE EMPRESAS LABORAL *****************/

|RUTINA ClaveBaja1_1000;
     |SI FSalida = "POSICION";
         #1000#0 = #0nCampoCliente;
         |ACABA;
     |FINSI;

     #1000#0 = 1;
|FRUT;

|RUTINA ClaveAlta1_1000;
     #1000#0 = 99999;
|FRUT;

|RUTINA ClaveBaja2_1000;
     #1000#1 = "!                                       ";
|FRUT;

|RUTINA ClaveAlta2_1000;
     #1000#1 = "본본본본본본본본본본본본본본본본본본본본";
|FRUT;

|PROCESO MensaCliente; |TIPO 7;
     |HAZ CargoRef_1;

     nNume1 = Campo;
     aAlfa1 = ("000" + nNume1) % -103;
     aQueQuiero = "|C" + aAlfa1 + "IDCAMPO";
     aIDCampo = #program@#aQueQuiero#;
     |QBF aIDCampo;

     |SI aIDCampo = "parrilla";
          aAlfa1 = "     <00000> Seleccion Parrilla. Consulta Cliente: <F2> por Codigo; <F3> por Nombre";
     |SINO;
         aAlfa1 = "      <F2> Consulta Clientes por Codigo; <F3> Consulta Clientes por Nombre";
     |FINSI;
     |MENSA aAlfa1;
|FPRC;

|PROCESO ConsuCliente; |TIPO 0;
     |SI enLabornet = 1;
          |HAZ ConsuCliente_Lab;
     |SINO;
          |HAZ CargoRef_1;

          |ATRI R; |BLIN 04; |ATRI 0;

          |SI FSalida ! 10; |Y FSalida ! 11; |ACABA; |FINSI;

          |PUSHV 0501 2380;
          nCampoCliente  = Campo;

          |SI FSalida = 10;
              |C_M #1000#0, 1, "S";  |C_M #1000#1, 1, "N";
              |ENTLINEAL #1#1000, -1, 4, 22, 1, ClaveBaja1_1000, ClaveAlta1_1000;
          |FINSI;

          |SI FSalida = 11;
              |C_M #1000#0, 1, "N";  |C_M #1000#1, 1, "S";
              |ENTLINEAL #2#1000, -1, 4, 22, 1, ClaveBaja2_1000, ClaveAlta2_1000;
          |FINSI;

          |POPV;
          |PINTA #0nCampoCliente;
     |FINSI;
|FPRC;

|PROCESO RecogeCod; |TIPO 11;
     |SI enLabornet = 1;
          |HAZ RecogeCod_Lab;
     |SINO;
          |SI FSalida ! 10;  |ACABA;  |FINSI;
          #0nCampoCliente = #1000#0;
          |PTEC 807;
     |FINSI;
|FPRC;

|PROCESO CargoRef_1;
     |SI eaElProm = "";
         aPrograma = Control;  |QBF aPrograma;
     |SINO;
         aPrograma = eaElProm;  |QBF aPrograma;
     |FINSI;

