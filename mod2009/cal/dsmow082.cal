|PROCESO Leew82_Aux83;

  |SI #983#7 = 0; |Y #983#15 = 0;
      nNume2 = 100;
  |SINO;
      nNume1 = (( #983#6 / (#983#8 + #983#15 + #983#32) ) * 100) ! 2;
      nNume2 = nNume1 ! 0;
      |SI nNume2 < nNume1;
          nNume2 = nNume1 ! 0 + 1;
      |FINSI;
  |FINSI;
  |SI nNume2 > 100; nNume2 = 100; |FINSI;

  #983#5  = nNume2;

  |SI aTipoPr ! "E";
      #983#9 = #982#4;
  |SINO;
      #983#9 = #983#5;
  |FINSI;
  |GRABA 020#983a;
|FPRC;

|DEFBUCLE Leew82_Aux83;
 #983#1;
 ;
 #982#0,#982#1, 00;
 #982#0,#982#1, 99;
 ;
 NULL, Leew82_Aux83;
|FIN;

|PROCESO CalNueProrr;

    |SI #982#6 = 0; |Y #982#9 = 0;
         nNume2 = 100;
    |SINO;
         nNume1 = (( #982#5 / (#982#7 + #982#9 + #982#22) ) * 100) ! 2;
         nNume2 = nNume1 ! 0;
         |SI nNume2 < nNume1;
             nNume2 = nNume1 ! 0 + 1;
         |FINSI;
    |FINSI;
    |SI nNume2 > 100; nNume2 = 100; |FINSI;
    |SI nSwHay84 = 1; nNume2 = 100; |FINSI;

    #982#18 = nNume2;
    |SI aTipoPr = "G";
        #982#4 = #982#18;
    |SINO;
        #982#4 = #982#18;
    |FINSI;
    |BUCLE Leew82_Aux83;
|FPRC;

