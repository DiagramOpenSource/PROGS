|FICHEROS;
     dsm180ca #1;
     dsm180li #2,MANTE;

     &regisnif@ #709;
|FIN;

|VARIABLES;
     aMenav      = "";
     aPathModelo = "";
     aDef        = "";
     aAlfa       = "";
|FIN;

|INCLUYE i_variar;

|PROCESO PonDatosNif;  |TIPO 7;
     eaCodNif  = #2#6;
     |HAZ LeeNifs;

     #2#10 = #709#1;   |PINTA #2#10;
     #2#14 = #709#4;   |PINTA #2#14;
|FPRC;

|PROCESO Importe;
     #2#13 = #2#11 % #2#12;
     |PINTA #2#13;
|FPRC;

|PROCESO Tipo2m180li;  |TIPO 2;
     #1#6 = #1#6 +. 1;
     #1#7 = #1#7 +. #2#11;
     #1#8 = #1#8 +. #2#13;
|FPRC;

|PROCESO Tipo11m180li;  |TIPO 11;
     |SI FSalida = 10;
         eaVarDni  = #2Campo;
         enCalcCif = 1;
         |HAZ CalculameDNI;
         #2Campo = eaVarDni;
         |PINTA #2Campo;
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO MiraDni180li;  |TIPO 6;
     eaVarDni  = #2Campo;
     enCalcCif = 0;
     |HAZ CalculameDNI;
     eaVarDni = (eaVarDni + "             ") % 112;
     |SI #2Campo ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #2Campo = eaVarDni;  |PINTA #2Campo;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;
