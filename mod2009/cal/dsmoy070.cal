|FICHEROS;
     dsmoy070 #0;         || Este def.
     dsmom070 #70;        || Cabeceras Remesas
     dsmom071 #71;        || Lineas Remesas

     :/integral/def/dsam0001 #1;
|FIN;

|VARIABLES;
     nExiste = 0;
     aMensaje = "";
|FIN;

|PROCESO Imprime;
     |PRINT;
|FPRC;

|DEFBUCLE dsmom071;
     #71#2;
     ;
     #70#0, 00000, "   ";
     #70#0, 99999, "zzz";
     ;
     NULL, Imprime;
|FIN;

|PROCESO Lineas;
     |BUCLE dsmom071;
|FPRC;

|PROCESO PieInf;
     |PIEINF;
|FPRC;

|DEFBUCLE dsmom070;
     #70#1;
     1, 2, 3;
     #0#0, #0#2, #0#4, #0#6;
     #0#1, #0#3, #0#5, #0#7;
     ;
     NULL, Lineas, NULL, PieInf;
|FIN;

|PROCESO Tipo3; |TIPO 3;
     |ABRE #1;
     |LEE 040#1p;
     |SI FSalida ! 0;  |DDEFECTO #1;  |FINSI;
     |CIERRA #1;

     |IMPRE 0;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |INFOR "dsmoy070";
     |SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;

     |BUCLE dsmom070;

     |FININF;
     |FINIMP;
|FPRC;

|PROGRAMA;
     |CLS;
     |ABRE #0;
     |DDEFECTO #0;
     |LEE 101#0p;
     nExiste = FSalida;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |SI nExiste = 0;
               |GRABA 020#0a;
          |SINO;
               |GRABA 020#0n;
          |FINSI;
          |HAZ Tipo3;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;
