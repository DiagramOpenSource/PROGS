|PROCESO LeeElAux83;

  enActividad = #983#2;

  |SI nSwOpe83 = 0;   || = Calcular las Cuotas por Actividad

      |SI enActividad = 00; |O enActividad = 99; |ACABA; |FINSI;

      nVentaSub = 0;
      nVentaCon = 0;
      nVentaSin = 0;
      nVentaSuI = 0;
      nIvaSub   = 0;
      nVentaNue = 0;

      aAlfa1 = #983#30; |QBF aAlfa1;
      |SI aAlfa1 = "REGISTROS";
          nIvaComun = 1;  || Tiene Contabilidad
      |FINSI;

      nAny = #0#0 - 1;
      |SI enEjer < enEjerPro;
          |BUCLE LeeElModelo;
      |SINO;
          |ABRE #81;
          #81#0 = enEmpresa;
          #81#1 = nAny;
          #81#2 = enActividad;
          |LEE 001#81=;
          |SI FSalida = 0;
              nVentaCon = #81#7;
              nVentaSin = #81#8;
              nVentaSub = #81#10;
              nIvaSub   = #81#12;
              nVentaSuI = #81#15;
              nVentaNue = #81#41;
          |FINSI;
          |CIERRA #81;
      |FINSI;

      #983#6  = #983#6  + nVentaCon;
      #983#7  = #983#7  + nVentaSin;
      #983#8  = #983#6  + #983#7;
      #983#15 = #983#15 + nVentaSub;
      #983#28 = #983#28 + nIvaSub;
      #983#29 = #983#29 + nVentaSuI;
      #983#32 = #983#32 + nVentaNue;
      |GRABA 020#983a;

      #982#5  = #982#5  + nVentaCon;
      #982#6  = #982#6  + nVentaSin;
      #982#7  = #982#5  + #982#6;
      #982#9  = #982#9  + nVentaSub;
      #982#13 = #982#13 + nIvaSub;
      #982#16 = #982#16 + nVentaSuI;
      #982#22 = #982#22 + nVentaNue;

      nNumAct = nNumAct + 1;
  |FINSI;

  |SI nSwOpe83 = 1;   || = determina actividad Principal y otros detalles

      |SI enActividad = 00; |O enActividad = 99; |ACABA; |FINSI;
      |SI nSw = 0;
          nImporteMas = #983#8;
          nActivMas   = #983#2;
          nRegIVA     = #983#14;
          nSw = 1;
      |FINSI;
      |SI nImporteMas < #983#8;  || la actividad principal
          nImporteMas = #983#8;
          nActivMas   = #983#2;
      |FINSI;
      |SI nRegIVA ! #983#14;
          nSwVarios = 1;
          nRegIVA   = #983#14;
      |FINSI;

      |SI #983#14 = 3; |O #983#14 = 9; nSwRegEsp = 1; |FINSI;

      |SI #983#7 ! 0; |O #983#15 ! 0; nSwExentas = 1; |FINSI;

      |SI #983#28 ! 0; nSwSubv = 1; |FINSI;

      nNumAct = nNumAct + 1;        || todas las actividades

 |FINSI;

 |SI nSwOpe83 = 2;   || Busca todas las complementarias de la Principal

     |SI enActividad = 00; |O enActividad = 99; |ACABA; |FINSI;
     |SI #983#2 = nDepar; |ACABA; |FINSI;

     |SI #983#13 = aCNAE;
         #983#10 = aTipo;
         #983#11 = aLit;
         #983#12 = nDepar;
     |SINO;
         #983#10 = "O";
         #983#11 = "OTRA";
     |FINSI;
     |GRABA 020#983a;

 |FINSI;

 |SI nSwOpe83 = 3;   || mira acumulados
     |SI #983#9 ! 100;
         nSwBorro = 0;
     |FINSI;
 |FINSI;
|FPRC;

