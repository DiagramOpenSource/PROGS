        nSwExentas  = 0;  || tiene operaciones exentas
        nSwVarios   = 0;  || varios Regimenes de IVA
        nNumAct     = 0;  || Numero de Actividades
        nSwOpe83    = 1;
        nImporteMas = 0;
        nActivMas   = 0;  || actividad principal
        |BUCLE LeeElAux83;

        nDepar     = nActivMas;
        aTipo      = "P";
        aLit       = "PRINCIPAL";
        |HAZ ActividadPrincipal;

