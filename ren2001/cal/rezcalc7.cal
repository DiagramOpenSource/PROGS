            #1001#1 = enComplementaria;

            |SI nConta = 1;   #1001#2 = "ZPATRITIT ";  |FINSI;
            |SI nConta = 2;   #1001#2 = "ZPATRICON ";  |FINSI;

            |LEE 101#1001=;
