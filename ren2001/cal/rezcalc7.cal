             enImporte = nIntegra % 80;
             enBloque  = 23; eaCasilla = " 34";  |HAZ GrabaLineal;

             nValor    = (nValor1 + nParte) - nValor2;
             |SI nValor > enImporte;  nValor = enImporte;  |FINSI;

             enImporte = nIntegra - nValor;
             enBloque  = 24; eaCasilla = " 35";  |HAZ GrabaLineal;
