                |FINSI;

                |SI nConta = 2;  |Y eaMuerto = "C";
                    |LIBERA #1001;

                    eaLiquida = #1001#2;
                    |HAZ BorraLiquidacion;
                    eaLiquida = "";
                |FINSI;

                |SI eaMuerto = "";
                    |HAZ CalculaPatrim;
                    |GRABA 020#1001a;
                |SINO;
                    |SI nConta = 1;  |Y eaMuerto = "C";
                        |HAZ CalculaPatrim;
                        |GRABA 020#1001a;
                    |FINSI;

                    |SI nConta = 2;  |Y eaMuerto = "T";
                        |HAZ CalculaPatrim;
                        |GRABA 020#1001a;
                    |FINSI;
                |FINSI;
            |FINSI;
