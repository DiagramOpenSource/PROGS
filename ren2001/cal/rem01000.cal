|PROCESO Tipo0;
     |SI eaPath = "";  |ACABA;  |FINSI;

     enCapitulo = #1000#0;
     eaCasilla  = #1000#1;

     |PTEC 806;
|FPRC;

|PROGRAMA;
     |ABRE #1000;
     |LEE 040#1000p;
     |SI FSalida ! 0;
         |HAZ Recoge;
     |FINSI;
     |CIERRA #1000;

     |SI eaPath = "";
         |ENTLINEAL #1#1000, -1, 2, 22, 1, ClaveBaja1000, ClaveAlta1000;
     |SINO;
         |PATH_DAT #1000 eaPath;
         |ENTLINEAL #1#1000, -1, 4, 22, 1, ClaveBaja1000, ClaveAlta1000;
     |FINSI;
|FPRO;
