|FICHEROS;
     dsmopl10,MANTE;

     dsmom010;
    :/contagen/def/cacuenta;
|FIN;

|VARIABLES;
     aConcepto = "";
     nDosIntro = 0;
     nCampo    = 0;
     nCampo1   = 0;
     nTecla    = 0;
     &enCalculado = 0;
     nConte    = 0;
     nModo     = 0;
     aAlfa     = "";
     aAlfa1    = "";
     aAlfa2    = "";
     aAlfa3    = "";
     nNume1    = 0;
     nNume2    = 0;
     swError   = 0;
     nAlta     = 0;
     &eaDCta   = "";
     &eaHCta   = "";
     nMaestra  = "";
|FIN;

|INCLUYE i_variar;
|INCLUYE i_varcon;

|RUTINA inf2_10;
   #cacuenta#0 = eaDCta;
   #cacuenta#3 = "S";
|FRUT;

|RUTINA sup2_10;
   #cacuenta#0 = eaHCta;
   #cacuenta#3 = "S";
|FRUT;

|CALCULO FiltraCta10; |TIPO 6;
     eaCuenta = #dsmopl10#5;
     salidas  = FSalida;
     |HAZ FiltraPuntos;
     #dsmopl10#5 = eaCuenta;
     |PINTA #dsmopl10#5;
     #dsmopl10#7 = #dsmom010#7;

     aAlfa1  = #dsmopl10#5; |QBF aAlfa1;
     |SI aAlfa1 = "";
         |MENAV "    Error. Introcuzca Cuenta";
         |ERROR;
         |ACABA;
     |FINSI;

     swError = 0;
     aAlfa2 = aAlfa1 % 0;
     nNume1 = aAlfa2;
     |SI nNume1 < 4;
         |MENAV "    Error. Introcuzca Cuenta con longitud mayor o igual a 4";
         |ERROR;
         |ACABA;
     |FINSI;
     |PARA nNume2 = 1; |SI nNume2 [ nNume1; |HACIENDO nNume2 = nNume2 + 1;
           aAlfa2 = nNume2; |QBF aAlfa2;
           aAlfa2 = aAlfa2 + "01";
           aAlfa3 = aAlfa1 % aAlfa2;
           |SI aAlfa3 < "0"; |O aAlfa3 > "9";
               swError = 1;
               |SAL;
           |FINSI;
      |SIGUE;
      |SI swError = 1;
          |MENAV "    Cuenta Erronea ... ";
          |ERROR;
          |ACABA;
      |FINSI;

      aAlfa1 = "S";
      |ABRE #cacuenta;
      #cacuenta#0 = #dsmopl10#5;
      |LEE 041#cacuenta.=;
      |SI FSalida = 0;
          aAlfa1 = "N";
          #dsmopl10#6 = #cacuenta#2;
          |PINTA #dsmopl10#6;
      |FINSI;
      |CIERRA #cacuenta;
      |C_M #dsmopl10#6,1,aAlfa1;
|FCAL;

|CALCULO Tipo11pl10; |TIPO 11;
   |SI FSalida = 10;
      |ABRE #cacuenta;
      |SI nMaestra = 1;
          |CONSULTA_F_CLAVE #1#cacuenta,inf2_10,sup2_10;
      |SINO;
          |CONSULTA_F_CLAVE #6#cacuenta,inf2_10,sup2_10;
      |FINSI;
      #dsmopl10#5 = #cacuenta#0; |PINTA #dsmopl10#5;
      #dsmopl10#6 = #cacuenta#2; |PINTA #dsmopl10#6;
      |ERROR;
      |CIERRA #cacuenta;
   |FINSI;

   |SI FSalida = 20;
     eaCuenta = #dsmopl10#5;
     salidas  = FSalida;
     |HAZ FiltraPuntos;
   |FINSI;
|FCAL;

|PROCESO Teclapl10; |TIPO 0;
     nTecla = FSalida;
     |SI nTecla = 10;
         enCalculado = #dsmopl10#8;
         |HAZ Calculadora;
         #dsmopl10#8 = enCalculado;
         |PINTA #dsmopl10#8;
     |FINSI;
|FPRC;

|PROCESO Tipo2pl10;  |TIPO 2;
     |SI #dsmom010#7 = "+";
         #dsmom010#8 = #dsmom010#8 +. #dsmopl10#8;
     |SINO;
         #dsmom010#8 = #dsmom010#8 -. #dsmopl10#8;
     |FINSI;

     |ATRI S;  |PINTA 2069, #dsmom010#8;  |ATRI s;
|FPRC;

|PROCESO Tipo5pl10; |TIPO 5;
  nMaestra   = 0;
  enConcreto = 1;
  enPerConta = -1;
  enSwSinMen = 1;
  |HAZ SituaContagen;
  |SI swerror = 1;
      |HAZ SituaMaestra;
      nMaestra   = 1;
  |FINSI;
  nLongMax = MaximoDigitos;
  |PATH_DAT #cacuenta#aPathConta#;
|FPRC;
