|FICHEROS;
     dsmoz018 #0;
     dsmod045 #45;
     dsmow097 #999;
|FIN;

|VARIABLES;
     &enBoton1 = 0;
     &enBoton2 = 0;
|FIN;

|INCLUYE i_variar;


|RUTINA ClaveBaja045;
     #45#0 = #999#0;
     #45#1 = #999#1;
     #45#2 = #999#2;
     #45#3 = 01;
     #45#4 = 00;
|FRUT;

|RUTINA ClaveAlta045;
     #45#0 = #999#0;
     #45#1 = #999#1;
     #45#2 = #999#2;
     #45#3 = 99;
     #45#4 = 99;
|FRUT;

|PROCESO Tipo7;  |TIPO 7;
     |ENTLINEAL #1#45, -4, 7, 22, 0, ClaveBaja045, ClaveAlta045;
|FPRC;

|PROCESO Tipo11;  |TIPO 11;
     |SI FSalida = 10;
         enEmpresa = #dsmow097#0;
         |HAZ MiraBloqueo;
         |SI eaBloqueo = "3";
             |ACABA;
         |FINSI;

         |ESTADO_CONTROL enBoton1, "HIDE";
         |ESTADO_CONTROL enBoton2, "HIDE";
         |ENTLINEAL #1#45, -4, 2, 22, 0, ClaveBaja045, ClaveAlta045;
         |ESTADO_CONTROL enBoton1, "SHOW";
         |ESTADO_CONTROL enBoton2, "SHOW";
     |FINSI;

     |SI FSalida = 11;
         ||HAZ EmisionLinea;
     |FINSI;

     |SI FSalida = 12;
         ||HAZ EmisionTodo;
     |FINSI;
|FPRC;
