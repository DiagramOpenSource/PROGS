|FICHEROS;
     dsm18408;
     dsm18401;
|FIN;

|VARIABLES;
   &enEmpresa = 0; &enEjer     = 0; &eaFichero = ""; &enCampo = 0;
   &eaTabla = "";  &enPosicion = 0; &eaDestino = ""; &enComp = 0;
   &eaAlfa  = "";  &enError    = 0; &enTotalCas = 0; &eaTitulo = "";

   nLCod  = 0;
   nLDes  = 0;
   nCampo = 0;
   nCalc  = 0;
   nLinea = 0;
   nSwError = 0;

   aAlfa  = "";
   aAlfa1 = "";
   aAlfa2 = "";

   aPais  = "";
   aTipo  = "";
|FIN;

|PROCESO SacaPais08; |TIPO 7;
   eaTabla = "ztabla05.txt";  nLCod = 2; nLDes = 30; |HAZ CargaTabla;
   |MENSA "    <F2> Consulta paises ";
|FPRC;

|PROCESO CompPais08;

     |SI #dsm18408#4 = "  "; #dsm18408#4 = "ES"; |FINSI;

     |MENSA "     ";
     nCampo   = Campo + 1;

     |SI FSalida = 10;
         enPosicion = 20;
         |HAZ SacaTabla;
         nCalc = 100 + nLCod;
         aAlfa1   = eaDestino % nCalc;
         #dsm18408#Campo#  = aAlfa1; |PINTA #dsm18408#Campo#;
     |FINSI;

     enComp = nLCod;
     aAlfa = "|C" + (("000" + Campo) % -103) + "TIPO"; aAlfa1 = #dsm18408#aAlfa#;
     aAlfa = #dsm18408#Campo#; |QBF aAlfa;
     |SI aAlfa1 = "N"; nCalc = - (100 + nLCod); aAlfa = (("0" * enComp) + aAlfa) % nCalc; |FINSI;
     |SI aAlfa1 ! "N"; nCalc = 100 + nLCod;     aAlfa = (aAlfa + (" " * enComp)) % nCalc; |FINSI;
     eaAlfa = aAlfa;
     |HAZ CompruebaTabla;

     |SI enError = 1;
         |MENAV "0000 Contenido Incorrecto. Consulta la Tabla con F2";
         |ERROR;
         |ACABA;
     |FINSI;

     nCalc = 100 + nLCod;                 aAlfa1   = eaDestino % nCalc;
     nCalc = ((nLCod + 2) * 100) + nLDes; aAlfa2   = eaDestino % nCalc;
     #dsm18408#Campo#  = aAlfa1;
     |C_P #dsm18408#Campo#,0,nCalc; nCalc = nCalc + nLCod + 1;
     |PINTA #dsm18408#Campo#;
     #dsm18408#8 = aAlfa2; |PINTA #dsm18408#8;

     |HAZ FinTabla;
|FPRC;

|PROCESO Pais08;
   aPais = #dsm18408#4; nLinea = #dsm18408#3; aTipo = #dsm18408#5;

   nSwError = 0;
   |SALVA_FICHA #dsm18408;
   #dsm18408#0 = #dsm18401#0;
   #dsm18408#1 = #dsm18401#1;
   #dsm18408#2 = #dsm18401#3;
   #dsm18408#3 = 1;
   |LEE 000#dsm18408.];
   |PARA; |SI FSalida = 0; |Y #dsm18408#0 = #dsm18401#0; |Y #dsm18408#1 = #dsm18401#1;
           |Y #dsm18408#2 = #dsm18401#3; |HACIENDO;
      aAlfa = #dsm18408#4;
      |SI aAlfa = aPais; |Y #dsm18408#5 = aTipo; |Y #dsm18408#3 ! nLinea; nSwError = 1; |SAL; |FINSI;
      |LEE 000#dsm18408.s;
   |SIGUE;
   |REPON_FICHA #dsm18408;

   |SI nSwError ! 0;
      |SI #dsm18408#5 = "G";
         |MENAV "    Ya existe una linea de ganancias con ese Pais";
      |SINO;
         |MENAV "    Ya existe una linea de perdidas con ese Pais";
      |FINSI;
      |ERROR;
   |FINSI;
|FPRC;

|PROCESO Conv08;
   |SI #dsm18408#4 = "ES";
      #dsm18408#7 = "S"; |PINTA #dsm18408#7;
      |C_M #dsm18408#7, 1, "N";
   |SINO;
      |C_M #dsm18408#7, 1, "S";
   |FINSI;
|FPRC;
