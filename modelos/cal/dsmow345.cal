|FICHEROS;
   dsmow345 #10,MANTE;
|FIN;

|VARIABLES;
   &eaConyuge = "";

   aTipoAnt = "";

   nVal1 = 0;
   nVal2 = 0;
|FIN;

|PROCESO Tipo12w345; |TIPO 12;
|FPRC;

|PROCESO MiraValorAnt; |TIPO 7;
   aTipoAnt = #dsmow345#2;
   nVal1    = #dsmow345#0;
   nVal2    = #dsmow345#1;
|FPRC;

|PROCESO MiraRestric;
   |SI eaConyuge = "N";
      |SI #dsmow345#2 = "G"; |O #dsmow345#2 = "C";
         #dsmow345#2 = aTipoAnt; |PINTA #dsmow345#2;
         |ERROR;
      |FINSI;
      |SI #dsmow345#2 = "V"; |Y #dsmow345#1 ! 0;
         #dsmow345#0 = nVal1; |PINTA #dsmow345#0;
         #dsmow345#1 = nVal2; |PINTA #dsmow345#1;
         #dsmow345#2 = aTipoAnt; |PINTA #dsmow345#2;
         |ERROR;
      |FINSI;
   |FINSI;
|FPRC;

|PROCESO MiraVarios;
   |SI #dsmow345#2 = "V";
      |C_M #dsmow345#0, 1, "S";
      |C_M #dsmow345#1, 1, "S";
   |SINO;
      |C_M #dsmow345#0, 1, "N";
      |C_M #dsmow345#1, 1, "N";
   |FINSI;
|FPRC;
