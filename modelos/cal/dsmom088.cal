|FICHEROS;
   dsmom088 #88;
   :/contagen/def/cacuenta #2;
   :/contagen/def/ca8cumay #3;
|FIN;

|VARIABLES;
   aAlfa   = "";
   nModo   = 0;
|FIN;

|INCLUYE i_variar;
|INCLUYE i_varcon;

|PROCESO inf2;
   #3#10 = 0;
   #3#0  = "6000";
|FPRC;

|PROCESO sup2;
   #3#10 = 0;
   #3#0  = "7999";
|FPRC;

|CALCULO Tipo0C0; |TIPO 0;
   aAlfa = #88#0; |QBF aAlfa;
   aAlfa = (aAlfa + "0000") % 104;
   |SI aAlfa ! #88#0;
       #88#0 = aAlfa; |PINTA #88#0;
       |ERROR;
   |FINSI;
|FCAL;

|CALCULO Tipo11m88; |TIPO 11;
   |SI FSalida = 10;
      |ABRE #3;
      |CONSULTA_F_CLAVE #1#3,inf2,sup2;
      #88#0 = #3#0;  |PINTA #88#0;
      #88#1 = #3#1;  |PINTA #88#1;
      #88#2 = #3#11; |PINTA #88#2;
      |CIERRA #3;
   |FINSI;
|FCAL;

|CALCULO AntesCuenta; |TIPO 7;

   nModo = (FEntrada / 100) ! 0;
   |SI nModo ! 1; |ACABA; |FINSI;

   aAlfa = #88#0 % 101;
   |SI aAlfa = "6";
       #88#2 = "+";
       #88#4 = "C";
   |FINSI;
   |SI aAlfa = "7";
       #88#2 = "-";
       #88#4 = "V";
   |FINSI;
   |PINTA #88#2;
   |PINTA #88#4;
|FCAL;

|CALCULO LeeLaCuenta;

   |ABRE #3;
   |DDEFECTO #3;
   #3#10 = 0;
   #3#0  = #88#0;
   |LEE 000#3=;
   |SI FSalida ! 0;
       |MENAV "    Error No Existe Cuenta ";
       |ERROR;
   |FINSI;
   |CIERRA #3;
   #88#1 = #3#1;

   |PINTA #88#1;
|FCAL;

|RUTINA ClaveBaja88;
    #88#3 = enPlan;
    #88#0 = "6000";
|FRUT;

|RUTINA ClaveAlta88;
    #88#3 = enPlan;
    #88#0 = "7999";
|FRUT;

|PROGRAMA;
  || ..........................................
  enPlan = 2008;
  eaPlanilla = "dsmom088";  |HAZ PlanDePlanillas;

  |CLS;
  |PINPA #0#88;
  |ENTLINEAL #1#88, -2, 2, 22, 0, ClaveBaja88, ClaveAlta88;
|FPRO;
