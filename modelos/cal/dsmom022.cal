|FICHEROS;
     dsmom022 #22;
     :/contagen/def/cacuenta #3;
     :/modelos/def/dsmom100 #100;
|FIN;

|VARIABLES;
     aAlfa = "";
|FIN;

|INCLUYE i_variar;
|INCLUYE i_varcon;

|| \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\

|CALCULO FiltraReten; |TIPO 6;
     eaCuenta = #22Campo;
     salidas  = FSalida;
     |HAZ FiltraPuntos;
     #22Campo = eaCuenta;
     |PINTA #22Campo;
|FCAL;

|RUTINA inf2;
   #3#0 = "473         ";
   #3#3 = "S";
|FRUT;

|RUTINA sup2;
   #3#0 = "zzzzzzzzzzzz";
   #3#3 = "S";
|FRUT;

|CALCULO ConsultaRen; |TIPO 11;
   |SI FSalida = 10;
      |ABRE #3;
      |CONSULTA_F_CLAVE #6#3,inf2,sup2;
      #22#0 = #3#0; |PINTA #22#0;
      #22#1 = #3#2; |PINTA #22#1;
      |ERROR;
      |CIERRA #3;
   |FINSI;

   |SI FSalida = 20;
     eaCuenta = #22Campo;
     salidas  = FSalida;
     |HAZ FiltraPuntos;
  |FINSI;

|FCAL;

|| /////////////////////////////////////////////////////////////////////////

|RUTINA ClaveBaja22;
     #22#0 = "            ";
|FRUT;

|RUTINA ClaveAlta22;
     #22#0 = "zzzzzzzzzzzz";
|FRUT;

|PROGRAMA;
     |CLS;
     |HAZ LaContab;
     |SI swerror = 1; |ACABA; |FINSI;
     |ENTLINEAL #1#22, -1, 1, 22, 1, ClaveBaja22, ClaveAlta22;
|FPRO;

|PROCESO LaContab;
   |ABRE #100;
   |LEE 040#100p;
   |SI FSalida ! 0;
       |CIERRA #100;
       |MENAV "    No esta configurada las Empresas Maestras de Contabilidad";
       swerror = 1;
       |ACABA;
   |FINSI;
   |CIERRA #100;

    enEmpresa  = #100#1;
    enPerConta = #100#2;
    |HAZ LeeEmpConta;
    |PATH_DAT #3 aPathConta;
|FPRC;
