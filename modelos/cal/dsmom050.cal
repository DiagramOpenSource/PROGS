|FICHEROS;
   dsmom050 #50;
   :/modelos/def/dsmom100 #100;

   :/contagen/def/cacuenta #2;
   :/contagen/def/ca8cumay #3;
|FIN;

|VARIABLES;
   aAlfa   = "";
   nCuenta = 0;
|FIN;

|INCLUYE i_variar;
|INCLUYE i_varcon;

|CALCULO AntesCuenta; |TIPO 7;
 |C_M #50#1,1,"N";
 |SI nCuenta = 1;  |C_M #50#1,1,"S"; |FINSI;
|FCAL;

|CALCULO LeeLaCuenta;
 |SI nCuenta = 1; |ACABA; |FINSI;

 |SI enPlan = 1990;
     |ABRE #2;
     #2#0 = #50#0;
     |LEE 001#2=;
     |SI FSalida ! 0;
         |MENAV "    Error No Existe Cuenta ";
         |ERROR;
     |FINSI;
     |CIERRA #2;
     #50#1 = #2#2;
 |FINSI;

 |SI enPlan = 2008;
     |ABRE #3;
     #3#10 = 0;
     #3#0  = #50#0;
     |LEE 000#3=;
     |SI FSalida ! 0;
         |MENAV "    Error No Existe Cuenta ";
     |FINSI;
     |CIERRA #3;
     #50#1 = #3#1;
 |FINSI;

 |PINTA #50#1;
|FCAL;

|RUTINA ClaveBaja50;
     #50#3 = enPlan;
     #50#0 = "1000";
|FRUT;

|RUTINA ClaveAlta50;
     #50#3 = enPlan;
     #50#0 = "5999";
|FRUT;

|PROGRAMA;
  || ..........................................
  nCuenta = 0;
  |SI enPlan = 1990;
      |ABRE #100;
      |LEE 040#100p;
      |SI FSalida ! 0;
          |MENAV "    No esta configurada las Empresas Maestras de Contabilidad";
          nCuenta = 1;
      |FINSI;
      |CIERRA #100;

      enEmpresa  = #100#1;
      enPerConta = #100#2;
      |HAZ LeeEmpConta;
      |PATH_DAT #2 aPathConta;
  |FINSI;

  |CLS;
  |PINPA #0#50;
  |ENTLINEAL #1#50, -2, 2, 22, 0, ClaveBaja50, ClaveAlta50;
|FPRO;
