         |SINO;
             |SI #901#7 ! nVHoSem; |Y #901#17 = diasanyo;
                 #901#18 = ((#901#7 * nVHoras) / nVHoSem) ! 2;
             |SINO;
                 #901#18 = ( (#901#7 * nVHoras * #901#17) / ( nVHoSem * diasanyo) ) ! 2;
             |FINSI;
         |FINSI;
     |FINSI;
     #901#12 = ((#901#18 / nVHoras)) ! 2;
