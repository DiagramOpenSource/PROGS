|FICHEROS;
     dsm296ca #1;
     dsm296li #2;
     dsmom004 #4;
|FIN;

|VARIABLES;
     nSwChequea = 0;
     nBorra     = 0;
     nComp      = 0;
     nSuma      = 0;
     nBoton1    = 0;

     aTecla     = "";
     aF2        = "";
     aEsc       = "";

     fDd        = @;
     fHd        = @;
|FIN;

|INCLUYE i_variar;

|RUTINA ClaveBaja2;
     #2#0  = #1#0;
     #2#1  = #1#1;
     #2#2  = #1#2;
     #2#3  = #1#3;
     #2#4  = #1#4;
     #2#5  = #1#5;
     #2#6  = "            ";
     #2#7  = "01.01.0000";
     #2#8  = 1;
     #2#9  = " ";
     #2#10 = 00;
|FRUT;

|RUTINA ClaveAlta2;
     #2#0  = #1#0;
     #2#1  = #1#1;
     #2#2  = #1#2;
     #2#3  = #1#3;
     #2#4  = #1#4;
     #2#5  = #1#5;
     #2#6  = "zzzzzzzzzzzz";
     #2#7  = "31.12.2999";
     #2#8  = 2;
     #2#9  = "z";
     #2#10 = 99;
|FRUT;

|PROCESO Boton1;
     |PUSHV 0501 2380;
     |ESTADO_CONTROL nBoton1, "HIDE";

     |PINPA #0#2;

     |SI enTipoEntrada = 4;
         |ENTLINEAL #1#2, -7, 4, 22, 0, ClaveBaja2, ClaveAlta2;
     |SINO;
         |ENTLINEAL #1#2, -7, 2, 22, 0, ClaveBaja2, ClaveAlta2;
     |FINSI;

     |ESTADO_CONTROL nBoton1, "SHOW";
     |POPV;

     |HAZ Recalculo;

     |PINTA #1#6;
     |PINTA #1#7;
     |PINTA #1#8;
     |PINTA #1#9;
|FPRC;

|PROCESO RecalculaLi;
     |SI nBorra = 1;
         |BORRA 020#2a;
         |LIBERA #2;
         |ACABA;
     |FINSI;

     |SI nSwChequea = 0;
         nSwChequea = 1;
         |ERROR10;
         |ACABA;
     |FINSI;

     |HAZ Tipo2m296li;
|FPRC;

|DEFBUCLE dsm296li;
     #2#1;
     ;
     #1#0, #1#1, #1#2, #1#3, #1#4, #1#5, "            ", fDd, 1, " ", 00;
     #1#0, #1#1, #1#2, #1#3, #1#4, #1#5, "zzzzzzzzzzzz", fHd, 2, "z", 99;
     be;
     NULL, RecalculaLi;
|FIN;

|PROCESO Recalculo;
     nSwChequea = 0;  |BUCLE dsm296li;

     |SI nSwChequea = 0;  |ACABA;  |FINSI;

     #1#6 = 0;
     #1#7 = 0;
     #1#8 = 0;
     #1#9 = 0;

     |BUCLE dsm296li;
|FPRC;

|PROCESO Traedsm296li;
     #2#4 = #1#4;
     |GRABA 020#2n;
     |LIBERA #2;
|FPRC;

|DEFBUCLE dsm296liT;
     #2#1;
     ;
     #1#0, #1#1, #1#2, #1#3, nComp, #1#5, "            ", fDd, 1, " ", 00;
     #1#0, #1#1, #1#2, #1#3, nComp, #1#5, "zzzzzzzzzzzz", fHd, 2, "z", 99;
     be;
     NULL, Traedsm296li;
|FIN;

|PROCESO Justificante;
     |SI #1#4 = 0;  |ACABA;  |FINSI;

     |SI #1#11 ! "C";  |Y #1#11 ! "S";
         |MENAV "      Introduzca Complementaria o Sustitutoria";
         |ERROR;
     |FINSI;

     |C_M #1#10, 1, "N";
     |SI #1#11 = "S";
         |C_M #1#10, 1, "S";

         |SI #1#10 = "             ";
             |ABRE #4;
             #4#0 = #1#0;
             #4#1 = #1#1;
             #4#2 = #1#2;
             #4#3 = #1#3;
             #4#4 = #1#4 - 1;
             |LEE 000#4=;
             |SI FSalida ! 0;  |DDEFECTO #4;  |FINSI;
             |CIERRA #4;

             #1#10 = #4#25;  |PINTA #1#10;
         |FINSI;
     |SINO;
         #1#10 = "";  |PINTA #1#10;
     |FINSI;

     |SI #1#11 = "S";
         nSuma = 0;
         nSuma = #1#7 + #1#8 + #1#9;

         |SI nSuma = 0;
             nComp = #1#4 - 1;
             |BUCLE dsm296liT;

             |HAZ Recalculo;

             |PINPA #0#1;
             |PINDA #0#1;
             |HAZ PintaDatos;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo12moc1296;  |TIPO 12;
|FPRC;

|PROCESO PulsoF2;
     |PTEC 824;
|FPRC;

|PROGRAMA;
     |HAZ LeeEURODBMOD;

     eaNomDef    = "dsm296ca";
     enResultado = 0;
     fDd         = "01.01.0000";
     fHd         = "31.12.2999";

|| \\\\\\ Alta del Modelo ////////

     |SI enTipoEntrada = 1;              || Consulta del Modelo
         |CABEZA "Modelo 296";
         |ABRE #1;
         #1#0 = enEmpresa;
         #1#1 = enEjer;
         #1#2 = enPeriodo;
         #1#3 = enModelo;
         #1#4 = enComplem;
         #1#5 = enActividad;
         |LEE 040#1=;
         |SI FSalida ! 0;
             |DDEFECTO #1;
             #1#0 = enEmpresa;
             #1#1 = enEjer;
             #1#2 = enPeriodo;
             #1#3 = enModelo;
             #1#4 = enComplem;
             #1#5 = enActividad;
             |GRABA 020#1b;
         |FINSI;

         |PINPA #0#1;
         |PINDA #0#1;
         |HAZ PintaDatos;

         |CONTROL_SIMPLE 0, "BOTON,<F2> Entrada Perceptores  ", 1155, 1379, PulsoF2;
         nBoton1 = FSalida;

         |SI #1#4 ! 0;
             |C_M #1#11, 1, "S";
             |ENDATOS #1#1;
         |FINSI;

         aF2  = &255 + "824";
         aEsc = &255 + "806";
         |PARA; |SI;  |HACIENDO;
             |LEETECLA aTecla;
             |SI aTecla = aF2;    |HAZ Boton1;  |FINSI;
             |SI aTecla = aEsc;   |SAL;         |FINSI;
         |SIGUE;

         |FIN_CONTROL_WINDOWS nBoton1;

         |PULSATECLA;

         nSuma = #1#7  + #1#8  + #1#9;

         |SI nSuma = 0;
             |CONFI "0000NNo hay perceptores introducidos, quiere emitirlo sin actividad";
             |SI FSalida ! 0;
                 |BORRA 020#1a;
                 enEntrada = 0;
             |SINO;
                 |GRABA 020#1a;
                 enEntrada = 1;
             |FINSI;
         |SINO;
             |GRABA 020#1a;
             enEntrada = 1;
         |FINSI;
         |LIBERA #1;
         |CIERRA #1;

         |ACABA;
     |FINSI;

|| \\\\\\ Consulta del Modelo ////////

     |SI enTipoEntrada = 4;              || Consulta del Modelo
         |CABEZA "Modelo 296";
         |ABRE #1;
         #1#0 = enEmpresa;
         #1#1 = enEjer;
         #1#2 = enPeriodo;
         #1#3 = enModelo;
         #1#4 = enComplem;
         #1#5 = enActividad;
         |LEE 040#1=;
         |SI FSalida ! 0;
             |MENAV "      No existe Registro.";
             |CIERRA #1;
             |ACABA;
         |FINSI;

         |PINPA #0#1;
         |PINDA #0#1;
         |HAZ PintaDatos;

         |CONTROL_SIMPLE 0, "BOTON,<F2> Entrada Perceptores  ", 1155, 1379, Boton1;
         nBoton1 = FSalida;

         aF2  = &255 + "824";
         aEsc = &255 + "806";
         |PARA; |SI;  |HACIENDO;
             |LEETECLA aTecla;
             |SI aTecla = aF2;    |HAZ Boton1;  |FINSI;
             |SI aTecla = aEsc;   |SAL;         |FINSI;
         |SIGUE;

         |FIN_CONTROL_WINDOWS nBoton1;

         |PULSATECLA;

         |CIERRA #1;

         |ACABA;
     |FINSI;

|| \\\\\\ Baja del Modelo ////////

     |SI enTipoEntrada = 3;              || Baja del Modelo
         |ABRE #1;
         #1#0 = enEmpresa;
         #1#1 = enEjer;
         #1#2 = enPeriodo;
         #1#3 = enModelo;
         #1#4 = enComplem;
         #1#5 = enActividad;
         |LEE 040#1=;
         |SI FSalida ! 0;
             |SI enMensa = 0;
                 |MENAV "      No existe Registro.";
             |FINSI;
             |CIERRA #1;
             |ACABA;
         |FINSI;

         nBorra = 1;  |BUCLE dsm296li;
         nBorra = 0;

         |BORRA 020#1a;
         |LIBERA #1;

         |CIERRA #1;

         |ACABA;
     |FINSI;

|| \\\\\\ Alta desde el Pase del Modelo

     |SI enTipoEntrada = 98;
         |ABRE #1;
         #1#0 = enEmpresa;
         #1#1 = enEjer;
         #1#2 = enPeriodo;
         #1#3 = enModelo;
         #1#4 = enComplem;
         #1#5 = enActividad;
         |LEE 040#1=;
         |SI FSalida ! 0;
             #1#0 = enEmpresa;
             #1#1 = enEjer;
             #1#2 = enPeriodo;
             #1#3 = enModelo;
             #1#4 = enComplem;
             #1#5 = enActividad;
             |GRABA 020#1b;
         |FINSI;

         |HAZ Recalculo;
         |GRABA 020#1a;

         |LIBERA #1;
         |CIERRA #1;

         |ACABA;
     |FINSI;

|| \\\\\\ Comprobacion Existencia del Modelo

     |SI enTipoEntrada = 99;              || Existencia del Modelo ????
         |ABRE #1;
         #1#0 = enEmpresa;
         #1#1 = enEjer;
         #1#2 = enPeriodo;
         #1#3 = enModelo;
         #1#4 = enComplem;
         #1#5 = enActividad;
         |LEE 040#1=;
         |SI FSalida ! 0;  enExistencia = 1;  |FINSI;
         |CIERRA #1;

         |SI enActividad = 0;  enResultado = 0;  |FINSI;
     |FINSI;
|FPRO;
