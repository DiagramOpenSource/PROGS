|FICHEROS;
     dsmoz100 #100;
|FIN;

|VARIABLES;
     aAlfa      = "";
     aFichero   = "";
     aParametro = "";

     &enPlan     = 0;
     &eaPlanilla = "";
     &enEjer     = 0;
     aPassword   = "";
|FIN;

|PROCESO Baja100;
     #100#0 = "";
     |SI FSalida = "POSICION";
         #100#0 = "PLAN CONTABLE DE 2008";
     |FINSI;
|FPRC;

|PROCESO Alta100;
     #100#0 = "z" * 40;
|FPRC;

|PROCESO Tipo0;
     aAlfa  = #100#0;  |QBF aAlfa;
     aAlfa  = aAlfa % -104;
     enPlan = aAlfa;
     enEjer = 0;

     aParametro = PARAMETRO;

     |SI enPlan = 2008;
         |SI aParametro = "dsmom014"; eaPlanilla = "dsmom014";  |HAZ PlanDePlanillas; |FINSI;
         |SI aParametro = "dsmom050"; eaPlanilla = "dsmom050";  |HAZ PlanDePlanillas; |FINSI;
         |SI aParametro = "dsmom062"; eaPlanilla = "dsmom062";  |HAZ PlanDePlanillas; |FINSI;
         |SI aParametro = "dsmom088"; eaPlanilla = "dsmom088";  |HAZ PlanDePlanillas; |FINSI;
     |FINSI;

     aParametro = PARAMETRO;
     |SUB_EJECUTA_NP aParametro;
     |ERROR;
|FPRC;

|PROGRAMA;
     aFichero = ("CC" + Usuario + "zzzzzzzz") % 108;
     |NOME_DAT #100 aFichero;

     |ABRE #100;  |CIERRA #100;  |DELINDEX #100;

     |ABRE #100;
     |LEE 000#100p;
     |SI FSalida ! 0;
         #100#0 = "PLAN CONTABLE DE 1990";  |GRABA 020#100n;  |LIBERA #100;
         #100#0 = "PLAN CONTABLE DE 2008";  |GRABA 020#100n;  |LIBERA #100;
     |FINSI;

     aParametro = PARAMETRO;
     |SI aParametro = "dsmoz052N"; |O aParametro = "dsmoz052S"; |O aParametro = "dsmom014";
         enPlan = 2008;
         enEjer = 0;
         |SI aParametro = "dsmoz052N"; aParametro = "dsmoz052;N"; |FINSI;
         |SI aParametro = "dsmoz052S"; aParametro = "dsmoz052;S"; |FINSI;
         |SUB_EJECUTA_NP aParametro;
         |ACABA;
     |FINSI;

     |CLS;
     |PINPA #0#100;
     |ENTLINEAL #1#100, -1, 4, 13, 1, Baja100, Alta100;

     |CIERRA #100;  |DELINDEX #100;
|FPRO;
