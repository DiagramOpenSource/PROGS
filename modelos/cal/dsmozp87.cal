|PROCESO BuscoDatos;
    nSwExiste = 0;
    |SI #0#0 [ 2010;
        |ABRE #80;
        #80#0 = #84#0;
        #80#1 = #84#1;
        |LEE 001#80=;
        |SI FSalida = 0;
            #84#2 = #80#2;
            #84#3 = #80#3;
            #84#4 = #80#4;
            #84#5 = #80#5;
            #84#7 = #80#6;
            #84#8 = #80#7;
            #84#9 = #80#9;
            #84#10 = #80#43;
            #84#11 = #80#8;
            |SI #80#13 ! 0;
                #84#6 = "S";
            |SINO;
                #84#6 = "N";
                |BUCLE Leom082;
            |FINSI;
            nSwExiste = 1;
        |FINSI;
        |CIERRA #80;
    |SINO;
        |ABRE #85;
        #85#0 = #84#0;
        #85#1 = #84#1;
        |LEE 001#85=;
        |SI FSalida = 0;
            #84#2 = #85#2;
            #84#3 = #85#3;
            #84#4 = #85#4;
            #84#5 = #85#5;
            #84#7 = #85#6;
            #84#8 = #85#7;
            #84#11 = #80#8;
            #84#6 = "N";
            |BUCLE Leom087;
        |FINSI;
        |CIERRA #85;
    |FINSI;

    |SI nSwExiste = 0;
        nSwConPro = -1;
        |BUCLE MiraSiDIVA;
        |SI nSwConPro = -1;
            nSwExiste = -1;
        |FINSI;
    |FINSI;

    |SI nSwExiste ! -1;
        |HAZ Actualiza;
        |SI nImpreso = 1;
            nSwExiste = -1;
        |FINSI;
    |FINSI;
|FPRC;

