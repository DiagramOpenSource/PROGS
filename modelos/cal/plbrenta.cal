|| .... Seleccion de Modelos para el pase a Historico de Tributacion

|VARIABLES;
 aAlfa1     = "";
 aDirInicio = "";
|FIN;

|INCLUYE i_variar;
|INCLUYE i_varren;

|PROCESO DRentaFich;
    aAlfa1 = enAnyRen; |QBF aAlfa1;
    aAlfa1 = "ren" + aAlfa1;

    |DBASS aDirInicio; |QBF aDirInicio;
    eaPathFichRenta = aDirInicio + aAlfa1 + "/fich/";
    eaPathDefRenta  = aDirInicio + aAlfa1 + "/def/";

    eaMonedaOtra = "E";
    |SI enAnyRen < 2001;
        eaMonedaOtra = "P";
    |FINSI;

    |SI enAnyRen [ 1998;
        eaFicRen    = "reliquid";
        enRNoBlanco = 1;
        enRImpreso  = 43;
    |FINSI;
    |SI enAnyRen ] 1999;
        eaFicRen    = "rem01001";
        enRImporte  = 3;
        enRNoBlanco = 5;
        enRImpreso  = 6;
    |FINSI;
|FPRC;
