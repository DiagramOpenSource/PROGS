|PROCESO Imprimir;

   |DBASE aBase;  |QBT aBase;
   aDef = aBase + "def/dsmow088.mas";
   |CARGA_DEF aDef, reflejo@;
   |SI FSalida ! 0;
       aAlfa = "     Error en cargar el Def dsmow088";
       |MENAV aAlfa;
       |ACABA;
   |FINSI;
   |NOME_DAT #188 aFichero;

   |IMPRE 0;
   |SI FSalida ! 0;
       |DESCARGA_DEF #reflejo@;
       |ACABA;
   |FINSI;

   |INFOR informe;
   |SI FSalida ! 0;
       |FINIMP;
       |DESCARGA_DEF #reflejo@;
       |ACABA;
   |FINSI;

   |ABRE #47;
   |BUCLE LeoReflejo;
   |CIERRA #47;

   |PIEINF;
   |FININF;
   |FINIMP;
   |DESCARGA_DEF #reflejo@;
|FPRC;

