|PROCESO GrabaTempo;

     #999#0 = #4#0;
     |LEE 101#999=;
     |SI FSalida ! 0;
         #1000#0 = #4#0;
         |LEE 040#1000=;
         |SI FSalida ! 0;  |DDEFECTO #1000;  |FINSI;

         #999#0 = #4#0;
