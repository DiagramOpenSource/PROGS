|FICHEROS;
   dsmow310;

   dsm210dp #10;
   dsmow353 #11,MANTE;
|FIN;

|PROCESO TrasAgrupar;
   |SI #dsmow310#5 = "S";
      |C_M #dsmow310#0, 1, "S";
      |C_M #dsmow310#1, 1, "N";
      #dsmow310#1 = "01.01.0000"; |PINTA #dsmow310#1;
   |SINO;
      |C_M #dsmow310#0, 1, "N";
      |C_M #dsmow310#1, 1, "S";
      #dsmow310#0 = 0; |PINTA #dsmow310#0;
   |FINSI;
|FPRC;

|PROCESO MiraTrimestre;
   |SI #dsmow310#0 = 0;
      |C_M #dsmow310#1, 1, "S";
   |SINO;
      #dsmow310#1 = "01.01.0000"; |PINTA #dsmow310#1;
      |C_M #dsmow310#1, 1, "N";
   |FINSI;
|FPRC;

|PROCESO MiraPresent;
   |SI #dsmow310#4 = "S";
      #dsmow310#2 = ""; |PINTA #dsmow310#2;
      #dsmow310#3 = ""; |PINTA #dsmow310#3;
      |C_M #dsmow310#2, 1, "N"; |C_M #dsmow310#3, 1, "N";
   |FINSI;
   |SI #dsmow310#4 = "R";
      #dsmow310#2 = #dsm210dp#37; |PINTA #dsmow310#2;
      #dsmow310#3 = #dsm210dp#39; |PINTA #dsmow310#3;
      |C_M #dsmow310#2, 1, "N"; |C_M #dsmow310#3, 1, "N";
   |FINSI;
   |SI #dsmow310#4 ! "S"; |Y #dsmow310#4 ! "R";
      #dsmow310#2 = ""; |PINTA #dsmow310#2;
      #dsmow310#3 = ""; |PINTA #dsmow310#3;
      |C_M #dsmow310#2, 1, "S"; |C_M #dsmow310#3, 1, "S";
   |FINSI;
|FPRC;

||aqui
