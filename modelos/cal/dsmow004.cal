|FICHEROS;
     dsmoz003 #0;
     dsmom004 #4;
     dsmow004 #999,MANTE;
|FIN;

|VARIABLES;
     nBoton1     = 0;
     nBoton2     = 0;
     nTecla      = 0;

     &enEmpresaA = 0;
     &enAnyo     = 0;
|FIN;

|INCLUYE i_variar;

|RUTINA ClaveBaja4;
     #4#0 = #999#0;
     #4#1 = #999#1;
     #4#2 = #999#2;
     #4#3 = #0#0;
     #4#4 = 00;
|FRUT;

|RUTINA ClaveAlta4;
     #4#0 = #999#0;
     #4#1 = #999#1;
     #4#2 = #999#2;
     #4#3 = #0#0;
     #4#4 = 99;
|FRUT;

|PROCESO Boton1;
     |PTEC 824;
|FPRC;

|PROCESO Boton2;
     |PTEC 825;
|FPRC;

|PROCESO Tipo0;  |TIPO 0;
     |FIN_CONTROL_WINDOWS nBoton1;
     |FIN_CONTROL_WINDOWS nBoton2;
     |PULSATECLA;
|FPRC;

|PROCESO Tipo7;  |TIPO 7;
     |PINTA 0605, #999#0;

     |ENTLINEAL #1#4, -4, 7, 22, 0, ClaveBaja4, ClaveAlta4;

     |BLIN 04;

     |CONTROL_SIMPLE 0, "BOTON,F2 Entrada al Lineal", 0460, 0480, Boton1;
     nBoton1 = FSalida;

     |SI #0#0 = 349;  |O #0#0 = 303;  |O #0#0 = 310;
         |SI #0#3 ] 2010;
              |CONTROL_SIMPLE 0, "LBOTON,F3 Configurar modelo 349", 0560, 0580, Boton2;
              nBoton2 = FSalida;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo11;  |TIPO 11;
     nTecla = FSalida;

     |FIN_CONTROL_WINDOWS nBoton1;
     |FIN_CONTROL_WINDOWS nBoton2;
     |PULSATECLA;

     |SI nTecla = 10;
         enEmpresa = #999#0;
         |HAZ MiraBloqueo;
         |SI eaBloqueo = "3";
             |ACABA;
         |FINSI;

         |SI enSwAviso = 1; |Y #0#0 ! 110;
             enEmpresa = #999#0;
             enEjer    = #999#1;
             enPeriodo = #999#2;
             |HAZ inicial;
         |FINSI;
         eaNombreEmp = #999#3;
         |ENTLINEAL #1#4, -4, 4, 22, 0, ClaveBaja4, ClaveAlta4;
     |FINSI;

     |SI nTecla = 11;
         |CONFI "0000NSeguro que quiere configurar el 349 a esta Empresa/Ejercicio";
         |SI FSalida ! 0;  |ACABA;  |FINSI;

         enEmpresa = #999#0;
         enEjer    = #999#1;
         |SUB_EJECUTA "dsmom095";

         enEmpresaA = #999#0;
         enAnyo     = #999#1;
         |SUB_EJECUTA "dsmoz001";

         |PTEC 809;
         |PTEC 808;
         |PONCONTROL  23, "SI";
     |FINSI;
|FPRC;
