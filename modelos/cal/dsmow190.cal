|FICHEROS;
     dsmoy000;
     dsmow190;
|FIN;

|VARIABLES;
     aAlfa        =  "";

     &eaPathLanza = "";
|FIN;

|PROCESO Tipo7C0; |TIPO 7;
     |SI #dsmoy000#0 = 390;
         |MENSA "0000Editar <F2> Errores; <F3> XML";
         |ACABA;
     |FINSI;

     |SI #dsmoy000#0 = 110;  |O #dsmoy000#0 = 115;  |O #dsmoy000#0 = 123;
      |O #dsmoy000#0 = 130;  |O #dsmoy000#0 = 131;  |O #dsmoy000#0 = 303;
      |O #dsmoy000#0 = 310;
         |MENSA "0000Editar <F2> Errores; <F3> TXT";
         |ACABA;
     |FINSI;

     |MENSA "0000Editar <F2> Errores";
|FPRC;

|PROCESO Tipo11w190;  |TIPO 11;
     |SI FSalida = 10;
         aAlfa = "START " + eaPathLanza + #dsmow190#0;  |QBF aAlfa;
         |RSYSTEM aAlfa;
     |FINSI;

     |SI FSalida = 11;
         |SI #dsmoy000#0 = 390;
             aAlfa = "390" + (#dsmow190#0 % 405) + ".xml";
         |SINO;
             |SI #dsmoy000#0 = 110;
                 aAlfa = "111" + (#dsmow190#0 % 405) + ".txt";
             |SINO;
                 aAlfa = #dsmoy000#0 + (#dsmow190#0 % 405) + ".txt";
             |FINSI;
         |FINSI;
         aAlfa = "START " + eaPathLanza + aAlfa;
         |RSYSTEM aAlfa;
     |FINSI;
|FPRC;
