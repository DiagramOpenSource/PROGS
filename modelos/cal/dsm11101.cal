             #dsmod110#11 = #dsmod110#11 + #dsm11101#15 + #dsm11101#16;
             #dsmod110#11 = #dsmod110#11 + #dsm11101#28 + #dsm11101#29;
        |FINSI;

        |SI nGrupo = 2;
             |SI aDNIAnt ! #dsm11101#6;  |Y #dsm11101#6 ! "PASEANTERIOR";  |Y #dsm11101#6 ! "MANUAL      ";
                 |SI #dsm11101#12 ! 0; |O #dsm11101#13 ! 0;
                  |O #dsm11101#25 ! 0; |O #dsm11101#26 ! 0;
                     #dsmod110#12 = #dsmod110#12 + 1;
                 |FINSI;
             |FINSI;

             |SI aDNIAnt ! #dsm11101#6;  |Y #dsm11101#6 ! "PASEANTERIOR";  |Y #dsm11101#6 ! "MANUAL      ";
                 |SI #dsm11101#14 ! 0; |O #dsm11101#15 ! 0; |O #dsm11101#16 ! 0;
                  |O #dsm11101#27 ! 0; |O #dsm11101#28 ! 0; |O #dsm11101#29 ! 0;
                     #dsmod110#15 = #dsmod110#15 + 1;
                 |FINSI;
             |FINSI;

             |SI #dsm11101#6 = "PASEANTERIOR";  |O #dsm11101#6 = "MANUAL      ";
                 |SI #dsm11101#22 ! 0;  #dsmod110#12 = #dsm11101#22;  |FINSI;
                 |SI #dsm11101#23 ! 0;  #dsmod110#15 = #dsm11101#23;  |FINSI;
             |FINSI;

             #dsmod110#13 = #dsmod110#13 + #dsm11101#12 + #dsm11101#25;
             #dsmod110#14 = #dsmod110#14 + #dsm11101#13 + #dsm11101#26;
             #dsmod110#16 = #dsmod110#16 + #dsm11101#14 + #dsm11101#27;
