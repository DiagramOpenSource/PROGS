     |SI #dsm11101#9 = "L";  |ACABA;  |FINSI;

     nGrupoAnt = nGrupo;
     |HAZ LeeGrupo;
     |SI nGrupoAnt ! nGrupo;
         aDNIAnt = "";
     |FINSI;

     |SI nGrupo = 0;  |ACABA;  |FINSI;

     |SI aCero = "N"; |O #dsm11101#9 ! "A";
