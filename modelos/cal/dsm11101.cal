         |C_M #dsm11101#23, 1, "S";  |C_V #dsm11101#23, 1, "S";
     |FINSI;

     aAlfa1 = "N"; aAlfa2 = "N";
     |SI #dsm11101#9 = "A";            aAlfa1 = "S"; aAlfa2 = "S";  |FINSI;
     |SI #dsm11101#9 = "B"; |Y #dsm11101#10 = "01";  aAlfa1 = "S";  |FINSI;
     |SI #dsm11101#1 < 2019;           aAlfa1 = "S"; aAlfa2 = "S";  |FINSI;

     |C_M #dsm11101#25, 1, aAlfa1;
     |C_M #dsm11101#26, 1, aAlfa1;
     |C_M #dsm11101#27, 1, aAlfa2;
     |C_M #dsm11101#28, 1, aAlfa2;
     |C_M #dsm11101#29, 1, aAlfa2;

     |PINTA #dsm11101#22;
     |PINTA #dsm11101#23;
