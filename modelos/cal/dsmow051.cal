|FICHEROS;
     dsmow051 #900,MANTE;
     :/contagen/def/canempre #200;
|FIN;

|VARIABLES;
     &enNivel  = 0;
     &enNivel1 = 0;
     &enNivel2 = 0;
     &enNivel3 = 0;
     &enNivel4 = 0;
     &enNivel5 = 0;
     &enNivel6 = 0;
|FIN;

|PROCESO Tipo;
     |C_M #900#2, 1, "S";
     |C_M #900#3, 1, "S";
     |C_M #900#5, 1, "S";
     |C_M #900#6, 1, "S";
     |C_M #900#7, 1, "N";
     |SI #900#1 ! 3;
         |C_M #900#7, 1, "S";
         |C_M #900#2, 1, "N";
         |C_M #900#3, 1, "N";
         |C_M #900#5, 1, "N";
         |C_M #900#6, 1, "N";
         #900#2 = 0;    |PINTA #900#2;
         #900#3 = 0;    |PINTA #900#3;
         #900#4 = "";   |PINTA #900#4;
         #900#5 = "";   |PINTA #900#5;
         #900#6 = "";   |PINTA #900#6;
     |FINSI;
|FPRC;

|PROCESO Lee;
     |SI #900#1 ! 3;  |ACABA;  |FINSI;

     |ABRE #200;
     #200#2 = #900#2;
     #200#3 = #900#3;
     |LEE 040#200=;
     |SI FSalida ! 0;
         |MENAV "     Empresa Inexistente";
         |ERROR;
     |FINSI;
     |CIERRA #200;

     #900#4   = #200#1;   |PINTA #900#4;

     enNivel  = #200#13;
     enNivel1 = #200#14;
     enNivel2 = #200#15;
     enNivel3 = #200#16;
     enNivel4 = #200#17;
     enNivel5 = #200#18;
     enNivel6 = #200#19;
|FPRC;
