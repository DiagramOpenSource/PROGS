|FICHEROS;
     dsmom103 #103;
|FIN;

|VARIABLES;
     aParametro = "";
     nDModelo   = 0;
     nHModelo   = 0;
|FIN;

|RUTINA ClaveBaja103;
     #103#0 = nDModelo;
     #103#1 = 00001;
     #103#2 = 00;
     #103#3 = 0000;
     #103#4 = 01;
|FRUT;

|RUTINA ClaveAlta103;
     #103#0 = nHModelo;
     #103#1 = 99999;
     #103#2 = 99;
     #103#3 = 2999;
     #103#4 = 12;
|FRUT;

|PROCESO VerTexto; |TIPO 7;
 |SI #103#0 = 310;
     |MENSA "    Introduzca Actividad 00, suma total del Importe a compensar";
 |SINO;
     |MENSA "    Introduzca Numero Actividad ";
 |FINSI;
|FPRC;

|PROCESO VerNoTexto; |TIPO 0;
 |MENSA "      ";
|FPRC;

|PROGRAMA;
     aParametro = PARAMETRO;  |QBT aParametro;
     nDModelo   = 0;
     nHModelo   = 999;
     |SI aParametro ! "";
         nDModelo = aParametro;
         nHModelo = aParametro;
     |FINSI;

     |ENTLINEAL #1#103, -1, 2, 22, 1, ClaveBaja103, ClaveAlta103;
|FPRO;
