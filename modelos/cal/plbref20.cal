|FICHEROS;
     program@ #0;

     dsmoz076;
     dsmoz078;
     dsmoz079;

    :/basica/def/agifigen;
|FIN;

|VARIABLES;
     &aPrograma = "";
     &enSw      = 0;
     aAlfa1     = "";

     nCampoCliente = 0;

     nNume1     = 0;
     aQueQuiero = "";
     aIDCampo   = "";
     aAlfa      = "";
|FIN;

|INCLUYE i_variar;

|PROCESO MensaCli; |TIPO 7;
   |HAZ MasRefer21
   |SI enSw = 0; |ACABA; |FINSI;

   nNume1 = Campo;
   aAlfa1 = ("000" + nNume1) % -103;
   aQueQuiero = "|C" + aAlfa1 + "IDCAMPO";
   aIDCampo = #program@#aQueQuiero#;
   |QBF aIDCampo;

   |SI aIDCampo = "parrilla";
       aAlfa1 = "     <00000> Seleccion Parrilla. <F2> Consulta Clientes";
   |SINO;
       aAlfa1 = "      <F2> Consulta Clientes";
   |FINSI;
   |MENSA aAlfa1;
|FPRC;

|PROCESO ConsultaCli; |TIPO 0;
     |HAZ MasRefer21
     |SI enSw = 0; |ACABA; |FINSI;

     |ATRI R; |BLIN 04; |ATRI 0;

     |SI FSalida ! 10; |ACABA; |FINSI;

     |PUSHV 0501 2380;
     nCampoCliente  = Campo;

     |ABRE #agifigen;
     #agifigen#0 = #0nCampoCliente;
     |LEE 040#agifigen.=;

     |CONSULTA_CLAVE #2#agifigen;
     |CIERRA #agifigen;
     #0nCampoCliente = #agifigen#0;

     |POPV;
     |PINTA #0nCampoCliente;
|FPRC;

|PROCESO MasRefer21;
     |SI eaElProm = "";
         aPrograma = Control;  |QBF aPrograma;
     |SINO;
         aPrograma = eaElProm;  |QBF aPrograma;
     |FINSI;
     enSw = 0;

     |SI aPrograma = "dsmoz076"; |REFERENCIA_DEF program@, #dsmoz078; enSw = 1; |FINSI;
     |SI aPrograma = "dsmoz078"; |REFERENCIA_DEF program@, #dsmoz078; enSw = 1; |FINSI;
     |SI aPrograma = "dsmoz079"; |REFERENCIA_DEF program@, #dsmoz079; enSw = 1; |FINSI;
|FPRC;
