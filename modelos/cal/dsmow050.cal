      IepTAmorc = epTAmorc17 <-;   epTAmorc = epTAmorc + #0#10;
  |FINSI;

  |SI #0#11 = 0;
      |SI eaIniActi = "P";
          IepTAmorc = epTAmorc18 <-;   epTAmorc = epTAmorc + #0#3;
      |FINSI;

      IepTAmorc = epTAmorc9  <-;   epTAmorc = epTAmorc + #0#10;
  |FINSI;
|FPRC;

|DEFBUCLE eosw0064;
