|FICHEROS;
     dsm35301;
     dsm35302;
     dsmow219;
|FIN;

|VARIABLES;
     aAlfa         = "";

     &eaVarDni     = "";
     &enCalcCif    = 0;
|FIN;

|PROCESO T12Fw219;  |TIPO 12;
|FPRC;

|PROCESO T6C0Fw219;  |TIPO 6;
     |C_M #dsmow219#0, 0, aAlfa;
     |SI aAlfa = "N";  |ACABA;  |FINSI;

     eaVarDni  = #dsmow219#0;
     enCalcCif = 0;
     |HAZ CalculameDNI;
     eaVarDni = (eaVarDni + "             ") % 112;
     |SI #dsmow219#0 ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #dsmow219#0 = eaVarDni;
             |PINTA #dsmow219#0;

             |ERROR;
             |ACABA;
         |FINSI;
     |FINSI;

     #dsm35302#0  = #dsm35301#0;
     #dsm35302#1  = #dsm35301#1;
     #dsm35302#2  = #dsm35301#2;
     #dsm35302#3  = #dsm35301#3;
     #dsm35302#4  = #dsm35301#4;
     #dsm35302#5  = #dsm35301#5;
     #dsm35302#6  = "DEPENDIENTE";
     #dsm35302#7  = #dsmow219#0;
     |LEE 000#dsm35302.=;
     |SI FSalida = 0;
         |MENAV "0000En NIF ya est  introducido";
         |ERROR;
     |FINSI;
|FPRC;
