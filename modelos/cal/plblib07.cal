|PROCESO CopiaFichero;
     aAlfa1= (Usuario % -108); |QBF aAlfa1;
     aAlfa = eaUnidadBasico + "\DOCUMENTOS";               |RMKDIR aAlfa;
     aAlfa = eaUnidadBasico + "\DOCUMENTOS\tmp";           |RMKDIR aAlfa;
