                   nNume1 = eaAlfa;
                   |SI nNume1 = 0;
                       eaAlfa = "";
                   |SINO;
                       eaAlfa = "=" + eaAlfa;
                   |FINSI;
               |FINSI;

               |HAZ Una;
           |FINSI;
     |SIGUE;
|FPRC;

