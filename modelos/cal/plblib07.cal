     |FINSI;
     |SI aTipoIVA = "S"; |O aTipoIVA = "G";
         aNumCampos = #dsmow176#aQueQuiero#;
     |FINSI;
     |SI aTipoIVA = "B"; |O aTipoIVA = "X";
