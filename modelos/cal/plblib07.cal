                   eaAlfa   = #dsmow176#nPara1#;
               |FINSI;
               |SI aTipoIVA = "B"; |O aTipoIVA = "X";
                   eaTCampo = #dsmow177#aQueQuiero#;
                   eaAlfa   = #dsmow177#nPara1#;
               |FINSI;

