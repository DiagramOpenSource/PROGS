|PROCESO LeeElAux83;

  nDepar = #983#2;

  |SI nSwOpe83 = 0;   || = Calcular las Cuotas por Actividad
      nExiste   = 0;
      nVentaCon = 0;
      nVentaSin = 0;
      nVentaSub = 0;
      nAny = #0#0 - 1;
      |SI nAny [ 2010;
          |ABRE #81;
          #81#0 = enEmpresa;
          #81#1 = nAny;
          #81#2 = nDepar;
          |LEE 001#81=;
          |SI FSalida = 0;
              nVentaCon = #81#7;
              nVentaSin = #81#8;
              nVentaSub = #81#10 + #81#15;
              nExiste   = 1;
          |FINSI;
          |CIERRA #81;
      |SINO;
          |ABRE #86;
          #86#0 = enEmpresa;
          #86#1 = nAny;
          #86#2 = nDepar;
          |LEE 001#86=;
          |SI FSalida = 0;
              nVentaCon = #86#7;
              nVentaSin = #86#8;
              #983#15   = #86#18; nCalculado = 1;
              nExiste   = 1;
          |FINSI;
          |CIERRA #86;
      |FINSI;

      |SI nExiste = 0; |Y nDepar ! 0;
          |HAZ CalculoPorIVA;
      |FINSI;

      #983#6  = nVentaCon;
      #983#7  = nVentaSin;
      #983#32 = nVentaSub;
      #983#8  = #983#6  + #983#7 + #983#32;
      |GRABA 020#983a;

      |SI nDepar ! 0;
          #982#5  = #982#5  + nVentaCon;
          #982#6  = #982#6  + nVentaSin;
          #982#13 = #982#13 + nVentaSub;
          #982#7  = #982#5  + #982#6 + #982#13;
          |SI nDepar ! 99;
              nNumAct = nNumAct + 1;
          |FINSI;
      |FINSI;
  |FINSI;

  |SI nSwOpe83 = 1;   || = determina actividad Principal y otros detalles

      |SI nSw = 0; |Y nDepar ! 00;
          nImporteMas = #983#8;
          nActivMas   = #983#2;
          nRegIVA     = #983#14;
          nSw = 1;
      |FINSI;
      |SI nDepar ! 00;
         |SI nImporteMas < #983#8;  || la actividad principal
             nImporteMas = #983#8;
             nActivMas   = #983#2;
         |FINSI;
      |FINSI;

      |SI nDepar ! 0;
          |SI nCalculado = 0;
              #983#15 = ((#983#8 * 100) / #982#7 ) ! 2;
              |GRABA 020#983a;
          |FINSI;
          |SI nDepar ! 99;
              nNumAct = nNumAct + 1;        || todas las actividades
         |FINSI;
      |FINSI;
 |FINSI;

|FPRC;

