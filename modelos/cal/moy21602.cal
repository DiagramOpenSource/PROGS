     fFecFin = @;
|FIN;

|INCLUYE i_varemi;

|PROCESO Portal216_02;
     || Cuando se active copiar rutina del 123 o 115
|FPRC;

|PROCESO PresentaModelo_02;
     |SI eaOpcion = "1";
         eaNIF = #t1521601#7;
         |HAZ PortalPDFPeriodicos;
         |HAZ GrabaImpreso;
     |FINSI;

     |SI eaOpcion = "3";
         eaFOrigen  = eaPathEmision  + eaFicheroPlano;
         eaFDestino = eaPathInternet + eaFicheroPlano;
         |HAZ CopiaFichero;
         |HAZ GrabaImpreso;
     |FINSI;

     |SI eaOpcion = "6";
         eaX002_10    = #t1521601#7;
         eaX002_11    = #t1521601#8;
         eaX002_12    = #t1521601#2;
         eaX002_13    = #t1521601#10;
         eaX002_14    = #t1521601#11;

         |SI #dsmod216#4 > 0;
             eaX002_15 = "C";
             eaX002_16 = #t1521601#22;
         |FINSI;

         eaX002_17 = #dsmod216#11;
         eaX002_18 = #t1521601#6;
         eaX002_19 = #t1521601#19;
         eaX002_48 = eaNomEnt;

         |HAZ PresentaPortal;
     |FINSI;
|FPRC;

|PROCESO AbrePagina;
     eaPathAEAT = eaUnidadBasico + "\AEAT";        |RMKDIR eaPathAEAT;
     eaPathAEAT = eaPathAEAT + "\" + #dsmoy000#0;  |RMKDIR eaPathAEAT;
     eaPathAEAT = eaPathAEAT + "\" + #dsmoy000#6;  |RMKDIR eaPathAEAT;
     |SI #dsmoy000#4 ! 99;
         eaPathAEAT = eaPathAEAT + "\" + (("00" + #dsmoy000#4) % -102);  |RMKDIR eaPathAEAT;
     |FINSI;
     eaPathAEAT = eaPathAEAT + "\";

     eaFOrigen  = eaPathEmision  + eaFicheroPlano;
     eaFDestino = eaPathAEAT + eaFicheroPlano;
     |HAZ CopiaFichero;

     aAlfa       = eaPathLanza;
     eaPathLanza = eaPathAEAT;
     |HAZ TraeSoloWait;
     eaPathLanza = aAlfa;

     eaForzExplorador = "N";
     |HAZ Explorador;

     aAbre = eaPathAEAT + "imp" + Usuario + ".bat";
     |XABRE "WBC", aAbre, enHandlePortal;

     eaAlfa = eaPathAEAT + "wait ";
     |HAZ GrabaPortalSin;

     eaAlfa = eaPathExplorador + " " + &34;
     |HAZ GrabaPortalSin;

     eaAlfa = "https://www1.agenciatributaria.gob.es/wlpl/PAMW-M216/index.zul";
     |HAZ GrabaPortalSin;

     |XCIERRA enHandlePortal;

     |RSYSTEM aAbre;

     aAlfa = eaPathExplorador > "";
     aAlfa = aAlfa - "CHROME.EXE";
     |SI FSalida ! 0;  |SLEEP 30;  |FINSI;

     aAlfa = eaPathAEAT + aFichero;
     |RFBORRA aAlfa;
     |RFBORRA aAbre;
|FPRC;

|PROCESO TDeclaracion2015;
     enModelo = 216;  enForzar = 1;  |HAZ CogeDatosBancos;

     |SI #dsmod216#11 > 0;
         enNume = #dsmod216#11;  |HAZ Conver17;  #t1521601#18 = eaAlfa;

                                 #t1521601#6 = "I";
         |SI eaFormaPago = "3";  #t1521601#6 = "U";  |FINSI;
         |SI eaFormaPago = "6";  #t1521601#6 = "G";  |FINSI;

         #t1521601#19 = eaIBAN;
     |SINO;
         enNume = 0;  |HAZ Conver17;  #t1521601#18 = eaAlfa;
         #t1521601#6 = "N";
     |FINSI;
|FPRC;

|PROCESO PersonaFis2015;
     aAlfa = eaVarDni % 101;
     |SI aAlfa = "0"; |O aAlfa = "1"; |O aAlfa = "2"; |O aAlfa = "3";
      |O aAlfa = "4"; |O aAlfa = "5"; |O aAlfa = "6"; |O aAlfa = "7";
      |O aAlfa = "8"; |O aAlfa = "9"; |O aAlfa = "X"; |O aAlfa = "Y";
      |O aAlfa = "Z";
         eaAlfa = #clientes@#1;     |HAZ BuscaNombre;
         eaAlfa = eaAlfa1;    |HAZ QuitaRaros;     #t1521601#8   = eaAlfa;
         eaAlfa = eaAlfa2;    |HAZ QuitaRaros;     #t1521601#9   = eaAlfa;
      |SINO;
         eaAlfa   = #clientes@#1;    |HAZ QuitaRaros;    #t1521601#8   = eaAlfa;
                                                         #t1521601#9   = " ";
      |FINSI;
|FPRC;

|PROCESO Carga216_2015;
     enCodCli = #dsmod216#0;
     |HAZ LeePersona;

     |DDEFECTO #t1521601;

     |SI enSwDeDonde = 1;
         eaVarDni = #clientes@#3;  |HAZ CalculameDNI;   #t1521601#7  = eaVarDni;

         aAlfa1   = #datosper@#2;  |QBT aAlfa1;
         |SI aAlfa1 ! "";
             eaAlfa = #datosper@#2;  |HAZ QuitaRaros;  aAlfa1 = eaAlfa;  |QBF aAlfa1;
             eaAlfa = #datosper@#3;  |HAZ QuitaRaros;  aAlfa2 = eaAlfa;  |QBF aAlfa2;
             #t1521601#8 = aAlfa1 + " " + aAlfa2;

             eaAlfa = #datosper@#4;  |HAZ QuitaRaros;
             #t1521601#9 = eaAlfa;
         |SINO;
             |HAZ PersonaFis2015;
         |FINSI;
     |SINO;
         eaVarDni = #clientes@#13;  |HAZ CalculameDNI;   #t1521601#7   = eaVarDni;
         |HAZ PersonaFis2015;
     |FINSI;

     |HAZ TDeclaracion2015;

     #t1521601#10  = #dsmod216#1;

     |SI aPeriodo = "T";
         |SI #dsmod216#2 = 3;   #t1521601#11 = "1T";  |FINSI;
         |SI #dsmod216#2 = 6;   #t1521601#11 = "2T";  |FINSI;
         |SI #dsmod216#2 = 9;   #t1521601#11 = "3T";  |FINSI;
         |SI #dsmod216#2 = 12;  #t1521601#11 = "4T";  |FINSI;
     |SINO;
         #t1521601#11 = ("00" + #dsmod216#2) % -102;
     |FINSI;

     eaAlfa  = ("00000000000000000" + #dsmod216#6) % -117;    #t1521601#12 = eaAlfa;
     enNume = #dsmod216#7;    |HAZ Conver17;   #t1521601#13 = eaAlfa;
     enNume = #dsmod216#8;    |HAZ Conver17;   #t1521601#14 = eaAlfa;
     eaAlfa  = ("00000000000000000" + #dsmod216#9) % -117;    #t1521601#15 = eaAlfa;
     enNume = #dsmod216#10;   |HAZ Conver17;   #t1521601#16 = eaAlfa;
     enNume = #dsmod216#12;   |HAZ Conver17;   #t1521601#17 = eaAlfa;

     |SI #dsmod216#4 > 0;
         #t1521601#21 = "X";
         #t1521601#22 = #dsmom004#21;
     |FINSI;

     #t1521601#25 = &13 + &10;

     aAlfa = "<T2160" + #t1521601#10 + #t1521601#11 + "0000>";  |XGRABA nHandle2, aAlfa;
     aAlfa = "<AUX>";                                           |XGRABA nHandle2, aAlfa;
     aAlfa = " " * 70;                                          |XGRABA nHandle2, aAlfa;
     aAlfa = eaVersMod;                                         |XGRABA nHandle2, aAlfa;
     aAlfa = " " * 4;                                           |XGRABA nHandle2, aAlfa;
     aAlfa = eaNIFDs;                                           |XGRABA nHandle2, aAlfa;
     aAlfa = " " * 213;                                         |XGRABA nHandle2, aAlfa;
     aAlfa = "</AUX>";                                          |XGRABA nHandle2, aAlfa;

     aAlfa = "";
     |PARA nCampo = 1;  |SI nCampo [ 24;  |HACIENDO nCampo = nCampo + 1;
           aAlfa = #t1521601#nCampo#;
           |XGRABA nHandle2, aAlfa;
     |SIGUE;

     aAlfa = "</T2160" + #t1521601#10 + #t1521601#11 + "0000>";  |XGRABA nHandle2, aAlfa;
|FPRC;

|PROCESO dsmom002_02;
     |SI #dsmom002#8 > "01.01.0000";
         |SI #dsmom002#8 > fFecFin;  |ACABA;  |FINSI;
     |FINSI;

     |SI #dsmom002#9 > "01.01.0000";
         |SI #dsmom002#9 < fFecIni;  |ACABA;  |FINSI;
     |FINSI;

     |SI #dsmom002#7 = "TRIMESTRAL";
         aPeriodo = "T";
     |SINO;
         aPeriodo = "M";
     |FINSI;

     |ERROR10;
|FPRC;

|DEFBUCLE dsmom002_02;
     #dsmom002#1;
     ;
     #dsmom004#0, 01, 216;
     #dsmom004#0, 98, 216;
     ;
     NULL, dsmom002_02;
|FIN;

|PROCESO dsmow014_2015;
     |ABRE #dsmom004;
     #dsmom004#0  = #dsmow014#0;
     #dsmom004#1  = #dsmow014#1;
     #dsmom004#2  = #dsmow014#2;
     #dsmom004#3  = #dsmow014#5;
     #dsmom004#4  = #dsmow014#7;
     |LEE 040#dsmom004.=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;
     |CIERRA #dsmom004;

     |ABRE #dsmod216;
     #dsmod216#0 = #dsmom004#0;
     #dsmod216#1 = #dsmom004#1;
     #dsmod216#2 = #dsmom004#2;
     #dsmod216#3 = #dsmom004#3;
     #dsmod216#4 = #dsmom004#4;
     #dsmod216#5 = 0;
     |LEE 040#dsmod216.=;
     |SI FSalida ! 0;  |CIERRA #dsmod216;  |ACABA;  |FINSI;
     |CIERRA #dsmod216;

     aPeriodo = "T";
     |BUCLE dsmom002_02;

     eaFicheroPlano = "216" + (("00000" + #dsmow014#0) % -105) + ".txt";
     eaFicheroImpre = "216" + (("00000" + #dsmow014#0) % -105) + ".pdf";
     eaFicheroError = "err" + (("00000" + #dsmow014#0) % -105) + ".txt";

     aAlfa = eaPathLanza + eaFicheroPlano;  |RFBORRA aAlfa;
     aAlfa = eaPathLanza + eaFicheroImpre;  |RFBORRA aAlfa;
     aAlfa = eaPathLanza + eaFicheroError;  |RFBORRA aAlfa;

     aAbre = eaPathEmision + eaFicheroPlano;
     |XABRE "BW", aAbre, nHandle2;

     |HAZ Carga216_2015;

     |XCIERRA nHandle2;

     enEmpresa    = #dsmow014#0;
     enComple     = #dsmow014#7;
     eaNomEmpresa = #dsmow014#3;

     |HAZ PresentaModelo_02;
|FPRC;

