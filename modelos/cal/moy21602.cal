     |FINSI;

     |LEE 000#dsmom107.p;
     |SI FSalida ! 0;  |DDEFECTO #dsmom107;  |FINSI;
     |CIERRA #dsmom107;

     aAlfa1 = #dsmoy000#6;
     aAlfa1 = "01.01." + aAlfa1; fFecIni = aAlfa1;
     aAlfa1 = #dsmoy000#6;
     aAlfa1 = "31.12." + aAlfa1; fFecFin = aAlfa1;

     enSwTrata      = 0;
     enCambiaMoneda = 1;

     |HAZ Bloque2015;
|FPRC;
