|PROCESO Bloque2015;
     |SI #dsmoy000#6 [ 2017;
         |HAZ BtnPeriodicas2015;
     |FINSI;

     |SI #dsmoy000#6 = 2018;
         |HAZ BtnPeriodicas2018;
     |FINSI;

     |SI #dsmoy000#6 ] 2019;
         |HAZ BtnPeriodicas2019;
     |FINSI;

     |SI enTecla = 0;  |ACABA;  |FINSI;

     nSwLee = 0;
     |SI #dsmoy000#8 = "C";
         |BUCLE dsmow014C_2015;
     |SINO;
         |BUCLE dsmow014N_2015;
     |FINSI;

     |HAZ SacaErrorPtl;

     |SI eaOpcion = "3";
         aAlfa =  "      Los ficheros generados estan: " + eaPathInternet + " de su PC";
         |MENAV aAlfa;
         |SI #dsmoy000#6 [ 2018;
             |CONFI "0000SQuiere cargar la pagina de hacienda del modelo";
             |SI FSalida = 0;
                 |HAZ AbrePagina;
             |FINSI;
         |FINSI;
     |FINSI;

     |SI enDirecta = 1;  |Y enMsv = 1;  |Y enParar = 0;
         |SUB_EJECUTA "|dsmox007";
     |FINSI;
|FPRC;

|PROCESO moy21602;
