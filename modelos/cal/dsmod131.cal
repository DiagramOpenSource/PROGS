|| \\\\\\ Consulta del Modelo ////////

     |SI enTipoEntrada = 4;              || Consulta del Modelo
         |CABEZA "Modelo 0";

         |ABRE #0;
         #0#0 = enEmpresa;
         #0#1 = enEjer;
         #0#2 = enPeriodo;
         #0#3 = enModelo;
         #0#4 = enComplem;
         #0#5 = enActividad;
         |LEE 000#0=;
         |SI FSalida = 0;
             |SI #0#1 [ 2006;
                 #0#28 = #0#26 - #0#27;
                 |SI #0#28 < 0;  #0#28 = 0;  |FINSI;

                 #0#32 = #0#30 - #0#31;
                 |SI #0#32 < 0;  #0#32 = 0;  |FINSI;

                 #0#36 = #0#26 + #0#35 + #0#30;
                 #0#37 = #0#27 + #0#31;
                 #0#38 = #0#28 - #0#32;
                 #0#33 = #0#28 - #0#32;
             |SINO;
                 #0#36 = #0#26 + #0#35 + #0#30;
                 #0#38 = #0#36 - #0#37 - #0#42;
                 #0#40 = #0#38 - #0#39 - #0#44;
                 #0#33 = #0#40 - #0#41;
             |FINSI;
             |GRABA 020#0a;
             |LIBERA #0;
        |FINSI;
        |CIERRA #0;

         |SI enEjer [ 2007;
             |HAZ SacaPanta2007_131;
         |FINSI;

         |SI enEjer ] 2008;
             |SI enPeriodo [ 3; |Y enEjer = 2008;
                 |HAZ SacaPanta2007_131;
             |SINO;
                 |HAZ SacaPanta2008_131;
             |FINSI;
         |FINSI;
         |ACABA;
     |FINSI;

