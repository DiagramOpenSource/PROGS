             aAlfa = aAlfa % 0402;
             #102#5 = aAlfa;
             |SI #104#149 = "T";
                 |SI aAlfa ] "01"; |Y aAlfa [ "03"; #102#5 = 1; |FINSI;
                 |SI aAlfa ] "04"; |Y aAlfa [ "06"; #102#5 = 2; |FINSI;
                 |SI aAlfa ] "07"; |Y aAlfa [ "09"; #102#5 = 3; |FINSI;
                 |SI aAlfa ] "10"; |Y aAlfa [ "12"; #102#5 = 4; |FINSI;
             |SINO;
                 |SI #104#149 = "A"; #102#5 = 0; |FINSI;
              |FINSI;
         |FINSI;
