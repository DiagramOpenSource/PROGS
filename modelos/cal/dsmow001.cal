     |LEE 101#900=;
     |SI FSalida = 0;
         #900#4 = aCampo4;
         #900#5 = aCampo5;
         |GRABA 020#900a;
     |FINSI;
     |LIBERA #900;

     |SI nPrimero = 300; |O nPrimero = 310; |O nPrimero = 303;
         #900#0 = 347;
         |LEE 101#900=;
         |SI FSalida = 0;
             #900#4 = aCampo4;
             #900#5 = aCampo5;
             |GRABA 020#900a;
         |FINSI;
         |LIBERA #900;

         #900#0 = 349;
         |LEE 101#900=;
         |SI FSalida = 0;
             #900#4 = aCampo4;
             #900#5 = aCampo5;
             |GRABA 020#900a;
