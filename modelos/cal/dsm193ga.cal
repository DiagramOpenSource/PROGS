|FICHEROS;
     dsm193ca;
     dsm193ga;

     &regisnif@;
|FIN;

|VARIABLES;
     nCampo      = 0;
     nPtec       = 0;
     nCampoD     = 0;
     nCampoO     = 0;
     nForma      = 0;

     aMenav      = "";
     aPathModelo = "";
     aDef        = "";
     aAlfa       = "";
     aTecla      = "";

     &enError    = 0;
|FIN;

|INCLUYE i_variar;

|PROCESO PonDatosNifga;  |TIPO 7;
     eaCodNif  = #dsm193ga#6;
     |HAZ LeeNifs;

     #dsm193ga#8 = #regisnif@#1;   |PINTA #dsm193ga#8;
|FPRC;

|PROCESO Tipo2m193ga;  |TIPO 2;
     #dsm193ca#6  = #dsm193ca#6  +. 1;
     #dsm193ca#12 = #dsm193ca#12 +. #dsm193ga#9;
|FPRC;

|PROCESO Tipo11m193ga;  |TIPO 11;
     |SI FSalida = 10;
         eaVarDni  = #dsm193ga#Campo#;
         enCalcCif = 1;
         |HAZ CalculameDNI;
         #dsm193ga#Campo# = eaVarDni;
         |PINTA #dsm193ga#Campo#;
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO MiraDni193ga;  |TIPO 6;
     eaVarDni  = #dsm193ga#Campo#;
     enCalcCif = 0;
     |HAZ CalculameDNI;
     eaVarDni = (eaVarDni + "             ") % 112;
     |SI #dsm193ga#Campo# ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #dsm193ga#Campo# = eaVarDni;  |PINTA #dsm193ga#Campo#;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;
