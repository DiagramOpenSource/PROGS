|FICHEROS;
     dsmod309;
|FIN;

|VARIABLES;
  &enDatosTec  = 0;
  &enActividad = 0;
  &enEjer      = 0;
  &enPeriodo   = 0;
  &enComplem   = 0;
|FIN;

|INCLUYE i_varemi;

|PROGRAMA;
     |ABRE #dsmod309;
     #dsmod309#0 = enEmpresa;
     #dsmod309#1 = enActividad;
     #dsmod309#2 = enEjer;
     #dsmod309#3 = enPeriodo;
     #dsmod309#4 = enComplem;
     #dsmod309#5 = enDatosTec;
     |LEE 041#dsmod309.=;
     |SI FSalida ! 0;
         |CIERRA #dsmod309;
         |ACABA;
     |FINSI;

     |SI enEjer [ 2015;
         |SI eaOpcion = "1";
             |HAZ BloquePapel;
         |SINO;
             |HAZ Bloque2015;
         |FINSI;
     |FINSI;

     |HAZ eVersionMod;

     |SI enEjer = 2016; |O enEjer = 2017;
         |HAZ Bloque2016;
     |FINSI;

     |SI enEjer ] 2018;
         |HAZ Bloque2018;
     |FINSI;
     |CIERRA #dsmod309;
|FPRO;
