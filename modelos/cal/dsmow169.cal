|FICHEROS;
     dsmow169,MANTE;

     dsm39001;
     :/basica/def/agicen14;

     &regisnif@;
|FIN;

|VARIABLES;
     nCampo1 = 0;
     nCampo2 = 0;
     nCampo3 = 0;

     aAlfa   = "";
|FIN;

|INCLUYE i_variar;

|PROCESO Tipo0C28Fw169;  |TIPO 0;
     |C_M #dsmow169#30, 1, "S";
     |C_M #dsmow169#31, 1, "S";
     |C_M #dsmow169#32, 1, "S";

     |C_M #dsmow169#42, 1, "S";

     |SI #dsmow169#28 = "N";
         #dsmow169#42 = "";  |C_M #dsmow169#42, 1, "N";  |PINTA #dsmow169#42;

         #dsmow169#30 = "N";   |C_M #dsmow169#30, 1, "N";  |PINTA #dsmow169#30;
         #dsmow169#31 = "N";   |C_M #dsmow169#31, 1, "N";  |PINTA #dsmow169#31;
         #dsmow169#32 = "N";   |C_M #dsmow169#32, 1, "N";  |PINTA #dsmow169#32;
     |FINSI;
|FPRC;

|PROCESO Tipo0C31Fw169;  |TIPO 0;
     |C_M #dsmow169#32, 1, "S";
     |C_M #dsmow169#33, 1, "S";

     |SI #dsmow169#31 = "N";
         #dsmow169#32 = "N";   |C_M #dsmow169#32, 1, "N";  |PINTA #dsmow169#32;
         #dsmow169#33 = "";    |C_M #dsmow169#33, 1, "N";  |PINTA #dsmow169#33;
     |FINSI;
|FPRC;

|PROCESO Actividadw169;
     nCampo1 = Campo + 1;
     nCampo2 = Campo + 2;
     nCampo3 = Campo + 3;

     |C_M #dsmow169#nCampo3#, 1, "S";
     |SI #dsmow169#Campo# = 0;
         #dsmow169#nCampo1# = "";  |PINTA #dsmow169#nCampo1#;
         #dsmow169#nCampo2# = "";  |PINTA #dsmow169#nCampo2#;
         #dsmow169#nCampo3# = 0;   |PINTA #dsmow169#nCampo3#;
         |C_M #dsmow169#nCampo3#, 1, "N";

         |ACABA;
     |FINSI;

     |ABRE #agicen14;
     #agicen14#0 = #dsm39001#0;
     #agicen14#1 = #dsmow169#Campo#;
     |LEE 040#agicen14.=;
     |SI FSalida ! 0;
         |MENAV "     No existe Actividad";
         |ERROR;
     |SINO;
         aAlfa = #agicen14#5 % -104;
         |SI aAlfa ! "0000";
             aAlfa = "31.12." + #dsm39001#1;
             |SI #agicen14#5 > aAlfa;
                 |MENAV "    La Actividad se inicia en a¤os posteriores."
                 #dsmow169#Campo#   = 0;   |PINTA #dsmow169#Campo#;
                 #dsmow169#nCampo1# = "";  |PINTA #dsmow169#nCampo1#;
                 #dsmow169#nCampo2# = "";  |PINTA #dsmow169#nCampo2#;
                 #dsmow169#nCampo3# = 0;   |PINTA #dsmow169#nCampo3#;
                 |ACABA;
             |FINSI;
         |FINSI;

         aAlfa = #agicen14#6 % -104;
         |SI aAlfa ! "0000";
             aAlfa = "01.01." + #dsm39001#1;
             |SI #agicen14#6 < aAlfa;
                 |MENAV "    La Actividad a finalizado en a¤os anteriores.";
                 #dsmow169#Campo#   = 0;   |PINTA #dsmow169#Campo#;
                 #dsmow169#nCampo1# = "";  |PINTA #dsmow169#nCampo1#;
                 #dsmow169#nCampo2# = "";  |PINTA #dsmow169#nCampo2#;
                 #dsmow169#nCampo3# = 0;   |PINTA #dsmow169#nCampo3#;
                 |ACABA;
             |FINSI;
         |FINSI;

         |SI #agicen14#7 = 1;
             |MENAV "    La Actividad tiene IVA Exento.";
             #dsmow169#Campo#   = 0;   |PINTA #dsmow169#Campo#;
             #dsmow169#nCampo1# = "";  |PINTA #dsmow169#nCampo1#;
             #dsmow169#nCampo2# = "";  |PINTA #dsmow169#nCampo2#;
             #dsmow169#nCampo3# = 0;   |PINTA #dsmow169#nCampo3#;
             |ACABA;
         |FINSI;

         #dsmow169#nCampo1# = #agicen14#3;  |PINTA #dsmow169#nCampo1#;
         #dsmow169#nCampo2# = #agicen14#4;  |PINTA #dsmow169#nCampo2#;

         |SI #agicen14#7  = 11;  |O #agicen14#7 = 13;
             #dsmow169#25 = "X";
         |FINSI;

         |SI #agicen14#16 = 1;  |O #agicen14#16 = 2;
             #dsmow169#nCampo3# = 1;  |PINTA #dsmow169#nCampo3#;
         |FINSI;

         |SI #agicen14#16 = 4;  |O #agicen14#16 = 5;
             #dsmow169#nCampo3# = 2;  |PINTA #dsmow169#nCampo3#;
         |FINSI;

         |SI #agicen14#3 = "8612"; |O #agicen14#3  = "8611";
             #dsmow169#nCampo3# = 3;  |PINTA #dsmow169#nCampo3#;
         |FINSI;

         |SI #agicen14#16 = 3;
             #dsmow169#nCampo3# = 1;  |PINTA #dsmow169#nCampo3#;
         |FINSI;

         |SI #agicen14#16 = 6;
             #dsmow169#nCampo3# = 6;  |PINTA #dsmow169#nCampo3#;
         |FINSI;

         |SI #agicen14#3 = "9999";  |Y #agicen14#16 = 3;
             #dsmow169#nCampo3# = 4;  |PINTA #dsmow169#nCampo3#;
         |FINSI;

         |SI #agicen14#3 = "9999";  |Y #agicen14#16 = 6;
             #dsmow169#nCampo3# = 6;  |PINTA #dsmow169#nCampo3#;
         |FINSI;

         |SI #dsmow169#nCampo3# = 0;  |O #dsmow169#nCampo3# = 4;  |O #dsmow169#nCampo3# = 5;
          |O #dsmow169#nCampo3# = 6;
             #dsmow169#nCampo1# = "";  |PINTA #dsmow169#nCampo1#;
         |FINSI;
     |FINSI;

     |CIERRA #agicen14;
|FPRC;

|PROCESO LetraNifw169;  |TIPO 6;
     |SI FSalida = 10;
         eaVarDni  = #dsmow169#Campo#;
         enCalcCif = 1;
         |HAZ CalculameDNI;
         #dsmow169#Campo# = eaVarDni;
         |PINTA #dsmow169#Campo#;
         |ERROR;
         |ACABA;
     |FINSI;

     eaVarDni  = #dsmow169#Campo#;
     enCalcCif = 0;
     |HAZ CalculameDNI;
     eaVarDni = (eaVarDni + "             ") % 112;
     |SI #dsmow169#Campo# ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #dsmow169#Campo# = eaVarDni;  |PINTA #dsmow169#Campo#;
             |ERROR;
             |ACABA;
         |FINSI;
     |FINSI;

     eaCodNif  = #dsmow169#Campo#;
     |HAZ LeeNifs;

     #dsmow169#27 = #regisnif@#1;   |PINTA #dsmow169#27;
|FPRC;

|PROCESO Tipo12w169;  |TIPO 12;
|FPRC;
