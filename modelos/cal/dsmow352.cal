|FICHEROS;
   dsmow352 #0,MANTE;
|FIN;

|PROCESO MiraCarga;
   |SI #dsmow352#1 = "M";
      |C_M #dsmow352#2, 1, "N";
      |C_M #dsmow352#3, 1, "N";
      |C_M #dsmow352#8, 1, "N";
      #dsmow352#2 = 0;   |PINTA #dsmow352#2;
      #dsmow352#3 = " "; |PINTA #dsmow352#3;
      #dsmow352#8 = " "; |PINTA #dsmow352#8;
   |SINO;
      |C_M #dsmow352#2, 1, "S";
      |C_M #dsmow352#3, 1, "S";
      |C_M #dsmow352#8, 1, "S";
      |SI #dsmow352#3 = " "; #dsmow352#3 = "N"; |FINSI;
      |SI #dsmow352#8 = " "; #dsmow352#8 = "N"; |FINSI;
      |PINTA #dsmow352#3;
      |PINTA #dsmow352#8;
   |FINSI;
|FPRC;

|PROCESO MiraCpostal;
   |C_M #dsmow352#7,1,"S";
   |SI #dsmow352#6 = 0;
       |C_M #dsmow352#7,1,"N";
       #dsmow352#7 = 0;
       |PINTA #dsmow352#7;
   |FINSI;
|FPRC;
