|FICHEROS;
     dsmoz012 #0;
     dsmod309 #309;
     dsmow069 #999;
|FIN;

|VARIABLES;
     &enBoton1 = 0;
     &enBoton2 = 0;
|FIN;

|INCLUYE i_variar;

|RUTINA ClaveBaja309;
     #309#0 = #999#0;
     #309#1 = #999#1;
     #309#2 = #999#2;
     #309#3 = 01;
     #309#4 = 00;
     #309#5 = 01;
|FRUT;

|RUTINA ClaveAlta309;
     #309#0 = #999#0;
     #309#1 = #999#1;
     #309#2 = #999#2;
     #309#3 = 99;
     #309#4 = 99;
     #309#5 = 99;
|FRUT;

|PROCESO Tipo7;  |TIPO 7;
     |ENTLINEAL #1#309, -4, 7, 20, 0, ClaveBaja309, ClaveAlta309;
|FPRC;

|PROCESO Tipo11;  |TIPO 11;
     |SI FSalida = 10;
         enEmpresa = #dsmow069#0;
         |HAZ MiraBloqueo;
         |SI eaBloqueo = "3";
             |ACABA;
         |FINSI;

         |ENTLINEAL #1#309, -4, 2, 20, 0, ClaveBaja309, ClaveAlta309;
     |FINSI;

     |SI FSalida = 11;
         ||HAZ EmisionLinea;
     |FINSI;

     |SI FSalida = 12;
         ||HAZ EmisionTodo;
     |FINSI;
|FPRC;
