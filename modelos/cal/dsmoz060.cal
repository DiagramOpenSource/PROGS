|PROCESO dsmom004;
     aAlfa = "0000 Chequeando la Empresa : " + (("00000" + #4#0) % -105);
     |MENSA aAlfa;

     |SI #4#5 = "ACTIVIDAD ";  |O #4#5 = "ACT.+ REG.";
         enExistencia    = 0;
         nSinDatos       = 0;
         nContador       = 0;
         nRegulariza     = 0;
         enResultado     = 0;
         nExistencia0    = 0;
         |BUCLE dsmom008;

         |SI nSinDatos = 0;  |Y nRegulariza = 0;
             #4#17 = "SIN DATOS   ";
             |SI nExistencia0 ! 0;  |Y enResultado ! 0;
                 |PRINT;
                 nSwHay = 1;
             |FINSI;
         |FINSI;

         |SI nRegulariza ! 0;
             #4#17 = "PARCIALES   ";
         |FINSI;

         |SI nSinDatos ! 0;  |Y nSinDatos ! nContador;
             #4#17 = "PARCIALES   ";
         |FINSI;

         |SI nSinDatos = nContador;  |Y nSinDatos ! 0;
             #4#17 = "COMPLETADO  ";
         |FINSI;

         |GRABA 020#4a;
         |LIBERA #4;

         |SI #4#15 ! enResultado;
             |PRINT;
             nSwHay = 1;
         |FINSI;

         |ACABA;
     |FINSI;

     enTipoEntrada = 99;
     enExistencia  = 0;
     enEmpresa     = #4#0;
     enEjer        = #4#1;
     enPeriodo     = #4#2;
     enModelo      = #4#3;
     enComplem     = #4#4;
     enActividad   = 0;

     |HAZ EntraModeloPorEl4;

     |SI enExistencia = 0;
         |SI #4#17 = "SIN DATOS   ";
             |PRINT;
             nSwHay = 1;
             |ACABA;
         |FINSI;
     |SINO;
         |SI #4#17 ! "SIN DATOS   ";
             |PRINT;
             nSwHay = 1;
             |ACABA;
         |FINSI;
     |FINSI;

     |SI #4#15 ! enResultado;
         |PRINT;
         nSwHay = 1;
         |ACABA;
    |FINSI;
|FPRC;

