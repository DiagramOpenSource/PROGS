|PROCESO dsmom008;
     enTipoEntrada = 99;
     enExistencia  = 0;
     enEmpresa     = #8#0;
     enEjer        = #8#1;
     enPeriodo     = #8#2;
     enModelo      = #8#3;
     enComplem     = #8#4;
     enActividad   = #8#5;

     |SI #8#5 ! 0;  |Y #8#5 ! 99;
         nContador     = nContador + 1;
     |FINSI;

     |HAZ EntraModeloPorEl8;

     |SI #8#5  = 0;    |ACABA;  |FINSI;
     |SI #8#12 = 370;  |ACABA;  |FINSI;

     |SI enExistencia = 0;
         |SI #8#5 = 99;
             nRegulariza = 1;
             |ACABA;
         |FINSI;
         nSinDatos = nSinDatos + 1;
     |FINSI;
|FPRC;

