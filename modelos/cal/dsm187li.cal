     |SI aAlfa = "N"; |ACABA; |FINSI;

     |SI #dsm187li#27 = " ";
         |MENAV "0000Introduzca A o B";
         |ERROR;
         |ACABA;
     |FINSI;

     |C_M #dsm187li#28, 1, "S";
     |C_M #dsm187li#29, 1, "S";
     |C_M #dsm187li#30, 1, "S";
     |C_M #dsm187li#31, 1, "S";

     |SI #dsm187li#27 = "B";
         |C_M #dsm187li#28, 1, "N";  #dsm187li#28 = "01.01.0000"; |PINTA #dsm187li#28;
         |C_M #dsm187li#29, 1, "N";  #dsm187li#29 = 0; |PINTA #dsm187li#29
         |C_M #dsm187li#30, 1, "N";  #dsm187li#30 = 0; |PINTA #dsm187li#30
         |C_M #dsm187li#31, 1, "N";  #dsm187li#31 = 0; |PINTA #dsm187li#31
     |FINSI;
|FPRC;

|PROCESO Tipo0C28a31F187li;  |TIPO 0;
     |C_M #dsm187li#Campo#, 0, aAlfa;
     |SI aAlfa = "N"; |ACABA; |FINSI;

     nOk = 0;
     |SI Campo = 28; |Y #dsm187li#Campo# [ "01.01.0000"; nOk = 1; |FINSI;
     |SI Campo ! 28; |Y #dsm187li#Campo# = 0;            nOk = 1; |FINSI;
     |SI nOk = 1;
         |MENAV "0000Introduzca valor";
         |ERROR;
     |FINSI;
|FPRC;
