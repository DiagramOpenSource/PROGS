|FICHEROS;
     reauxpai #0;
|FIN;

|VARIABLES;
     aAlfa   = "";
     aAlfa1  = "";
     aAlfa2  = "";
     nHandle = 0;
|FIN;

|PROCESO RecogePaises;
     |DBASE aAlfa;   |QBF aAlfa;
     aAlfa = aAlfa + "CogeTxt/";
     aAlfa = aAlfa + "zpaises.txt";
     |XABRE "U", aAlfa, nHandle;
     |DDEFECTO #0;
     |PARA;  |SI  FSalida ] 0;  |HACIENDO;
             |XLEE nHandle, 60, aAlfa1;
             |SI FSalida < 0;  |SAL;  |FINSI;

             aAlfa2 = aAlfa1 % 103;    #0#0  = aAlfa2;
             |LEE 101#0=;
             |SI FSalida ! 0;
                 |DDEFECTO #0;
                 aAlfa2 = aAlfa1 % 103;    #0#0  = aAlfa2;
                 aAlfa2 = aAlfa1 % 430;    #0#1  = aAlfa2;
                 |GRABA 020#0n;
             |FINSI;
             |LIBERA #0;
     |SIGUE;
     |XCIERRA nHandle;
|FPRC;

|PROCESO Tipo8Paises;  |TIPO 8;
     |ACABA;

     |ABRE #0;
     |LEE 040#0p;
     |SI FSalida ! 0;
         |HAZ RecogePaises;
     |FINSI;
     |CIERRA #0;
|FPRC;
