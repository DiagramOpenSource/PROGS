     |ATAR aAlfa, aPathRenta;

     aAlfa = aPathRenta + aFichero + ".tar";
     |COMPRIME aAlfa;

     aAlfa = aPathRenta + aFichero + ".tar";  |FBORRA aAlfa;
     aAlfa = aPathRenta + aFichero + ".txt";  |FBORRA aAlfa;

     aOrigen  = aPathRenta + aFichero + ".tgz";
     aDestino = eaUnidadBasico + "\MODELOS\TMP\"  + aFichero + ".tgz";

     |RFBORRA aDestino;
     aIPRemoto = "";
     |IP_REMOTO aIPRemoto;  |QBT aIPRemoto;

     |SI aIPRemoto ! "";
         |ENVIA_FICHERO aOrigen, aDestino;
     |SINO;
         |COPIA_FICHERO aOrigen, aDestino;
     |FINSI;

     Pos    = -1;
     aAlfa  = eaUnidadBasico + "\MODELOS\TMP\" + aFichero + ".tgz";
     aAlfa1 = eaUnidadBasico + "\MODELOS\TMP\";
     |RDESCOMPRIME aAlfa;
     |RDETAR aAlfa, aAlfa1;

     aAlfa = aPathRenta + aFichero + ".tgz";         |FBORRA aAlfa;
     aAlfa = eaUnidadBasico + "\MODELOS\TMP\"  + aFichero + ".tgz";  |RFBORRA aAlfa;
     aAlfa = eaUnidadBasico + "\MODELOS\TMP\"  + aFichero + ".tar";  |RFBORRA aAlfa;
|FPRC;

|PROCESO CreaFicheros;
     aFichero = "003" + (("00000" + #1001#0) % -105);

     |DBASE aPathRenta;  |QBT aPathRenta;
     aPathRenta = aPathRenta + "tmp";  |MKDIR aPathRenta;
     aPathRenta = aPathRenta + "/";

     aOrigen    = aPathRenta + aFichero + ".txt";

     |XABRE "WB", aOrigen, nHandle2;

     |SI #1001#2 ! "ZPATRITIT ";  |Y #1001#2 ! "ZPATRICON ";
         |BUCLE rem00901;
     |SINO;
         |BUCLE rem00902;
     |FINSI;

     |XCIERRA nHandle2;

     |HAZ TraeTxtComprimido;

     aDestino = eaUnidadBasico + "\MODELOS";      |RMKDIR aDestino;
     aDestino = eaUnidadBasico + "\MODELOS\TMP";  |RMKDIR aDestino;
     aDestino = eaUnidadBasico + "\MODELOS\TMP\"  + "003" + (("00000" + #1001#0) % -105) + ".TXT";
|FPRC;

|PROCESO LanzaPrograma;
     |PULSATECLA;

     eaNomDef    = "";
     enLineal    = 0;
     enSwRetorno = 0;
     aAlfa       = aPrograma % -102;
     |SI aAlfa = "LI";
         eaNomDef = aPrograma % 108;
         enLineal = 1;

         |HAZ PresentaPanta;

         |ACABA;
     |FINSI;

     |SI aAlfa = "PA";
         eaNomDef = aPrograma % 108;
         enLineal = 0;

         |HAZ PresentaPanta;

         |ACABA;
     |FINSI;

     aAlfa = aPrograma % 103;
     |SI aAlfa ! "LIQ";
         aAlfa   = aPrograma % 108;
         eaLetra = aPrograma % 902;

         |SUB_EJECUTA_NP aAlfa;
     |FINSI;
|FPRC;

|PROCESO CreaMenu;
     |HAZ CreaFicheros;

     aPrograma = "";

     |SI aGuardaProg ! "";
         aPrograma       = aGuardaProg;
         aGuardaProg = "";
     |FINSI;

     aTitulo   = (("00000" + #1001#0) % -105) + " - " + #1001#7;  |QBF aTitulo;
     aTitulo   = aTitulo + " - " + #1001#2 + " - RESULTADO: " + #1001#3;
     |PARA;  |SI;  |HACIENDO;
          aAbre = eaUnidadBasico + "\MODELOS\TMP\"  + "002" + (("00000" + #1001#0) % -105) + ".TXT";
          |XABRE "WCB", aAbre, nHandle;

          aAlfa = ((aDestino + (" " * 99)) % 198) + &13 + &10;
          |XGRABA nHandle, aAlfa;

          aAlfa = ((aTitulo + (" " * 99)) % 198) + &13 + &10;
          |XGRABA nHandle, aAlfa;

          aAlfa = ((aPrograma + (" " * 99)) % 198) + &13 + &10;
          |XGRABA nHandle, aAlfa;

          |SI #1001#2 ! "ZPATRITIT ";  |Y #1001#2 ! "ZPATRICON ";
              aAlfa = (("RE" + (" " * 99)) % 198) + &13 + &10;
          |SINO;
              aAlfa = (("PA" + (" " * 99)) % 198) + &13 + &10;
          |FINSI;
          |XGRABA nHandle, aAlfa;

          aAlfa = ("00000" + #1001#0) % -105;
          aAlfa = aAlfa + (("00" + #1001#1) % -102);
          aAlfa = aAlfa + ((#1001#2 + (" " * 99)) % 198) + &13 + &10;
          |XGRABA nHandle, aAlfa;

          |XCIERRA nHandle;

          aAlfa    = "|:ren2009/rez99999;" + aAbre;
          |SUB_EJECUTA_NP aAlfa;

          nLinea = 1;
          |XABRE "ABC",  aAbre,  nHandle;
          |PARA;  |SI;  |HACIENDO;
              |XLEE nHandle, 100, aAlfa;
              |SI FSalida [ 0;  |SAL;  |FINSI;

              |SI nLinea = 1;  aAbre     = aAlfa % 198;  |FINSI;
              |SI nLinea = 2;  aTitulo   = aAlfa % 198;  |FINSI;
              |SI nLinea = 3;  aPrograma = aAlfa % 198;  |FINSI;

              nLinea = nLinea + 1;
          |SIGUE;
          |XCIERRA nHandle;

          |QBF aAbre;
          |QBF aTitulo;
          |QBF aPrograma;

          |SI aPrograma = "";  |SAL;  |FINSI;

          aAlfa = aPrograma % 106;
          |SI aAlfa = "CAMBIO";
              aPrograma = aPrograma % 898;
              #1001#2 = aPrograma;
              |LEE 000#1001=;

              |PTEC 824;
              |SAL;
          |FINSI;

          |HAZ LanzaPrograma;

          |SI enSwRetorno = 1;
              aGuardaProg = aPrograma;
              aPrograma   = "";
              |PTEC 806;
          |FINSI;

          |SI aPrograma = "";
              |SAL;
          |FINSI;
     |SIGUE;

     aAbre = eaUnidadBasico + "\MODELOS\TMP\"  + "002" + (("00000" + #1001#0) % -105) + ".TXT";
     |RFBORRA aAbre;

     aAbre = eaUnidadBasico + "\MODELOS\TMP\"  + "003" + (("00000" + #1001#0) % -105) + ".TXT";
     |RFBORRA aAbre;
|FPRC;

|PROCESO Tipo11;  |TIPO 11;
     |SI FSalida = 10;
         enModoLiq = 4;
         |HAZ CreaMenu;
         |ERROR;
         |ACABA;
     |FINSI;

     |SI FSalida = 11;
         |SI #1001#2 ! "ZPATRITIT ";  |Y #1001#2 ! "ZPATRICON ";
             |ACABA;
         |FINSI;

         |LEE 101#1001=;
         |SI FSalida = 0;
             |ENCAMPO #9#1001;
             |GRABA 020#1001a;
             |LIBERA #1001;
         |FINSI;
         |ACABA;
     |FINSI;

     |SI FSalida = 13;  |Y enModo ! 4;
           ||MENAV "    La Renta aun no es la definitiva. ";  ||Quitar
           ||ACABA;
         |HAZ Seleccion;
         |ERROR;
     |FINSI;

     |SI FSalida = 14;  |Y enModo ! 4;
         |SI #1001#2 ! "ZPATRITIT ";  |Y #1001#2 ! "ZPATRICON ";
             |MENAV "       Esta linea no es de Patrimonio.";
             |ACABA;
         |FINSI;

         |LEE 101#1001=;
         |SI #1001#5 = "714";
             #1001#5 = "";
         |SINO;
             #1001#5 = "714";

         |FINSI;
         |GRABA 020#1001a;
         |LIBERA #1001;

         |PINTA #1001#5;
     |FINSI;

     |SI FSalida = 16;
         |PUSHV 0501 2380;
         |ABRE #1003;
         #1003#0 = enEmpresa;
         #1003#1 = "        ";
         #1003#2 = 1;
         |LEE 040#1003];
         |SI FSalida = 0;  |Y  #1003#0 = enEmpresa;
             |ENTLINEAL #1#1003, -3, 4, 22, 1, ClaveBaja1003, ClaveAlta1003;
         |SINO;
             |MENAV "     No hay Rendimientos marcados con X";
         |FINSI;
         |CIERRA #1003;
         |POPV;
     |FINSI;

     |SI FSalida = 17;  |Y enModo ! 4;
         |SI #1001#2 ! "ZPATRITIT ";  |Y #1001#2 ! "ZPATRICON ";
             |MENAV "       Esta linea no es de Patrimonio.";
             |ACABA;
         |FINSI;

         |LEE 101#1001=;
         |SI FSalida ! 0;     |ACABA;  |FINSI;
         |SI #1001#5 ! "714"; |LIBERA #1001;  |ACABA;  |FINSI;

         |PUSHV 0501 2380;
         |BLANCO 1115 2359;
         |CUADRO 1115 2359;
         |ATRI R;
         |PINTA 1216, "     ACTIVADO COMPLEMENTARIA PATRIMONIO    ";
         |ATRI r;
         |PINTA 1416, "    Resultado Patrimonio : ";
         |PINTA 1516, "    Resultado Anteriores : ";
         |PINTA 1716, "    Resultado Patrimonio : ";

         |HAZ LeePatrimonio;
         |PINTA 1443, #1002#6;

         |ET Anterior;
             nAnterior = 0;
             E_inf     = "-999999999.?EURO";
             E_sup     = "9999999999";
             nAnterior = 1543 ? 1;
             |SI FSalida = 2;  |VETE Anterior;  |FINSI;

         enImporte = #1002#6 - nAnterior;
         |SI enImporte < 0;
             |MENAV "0000 La Renta se queda negativa, no se puede imprimir, debera hacer un escrito para Hacienda.";
         |FINSI;

         #1002#6   = enImporte;
         |PINTA 1743, #1002#6;

         |ABRE #1002;
         #1002#0 = #1001#0;
         #1002#1 = #1001#1;
         #1002#2 = #1001#2;
         #1002#3 = 12;
         #1002#4 = "054";
         |LEE 101#1002=;
         |SI FSalida ! 0;
             #1002#0 = #1001#0;
             #1002#1 = #1001#1;
             #1002#2 = #1001#2;
             #1002#3 = 12;
             #1002#4 = "054";
             #1002#5 = "Cuota a Ingresar Complementaria ";
             |GRABA 020#1002n;
             |LIBERA #1002;
         |FINSI;
         |CIERRA #1002;

         enBloque  = 12;
         eaCasilla = "054";
         |HAZ GrabaComple;

         |ABRE #1002;
         #1002#0 = #1001#0;
         #1002#1 = #1001#1;
         #1002#2 = #1001#2;
         #1002#3 = 12;
         #1002#4 = "053";
         |LEE 101#1002=;
         |SI FSalida ! 0;
             #1002#0 = #1001#0;
             #1002#1 = #1001#1;
             #1002#2 = #1001#2;
             #1002#3 = 12;
             #1002#4 = "053";
             #1002#5 = "Cuota declaracion anterior";
             |GRABA 020#1002n;
             |LIBERA #1002;
         |FINSI;
         |CIERRA #1002;

         #1001#3 = enImporte;

         enImporte = nAnterior;
         enBloque  = 12;
         eaCasilla = "053";
         |HAZ GrabaComple;

         |LEETECLA aAlfa;

         |POPV;

         |GRABA 020#1001a;
         |LIBERA #1001;

         |PINTA #1001#3;
     |FINSI;

     |SI FSalida = 18;
         enDesdeRenta2009 = 1;
         |SUB_EJECUTA_NP ":ren2008/rem01001";

         |ERROR;
     |FINSI;

     |SI FSalida = 20;  |O FSalida = 9;
         aAlfa    = "|:ren2009/rem01005;" + (("00000" + #1001#0) % -105);
         |SUB_EJECUTA_NP aAlfa;
         |ERROR;
     |FINSI;
|FPRC;

