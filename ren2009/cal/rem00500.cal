|FICHEROS;
     rem00500 #500;
|FIN;

|VARIABLES;
     aParametro   = "";
     aAlfa        = "";
     aDTipo       = "";
     aHTipo       = "";

     nHandle      = 0;

     &enCultivo   = 0;
     &enTipo      = 0;
     &eaTipo      = "";
|FIN;

|RUTINA ClaveBaja500;
     #500#0 = 1;
     #500#1 = " ";
     #500#3 = 00;
     #500#6 = aDTipo;
|FRUT;

|RUTINA ClaveAlta500;
     #500#0 = 999;
     #500#1 = aAlfa;
     #500#3 = 99;
     #500#6 = aHTipo;
|FRUT;

|PROGRAMA;
     aParametro = PARAMETRO;   |QBF aParametro;
     |SI aParametro = "ENTRADA";
         |ENTLINEAL #1#500, -1, 2, 22, 1, ClaveBaja500, ClaveAlta500;
         |ACABA;
     |FINSI;

     aAlfa = "z" * 140;

     |CAMPO_ANCHO #500#1, 1, 100;
     |CAMPO_ANCHO #500#4, 1, 100;
     |CAMPO_ANCHO #500#6, 1, 1;

     |ABRE #500;
     |SELECT #1#500;
     #500#0 = enCultivo;
     |LEE 040#500=;
     |SI FSalida ! 0;  |DDEFECTO #500;  |FINSI;

     aDTipo = eaTipo;
     aHTipo = eaTipo;
     |SI eaTipo = "";
         aDTipo = " ";
         aHTipo = "z";
     |FINSI;

     |SI enTipo = 1;
         |CONSULTA_F_CLAVE #3#500, ClaveBaja500, ClaveAlta500;
     |FINSI;

     |SI enTipo = 2;
         |CONSULTA_F_CLAVE #2#500, ClaveBaja500, ClaveAlta500;
     |FINSI;

     |SI enTipo = 3;
         |CONSULTA_F_CLAVE #1#500, ClaveBaja500, ClaveAlta500;
     |FINSI;

     |SI FSalida = 0;  enCultivo = #500#0;  |FINSI;
     |CIERRA #500;
|FPRO;
