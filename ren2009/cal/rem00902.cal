|FICHEROS;
     rem00902 #902;
|FIN;

|VARIABLES;
     aCodigo1   = "";
     aCodigo2   = "";
     aCodigo3   = "";
     aCodigo4   = "";
     aContenido = "";
     aCaracter  = "";
     aLineaMenu = "";
     aAlfa      = "";
     aAlfa1     = "";
     aAlfa2     = "";
     aAbre      = "";
     aPrograma  = "";

     nCodigos   = 0;
     nPos       = 0;
     nPosi      = 0;
     nRango     = 0;
     nBoton1    = 0;
     nHandle    = 0;
     nLong      = 0;
     nCaracter  = 0;
     nLLaves    = 0;
|FIN;

|PROCESO RecogeDatos;
     aAlfa = "/u/dsprofes/ren2009/cogetxt/rem00902.txt";
     |XABRE "AB",  aAlfa,  nHandle;

     |PARA;  |SI;  |HACIENDO;
          |XLEE nHandle, 151, aLineaMenu;
          |SI FSalida [ 0;  |SAL;  |FINSI;

          aCodigo1   = "";
          aCodigo2   = "";
          aCodigo3   = "";
          aCodigo4   = "";
          aPrograma  = "";
          nCodigos   = 1;
          nPos       = 1;
          nLLaves    = 0;
          aContenido = "";
          |PARA;  |SI;  |HACIENDO nPos = nPos + 1;
               nPosi = (nPos * 100) + 1;

               aCaracter = aLineaMenu % nPosi;

               |SI aCaracter = "}";  |SAL;  |FINSI;

               |SI aCaracter = ":";
                   nCodigos = 0;
               |FINSI;

               |SI aCaracter = "{";
                   nLLaves   = nLLaves + 1;
               |FINSI;

               |SI aCaracter = ",";  |Y nCodigos ! 0;
                   nCodigos = nCodigos + 1;
               |FINSI;

               |SI nCodigos = 1;  |Y aCaracter ! ",";
                   aCodigo1 = aCodigo1 + aCaracter;
               |FINSI;

               |SI nCodigos = 2;  |Y aCaracter ! ",";
                   aCodigo2 = aCodigo2 + aCaracter;
               |FINSI;

               |SI nCodigos = 3;  |Y aCaracter ! ",";
                   aCodigo3 = aCodigo3 + aCaracter;
               |FINSI;

               |SI nCodigos = 4;  |Y aCaracter ! ",";
                   aCodigo4 = aCodigo4 + aCaracter;
               |FINSI;

               |SI nCodigos = 0;  |Y nLLaves = 0;  |Y aCaracter ! ":";
                   aContenido = aContenido + aCaracter;
               |FINSI;

               |SI nLLaves = 2;  |Y aCaracter ! "{";
                   aPrograma = aPrograma + aCaracter;
               |FINSI;
          |SIGUE;

          |DDEFECTO #902;
          #902#0 = aCodigo1;
          #902#1 = aCodigo2;
          #902#2 = aCodigo3;
          #902#3 = aCodigo4;
          #902#4 = aContenido;
          #902#5 = aPrograma;
          |GRABA 020#902n;
          |LIBERA #902;
     |SIGUE;
     |XCIERRA nHandle;
|FPRC;

|PROCESO rem00902;
     aAlfa      = "";
     aContenido = #902#5;  |QBF aContenido;

                      aAlfa2 = "{{" + aContenido + "}}";
                      aAlfa1 = #902#0;
     |SI #902#1 ! 0;  aAlfa1 = aAlfa1 + "," + #902#1;  |FINSI;
     |SI #902#2 ! 0;  aAlfa1 = aAlfa1 + "," + #902#2;  |FINSI;
     |SI #902#3 ! 0;  aAlfa1 = aAlfa1 + "," + #902#3;  |FINSI;

     aAlfa1 = aAlfa1 + ":" + #902#4;  |QBF aAlfa1;
     aAlfa1 = aAlfa1 + " " + aAlfa + " " + aAlfa2;  |QBF aAlfa1;

     aAlfa  = aAlfa1 % 0;
     nLong  = aAlfa;
     |PARA nCaracter = nLong;  |SI nCaracter [ 148;  |HACIENDO nCaracter = nCaracter + 1;
           aAlfa1  = aAlfa1 + " ";
     |SIGUE;

     aAlfa1 = aAlfa1 + &13 + &10;
     |XGRABA nHandle, aAlfa1;
|FPRC;

|DEFBUCLE rem00902;
     #902#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, rem00902;
|FIN;

|PROCESO HazElTxt;
     aAbre = "/u/dsprofes/ren2009/cogetxt/rem00902.txt";

     |XABRE "WB", aAbre, nHandle;
     |BUCLE rem00902;
     |XCIERRA nHandle;
|FPRC;

|PROCESO Tipo80Fm902;  |TIPO 80;
||     |HAZ HazElTxt;

     |ABRET #902;
     |LEE 000#902p;
     |SI FSalida ! 0;
         |HAZ RecogeDatos;
     |FINSI;

     FSalida = 2799;
|FPRC;

|PROCESO Tipo90Fm902;  |TIPO 90;
     |CIERRAT #902;
|FPRC;

|PROCESO Salir;
     |PTEC 806;
|FPRC;

|PROGRAMA;
     |CABEZA "Liquidacion";

     |PINPA #0#902;

     |CONTROL_SIMPLE 0, "BOTON,{{001}}Salir", 2785, 2795, Salir;
     nBoton1 = FSalida;

     nRango = 1 + 4 + 8 + 16 + 128;

     |LEE 040#902p;

     |LINEAL_SIMPLE #1#902, nRango, 0602, 2698, NULL, NULL, NULL;

     |PULSATECLA;

     |FIN_CONTROL_WINDOWS nBoton1;
|FPRO;
