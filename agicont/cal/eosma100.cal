|FICHEROS;
     eosma100 #100;
|FIN;

|VARIABLES;
     nCampoBas = 0;
     nCampoIva = 0;
     nCampoCuo = 0;
|FIN;

|PROCESO Tipo12Fma100;  |TIPO 12;
|FPRC;

|PROCESO CalculaBase;  |TIPO 0;
     |SI #100Campo = Contenido;  |ACABA;  |FINSI;

     nCampoIva     = Campo + 1;
     nCampoCuo     = Campo + 2;
     #100nCampoCuo = #100Campo % #100nCampoIva;
     |PINTA #100nCampoCuo;
|FPRC;

|PROCESO CalculaIVA;  |TIPO 0;
     |SI #100Campo = Contenido;  |ACABA;  |FINSI;

     nCampoBas     = Campo - 1;
     nCampoCuo     = Campo + 1;
     #100nCampoCuo = #100nCampoBas % #100Campo;  |PINTA #100nCampoCuo;
|FPRC;
