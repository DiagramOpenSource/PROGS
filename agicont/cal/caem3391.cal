|| Formatea las cantidades enteras a 8 posiciones

|PROCESO CambiCanti_8;
  CantiNum = CantiNum ! 0;
  |SI CantiNum ] 0;
      CantiAlfa = CantiNum;
      CantiAlfa = ("00000000" + CantiAlfa) % -108;
  |SINO;
      CantiNum  = -CantiNum;
      CantiAlfa = CantiNum;
      CantiAlfa = "N" + (("0000000" + CantiAlfa) % -107);
  |FINSI;
|FPRC;

|| Formatea las cantidades enteras a 12 psiciones

|PROCESO CambiCanti_12;
  enCanti  = CantiNum;
  |HAZ ConverMoneda;
  CantiNum = enCanti;

  |SI CantiNum ] 0;
      CantiAlfa = CantiNum;
      |SI elAnyo ] 2001;
         CantiAlfa = ("0000000000000" + CantiAlfa) % -113;
      |SINO;
         CantiAlfa = ("000000000000" + CantiAlfa) % -112;
      |FINSI;
  |SINO;
      CantiNum  = -CantiNum;
      CantiAlfa = CantiNum;
      |SI elAnyo ] 2001;
         CantiAlfa = "N" + ("0000000000000" + CantiAlfa) % -112;
      |SINO;
         CantiAlfa = "N" + ("000000000000" + CantiAlfa) % -111;
      |FINSI;
