|VARIABLES;
 nMensa   = 0;
 mensa1   = "";
 uno      = "";
 dos      = "";
 tres     = "";
 nuno     = 0;
 nNoBal   = 0;
 nNoBal8  = 0;
 unoU     = "";
|FIN;

|PROCESO defec2;

 |PDEFECTO #cacuenta,4,9;
 nNoBal  = 0;
 nNoBal8 = 0;
 uno     = #cacuenta#0; |QBF uno;
 |SI uno = ""; |ACABA; |FINSI;
 uno  = uno % 0;
 nuno = uno;

 nNoBal = 1;
 |ABRE #cacuebal;

 nNoBal8 = 0;
 |SI enEjercicio ] 2008;
     nNoBal8 = 1;
     |SI nuno [ 2; nNoBal8 = 0; |FINSI;
 |FINSI;

 uno = #cacuenta#0; |QBF uno;
 tres = uno;
 dos = uno + "000000000000";

 uno = dos % A105;
 |HAZ leodefect2;
 |SI nNoBal = 0; |Y nNoBal8 = 0; |ACABA;  |FINSI;

 uno = dos % A104;
 |HAZ leodefect2;
 |SI nNoBal = 0; |Y nNoBal8 = 0; |ACABA;  |FINSI;

 uno = dos % A103;
 |HAZ leodefect2;
 |SI nNoBal = 0; |Y nNoBal8 = 0; |ACABA;  |FINSI;

 uno = dos % A102;
 |HAZ leodefect2;
 |SI nNoBal = 0; |Y nNoBal8 = 0; |ACABA;  |FINSI;

 uno = dos % A101;
 |HAZ leodefect2;
 |SI nNoBal = 0; |Y nNoBal8 = 0;
     |ACABA;
 |SINO;
     |SI nNoBal = 1;
         mensa1 = "     No esta definida esta cuenta: " + tres + " en Cuentas de Balance 1990";
         |SI nMensa = 0; |MENAV mensa1; |FINSI;
         |CIERRA #cacuebal;
     |FINSI;
     |SI nNoBal8 = 1;
         mensa1 = "     No esta definida esta cuenta: " + tres + " en Cuentas de Balance 2008";
         |SI nMensa = 0; |MENAV mensa1; |FINSI;
     |FINSI;
 |FINSI;
|FPRC;

|PROCESO saca_2008_2;
   nNoBal8 = 0;
|FPRC;

|DEFBUCLE saca_2008_2;
 #ca8m0002#2;
 ;
 enEjerBal, 00, 0, uno;
 enEjerBal, 99, 2, unoU;
 ;
 NULL, saca_2008_2;
|FIN;

|PROCESO leodefect2;
     |SI nNoBal = 1;
         #cacuebal#0 = uno;
         |LEE 040#cacuebal.=;
         |SI FSalida = 0;
             |HAZ saca2;
         |FINSI;
     |FINSI;

     |SI nNoBal8 = 1;
         |SI nuno > 3;
             uno   = (uno + "     " ) % 105;
             unoU  = uno;
         |SINO;
             unoU  = (uno + "99999" ) % 105;
             uno   = (uno + "     " ) % 105;
         |FINSI;
         |BUCLE saca_2008_2;
     |FINSI;
|FPRC;

|PROCESO saca2;
   #cacuenta#4 = #cacuebal#1;
   #cacuenta#5 = #cacuebal#2;
   #cacuenta#6 = #cacuebal#3;
   #cacuenta#7 = #cacuebal#4;
   #cacuenta#8 = #cacuebal#5;
   nNoBal = 0;
   |CIERRA #cacuebal;
|FPRC;
