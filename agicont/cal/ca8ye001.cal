|PROCESO LosTotales;
  nCampo2 = #5#2;  ||Bloque
  aCampo3 = #5#3;  ||Tipo
  nCampo4 = #5#4;  ||Agrupacion
  nCampo5 = #5#5;  ||Subagrupacion
  nCampo6 = #5#6;  ||Epigrafe
  nCampo7 = #5#7;  ||Partida
  nCampo8 = #5#8;  ||subpartida

  |HAZ cargatra_3;

  |SI nCampo8 ! 0;    ||Subpartida (A/P = 0)
      nCampo8 = 0;
      |HAZ cargatra_3;
  |FINSI;

  |SI nCampo7 ! 0;   ||Partida
      nCampo8 = 0;
      nCampo7 = 0;
      |HAZ cargatra_3;
  |FINSI;

  saldo = (saldo + nImporte) ! EURODB;
|FPRC;

