|PROCESO ver_tipo; |TIPO 8;
    |ABRE #1;
    |LEE 000#1p;
    |SI FSalida = 0;
        |SI #1#1 = "I";
            |MENAV "    Control configurado para la Importacion ...";
            |PTEC 807;
        |SINO;
            |HAZ inicio;
        |FINSI;
    |SINO;
        |MENAV "    Debera definir el Control de Exportaciones/Importaciones ...";
        |PTEC 807;
    |FINSI;
    |CIERRA #1;
|FPRC;

|PROCESO fecha1; |TIPO 7;
    |SI sw_ya = 0;
        #0#1 = fec1;
        #0#2 = fec2;
        |PINDA #0#0;
        sw_ya = 1;
    |FINSI;
|FPRC;

|PROCESO fecha2; |TIPO 0;
    |SI #0Campo < fec1;|O #0Campo > fec2;
        |MENAV "    ATENCION!!! Fecha fuera de periodo ...";
        |ERROR;
    |FINSI;
|FPRC;

|PROCESO exporta; |TIPO 3;

    |SI #0#0 = "SI";
        aFichero  = ("0p" + Usuario) % 108;
        |NOME_DAT #caivatm4#aFichero#;
        |DELINDEX #caivatm4;

        |DFICE fpath;   fpath = "wb  " + fpath; |QBF fpath;

        |SI #1#2 = "S";  |HAZ apuntes0;                  |FINSI;
        |SI #1#3 = "S";
            enImpExp = 1;
            |HAZ repercu0;
            |HAZ soporta0;
            |SI #1#9 = "S"; |HAZ casiiman0; |FINSI;
            enImpExp = 0;
        |FINSI;
        |SI #1#4 = "S";  |HAZ cobros0;   |HAZ pagos0;    |FINSI;
        |SI #1#5 = "S";  |HAZ cliprov0;                  |FINSI;
        |SI #1#5 = "S";  |HAZ cuentas0;                  |FINSI;

        |SI sel_apun ! 0;|O sel_repe ! 0;|O sel_sopo ! 0;|O sel_cobr ! 0;
                         |O sel_pago ! 0;|O sel_cuen ! 0;
            |SUB_EJECUTA "caxcopia.run";
        |SINO;
            |MENAV "    Exportacion vacia ...";
        |FINSI;
    |FINSI;
|FPRC;
