|FICHEROS;
   caman130 #130;
|FIN;

|VARIABLES;
   nModelo    = 0;
   nEjer      = 0;
   nDPeriodo  = 0;
   nHPeriodo  = 0;
   &enPer     = 0;
   &enError15 = 0;
   &enEmpresa = 0;
|FIN;

|PROCESO Imprime130;
 |SI #130#49 ! 0;
     enError15 = 1;
     |ERROR10;
 |FINSI;
|FPRC;

|DEFBUCLE Leec130;
  #130#1;
  ;
  enEmpresa, 1, 01, nEjer;
  enEmpresa, 9, 12, nEjer;
  ;
  NULL, Imprime130;
|FIN;

|| *************************************************************************

|PROCESO eVerM130_2015;
    nDPeriodo = enPer;
    nHPeriodo = enPer;
    |SI enPer = 0;
        nDPeriodo = 01;
        nHPeriodo = 12;
    |FINSI;
    nEjer    = 15;
    enError15 = 0;
    |BUCLE Leec130;
    |SI enError15 ! 0;
        |SI enPer = 0;
            |MENAV "0000Hay Periodos con: Importe en la Reduc.creacion empleo y ejercicio 2015 incompatible. Modifiquelos";
        |SINO;
            |MENAV "0000Importe en la Reduc.creacion empleo y ejercicio 2015 incompatible.";
        |FINSI;
    |FINSI;
|FPRC;
