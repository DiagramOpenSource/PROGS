|PROGRAMA;     || Permite la entrada lineal de los datos en Modo modificacion

  |CLS;
  |CABEZA "Parametros Informe";
  |PINPA #0#0;
  |ABRE #0;
  |ENTLINEAL #1#0, 1, 2, 20, 1, pro1, pro2;
  |CIERRA #0;

|FPRO;



|PROCESO tipo;   ||Se activan unos campo u otro dependiendo del tipo de l¡nea

  |SI #0#1 = "T";  ||La l¡nea es un T¡tulo

      |CAMPO_MODIFICA #0#2, 1 , "N";    ||Bloque
      |CAMPO_MODIFICA #0#3, 1 , "N";    ||SubBloque
      |CAMPO_MODIFICA #0#4, 1 , "S";    ||Descripcion
      |CAMPO_MODIFICA #0#5, 1 , "N";    ||F¢rmula

  |FINSI;


  |SI #0#1 = "C"; ||La l¡nea es una cuenta

      |CAMPO_MODIFICA #0#2, 1 , "S";    ||Bloque
      |CAMPO_MODIFICA #0#3, 1 , "S";    ||SubBloque
      |CAMPO_MODIFICA #0#4, 1 , "N";    ||Descripcion
      |CAMPO_MODIFICA #0#5, 1 , "N";    ||F¢rmula

  |FINSI;


  |SI #0#1 = "F"; ||La l¡nea es una F¢rmula

      |CAMPO_MODIFICA #0#2, 1 , "N";    ||Bloque
      |CAMPO_MODIFICA #0#3, 1 , "N";    ||SubBloque
      |CAMPO_MODIFICA #0#4, 1 , "S";    ||Descripcion
      |CAMPO_MODIFICA #0#5, 1 , "S";    ||F¢rmula

  |FINSI;


|FPRC;
