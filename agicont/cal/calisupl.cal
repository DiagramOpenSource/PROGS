    asd        = "¿";
    aid        = "Ù";
|SINO;
    hor        = #0#11;
    asi        = #0#11;
    aii        = #0#11;
    tea        = #0#11;
    tei        = #0#11;
    cru        = #0#11;
    teb        = #0#11;
    ted        = #0#11;
    vert       = #0#10 * 35;
    asd        = #0#11;
    aid        = #0#11;
|FINSI;
