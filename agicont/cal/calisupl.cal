      has_dia = #0#2; ||                      99;
      des_fec = fec1;
      x = #0#3;
      x = x - 1;
      has_fec = x;
      des_doc = 000001;
      has_doc = 999999; || manda la fecha, #0#5 - 1;
      |BUCLE calisupl0;
  |FINSI;

  des_dia = #0#1;
  has_dia = #0#2;
  des_fec = #0#3;
  has_fec = #0#4;
  des_doc = #0#5;
  has_doc = #0#6;

  |ABRE #3;
  |SI #0#15 = 1; |BUCLE calisupl1; |FINSI;
  |SI #0#15 = 2; |BUCLE calisupl2; |FINSI;

  |CIERRA #3;
