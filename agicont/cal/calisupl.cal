|PROCESO sacaimpo;
  |HAZ mayus;
  |SI #0#16 = "S";
      |PARA x = 18; |SI x [ 27; |HACIENDO x = x + 1;
            |CAMPO_MODIFICA #0x,1,"N";
            #0x = 0; |PINTA #0x;
      |SIGUE;
  |SINO;
      |PARA x = 18; |SI x [ 26; |HACIENDO x = x + 1;
            |CAMPO_MODIFICA #0x,1,"S";
      |SIGUE;
  |FINSI;
|FPRC;

|PROCESO sumaimpo;
  #0#27 = 0;
  |PARA x = 18; |SI x [ 26; |HACIENDO x = x + 1;
        #0#27 = #0#27 + #0x;
  |SIGUE;
  |PINTA #0#27;
