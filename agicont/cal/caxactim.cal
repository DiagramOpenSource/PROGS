|SI #2#8 = "D";
        #14#4 = #14#4 + #2#9;
    |SI #14#09 = 0; #14#08 = #2#4; #14#09 = #2#5; |FINSI;      || contrp.HABER
|FINSI;
|SI #2#8 = "H";
        #14#5 = #14#5 + #2#9;
    |SI #14#11 = 0; #14#10 = #2#4; #14#11 = #2#5; |FINSI;      || contrp.DEBE
|FINSI;
