|PROCESO calculit;
  |HAZ digitos2;
  |SI d_error ! 0;  |ACABA;  |FINSI;
  |SI #0#4 = "S";
      |SI #1#55 [ nivel;
          |SI #1#3 = "S";
              |HAZ grabatra;
         |SINO;
              |SI #1#55 = nivel; |HAZ grabatra; |FINSI;
         |FINSI;
     |FINSI;
  |SINO;
     |SI #1#3 = "S"; |HAZ grabatra; |FINSI;
  |FINSI;
|FPRC;

|DEFBUCLE cabaexpr0;
#1#4;
;
d, 0, 01, 00, 00;
h, 4, 99, 99, 99;
be;
NULL,calculit;
|FIN;

|PROCESO impresio;
  |SI sw = 0;
      bloque = #3#1;
      tipo = #3#0;
      agrupaci = #3#2;
      epigrafe = #3#3;
      partida = #3#4;
      |SI #3#0 = "D"; doh = "DEBE";  |FINSI;
      |SI #3#0 = "H"; doh = "HABER"; |FINSI;
      sw_inf = 0;
      |PRINT;
      sw = 1;
  |FINSI;

  |SI bloque = #3#1; |Y tipo = #3#0;
      |HAZ epigraf;
  |SINO;
      |SI tipo ! #3#0;                    || ruptura por tipo
          bloque1 = bloque;
          bloque = 4;
          |HAZ mirablo;
          doh = "HABER";
          |PIEINF;
          sw_inf = 0;
          |PRINT;
          |HAZ epigraf;
          bloque = bloque1;
      |SINO;                              || ruptura por bloque
          |HAZ mirablo;
          |HAZ epigraf;
      |FINSI;
      bloque = #3#1;
      tipo = #3#0;
  |FINSI;
|FPRC;

|DEFBUCLE cabaexpr1;
#3#1;
;
d, 0, 01, 00, 00;
h, 4, 99, 99, 99;
be;
NULL,impresio;
|FIN;

|PROCESO miracuen;
  |SI #4#6 = #1#0; |Y #4#7 = #1#1;
      cantidad = cantidad + (#4#4 - #4#5);
      sw_pasa = 1;
  |FINSI;
|FPRC;

|DEFBUCLE cabaexpr4;
#4#1;
;
#0#6, 001;
#0#6, 999;
be;
NULL,miracuen;
|FIN;

|PROCESO cuentas;
  |HAZ digitos2;
  |SI d_error ! 0;  |ACABA;  |FINSI;
  sw_pasa = 0;
  cantidad = 0;
  cantidad_p = 0;
  sw_inf = 6;
  |SI #0#4 = "S";
      |SI #1#55 [ nivel;
          |SI #1#3 = "S";
              |BUCLE cabaexpr4;
              |SI sw_pasa = 1;
                  |SI cantidad ! 0; |PRINT; |FINSI;
              |SINO;
                  |HAZ recuenta;
              |FINSI;
          |SINO;
              |SI #1#55 = nivel;
                  |BUCLE cabaexpr4;
                  |SI sw_pasa = 1;
                      |SI cantidad ! 0; |PRINT; |FINSI;
                  |SINO;
                      |HAZ recuenta;
                  |FINSI;
              |FINSI;
          |FINSI;
      |FINSI;
  |SINO;
      |SI #1#3 = "S";
          |BUCLE cabaexpr4;
          |SI sw_pasa = 1;
              |SI cantidad ! 0; |PRINT; |FINSI;
          |SINO;
              |HAZ recuenta;
          |FINSI;
      |FINSI;
  |FINSI;
|FPRC;

|DEFBUCLE cabaexpr2;
 #1#4;
 ;
 #3#0, #3#1, #3#2, #3#3, #3#4;
 #3#0, #3#1, #3#2, #3#3, #3#4;
 be;
 NULL, NULL, NULL, NULL, NULL, cuentas;
 #1#0, #1#1;
|FIN;

|PROCESO descarg;
  |HAZ digitos2;
  |SI d_error ! 0;  |ACABA;  |FINSI;
  linea = linea + 1;
  #4#0 = linea;
  #4#8 = #0#6;
  |LEE 001#4=;
  |SI FSalida = 0;  |BORRA 020#4a;  |FINSI;
  |DDEFECTO #4;
  #4#0 = linea;    || linea
  #4#8 = #0#6;     || estructura
  #4#1 = #1#0;     || cuenta
  #4#2 = #1#1;     || digito
  #4#3 = #1#2;     || descripcion
  mes_fin = #0#10 + 11;              || mes iniciales
  #4#4 = #7mes_fin;                  || exist. iniciales
  mes_fin = #0#11 + 11;              || mes finales
  #4#5 = #7mes_fin;                  || exist. finales
  descar = #4#4 - #4#5;
  |SI descar ] 0;
      #4#6 = #7#5;     || cuenta descarga D
      #4#7 = #7#6;     || digito descarga D
  |SINO;
      #4#6 = #7#8;     || cuenta descarga H
      #4#7 = #7#9;     || digito descarga H
  |FINSI;
  |SI nivel < #4#7;
      |SI nivel = 1;  traraf = 100 + dig4;  |FINSI;
      |SI nivel = 2;  traraf = 100 + dig5;  |FINSI;
      |SI nivel = 3;  traraf = 100 + dig6;  |FINSI;
      |SI nivel = 4;  traraf = 100 + dig7;  |FINSI;
      |SI nivel = 5;  traraf = 100 + dig8;  |FINSI;
      #4#6 = #4#6 % traraf;
      #4#7 = nivel;
      #4#6 = #4#6 + "              ";
      #4#6 = #4#6 % 112;
  |FINSI;
  |GRABA 020#4n;
  |HAZ grabatr1;
|FPRC;

|DEFBUCLE cabaexpr3;
#1#1;
;
#7#2, #7#3;
#7#2, #7#3;
be;
NULL,descarg;
|FIN;

|PROCESO grabatra;
  #3#0 = #1#5;
  #3#1 = #1#4;
  #3#2 = #1#6;
  #3#3 = #1#7;
  #3#4 = #1#8;
  |LEE 101#3=;
  FEstado = FSalida;
  |SI FEstado ! 0;
      |DDEFECTO #3;
      #3#0 = #1#5;
      #3#1 = #1#4;
      #3#2 = #1#6;
      #3#3 = #1#7;
      #3#4 = #1#8;
  |FINSI;
  |HAZ leexplot;
  #3#5 = #2#3;
  #3#6 = #2#5;
  #3#7 = #2#7;
  sw_pasa = 0;
  |SI #3#9 = 1;
      |BUCLE cabaexpr4;
      #3#9 = 1;
  |SINO;
      #3#9 = 0;
  |FINSI;
  |SI sw_pasa = 0;
      |HAZ resulta;
      #3#9 = 0;
      |SI #3#8 ! 0;|O #3#10 ! 0;
          |SI FEstado = 0; |GRABA 020#3a; |SINO; |GRABA 020#3n; |FINSI;
      |FINSI;
  |FINSI;
  |LIBERA #3;
|FPRC;

|PROCESO grabatr1;
  |ABRE #1;
  #1#0 = #4#6;
  #1#1 = #4#7;
  |LEE 040#1=;
  |SI FSalida = 0;
      #3#0 = #1#5;
      #3#1 = #1#4;
      #3#2 = #1#6;
      #3#3 = #1#7;
      #3#4 = #1#8;
      |LEE 101#3=;
      FEstado = FSalida;
      |SI FEstado ! 0;
          |DDEFECTO #3;
          #3#0 = #1#5;
          #3#1 = #1#4;
          #3#2 = #1#6;
          #3#3 = #1#7;
          #3#4 = #1#8;
      |FINSI;
      |HAZ leexplot;
      #3#5 = #2#3;
      #3#6 = #2#5;
      #3#7 = #2#7;
      #3#8 = #3#8 + (#4#4 - #4#5);
      #3#9 = 1;
      |SI #3#8 ! 0;
          |SI FEstado = 0; |GRABA 020#3a; |SINO; |GRABA 020#3n; |FINSI;
      |FINSI;
      |LIBERA #3;
      |HAZ sumasal;
  |FINSI;
  |CIERRA #1;
|FPRC;

|PROCESO resulta;    || Calculamos cada cuenta
  |SI #3#0 = "D"; |Y #3#1 = 1;
      |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8 = #3#8   + #1x;
            debe1 = debe1 + #1x;
      |SIGUE;
      |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
            #3#10 = #3#10 + #1x;
            de_p1 = de_p1 + #1x;
      |SIGUE;
      sw_d1 = 1;
  |FINSI;

  |SI #3#0 = "D"; |Y #3#1 = 2;
      |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8 = #3#8   + #1x;
            debe2 = debe2 + #1x;
      |SIGUE;
      |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
            #3#10 = #3#10 + #1x;
            de_p2 = de_p2 + #1x;
      |SIGUE;
      sw_d2 = 1;
  |FINSI;

  |SI #3#0 = "D"; |Y #3#1 = 3;
      |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8 = #3#8   + #1x;
            debe3 = debe3 + #1x;
      |SIGUE;
      |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
            #3#10 = #3#10 + #1x;
            de_p3 = de_p3 + #1x;
      |SIGUE;
      sw_d3 = 1;
  |FINSI;

  |SI #3#0 = "D"; |Y #3#1 = 4;
      |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8 = #3#8   + #1x;
            debe4 = debe4 + #1x;
      |SIGUE;
      |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
            #3#10 = #3#10 + #1x;
            de_p4 = de_p4 + #1x;
      |SIGUE;
      sw_d4 = 0;
  |FINSI;

  |SI #3#0 = "H"; |Y #3#1 = 1;
      |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8   = #3#8   + (#1x * -1);
            haber1 = haber1 + (#1x * -1);
      |SIGUE;
      |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
            #3#10 = #3#10 + (#1x * -1);
            hab_p1 = hab_p1 + (#1x * -1);
      |SIGUE;
      sw_h1 = 1;
  |FINSI;

  |SI #3#0 = "H"; |Y #3#1 = 2;
      |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8   = #3#8   + (#1x * -1);
            haber2 = haber2 + (#1x * -1);
      |SIGUE;
      |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
            #3#10 = #3#10 + (#1x * -1);
            hab_p2 = hab_p2 + (#1x * -1);
      |SIGUE;
      sw_h2 = 1;
  |FINSI;

  |SI #3#0 = "H"; |Y #3#1 = 3;
      |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8   = #3#8   + (#1x * -1);
            haber3 = haber3 + (#1x * -1);
      |SIGUE;
      |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
            #3#10 = #3#10 + (#1x * -1);
            hab_p3 = hab_p3 + (#1x * -1);
      |SIGUE;
      sw_h3 = 1;
  |FINSI;

|FPRC;

|PROCESO sumasal;    || Calculamos cada cuenta
  |SI #3#0 = "D"; |Y #3#1 = 1;
       debe1 = debe1 + (#4#4 - #4#5);
  |FINSI;

  |SI #1#5 = "D"; |Y #1#4 = 2;
       debe2 = debe2 + (#4#4 - #4#5);
  |FINSI;

  |SI #1#5 = "D"; |Y #1#4 = 3;
       debe3 = debe3 + (#4#4 - #4#5);
  |FINSI;

  |SI #1#5 = "D"; |Y #1#4 = 4;
       debe4 = debe4 + (#4#4 - #4#5);
  |FINSI;

  |SI #1#5 = "H"; |Y #1#4 = 1;
       haber1 = haber1 + (#4#4 - #4#5);
  |FINSI;

  |SI #1#5 = "H"; |Y #1#4 = 2;
       haber2 = haber2 + (#4#4 - #4#5);
  |FINSI;

  |SI #1#5 = "H"; |Y #1#4 = 3;
       haber3 = haber3 + (#4#4 - #4#5);
  |FINSI;
|FPRC;

|PROCESO epigraf;
  |SI epigrafe = #3#3;
      |HAZ rotura;
  |SINO;
      sw_epi = 0;
      |HAZ rotura;
      epigrafe = #3#3;
  |FINSI;
|FPRC;

|PROCESO rotura;
  |SI #3#4 = 0;
      sw_inf = 2;
      |PRINT;
      |SI #0#4 = "S";
          |BUCLE cabaexpr2;
          sw_inf = 5; |PRINT;
      |FINSI;
  |SINO;
      |SI sw_epi = 0;
          sw_inf = 3;
          |PRINT;
          sw_epi = 1;
      |FINSI;
      sw_inf = 4;
      |PRINT;
      |SI #0#4 = "S";
          |BUCLE cabaexpr2;
          sw_inf = 5; |PRINT;
      |FINSI;
  |FINSI;
|FPRC;

|PROCESO leexplot;  || Lee el fichero de explotacion
   #2#0 = #3#1;
   #2#1 = #3#0;
   #2#2 = #3#2;
   #2#4 = #3#3;
   #2#6 = #3#4;
   |LEE 040#2=; |SI FSalida ! 0; |DDEFECTO #2; |FINSI;
|FPRC;

|PROCESO final;    || Para cuando acaba el defbucle
  |SI tipo = "D"; bloque = 4; |FINSI;
  |SI tipo = "H"; bloque = 3; |FINSI;
  |HAZ mirablo;
|FPRC;

|PROCESO recuenta;    || Calculamos cada cuenta
  |SI #3#0 =  "D";
      |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            cantidad = cantidad + #1x;
      |SIGUE;
      |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
            cantidad_p = cantidad_p + #1x;
      |SIGUE;
  |FINSI;

  |SI #3#0 = "H";
      |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            cantidad = cantidad + (#1x * -1);
      |SIGUE;
      |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
            cantidad_p = cantidad_p + (#1x * -1);
      |SIGUE;
  |FINSI;

  |SI cantidad ! 0;|O cantidad_p ! 0;   |PRINT;   |FINSI;
|FPRC;

|PROCESO mirablo;
  sw_inf = 1;
  |SI tipo = "D";
      |SI bloque = 4;
          |HAZ bloqued1;
          |HAZ bloqued2;
          |HAZ bloqued3;
          |HAZ bloqued4;
      |FINSI;
      |SI bloque = 3;
          |HAZ bloqued1;
          |HAZ bloqued2;
          |HAZ bloqued3;
      |FINSI;
      |SI bloque = 2;
          |HAZ bloqued1;
          |HAZ bloqued2;
          |SI #3#1 > 3;
              |HAZ bloqued3;   || si no hay del bloque 3
          |FINSI;
      |FINSI;
      |SI bloque = 1;          || anterior a la ruptura
          |HAZ bloqued1;
          |SI #3#1 > 2;
              |HAZ bloqued2;   || si no hay del bloque 2
              |SI #3#1 > 3;
                  |HAZ bloqued3;    || si no hay del bloque 3
              |FINSI;
          |FINSI;
      |FINSI;
  |FINSI;
  |SI tipo = "H";
      |SI bloque = 3;
          |HAZ bloqueh1;
          |HAZ bloqueh2;
          |HAZ bloqueh3;
      |FINSI;
      |SI bloque = 2;
          |HAZ bloqueh1;
          |HAZ bloqueh2;
      |FINSI;
      |SI bloque = 1;
          |HAZ bloqueh1;
          |SI #3#1 > 2;
              |HAZ bloqueh2;   || si no hay del bloque 2
          |FINSI;
      |FINSI;
  |FINSI;
|FPRC;

|PROCESO bloqued1;
