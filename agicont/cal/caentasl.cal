  |FINSI;
  fec_am = #16#3;
  imp_am = #16#8 + #16#9 + #16#10 + #16#52 + #16#53;
    c_am = #16#7;
  ct1_am = "";
  ct2_am = 0;
  prv_am = #16#50;
                            || primer = #16#4 % 101;
                            || |SI primer = "4";
                            ||     prv_am = #16#50;
                            || |FINSI;
  primer = ctab1 % 101;
  |SI primer = "2";
      ct1_am = ctab1;
