    |SINO;
        #16#19 = req3;
    |FINSI;
    |SI #16#60 ! 0;  |Y req4 = 0;             || (%) rec 4
        #16#58 = ((#16#60 * 100) / #16#52) ! 1;
    |SINO;
        #16#58 = req4;
    |FINSI;
    |SI #16#61 ! 0;  |Y req5 = 0;             || (%) rec 5
        #16#59 = ((#16#61 * 100) / #16#53) ! 1;
    |SINO;
        #16#59 = req5;
    |FINSI;
    #16#23 = #11#11;     || deducible
