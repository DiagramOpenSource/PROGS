        nuevo = 0;
        sw_ptec = 1; || por el 'PTEC' del 'pide_iva' de 'camaasic'(sobra)
        |PINTA 0855, "    ";
|| ... ver que hacer aqu¡
        |SI #11#15 = 2;
            #0#21 = #0#21 + 1;
            documuno = #0#21;
            |PINTA #0#21;
            sw_ptec = 2;
