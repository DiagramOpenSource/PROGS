|PROCESO a_cuentas;                || calculo 'refresca'
  cta_t = 4;  signo = #0#8;
        dig_t = cta_t + 1;
        |ABRET #4;
        #4#0 = #0cta_t;
        #4#1 = #0dig_t;           || actualizacion cuentas
        |LEE 101#4=;
        |SI FSalida = 0;
            |SI #0#2 = 0;
                |SI signo = "D";
                    #4#9 = #4#9 +. #0#9;        || diario cero (saldos iniciales)
                    #4#51 = #4#51 +. #0#9;
                |SINO;
                    #4#10 = #4#10 +. #0#9;
                    #4#52 = #4#52 +. #0#9;
                |FINSI;
                #4#11 = #4#9 - #4#10;
                #4#53 = #4#51 - #4#52;
            |SINO;
                |SI #0#2 = 99;
                    |SI signo = "D";
                        #4#48 = #4#48 +. #0#9;       || diario 99 (saldos finales)
                        #4#51 = #4#51 +. #0#9;
                    |SINO;
                        #4#49 = #4#49 +. #0#9;
                        #4#52 = #4#52 +. #0#9;
                    |FINSI;
                    #4#50 = #4#48 - #4#49;
                    #4#53 = #4#51 - #4#52;
                |SINO;
                    |HAZ a_actualiza;       || saldos del mes correspondiente
                |FINSI;
            |FINSI;
            |SI #4#58 < #0#1;
                #4#58 = #0#1;               || fecha ultimo apunte
            |FINSI;
            |GRABA 020#4a;
       |FINSI;
       |LIBERA #4;
       |CIERRAT #4;
|FPRC;

|PROCESO a_actualiza;                || rutina 'a_cuentas'
    ini = dig1;
    fecalf = #0#3;
    mes = fecalf % 402;
    mesfec = mes;
|SI ini = 1;
        mesac = mesfec;
|SINO;
        mesac = (mesfec - ini) + 1;
    |SI mesac [ 0;
            mesac = (13 - ini) + mesfec;
    |FINSI;
|FINSI;
    a = (mesac * 3) + 9;
    b = a + 1;
    c = a + 2;
|SI signo = "D";
        #4a = #4a +. #0#9;
        #4#51 = #4#51 +. #0#9;
|SINO;
        #4b = #4b +. #0#9;
        #4#52 = #4#52 +. #0#9;
|FINSI;
    #4c = #4a - #4b;
    #4#53 = #4#51 - #4#52;
|FPRC;

