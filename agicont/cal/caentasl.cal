|PROCESO lee_libro;
|ABRE #17;
    #17#0 = #11#4;
|LEE 001#17=;
|SI FSalida ! 0;
        #17#1 = "<LIBRO INEXISTENTE>";
|FINSI;
|CIERRA #17;
