|PROCESO DatosOrden;
   |SI FSalida ! 10; |ACABA; |FINSI;

   |PUSHV 0501 2380;
   |DDEFECTO #n19sepaw;
   #n19sepaw#0 = #n19sepam#25;
   aAlfa = #n19sepam#26; aAlfa = aAlfa % 0105;
   #n19sepaw#1 = aAlfa;
   aAlfa = #n19sepam#26; aAlfa = aAlfa % 0799;
   #n19sepaw#2 = aAlfa;
   #n19sepaw#3 = #n19sepam#27;
   #n19sepaw#4 = #n19sepam#28;
   |PINPA #0#n19sepaw; |PINDA #0#n19sepaw;
   |ENDATOS #1#n19sepaw;
   |SI FSalida = 0;
      #n19sepam#25 = #n19sepaw#0;
      aAlfa = ("00000" + #n19sepaw#1); |QBF aAlfa; aAlfa = aAlfa % -105;
      aAlfa = aAlfa + " " + #n19sepaw#2;
      #n19sepam#26 = aAlfa;
      #n19sepam#27 = #n19sepaw#3;
      #n19sepam#28 = #n19sepaw#4;
   |FINSI;
   |POPV;
|FPRC;

|PROCESO Tipo7IBAN_F19;  |TIPO 7;
     aAlfa = #n19sepam#32;   |QBF aAlfa;

     |SI aAlfa ! "";  |ACABA;  |FINSI;

     eaCta  = #n19sepam#4 + #n19sepam#5 + #n19sepam#11 + #n19sepam#6;
     eaPais = #n19sepam#38;
     |HAZ CalcuIBAN;

     #n19sepam#32 = eaIBAN;
|FPRC;

|PROCESO Tipo0IBAN_F19;  |TIPO 0;
     eaCta  = #n19sepam#4 + #n19sepam#5 + #n19sepam#11 + #n19sepam#6;
     eaPais = #n19sepam#38;
     |HAZ CheqIBAN;

     aAlfa = #n19sepam#32;    |QBF aAlfa;
     |SI aAlfa ! eaIBAN;
         |CONFI "0000SEl codigo IBAN puede ser erroneo, quiere calcularlo.";
         |SI FSalida = 0;
             #n19sepam#32 = eaIBAN;
             |PINTA #n19sepam#32;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;
