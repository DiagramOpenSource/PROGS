|PROCESO lee_asient11;             || CARGA LA PANTALLA DEL ASIENTO COMPLETO
   seleccio = 1;
   |HAZ cuenta1;
   |SI eaMonOrg ! eaMoneda;  |HAZ LoCambio;  |FINSI;

   |DDEFECTO #6;
   #caextrp4#00 = #7#3;        || apunte
   #caextrp4#01 = #7#16;       || cuenta DEBE
   #caextrp4#02 = #7#17;       || digito DEBE
   #caextrp4#03 = #7#10;       || cuenta HABER
   #caextrp4#04 = #7#11;       || digito HABER
   #caextrp4#05 = #7#6;        || concepto
   #caextrp4#06 = #7#7;        || descripcion
   |SI eaConDes ! "N";
       alfa1 = #7#24; |QBF alfa1;
       #caextrp4#6 = alfa1 + " " + #7#7;
   |FINSI;
   #caextrp4#07 = #7#21;       || departamento
   #caextrp4#08 = #7#9;        || importe
   #caextrp4#09 = #cacuenta#2;        || descripcion
   #caextrp4#10 = #cacuenta#51;       || debe
   #caextrp4#11 = #cacuenta#52;       || haber
   #caextrp4#12 = #cacuenta#53;       || saldo
   #caextrp4#13 = #7#0;        || diario
   #caextrp4#14 = #7#1;        || fecha
   #caextrp4#15 = #7#2;        || documento
   |SI #caextrp4#0 = #4#19;
      lalinea = #caextrp4#0;
   |FINSI;
   |GRABA 020#6n;
   totallin = totallin + 1;
|FPRC;

|DEFBUCLE lee_asient1;
   #7#1;
   ;
   #4#18, #4#8, #4#9,    1;
   #4#18, #4#8, #4#9, 9999;
   e;
   NULL, lee_asient11;
|FIN;

|PROCESO asient1;
   aFichero = ("r" + Usuario);
   aFichero = (aFichero + "zzzzzzzz") % 108;
   |NOME_DAT #6 aFichero;
   |DELINDEX #6;

   seleccio = 0;

   |ABRE #6;
   lalinea = 1;
   totallin = 0;
   |BUCLE lee_asient1;
   |CIERRA #6;
   |SI seleccio ! 0;
      |PUSHV 07012380;
      |ENTLINEAL #1#6, -1, 4, 20, 1, mini1, maxi1;
      |POPV;
   |SINO;
      |MENAV "    Seleccion vacia ...";
   |FINSI;

   |DELINDEX #6;

|FPRC;

|PROCESO mini1;
   |SI FSalida = "POSICION";  |Y totallin > 8;
      #caextrp4#0 = lalinea;
   |SINO;
      #caextrp4#0 = 1;
   |FINSI;
|FPRC;

|PROCESO maxi1;
   #caextrp4#0 = 9999;
|FPRC;

|PROCESO cuenta1;
   |ABRE #5;
   |SELECT #1#5;
   #cacuenta#0 = #7#4;         || cuenta
   #cacuenta#1 = #7#5;         || digito
   |LEE 001#5=;
   |SI FSalida ! 0;
      |DDEFECTO #5;
      #cacuenta#2 = "<CUENTA INEXISTENTE>";
   |FINSI;
   |CIERRA #5;
|FPRC;
_____________________________________________________________________________
|PROCESO extraco1;            || CARGA LA PANTALLA DE LA ANOT.EXTRACONTABLE
   |ABRE #8;
   #caextras#0 = #4#18;        || diario
   #caextras#1 = #4#8;         || fecha
   #caextras#2 = #4#9;         || documento
   |LEE 001#8=;
   |SI FSalida = 0;
      |PUSHV 08092067;
      |PINPA #1#4;
      |ATRI I; |PINTA 0927, #caextras#0;
      |PINTA 0937, #caextras#1;
      |PINTA 0959, #caextras#2;
      |PINTA 1128, #caextras#3;
      |ATRI 0;
      |HAZ mod_campos1;
      |ENTLINEAL #1#9, -4, 4, 18, 0, minA1, maxA1;
      |POPV;
   |SINO;
      |MENAV "    Asiento sin Anotaciones Extracontables ...";
   |FINSI;
   |CIERRA #8;
|FPRC;

|RUTINA minA1;
   #caextral#0 = #caextras#0;
   #caextral#1 = #caextras#1;
   #caextral#2 = #caextras#2;
   #caextral#3 = 1;
|FRUT;

|RUTINA maxA1;
   #caextral#0 = #caextras#0;
   #caextral#1 = #caextras#1;
   #caextral#2 = #caextras#2;
   #caextral#3 = 9999;
|FRUT;

|PROCESO mod_campos1;
   |C_M #caextral#4,1,"N";
   |C_P #caextral#3, 1, 1311;
   |C_P #caextral#4, 1, 1316;
   |ABRE #10;
   #caexttip#0 = #caextras#3;
   |LEE 001#10=;
   |SI FSalida = 0;
      |PINTA 1131, #caexttip#1;
   |FINSI;
   |CIERRA #10;
|FPRC;
