|PROCESO LeeAuxilia1;
 enDi  = #999#18;
 efFe  = #999#8;
 enDo  = #999#9;
 enAp  = #999#19;
 eaDes = #999#10;
 eaD   = #999#20;
 eaS   = #999#21;
 |SI #999#11 = "D";
     enDeb = #999#12;
     enHar = 0;
 |SINO;
     enHar = #999#12;
     enDeb = 0;
 |FINSI;
 enSal = #999#13;
 |SI nPrim = 0;
     nPrim = 1;
     enTD = #999#5 - enDeb;
     enTH = #999#6 - enHar;
 |FINSI;

 |PRINT;
 enTD = enTD + enDeb;
 enTH = enTH + enHar;
 enTS = enTD - enTH;
|FPRC;

|DEFBUCLE LeeAuxilia1;
 #999#2006;
 ;
 "01.01.0000", 00, 000001, 0001, 00001, 000001;
 "31.12.2999", 99, 999999, 9999, 99999, 999999;
 ;
 NULL, LeeAuxilia1;
|FIN;

|PROCESO ElInform1;
 |ABRE #30;
 |DDEFECTO #30;
 #30#2 = enEmpOrg;
 #30#3 = enPerOrg;
 |LEE 001#30=;
 |CIERRA #30;
 |SI #30#26 > 80;
     enEjercicio = 1900 + #30#26;
 |SINO;
     enEjercicio = 2000 + #30#26;
 |FINSI;

 |PATH_DAT #5 eaFEmp;
 |ABRE #5;
 |DDEFECTO #5;
 #5#0 = #4#1;
 #5#1 = ext2;
 |LEE 001#5=;
 |CIERRA #5;

 |DBASE alfa1;
 aMas  = alfa1 + "def/caextrp5.mas";

 |CARGA_DEF aMas, imagen@;
 |SI FSalida = 0;

     |IMPRE 0;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |INFOR informe;
     |SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;

     |NOME_DAT #999 aFitxer;
     enTD = 0;
     enTH = 0;
     enTS = 0;
     nPrim = 0;

     |BUCLE LeeAuxilia1;
     |PIEINF;

     |FININF;
     |FINIMP;
     |DESCARGA_DEF #imagen@;
 |FINSI;
 |PATH_DAT #5 dirfich0;
|FPRC;

