|FICHEROS;
   camedcob #0;

   camancob #2;
   camaasil #3;
   cacuenta #4;

   camedtmp #999;
|FIN;

|VARIABLES;

   Cont    = 0;
   Cont1   = 0;
   SwError = 0;

   Comodin  = "";
   Comodin1 = "";

   DRep     = "";
   HRep     = "";
   DTR      = 0;
   HTR      = 0;

   FechaCobro = @;
   ComodNum   = 0;

   Empres     = 1;
   DEmpre     = 0;
   DPerio     = 0;
   HEmpre     = 0;
   HPerio     = 0;
   aFichero   = "";

|FIN;

|INCLUYE acceso_i;
|INCLUYE puntos_i;

|CALCULO defectos;  |TIPO 7;
  #0#46 = fec1; |PINTA #0#46;
  #0#47 = fec2; |PINTA #0#47;
|FCAL;

|CALCULO cuenta; |TIPO 6;
    p_alfa1 = #0Campo;
    salidas = FSalida;
    |HAZ punto;
    |SI p_error ! 0;  |ERROR;  |ACABA;  |FINSI;
|FCAL;

|CALCULO Selec; |TIPO 0;
   |SI Campo = 2;
        |SI #0#2 = "S";
            |PARA Cont = 5; |SI Cont [ 14; |HACIENDO Cont = Cont + 1;
                  |C_M #0Cont,1,"N";
                  #0Cont = ""; |PINTA #0Cont;
            |SIGUE;
            |C_M #0#3,1,"S";
            |C_M #0#4,1,"S";
        |SINO;
            |PARA Cont = 5; |SI Cont [ 14; |HACIENDO Cont = Cont + 1;
                  |C_M #0Cont,1,"S";
            |SIGUE;
            |C_M #0#3,1,"N"; #0#3 = "  "; |PINTA #0#3;
            |C_M #0#4,1,"N"; #0#4 = "zz"; |PINTA #0#4;
        |FINSI;
   |FINSI;

   |SI Campo = 15;
       |SI #0#15 = "S";
           |PARA Cont = 18; |SI Cont [ 27; |HACIENDO Cont = Cont + 1;
                 |C_M #0Cont,1,"N";
                 #0Cont = 0; |PINTA #0Cont;
           |SIGUE;
           |C_M #0#16,1,"S";
           |C_M #0#17,1,"S";
       |SINO;
           |PARA Cont = 18; |SI Cont [ 27; |HACIENDO Cont = Cont + 1;
                 |C_M #0Cont,1,"S";
           |SIGUE;
           |C_M #0#16,1,"N"; #0#16 = 00; |PINTA #0#16;
           |C_M #0#17,1,"N"; #0#17 = 99; |PINTA #0#17;
       |FINSI;
   |FINSI;

   |SI Campo = 28
       |SI #0#28 = "S";
           |PARA Cont = 33; |SI Cont [ 44; |HACIENDO Cont = Cont + 1;
                 |C_M #0Cont,1,"N";
                 #0Cont = 0; |PINTA #0Cont;
           |SIGUE;
           |C_M #0#29,1,"S";
           |C_M #0#30,1,"S";
           |C_M #0#31,1,"S";
           |C_M #0#32,1,"S";
       |SINO;
           |PARA Cont = 33; |SI Cont [ 44; |HACIENDO Cont = Cont + 1;
                 |C_M #0Cont,1,"S";
           |SIGUE;
           |C_M #0#29,1,"N";
           |C_M #0#30,1,"N";
           |C_M #0#31,1,"N";
           |C_M #0#32,1,"N";
           #0#29 = 1;     |PINTA #0#29;
           #0#30 = 0;     |PINTA #0#30;
           #0#31 = 99999; |PINTA #0#31;
           #0#32 = 9;     |PINTA #0#32;
       |FINSI;
   |FINSI;
|FCAL;

|CALCULO MiraEmpresa; |TIPO 0;
   |SI FSalida = 2; |ACABA; |FINSI;
   |SI #0#28 = "S"; |ACABA; |FINSI;

   Empres = Campo - 1;
   |SI #0Empres = 0; |Y #0Campo = 0; |ACABA; |FINSI;

   |DBASE Comodin;
   Comodin1 = Comodin + "def/canempre.mas";
   Comodin = Comodin + "fich/";
   |CARGA_DEF Comodin1, canempr@;
   |SI FSalida ! 0;
      |MENAV "    Problemas al cargar el def";
      |PTEC 807;
   |SINO;
      |PATH_DAT #5 Comodin;
      |ABRE #canempr@;
      #canempr@#2 = #0Empres;
      #canempr@#3 = #0Campo;
      |LEE 010#canempr@.=;
      |SI FSalida ! 0;
         |MENAV "    Empresa inexistente";
         |ERROR;
      |FINSI;
      enEmpresa = #canempr@#2;
      enPeriodo = #canempr@#3;
      swOperacion = 1;
      |SUB_EJECUTA "camoneda.tab";

      |CIERRA #canempr@;
      |DESCARGA_DEF #canempr@;
   |FINSI;
|FCAL;

|| ********************************************************************

|PROCESO Cuentas;
   |SI #camedtmp#5 ! 0; |PRINT; |FINSI;
|FPRC;

|DEFBUCLE Cuentas;
#camedtmp#1;
;
INICIO;
FINAL;
;
NULL,Cuentas;
|FIN;

|PROCESO FechaCobro;
   |SELECT #4#camaasil;
   #camaasil#13 = #camancob#0;
   #camaasil#15 = #camancob#2;
   #camaasil#14 = #camancob#1;
   |LEE 010#camaasil.];
   |SI FSalida = 0; |Y #camaasil#13 = #camancob#0; |Y #camaasil#15 = #camancob#2;
   |Y #camaasil#14 = #camancob#1;
      |PARA; |SI FSalida = 0; |Y #camaasil#13 = #camancob#0;
      |Y #camaasil#15 = #camancob#2; |Y #camaasil#14 = #camancob#1; |HACIENDO;
         |SI #camaasil#22 = #camancob#3; |Y #camaasil#12 = "C";
            FechaCobro = #camaasil#1;
            SwError = 0;
         |FINSI;
         |LEE 010#camaasil.s;
      |SIGUE;
   |FINSI;
   |SELECT #1#camaasil;
|FPRC;

|PROCESO RellenaTempo;

   |SI #camancob#24 < 0; |ACABA; |FINSI;

   SwError = 1;
   |HAZ FechaCobro;
   |SI SwError = 1; |ACABA; |FINSI;

   #camedtmp#0 = #camancob#4;
   |LEE 010#camedtmp.=;
   |SI FSalida ! 0;
      |DDEFECTO #camedtmp;
      #camedtmp#0 = #camancob#4;

      #cacuenta#0 = #camancob#4;
      #cacuenta#1 = #camancob#5;
      |LEE 010#cacuenta.=;
      #camedtmp#1 = #cacuenta#2;
      |GRABA 020#camedtmp.b;
   |FINSI;

   #camedtmp#2 = #camancob#11;
   #camedtmp#3 = #camancob#28;
   #camedtmp#4 = #camedtmp#4 + 1;
   #camedtmp#5 = #camedtmp#5 + #camancob#24;
   ComodNum = FechaCobro - #camancob#6;
   #camedtmp#6 = #camedtmp#6 + ComodNum;
   #camedtmp#7 = #camedtmp#6 / #camedtmp#4;
   |GRABA 020#camedtmp.a;

|FPRC;

|DEFBUCLE RellenaTempo;
#camancob#1;
4,11,28,23;
01,"  ",000001,001,#0#0,DTR,DRep,"S";
99,"zz",999999,999,#0#1,HTR,HRep,"S";
;
NULL,RellenaTempo;
|FIN;

|PROCESO Empresas;
   Comodin1 = Comodin + "fich/";
   Comodin1 = Comodin1 + (("00000" + #canempr@#2) % -105);
   Comodin1 = Comodin1 + #canempr@#3 + "/";
   |PATH_DAT #2 Comodin1;
   |PATH_DAT #3 Comodin1;
   |PATH_DAT #4 Comodin1;
   |ABRE #cacuenta;
   |ABRE #camaasil;

   |SI #0#2 = "N";
      |PARA Cont = 5; |SI Cont [ 14; |HACIENDO Cont = Cont + 1;
         DRep = #0#Cont#; HRep = #0#Cont#;
         |SI #0#15 = "N";
            |PARA Cont1 = 18; |SI Cont1 [ 27; |HACIENDO Cont1 = Cont1 + 1;
               DTR = #0#Cont1#; HTR = #0#Cont1#;
            |SIGUE;
         |SINO;
            DTR = #0#16; HTR = #0#17;
         |FINSI;
         |BUCLE RellenaTempo;
      |SIGUE;
   |SINO;
      DRep = #0#3;
      HRep = #0#4;
      |SI #0#15 = "N";
         |PARA Cont1 = 18; |SI Cont1 [ 27; |HACIENDO Cont1 = Cont1 + 1;
            DTR = #0#Cont1#; HTR = #0#Cont1#;
         |SIGUE;
      |SINO;
         DTR = #0#16; HTR = #0#17;
      |FINSI;
      |BUCLE RellenaTempo;
   |FINSI;

   |CIERRA #camaasil;
   |CIERRA #cacuenta;
|FPRC;

|DEFBUCLE Empresas;
#canempr@#1002;
;
DEmpre,DPerio;
HEmpre,HPerio;
;
NULL,Empresas;
|FIN;


|PROCESO Listado; |TIPO 3;

   |IMPRE 0;
   |SI FSalida < 0; |ACABA; |FINSI;

   |INFOR "camedcob";
   |SI FSalida < 0; |ACABA; |FINSI;

   aFichero = "1x" + Usuario;
   |NOME_DAT #999 aFichero;
   |DELINDEX #999;


   |DBASE Comodin;
   Comodin1 = Comodin + "def/canempre.mas";
   |CARGA_DEF Comodin1, canempr@;
   |SI FSalida ! 0;
      |MENAV "    Problemas al cargar el def";
      |FININF;
      |FINIMP;
      |ACABA;
   |FINSI;

   |ABRE #canempr@;
   |SI #0#28 = "S";
       DEmpre = #0#29; DPerio = #0#30;
       HEmpre = #0#31; HPerio = #0#32;
       |BUCLE Empresas;
   |SINO;
       |PARA Cont = 33; |SI Cont [ 44; |HACIENDO Cont = Cont + 1;
             DEmpre = #0Cont; HEmpre = #0Cont;
             Cont = Cont + 1;
             DPerio = #0Cont; HPerio = #0Cont;

            |BUCLE Empresas;
      |SIGUE;
   |FINSI;

   |DESCARGA_DEF #canempr@;

   |BUCLE Cuentas;

   |CIERRA #camedtmp;

   |PIEINF;
   |FININF;
   |FINIMP;

|FPRC;
