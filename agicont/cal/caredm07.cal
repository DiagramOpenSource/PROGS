   |LEE 010#camingli.=;
*/
   |REPON_FICHA #camingli;

|FPRC;

|PROCESO ParteCadena;

   LongAlfa = Cadena % 0;
   Long = LongAlfa;
   |SI Long [ Long1;
      Parte1 = Cadena;
      Parte2 = "";
      |ACABA;
   |SINO;
      |PARA; |SI Parte1 ! " "; |HACIENDO;
         Calc = (Long1 * 100) + 1;
         Parte1 = Cadena % Calc;
         |SI Parte1 = " "; |O Parte1 = ",";
            Calc = 100 + Long1;
            Parte1 = Cadena % Calc;
            Parte2 = Cadena - Parte1;
            |SI Parte1 = Cadena; Parte2 = ""; |FINSI;
            Long2 = Long - Long1;
            |SAL;
         |SINO;
            Long1 = Long1 - 1;
         |FINSI;
      |SIGUE;
   |FINSI;

|FPRC;

|PROCESO SeparaMil;
   |SI Moneda = "E";
      Comodin = Cadena;
      aDecims = Comodin % -102;
      Comodin = Comodin - aDecims; |QBF aDecims; aDecims = "." + aDecims;
      Cadena = Comodin;
   |SINO;
      aDecims = "";
   |FINSI;
   Comodin = "";
   LongAlfa = Cadena % 0; Long = LongAlfa;
   |PARA; |SI Long > 3; |HACIENDO;
      Comodin = "," + (Cadena % -103) + Comodin;
      Comodin1 = Cadena % -103;
      Cadena = Cadena - Comodin1;
      LongAlfa = Cadena % 0; Long = LongAlfa;
   |SIGUE;
   Cadena = Cadena + Comodin + aDecims;
|FPRC;

|PROCESO PideMoneda;
   |PUSHV 1943 2177;
   |BLANCO 1943 2177;
   |CUADRO 1945 2175;
   |ATRI R; |PINTA 1946 , " Seleccione moneda de emision "; |ATRI N;
   |PINTA 2053, "(Pesetas/Euros)";
   |ATRI I; |PINTA 2054, "P"; |PINTA 2062, "E"; |ATRI N;
   E_sup  = 1;
   E_inf  = "EP";
   |SI MonBase = 1; Moneda = "P"; |SINO; Moneda = "E"; |FINSI;
   Moneda = 2069 ? 0;
   |POPV;
|FPRC;

|PROCESO Conversion;
   |SI MonBase = 1;
      |SI Moneda = "E";
         nCantidad = ((nCantidad / 166.386) ! 2) * 100;
      |FINSI;
   |SINO;
      |SI Moneda = "P";
         nCantidad = (nCantidad * 166.386) ! 0;
      |SINO;
         nCantidad = nCantidad * 100;
      |FINSI;
   |FINSI;
|FPRC;
