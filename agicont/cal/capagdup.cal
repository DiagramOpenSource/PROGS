|FICHEROS;
     caconimp #99;
|FIN;

|VARIABLES;
     &nPagDup = 0;
     &nSwPagDup = 0;
|FIN;

|PROCESO &PDuplex;
     |DDEFECTO #99;
     |ABRE #99;
     |LEE 000#99p;
     |CIERRA #99;

     |SI #99#1 = "L";|Y #99#5 = "S";
          |SI nSwPagDup = 0;
               nPagDup = nPagDup + 1;
               nSwPagDup = 1;
          |SINO;
               nSwPagDup = 0;
          |FINSI;
     |SINO;
          nPagDup = nPagDup + 1;
          nSwPagDup = 1;
     |FINSI;
|FPRC;
