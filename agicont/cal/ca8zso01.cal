 |PUSHV 0809 1457;
 |PINPA #0#50;
 #50#0 = #10#0;
 #50#1 = #10#0 + 1; |SI #50#1 > 0; #50#1 = 1; |FINSI;
 #50#4 = "Descripcion Lineas";
 |PINDA #0#50;
 |ENDATOS #1#50;
