|FICHEROS;
     cacuamod #0;
|FIN;

|VARIABLES;
     Fichero130   = "";
     Fichero300   = "";
     Fichero390   = "";
     Fichero110_1 = "";
     Fichero110_2 = "";
     PathDef      = "";
     CopiaOrigen  = "";
     CopiaDestino = "";
|FIN;

|PROCESO Tipo3; |TIPO 3;

/*
|SI #0#0 = 1; Fichero110_1 = "eosbn110.inf"; |FINSI;
|SI #0#0 = 2; Fichero110_1 = "eosbs110.inf"; |FINSI;
|SI #0#0 = 1; Fichero110_2 = "eosyn110.inf"; |FINSI;
|SI #0#0 = 2; Fichero110_2 = "eosys110.inf"; |FINSI;
*/
|SI #0#0 = 1; Fichero130   = "caemi130.01"; |FINSI;
|SI #0#0 = 2; Fichero130   = "caemi130.02"; |FINSI;
|SI #0#0 = 3; Fichero130   = "caemi130.03"; |FINSI;

|SI #0#1 = 1; Fichero300   = "caemi300.01"; |FINSI;
|SI #0#1 = 2; Fichero300   = "caemi300.02"; |FINSI;

|SI #0#2 = 1; Fichero390   = "caem1390.01"; |FINSI;
|SI #0#2 = 2; Fichero390   = "caem1390.02"; |FINSI;

|DDEF PathDef;

CopiaOrigen  = PathDef + Fichero130;
CopiaDestino = PathDef + "caemi130.inf";
|COPIA_FICHERO CopiaOrigen, CopiaDestino;

CopiaOrigen  = PathDef + Fichero300;
CopiaDestino = PathDef + "caemi300.inf";
|COPIA_FICHERO CopiaOrigen, CopiaDestino;

CopiaOrigen  = PathDef + Fichero390;
CopiaDestino = PathDef + "caem1390.inf";
|COPIA_FICHERO CopiaOrigen, CopiaDestino;

