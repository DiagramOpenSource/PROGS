#1#3;
18;
#0#0, #0#1, feini, 000000, "û";
#0#0, #0#1, fefin, 999998, "û";
e;
NULL, indexa;
|FIN;

|DEFBUCLE zconcili;
#2#1;
8;
0001, " ";
9999, " ";
e;
NULL, grabaapun;
|FIN;

