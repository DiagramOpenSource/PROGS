|PROCESO cuenta; |TIPO 6;
     salidas = FSalida;
    p_alfa1 = #0Campo;
|HAZ punto;
|SI p_error ! 0;  |ERROR;  |ACABA;  |FINSI;
   cta_t = Campo;
   dig_t = Campo + 1;
   |HAZ cuenta_t;
|FPRC;

|PROCESO cuenta_t;
   cta  = #0cta_t;
   long = cta % 0;
   sw   = 0;
|SI long = dig4; sw = 1; #0dig_t = 1; |FINSI;
|SI long = dig5; sw = 1; #0dig_t = 2; |FINSI;
|SI long = dig6; sw = 1; #0dig_t = 3; |FINSI;
|SI long = dig7; sw = 1; #0dig_t = 4; |FINSI;
|SI long = dig8; sw = 1; #0dig_t = 5; |FINSI;
|SI long = dig9; sw = 1; #0dig_t = 6; |FINSI;

|SI sw = 0;
    #0dig_t = 0;
    |MENAV "    Longitud de cuenta fuera de Nivel !";
    |ERROR;
    |ACABA;
|FINSI;
|FPRC;

|PROCESO cuefin; |TIPO 0;
error = 0;
|ABRE #10;
#10#0 = #0cta_t;
#10#1 = #0dig_t;
|LEE 040#10=;
|SI FSalida ! 0; error = 1; |CIERRA #10; |ERROR; |ACABA; |FINSI;
|SI #10#3 = "N";
    error = 1;
    |CIERRA #10;
    |MENAV "      La cuenta no tiene pase DIRECTO !";
    |ERROR;
    |ACABA;
|FINSI;
#0#4 = #10#2;
|PINTA #0#4;
|CIERRA #10;
|FPRC;

|PROCESO calcuant;
|SI #1#8 = "D";
        #0#2 = #0#2 + #1#9;
|SINO;
        #0#2 = #0#2 - #1#9;
|FINSI;
|FPRC;

|PROCESO indexa;
    contador = contador + 1;
    #2#0 = contador;
    #2#1 = #1#0;
    #2#2 = #1#1;
    #2#3 = #1#2;
    #2#4 = #1#3;
    #2#5 = #1#7;
|SI #1#8 = "D";
        #2#6 = #1#9;
        #2#7 = 0;
|SINO;
        #2#7 = #1#9;
        #2#6 = 0;
|FINSI;
    #2#8 = "*";
|GRABA 020#2n;
|FPRC;

|PROCESO grabaapun;
    #1#0 = #2#1;
    #1#1 = #2#2;
    #1#2 = #2#3;
    #1#3 = #2#4;
|LEE 101#1=;
|SI FSalida = 0;
        #1#18 = " ";
    |GRABA 020#1a;
|FINSI;
|LIBERA #1;
|FPRC;

|DEFBUCLE camaasil0;
#1#3;
18;
#0#0, #0#1, feini, 000000, " ";
#0#0, #0#1, fefin, 999998, " ";
e;
NULL, calcuant;
|FIN;

