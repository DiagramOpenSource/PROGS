    elegir5 = " Impresora ";
    elegir6 = " Pantalla ";
    posic = 0;
    mas = 0;
    lin_pan = 0;
    alfa1 = "";
    &vto1 = @;
    &vto2 = @;
    &vto3 = @;
    &vto4 = @;
    &imp1 = 0;
    &imp2 = 0;
