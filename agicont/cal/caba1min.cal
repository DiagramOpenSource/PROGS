|FIN;

|INCLUYE acceso_i;

|| ************************* calculos pantalla ******************************
|PROCESO mayor;
   alfa1 = #0Campo > " ";
   #0Campo = alfa1;
   |PINTA #0Campo;
|FPRC;

|PROCESO fechas;
   cc = Campo - 1;
   |SI #0Campo < #0cc;
       |MENAV " Error en la introduccion ";
       |ERROR;
       |ACABA;
   |FINSI;
|FPRC;

|| *************************************************************************

|PROCESO detalles;       || lee el banco y la cuenta
    ctab = banco;
    nomb = "";
    imp1 = 0;
    imp2 = 0;
    imp3 = 0;
    imp4 = 0;
    imp5 = 0;

    #2#10 = ctab;
    #2#11 = dbanc;
|LEE 001#2=;
|SI FSalida = 0;
      nomb = #2#1;
      |ACABA;
      |FINSI;

    #3#00 = ctab;
    #3#01 = dbanc;
|LEE 001#3=;
|SI FSalida ! 0;  |DDEFECTO #3;  |FINSI;
    nomb = #3#2;
|FPRC;

|PROCESO impre1;
  |SI #caba1min#17 ! "A";
      |SI #caba1min#17 ! #camingca#10;  |ACABA;  |FINSI;  || impreso confirme
  |FINSI;
  |SI #caba1min#11 ! "A";
      |SI #caba1min#11 ! #camingca#12;  |ACABA;  |FINSI;  || emision
  |FINSI;
  |SI #caba1min#12 ! "A";
      |SI #caba1min#12 ! #camingca#11;  |ACABA;  |FINSI;  || vencimiento
  |FINSI;

|SI sw = 0;
        banco = #camingca#4;
        dbanc = #camingca#5;
        |HAZ detalles;
        sw = 1;
|FINSI;
|SI banco ! #camingca#4;
    |PRINT;                   || imprime total banco anterior
    tot1 = tot1 + imp1;
    tot2 = tot2 + imp2;
    tot3 = tot3 + imp3;
    tot4 = tot4 + imp4;
    tot5 = tot5 + imp5;
        banco = #camingca#4;
        dbanc = #camingca#5;
        |HAZ detalles;
|FINSI;

|| ============ Calculo del importe a columna correcta

|SI #camingca#2 [ vto1;
    imp1 = imp1 + #camingca#9;
    |VETE sumado;
|FINSI;

|SI #camingca#2 > vto1;|Y #camingca#2 [ vto2;
    imp2 = imp2 + #camingca#9;
    |VETE sumado;
|FINSI;

|SI #camingca#2 > vto2;|Y #camingca#2 [ vto3;
    imp3 = imp3 + #camingca#9;
    |VETE sumado;
|FINSI;

|SI #camingca#2 > vto3;|Y #camingca#2 [ vto4;
    imp4 = imp4 + #camingca#9;
    |VETE sumado;
|FINSI;

    imp5 = imp5 + #camingca#9;

|ET sumado;
|FPRC;

|DEFBUCLE impre;
 #camingca#3;
 1; || ..... ordenado por banco+vto, selec. prov y emision
 #caba1min#2, 0, #caba1min#4, #caba1min#0, #caba1min#8;
 #caba1min#3, 9, #caba1min#5, #caba1min#1, #caba1min#9;
 e;
 NULL, impre1;
|FIN;

|PROCESO impresora;
|IMPRE 0;
|SI FSalida = 0;
    |INFOR informe;
    |ABRE #camanban;
    |SELECT #2#camanban;
    |ABRE #cacuenta;
    |BUCLE impre;
    |CIERRA #camanban;
    |CIERRA #cacuenta;
    |SI sw = 1;
        |PRINT;
        tot1 = tot1 + imp1;
        tot2 = tot2 + imp2;
        tot3 = tot3 + imp3;
        tot4 = tot4 + imp4;
        tot5 = tot5 + imp5;
        |PIEINF;
    |FINSI;
    |FININF;
|SINO;
    |MENAV "    Proceso Abortado ...";
|FINSI;
|FINIMP;
|FPRC;


|| ********************** PANTALLA

|PROCESO correte;
|CAMPO_POSICION #4#0,0,posic; posic=posic+mas; |CAMPO_POSICION #4#0,1,posic;
|CAMPO_POSICION #4#1,0,posic; posic=posic+mas; |CAMPO_POSICION #4#1,1,posic;
|CAMPO_POSICION #4#6,0,posic; posic=posic+mas; |CAMPO_POSICION #4#6,1,posic;
|CAMPO_POSICION #4#7,0,posic; posic=posic+mas; |CAMPO_POSICION #4#7,1,posic;
|CAMPO_POSICION #4#8,0,posic; posic=posic+mas; |CAMPO_POSICION #4#8,1,posic;
|CAMPO_POSICION #4#9,0,posic; posic=posic+mas; |CAMPO_POSICION #4#9,1,posic;
|CAMPO_POSICION #4#10,0,posic; posic=posic+mas; |CAMPO_POSICION #4#10,1,posic;
|FPRC;

|PROCESO pant4;
  |PINPA #0#4;
  |DDEFECTO #4;
      #4#2  = vto1;    || 1§ vto
      #4#3  = vto2;    || 2§ vto
      #4#4  = vto3;    || 3§ vto
      #4#5  = vto4;    || 4§ vto
  |PINTA #4#2;
  |PINTA #4#3;
  |PINTA #4#4;
  |PINTA #4#5;
|FPRC;

|PROCESO empieza;
  |HAZ pant4;
  mas = lin_pan * -100;
  lin_pan = 0;
  |HAZ correte;
|FPRC;


|PROCESO panta1;
  |SI #caba1min#17 ! "A";
      |SI #caba1min#17 ! #camingca#10;  |ACABA;  |FINSI;  || impreso confirme
  |FINSI;
  |SI #caba1min#11 ! "A";
      |SI #caba1min#11 ! #camingca#12;  |ACABA;  |FINSI;  || emision
  |FINSI;
  |SI #caba1min#12 ! "A";
      |SI #caba1min#12 ! #camingca#11;  |ACABA;  |FINSI;  || vencimiento
  |FINSI;

|SI sw = 0;
        banco = #camingca#4;
        dbanc = #camingca#5;
        |HAZ detalles;
        sw = 1;
|FINSI;
|SI banco ! #camingca#4;
    tot1 = tot1 + imp1;
    tot2 = tot2 + imp2;
    tot3 = tot3 + imp3;
    tot4 = tot4 + imp4;
    tot5 = tot5 + imp5;
    |HAZ imppan;                   || imprime total banco anterior
        banco = #camingca#4;
        dbanc = #camingca#5;
        |HAZ detalles;
|FINSI;

|| ============ Calculo del importe a la linea correcta

|SI #camingca#2 [ vto1;
    imp1 = imp1 + #camingca#9;
    |VETE sumad1;
|FINSI;

|SI #camingca#2 > vto1;|Y #camingca#6 [ vto2;
    imp2 = imp2 + #camingca#9;
    |VETE sumad1;
|FINSI;

|SI #camingca#2 > vto2;|Y #camingca#2 [ vto3;
    imp3 = imp3 + #camingca#9;
    |VETE sumad1;
|FINSI;

|SI #camingca#2 > vto3;|Y #camingca#2 [ vto4;
    imp4 = imp4 + #camingca#9;
    |VETE sumad1;
|FINSI;

    imp5 = imp5 + #camingca#9;

|ET sumad1;

|FPRC;

|PROCESO imppan;
    lin_pan = lin_pan + 1;
|DDEFECTO #4;
    #4#0  = ctab;    || cuenta banco
    #4#1  = nomb;    || nombre banco
    #4#2  = vto1;    || 1§ vto
    #4#3  = vto2;    || 2§ vto
    #4#4  = vto3;    || 3§ vto
    #4#5  = vto4;    || 4§ vto
    #4#6  = imp1;    || importe 1
    #4#7  = imp2;    || importe 2
    #4#8  = imp3;    || importe 3
    #4#9  = imp4;    || importe 4
    #4#10 = imp5;    || importe posterior
    #4#11 = tot1;    || total 1
    #4#12 = tot2;    || total 2
    #4#13 = tot3;    || total 3
    #4#14 = tot4;    || total 4
    #4#15 = tot5;    || total posterior
|PINDA #0#4;
|SI swfin = 0;
    |SI lin_pan < 11;
            mas = 100;
    |SINO;
            mas = -1000;
            lin_pan = 0;
        |PAUSA;
        |HAZ pant4; || PINPA #0#4;
    |FINSI;
    |HAZ correte;
|FINSI;
|FPRC;

|DEFBUCLE panta;
 #camingca#3;
 1; || ..... ordenado por banco+vto, selec. prov y emision
 #caba1min#2, 0, #caba1min#4, #caba1min#0, #caba1min#8;
 #caba1min#3, 9, #caba1min#5, #caba1min#1, #caba1min#9;
 e;
 NULL, panta1;
|FIN;

|PROCESO pantalla;
|HAZ pant4; || PINPA #0#4;
|ABRE #camanban;
|SELECT #2#camanban;
|ABRE #cacuenta;
|BUCLE panta;
      |SI sw = 1;
          swfin = 1
          tot1 = tot1 + imp1;
          tot2 = tot2 + imp2;
          tot3 = tot3 + imp3;
          tot4 = tot4 + imp4;
          tot5 = tot5 + imp5;
          |HAZ imppan;
          |PAUSA;
          |FINSI;
|CIERRA #camanban;
|CIERRA #cacuenta;

|FPRC;

|| -------------------------------------------------------------------------

|PROCESO impre; |TIPO 3;
|HAZ inicio;
         vto1 = #caba1min#13;
         vto2 = #caba1min#14;
         vto3 = #caba1min#15;
         vto4 = #caba1min#16;
|MENU elegir;
|SI FSalida > 0;
    |SI FSalida = 1;  |HAZ impresora;  |FINSI;
    |SI FSalida = 2;  |HAZ pantalla;   |FINSI;
|FINSI;
|FPRC;
