|PROCESO recibo;
  || alfa1 = directo + "enre" + fichero + ".seq";
  alfa1 = directo + "enco" + fichero + ".seq";
  |FABRE alfa1;
  |SI FSalida < 0;  Pos = -1;  |ACABA;  |FINSI;
  handle = FSalida;
  alfa1 = "enre" + fichero;
  alfa1 = alfa1 % -108;
  |PATH_DAT #12 dir_fich1;
  |NOME_DAT #12 alfa1;
  |ABRE #12;
  |CIERRA #12;
  |DELINDEX #12;
  |ABRE #12;
  |ABRE #18;
  |PARA; |SI; |HACIENDO;
/*
         canal  = handle + " 242";
         |FLEE canal;
         |SI FSalida ! 242;  |SAL;  |FINSI;
*/
