         |FLEE canal;
         |SI FSalida ! 201;  |SAL;  |FINSI;
         canal1  = handle + " 241";
         |FLEE canal1;
         |SI FSalida ! 241;  |SAL;  |FINSI;

         alfa2 = canal % 11101;
         |SI alfa2 = " ";
            alfa2 = canal % 11202;
            |SI alfa2 ! "99"; |HAZ sep_recibo; |FINSI;
         |FINSI;
  |SIGUE;
  |FCIERRA handle;
  |CIERRA #12;
  |CIERRA #18;
  Pos = -1;
|FPRC;

