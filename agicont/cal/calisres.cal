|PROCESO graba_reg;
  #5#0 = a_plan;
  #5#1 = a_tipo;
  #5#2 = a_nume;
  |LEE 101#5=;
  salidas = FSalida;
  |SI salidas ! 0;
      |DDEFECTO #5;
      #5#0 = a_plan;
      #5#1 = a_tipo;
      #5#2 = a_nume;
  |FINSI;
  #5#3 = a_desc;
  #5#4 = #5#4 + a_imp1;
  #5#5 = #5#5 + a_imp2;
  #5#6 = #5#6 + a_imp3;
  #5#7 = #5#7 + a_imp4;
  #5#8 = #5#8 + a_imp5;
  |SI salidas = 0; |GRABA 020#5a; |FINSI;
  |SI salidas ! 0; |GRABA 020#5n; |FINSI;
  |LIBERA #5;
|FPRC;

