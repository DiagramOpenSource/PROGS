  |ABRE #3;
  |LEE 000#3p;
    |SI FSalida = 0;
        #0#0 = #3#0;
        |PINTA #0#0;
    |FINSI;
  |CIERRA #3;
  |DDEFECTO #8;
  |ABRE #8;
  #8#0 = #30#2;
  #8#1 = #30#26;
  |LEE 000#8=;
    |SI FSalida = 0;
        |SI #8#83 = "N";
            |CAMPO_MODIFICA #0#1, 1, "S";
            #0#1 = #8#88;
            |PINTA #0#1;
