|PROCESO resumen; |TIPO 3;
 |MENU menu;
 |SI 0 < FSalida;          || Se ha elegido opcion
     menu2 = FSalida;

     |ABRE #5;
     |CIERRA #5;
     |DELINDEX #5;
     |SI menu2 = 2;
         |IMPRE 0;
         |SI FSalida ! 0; |ACABA; |FINSI;
         |INFOR informe;
         |SI FSalida ! 0;
             |MENAV men1;
             |FINIMP;
             |ACABA;
         |FINSI;
     |FINSI;

     a_plan = #0#0;
     |ABRE #5;
     |HAZ lee_exist;

     |ABRET #2;
     |ABRET #4;
     |BUCLE calisres0;

     |CIERRAT #2;
     |CIERRAT #4;
     |CIERRA #5;
     |SI menu2 = 1; |HAZ panta; |FINSI;  || Por pantalla
     |SI menu2 = 2; |HAZ impre; |FINSI;  || Por impresora
     |DELINDEX #5;
 |FINSI;
|FPRC;

|PROCESO impre;
 sw = 0;
 sw_def = 0;
 |HAZ calisres1;
 |SI sw_def = 1;
     |PIEINF;
 |FINSI;
 |FININF;
 |FINIMP;
|FPRC;

|PROCESO panta;
 alfa4  = &45 * 23 + &179 + &45 * 10 + &179 + &45 * 10 + &179 + &45 * 10;
 alfa4  = alfa4 + &179 + &45 * 10 + &179 + &45 * 12;lin = 100;
 |PINPA #0#5;       || Pinta la pantalla del informe
 |PINTA 0511, #30#1;
 |PINTA 0559, #30#26;
 |PINTA 0570, #0#2;
 sw = 0;
 sw_def = 0;
 |HAZ calisres1;
 |SI sw_def = 1;
     |PAUSA;
 |FINSI;
|FPRC;

|| **************************************************************************

|PROCESO operapanta;   || Operaciones por pantalla;
 |SI lin > 1400;
     |HAZ cambiapag;
     |SI sw_error = 1; |ACABA; |FINSI;
 |FINSI;
 |SI swlinea = 0; |O swlinea = 4;
     |SI lin > 1300;
         |HAZ cambiapag;
         |SI sw_error = 1; |ACABA; |FINSI;
     |FINSI;
 |FINSI;
 |SI swlinea = 0;
     yy = lin + 801
     |ATRI R;
     |PINTA yy, subt;
     |ATRI 0;
     lin = lin + 100;
 |FINSI;
 |SI swlinea = 1;
     Pos = lin;
     |PINDA #0#5;         || Pinta las lineas
     Pos = -1;
     lin = lin + 100;     || Posicionar una linea mas abajo
 |FINSI;
 |SI swlinea = 4;
     yy = lin + 801; |PINTA yy, alfa4;
     lin = lin + 100;
 |FINSI;
 |SI swlinea ] 2;
     subt = subt % 123;
     #5#4 = tot1; #5#5 = tot2;
     #5#6 = tot3; #5#7 = tot4;
     #5#8 = tot5;
     |ATRI N;
     yy = lin + 801; |PINTA yy, subt;
     yy = lin + 825; |PINTA yy, #5#4;
     yy = lin + 836; |PINTA yy, #5#5;
     yy = lin + 847; |PINTA yy, #5#6;
     yy = lin + 858; |PINTA yy, #5#7;
     yy = lin + 869; |PINTA yy, #5#8;
     |ATRI 0;
     lin = lin + 200;     || Posicionar una linea mas abajo
 |FINSI;
|FPRC;

|PROCESO cambiapag;
 sw_error = 0;
 lin = 100;
 |PAUSA;
 |SI 1 = FSalida;|O 7 = FSalida;
     |ERROR10;    || Rompe el bucle y acaba
     sw_error = 1;
 |SINO;
     |PINPA #0#5;
     |PINTA 0511, #30#1;
     |PINTA 0559, #30#26;
     |PINTA 0570, #0#2;
     FSalida = 0;
 |FINSI;
|FPRC;
