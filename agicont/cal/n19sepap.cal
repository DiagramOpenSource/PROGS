|PROCESO ChequeoDatos;
  swError = 0;

  #caclipro#23 = "C";
  #caclipro#10 = aCuenta;
  #caclipro#11 = nNivel;
  |LEE 040#caclipro.=;      ||** Ordena por Banco/Sucursal;
  |SI FSalida ! 0;
      swError = 1;
      eaError =  " Cliente Inexistente. ";
  |SINO;
      aRefer   = #caclipro#36; |QBF aRefer;
      aFecRef  = #caclipro#37;
      aCodIBAN = #caclipro#30; |QBF aCodIBAN;
      |SI aRefer = ""; |O aCodIBAN = ""; |O aFecRef = "01.01.0000";
          eaError = " Faltan datos para la Remesa: ";
          |SI aRefer   = ""; eaError = eaError + "-Referencia de mandato "; |FINSI;
          |SI aCodIBAN = ""; eaError = eaError + "-Codigo IBAN "; |FINSI;
          |SI aFecRef  [ "01.01.1900"; eaError = eaError + "-Fecha Firma mandato "; |FINSI;
          swError = 1;
      |FINSI;
  |FINSI;

  |SI swError = 1; |Y nSwOp = 1;
      |SI nError = 0;
          |IMPRE 0;
          |SI FSalida ! 0; nError = 2; |ERROR10; |ACABA; |FINSI;
          |INFOR "n19sepap";
          |SI FSalida ! 0; nError = 2; |ERROR10; |FINIMP; |ACABA; |FINSI;
      |FINSI;
      |PRINT;
      nError = 1;
  |FINSI;
|FPRC;

|PROCESO NuevoFinan;
      |DDEFECTO #901;
      #901#0 = #3#8;
      |LEE 101#901=;
      |SI FSalida ! 0;
          |GRABA 020#901b;
      |FINSI;
      #901#1 = #901#1 + 1;
      #901#2 = #901#2 + #3#7;
      |GRABA 020#901a;
      |LIBERA #901;
|FPRC;

