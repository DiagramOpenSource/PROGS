|PROCESO ReemplazaStr;
   aAlfa = aCadena - aChar1;
   |PARA; |SI FSalida ! 0; |HACIENDO;
      nCalc = FSalida + 98; aParte2 = aAlfa % nCalc;
      aParte1 = aAlfa - aParte2;
      aCadena = aParte1 + aChar2 + aParte2;

      aAlfa = aCadena - aChar1;
   |SIGUE;
|FPRC;

|PROCESO ObtenId;
