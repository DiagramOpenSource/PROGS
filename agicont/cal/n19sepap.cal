  nError = 0;

  |ABRE #caclipro;
  |ABRE #3;
  |SI #0#15 = "N";
      nSwOp = 1; |BUCLE camancob;
      |SI nError = 2;
          |CIERRA #3;
          |CIERRA #2;
          |DELINDEX #3;

          |MENAV "0000Error en Informe de errores.";
          |ACABA;
      |FINSI;
      |SI nError = 1;
          |PIEINF;
          |FININF;
          |FINIMP;
          |SI nConta ! 0;
              |CONFI "0000SHay errores en los Datos de los Recibos. Continuar con la Remesa";
              |SI FSalida = 0; nError = 0; |FINSI;
          |SINO;
              |MENAV "0000Hay errores en los Datos de los Recibos y no pasan a la Remesa";
          |FINSI;
      |FINSI;
      |SI nError = 0;
          nConta = 0;
          nTotal = 0;
          nSwOp  = 0;
          |BUCLE camancob;
      |FINSI;
  |SINO;
      |ABRE #camancob;
      nSwOp = 1; |BUCLE caremesl;
      |SI nError = 2;
          |CIERRA #3;
          |CIERRA #2;
          |DELINDEX #3;

          |MENAV "0000Error en Informe de errores.";
          |ACABA;
      |FINSI;
      |SI nError = 1;
          |PIEINF;
          |FININF;
          |FINIMP;
          |MENAV "0000Hay Errores en los Datos de los Recibos. Proceso interrumpdio";
      |SINO;
          nConta = 0;
          nTotal = 0;
          nSwOp  = 0;
          |BUCLE caremesl;
      |FINSI;
      |CIERRA #camancob;
  |FINSI;
  |CIERRA #caclipro;
  |CIERRA #3;

  |SI #0#26 = "S";
      |CIERRA #901;
  |FINSI;

  |SI nError = 0; |Y nConta = 0; |Y #0#15 = "N";
      |MENAV "    No existen Recibos para remesar";
      nError = 1;
  |FINSI;

  |SI nError = 0;

      |ABRE #5;
      #5#0 = #0#9;
      |LEE 040#5=;

      |SI #0#25 = "T";
          eaNombre1 = "assecrec.seq";
          |ABRE #3;
          |ABRE #2;
          |ABRE #6;
          |DFICE empresa;
          fiche = "wb  " + empresa + "assecrec.seq";
          |QBF fiche;
          |FABRE fiche;
          handle = FSalida;
          |SI FSalida ] 0;
              |HAZ cabecera_p;
              |HAZ cab_ordenante;
              |HAZ regindiv;
              |HAZ campo_totales;
          |FINSI;
          FSalida = handle;
          |FCIERRA FSalida;
          |CIERRA #6;
          |CIERRA #2;
          |CIERRA #3;
      |SINO;
          |HAZ NuevoXML;
      |FINSI;
      |CIERRA #5;
  |FINSI;

  |DELINDEX #3;
  |CIERRA #2;

  |SI #0#15 = "S";
      |ABRE #caremesc;
      #caremesc#0 = #0#16;
      |LEE 141#caremesc.=;
      |SI FSalida = 0;
           #caremesc#8 = "D";     || Marco emitida como "D"isquete
                                  || Se debera controlar que este valor no se cambie
                                  || al intentar imprimir la remesa
           |GRABA 021#caremesc.a;
      |FINSI;
      |LIBERA #caremesc;
      |CIERRA #caremesc;
  |FINSI;

   |SI nError = 0;
       E_sup = 1;
       E_inf = "SNsn";
       sino  = "S";
       sino  = 1769 ? 1;
       sino  = sino > " ";
       |SI sino = "S"; |SUB_EJECUTA_NP "cacopdis"; |FINSI;
   |FINSI;
|FCAL;

||***********************************************************;
||************ REGISTRO DE CABECERA DE PRESENTADOR **********;
||***********************************************************;
|PROCESO cabecera_p;
  fechalf = #0#8;
  dd = fechalf % A102;
  mm = fechalf % A402;
  aa = fechalf % A902;
  cabe2 = dd + mm + aa;
  cabe5 =  #5#7 + #5#8;
  varalfa1 = cabe1 + #5#2 + #0#12 + cabe2 + cabe3 + #5#3 + libre20+ cabe5 + libre12 + libre40+ final14;
  ||contenido = handle + espacio + varalfa1 + elefe;
  ||FGRABA contenido;

  aSufijo = ("000" + #n19sepap#12) % -103; aNif = #5#2; |HAZ ObtenId;

  |SI #n19sepap#23 = "S";
     varalfa1 = "0119143001";
     |SI #n19sepap#24 = "S"; varalfa1 = "0119154001"; |FINSI;
  |SINO;
     varalfa1 = "0119445001";
  |FINSI;

  aAlfa = (aIdent + (" " * 35)) % 0135;
  varalfa1 = varalfa1 + aAlfa;
  aAlfa = (#5#3 + (" " * 70)) % 0170;
  varalfa1 = varalfa1 + aAlfa;
  aAlfa = #n19sepap#8; |QBF aAlfa; aAlfa = (aAlfa % 0704) + (aAlfa % 0402) + (aAlfa % 0102);
  varalfa1 = varalfa1 + aAlfa;

  aAlfa = #n19sepap#8; |QBF aAlfa;  aAlfa  = (aAlfa  % 0704) + (aAlfa  % 0402) + (aAlfa  % 0102);
  |HORA aAlfa1;        |QBF aAlfa1; aAlfa1 = (aAlfa1 % 0102) + (aAlfa1 % 0402) + (aAlfa1 % 0702) + "00000";

  aAlfa = "PRE" + aAlfa + aAlfa1; |QBF aAlfa;
  aAlfa = aAlfa + #5#2;
  aAlfa = aAlfa + aSufijo;             || falta Nif y sufijo

  aAlfa = (aAlfa + (" " * 35)) % 0135;

  varalfa1 = varalfa1 + aAlfa;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;

  varalfa1 = #5#7 + #5#8;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;

  varalfa1 = " " * 200;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;
  varalfa1 = " " * 234;
  contenido = handle + espacio + varalfa1 + elefe; |FGRABA contenido;

  l = l + 1;
|FPRC;

||***********************************************************;
||************ REGISTRO DE CABECERA DE ORDENANTE ************;
||***********************************************************;
|PROCESO cab_ordenante;
  fechalf = #0#8;
  dd = fechalf % A102;
  mm = fechalf % A402;
  aa = fechalf % A902;
  orde2 = dd + mm + aa;
  fechacargo = #0#11;
  dd = fechacargo % A102;
  mm = fechacargo % A402;
  aa = fechacargo % A902;
  orde3 = dd + mm + aa;
  orde5 = #5#4 + #5#5 + #5#11 + #5#6;
  varalfa1="";
  varalfa1 = orde1 + #5#2 + #0#21 + orde2 + orde3 + #5#3 + orde5 + libre8 + orde6 + libre10 + libre40 + final14;
  ||contenido = handle + espacio + varalfa1 + elefe;
  ||FGRABA contenido;

  aSufijo = ("000" + #n19sepap#21) % -103; aNif = #5#23; |HAZ ObtenId;
  |QBF aIdent; aIdent = (aIdent + (" " * 35)) % 0135;
  aAlfa = fechacargo; |QBF aAlfa; aAlfa = (aAlfa % 0704) + (aAlfa % 0402) + (aAlfa % 0102);

  |SI #n19sepap#23 = "S"
     varalfa1 = "0219143002";
     |SI #n19sepap#24 = "S"; varalfa1 = "0219154002"; |FINSI;
  |SINO;
     varalfa1 = "0219445002";
  |FINSI;
  varalfa1 = varalfa1 + aIdent + aAlfa;
  aAlfa = #5#22; |QBF aAlfa; aAlfa = (aAlfa + (" " * 70)) % 0170;
  varalfa1 = varalfa1 + aAlfa;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;

  varalfa1 = #5#25 + #5#26 + #5#27 + "ES" + #5#32;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;

  varalfa1 = " " * 200;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;
  varalfa1 = " " * 101;
  contenido = handle + espacio + varalfa1 + elefe; |FGRABA contenido;

  l = l + 1;
|FPRC;

||***********************************************************;
||******************* REGISTRO INDIVIDUAL *******************;
||***********************************************************;
|PROCESO regindiv;
  |SELECT #3#3;          ||Cojo la tercera clave del fichero CCC y Cta.
  |LEE 040#3p;
  |SI FSalida ! 0;  |ACABA;  |FINSI;
|ET compara;
  |HAZ campo_indiv;
  trecibo = trecibo + 1;

  enSwTrata = 1;
  enCanti   = #3#7 * 100;
  timpsuc = timpsuc + #3#7;

  |LEE 040#3s;
  |SI FSalida = 0;
      |VETE compara;
  |FINSI;
|FPRC;

            ||Calculo registros individuales;
|PROCESO campo_indiv;
  indi4 = #3#5 % 108;
  indi4 = indi4 + vacio;
  indi4 = indi4 % 108;

  #caclipro#23 = "C";
  #caclipro#10 = #3#9;
  #caclipro#11 = #3#14;
  |LEE 000#caclipro.=;
  |SI FSalida ! 0; |DDEFECTO #caclipro; |FINSI;

  indi5 = #3#6 % 110;
  |QBT indi5;
  indi5="0000000000" + indi5;
  indi5= indi5 % -110;
  dc = #3#5 % 0902;
  |QBF dc;
  dc = dc + "**";
  dc = dc % 102;

  enCanti   = #3#7 * 100;
  enSwTrata = 0;

  indi6 = ("0000000000" + enCanti) % -110;

  fechalf = #3#8;
  dd      = fechalf % A102;
  mm      = fechalf % A402;
  aa      = fechalf % A902;
  indi7   = dd + mm + aa;

  posici  = #5#9 % -101;
  facalfa = #3#1;
  |SI posici = "*"; facalfa = #3#1;              |FINSI;
  |SI posici = "/"; facalfa = #3#0 + "-" + #3#1; |FINSI;
  |SI posici = "*"; |O posici = "/";
      indi8 = #5#9 % 0139; |QBF indi8;
  |SINO;
      indi8 = #5#9; |QBF indi8;
  |FINSI;
  indi8   = indi8 + " " + facalfa + "                                           ";
  indi8   = indi8 % A140;
  varalfa1 = indi1 + #5#2 + #0#21 + #3#3 + #3#2 + indi4 + dc + indi5 + indi6 + indi7 + indi10 + indi8 + indi9;
  ||contenido = handle + espacio + varalfa1 + elefe;
  |||FGRABA contenido;

  indi8   = indi8 % A135;
  aRefer  = #caclipro#36; aRefer = aRefer % 0135;
  aFecRef = #caclipro#37;
                                     || ... aCodIBAN = #caclipro#30 + #caclipro#31 + #caclipro#32 + #caclipro#33 + #caclipro#34 + #caclipro#35;
  aCodIBAN = #caclipro#30;
                                     || ... aCodIBAN = aCodIBAN + (" " * 10);
  aCodBIC  = #caclipro#29;

  indi6 = ("00000000000" + enCanti) % -111;
  |SI #n19sepap#23 = "S";
     varalfa1 = "0319143003";
     |SI #n19sepap#24 = "S"; varalfa1 = "0319154003"; |FINSI;
  |SINO;
     varalfa1 = "0319445003";
  |FINSI;

  aAlfa = #caclipro#9; |QBF aAlfa;
  aAlfa = aAlfa + "CLI" + #caclipro#10; |QBF aAlfa;
  aAlfa = (aAlfa + (" " * 35)) % 0135;

|| NIF + fecha + hora tique 5086_8:
  aAlfa = #caclipro#9; |QBF aAlfa;
  aAlfa = aAlfa + aFecha + aHora;
  aAlfa = (aAlfa + (" " * 35)) % 0135;

  varalfa1 = varalfa1 + aAlfa + aRefer;

  |SI #caclipro#38 = "R";
     aAlfa = "RCUR";
  |SINO;
     aAlfa = "OOFF";
  |FINSI;
  varalfa1 = varalfa1 + aAlfa + "SUPP" + indi6;
  aAlfa = #3#2; |QBF aAlfa; aAlfa = (aAlfa + (" " * 70)) % 0170;
  varalfa1 = varalfa1 + (aFecRef % 0704) + (aFecRef % 0402) + (aFecRef % 0102);
  varalfa1 = varalfa1 + aCodBIC + aAlfa;
  contenido = handle + espacio + varalfa1;          |FGRABA contenido;

  aAlfa = #3#10; |QBF aAlfa; aAlfa = (aAlfa + (" " * 50)) % 0150;
  varalfa1 = aAlfa;
  aAlfa = (("00" + #3#12) % -102) + (("000" + #3#13) % -103) + " " + #3#11;
  |QBF aAlfa; aAlfa = (aAlfa + (" " * 50)) % 0150;
  varalfa1 = varalfa1 + aAlfa;

  #caprovin#0 = #3#12;
  |LEE 000#caprovin.=;
  |SI FSalida ! 0; |DDEFECTO #caprovin; |FINSI;

  aAlfa = #caprovin#1; |QBF aAlfa; aAlfa = (aAlfa + (" " * 40)) % 0140;
  varalfa1 = varalfa1 + aAlfa + "ES";

  aAlfa = #caclipro#9; |QBF aAlfa; aAlfa = aAlfa % 0101;
  |SI aAlfa ] "A"; |Y aAlfa [ "W";
     varalfa1 = varalfa1 + "1";
     aAlfa = "1";
  |SINO;
     varalfa1 = varalfa1 + "2";
     aAlfa = "2";
  |FINSI;

  |QBF aCodBIC;
  |SI aAlfa = "1";
     |SI aCodBIC = "";
        varalfa1 = varalfa1 + "I";
        aAlfa = #3#3; |QBF aAlfa; aAlfa = (aAlfa + (" " * 12)) % 0112;
        aAlfa = aAlfa + (" " * 23);
     |SINO;
        varalfa1 = varalfa1 + "A";
        aAlfa = aCodBIC; aAlfa = aAlfa + (" " * 24);
     |FINSI;
  |SINO;
     varalfa1 = varalfa1 + "J";
     aAlfa = #3#3; |QBF aAlfa; aAlfa = (aAlfa + (" " * 12)) % 0112;
     aAlfa = aAlfa + (" " * 23);
  |FINSI;

  varalfa1 = varalfa1 + aAlfa + (" " * 35) + "A";
  contenido = handle + espacio + varalfa1;          |FGRABA contenido;

  varalfa1 = aCodIBAN + "SUPP" + indi8 + (" " * 55);
  varalfa1 = varalfa1 + (" " * 69);
  contenido = handle + espacio + varalfa1 + elefe;  |FGRABA contenido;

  l = l + 1;
|FPRC;

||***********************************************************;
||********** REGISTRO TOTAL ORDENANTE Y GENERAL *************;
||***********************************************************;
|PROCESO campo_totales;

|| **** REGISTRO TOTAL ORDENANTE

  tgen3 = trecibo;
  tgen3 = "0000000000" + tgen3;
  tgen3 = tgen3 % -110;

  |SI #0#22 = "EUROS  ";
      timpsuc = timpsuc * 100;
  |FINSI;

  l = l + 1;
  tnumsuc = l - 1;        ||Se quita un registro, porque no se cuenta
                          ||el primero
  tgen4  = ("00000000000000000" + timpsuc) % -117;
  nCalc = tnumsuc - 2; tgen5  = ("00000000" + nCalc) % -108;
  tgen6  = ("0000000000" + tnumsuc) % -110;
  aSufijo = ("000" + #n19sepap#21) % -103; aNif = #5#23; |HAZ ObtenId;
  |QBF aIdent; aIdent = (aIdent + (" " * 35)) % 0135;
  aAlfa = #n19sepap#11; aAlfa = (aAlfa % 0704) + (aAlfa % 0402) + (aAlfa % 0102);
  varalfa1 = "04" + aIdent + aAlfa + tgen4 + tgen5 + tgen6;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;
  varalfa1 = " " * 120;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;
  varalfa1 = " " * 200;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;
  contenido = handle + espacio + varalfa1 + elefe; |FGRABA contenido;

  l = l + 1;
  tnumsuc = tnumsuc + 1;
  tgen6  = ("0000000000" + tnumsuc) % -110;
  varalfa1 = "05" + aIdent + tgen4 + tgen5 + tgen6;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;
  varalfa1 = " " * 128;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;
  varalfa1 = " " * 200;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;
  contenido = handle + espacio + varalfa1 + elefe; |FGRABA contenido;

|| **** REGISTRO TOTAL GENERAL

  l = l + 1; tnumsuc = l;

  nCalc  = tnumsuc;                 ||** Total reg. sucursales
  tgen6  = ("0000000000" + nCalc) % -110;

  varalfa1 = "99" + tgen4 + tgen5 + tgen6;
  contenido = handle + espacio + varalfa1;
  |FGRABA contenido;
  varalfa1 = " " * 163;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;
  varalfa1 = " " * 200;
  contenido = handle + espacio + varalfa1;         |FGRABA contenido;
  contenido = handle + espacio + varalfa1 + elefe; |FGRABA contenido;

  #0#17 = tgen3;    |PINTA #0#17;
  #0#18 = tgen6;    |PINTA #0#18;
  #0#19 = timpsuc;
  |SI #0#22 = "EUROS  ";
      #0#19 = timpsuc / 100;
  |FINSI;
  |PINTA #0#19;
|FPRC;
