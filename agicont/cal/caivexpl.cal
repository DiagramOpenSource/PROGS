      #1#1 = #7#6;     || digito descarga D
  |SINO;
      #1#0 = #7#8;     || cuenta descarga H
      #1#1 = #7#9;     || digito descarga H
  |FINSI;
  |LEE 000#1=;             || lee cta. descarga
  |SI FSalida = 0;
      |HAZ cargatra;
      |SI #1#5 = "D";
          #5acumula = #5acumula + descar;
      |SINO;
          #5acumula = #5acumula - descar;
      |FINSI;
      importe = descar;
      |HAZ sumadeha;
      |GRABA 020#5a;
      |LIBERA #5;
  |FINSI;

|| --- acumulado
  acumula = 12;
  mesini = #0#15 + 11;              || Iniciales
  descar = #7mesini;
  mesini = #0#16 + 11;              || Finales
  descar = descar - #7mesini;
  |SI descar ] 0;
      #1#0 = #7#5;     || cuenta descarga D
      #1#1 = #7#6;     || digito descarga D
  |SINO;
      #1#0 = #7#8;     || cuenta descarga H
      #1#1 = #7#9;     || digito descarga H
  |FINSI;
  |LEE 000#1=;             || lee cta. descarga
  |SI FSalida = 0;
      |HAZ cargatra;
      |SI #1#5 = "D";
          #5acumula = #5acumula + descar;
      |SINO;
          #5acumula = #5acumula - descar;
      |FINSI;
      importe = descar;
      |HAZ sumadeha;
      |GRABA 020#5a;
      |LIBERA #5;
  |FINSI;
|FPRC;

