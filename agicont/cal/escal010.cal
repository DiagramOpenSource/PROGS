|PROCESO acumular;

   |QBF patron;
   trozo = "";
   cta= "";
   long = "";
   len = "";
   len_num = 0;

   cta = #5#0;
   |QBF cta;
   long = cta % A0;
   len = patron % A0;
   len_num = len + 100;
   trozo = cta % len_num;
   |SI patron = trozo;
      |SI #3#5 = "+";
          acumulado = acumulado + #5#1;
      |FINSI;
      |SI #3#5 = "-";
          acumulado = acumulado - #5#1;
      |FINSI;
   |FINSI;

|FPRC;



