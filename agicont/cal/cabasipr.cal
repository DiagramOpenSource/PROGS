|PROCESO impre; |TIPO 3;
  |HAZ antesque;
  |HAZ conexi;
  |DELINDEX #3;
  |DELINDEX #5;

   |IMPRE 0; |SI FSalida ! 0; |ACABA; |FINSI;

   |INFOR informe; |SI FSalida ! 0; |ACABA; |FINSI;

   t_agrupa = #0#12;
   des_cuen = "            ";
   has_cuen = "zzzzzzzzzzzz";
   |HAZ presup0;

   sw = 0;
   |HAZ sabercam; || Calculo para saber los limites de saldos

   |ABRET #1;
   |ABRE #2;
   |ABRE #3;
   |ABRE #5;

   |BUCLE cabasipr0;      || Bucle de Calculos de cuentas
   |HAZ gr_final;

   |CIERRA #1;
   |CIERRA #2;
   |CIERRA #3;
   |CIERRA #5;

   |SI sw_error = 1; |ACABA; |FINSI;

|| ========================================================================

   |BUCLE cabasipr1;
   |HAZ final;

   |PIEINF;
   |FININF;

   |DELINDEX #1;
   |DELINDEX #3;
   |DELINDEX #5;
|FPRC;

