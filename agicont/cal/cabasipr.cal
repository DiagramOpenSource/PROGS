|PROCESO grabatra;  || ... Graba temporal de Agrupaciones Exp0;

|SI saldo = 0; |Y saldo_p = 0; |ACABA; |FINSI;
  #3#0 = #1#5;
  #3#1 = #1#4;
  #3#2 = #1#6;
  #3#3 = #1#7;
  #3#4 = #1#8;
  |LEE 101#3=;
  |SI FSalida ! 0;
      |DDEFECTO #3;
      #3#0 = #1#5;
      #3#1 = #1#4;
      #3#2 = #1#6;
      #3#3 = #1#7;
      #3#4 = #1#8;
      |HAZ leexplot;
      #3#5 = t_agru;
      #3#6 = t_epig;
      #3#7 = t_part;
      |GRABA 020#3c;
  |FINSI;
  #3#8  = #3#8  + saldo;
  #3#10 = #3#10 + saldo_p;
  |GRABA 020#3a;
  |LIBERA #3;

|| ... Graba Auxiliar de Cuentas.

|SI #0#4 = "N"; |ACABA; |FINSI;

  #5#0 = #1#5;
  #5#1 = #1#4;
  #5#2 = #1#6;
  #5#3 = #1#7;
  #5#4 = #1#8;
  #5#5 = #1#0;
  #5#6 = #1#1;
  #5#7 = #1#2;
  |LEE 000#5=;
  |SI FSalida ! 0;
      |DDEFECTO #5;
      #5#0 = #1#5;
      #5#1 = #1#4;
      #5#2 = #1#6;
      #5#3 = #1#7;
      #5#4 = #1#8;
      #5#5 = #1#0;
      #5#6 = #1#1;
      #5#7 = #1#2;
      |HAZ leexplot;
      #5#8 = t_agru;
      #5#9 = t_epig;
      #5#10 = t_part;
      #5#11 = 0;
      #5#12 = 0;
      |GRABA 020#5c;
  |FINSI;

  #5#11 = #5#11 + saldo;
  #5#12 = #5#12 + saldo_p;

  |GRABA 020#5a;
  |LIBERA #5;
|FPRC;

|PROCESO exis_expl;
  descar = #7des_exi - #7has_exi; || iniciales - finales
  |SI descar = 0; |ACABA; |FINSI;
  |SI descar ] 0;
      act_cuent = #7#5;     || cuenta descarga D
      act_nivel = #7#6;     || digito descarga D
  |SINO;
      act_cuent = #7#8;     || cuenta descarga H
      act_nivel = #7#9;     || digito descarga H
  |FINSI;

  acor_c = act_cuent; acor_d = act_nivel;
  |HAZ acortacuen;
  act_cuent = acor_c; act_nivel = acor_d;

  |SI act_cuent = #1#0; |Y act_nivel = #1#1;
      sw_exp = 1;
      |SI descar ] 0;
          debe = debe + descar;
      |SINO;
          haber = haber - descar;
      |FINSI;
       saldo = debe - haber;
  |FINSI;
|FPRC;

|DEFBUCLE exis2;
 #7#1;
 ;
 #0#6, 01;
 #0#6, 99;
 be;
 NULL, exis_expl;
|FIN;

|PROCESO exis_3000;
  sw_exp  = 1;
  saldo   = saldo + #7has_exi;
|FPRC;

|DEFBUCLE exis1;
 #7#2;
 ;
 #0#6, #1#0, #1#1;
 #0#6, alar_c, 9;
 e;
 NULL,exis_3000;
|FIN;

|PROCESO calcular;
 saldo   = 0;  || Saldo Real
 saldo_p = 0;  || Saldo Presupuestado
 debe    = 0;
 haber   = 0;
 sw_exp  = 0;

 |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
       saldo_p = saldo_p + #1x;
 |SIGUE;

 alfa1   = #1#0 % 101;

 |SI alfa1 = "3";
     |BUCLE exis1;
     |SI sw_exp = 1; saldo_p = saldo; |FINSI;
 |FINSI;
 |SI alfa1 = "6"; |O alfa1 = "7";
     |BUCLE exis2;
     |SI sw_exp = 1; saldo_p = saldo; |FINSI;
 |FINSI;

 |SI sw_exp = 1; |ACABA; |FINSI;

 |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
       y = x + 1;
       debe  = debe  + #1x;
       haber = haber + #1y;
       saldo = debe - haber;
 |SIGUE;
|FPRC;

|PROCESO grabar;
  |HAZ digitos2;
  |SI d_error ! 0;  |ACABA;  |FINSI;
  |SI #0#4 = "N";  |Y #1#3 = "N";                     |ACABA;  |FINSI;
  |SI #0#4 = "S";  |Y #1#55 > nivel;                  |ACABA;  |FINSI;
  |SI #0#4 = "S";  |Y #1#3 = "N";  |Y #1#55 ! nivel;  |ACABA;  |FINSI;

  alar_c = #1#0;
  |HAZ alargacuen;

  |SI #1#0 ! cue_perdi; || no es perdidas y ganancias
      |HAZ calcular;
      |SI saldo ! 0;
          |SI #1#5  = "H"; |O #1#5 = "P"; saldo = - saldo; |FINSI;
          |SI #1#5 = "A"; |O #1#5 = "P"; |HAZ grabatra; |FINSI;
          |HAZ sumasaldo;
      |FINSI;
  |FINSI;
|FPRC;

|DEFBUCLE cabasipr0;
 #1#1;
 ;
 d_cuen, d_digi;
 h_cuen, h_digi;
 be;
 NULL, grabar;
|FIN;
|| ************************************************************************

|PROCESO sumasaldo;    || Calculamos cada cuenta
 |SI #1#5 = "A"; activo = activo + saldo; activo_p = activo_p + saldo_p; |FINSI;
 |SI #1#5 = "P"; pasivo = pasivo + saldo; pasivo_p = pasivo_p + saldo_p; |FINSI;

 |SI #1#5 = "D";
     |SI #1#4 = 1;
         debe1 = debe1 + saldo;
     |FINSI;
     |SI #1#4 = 2;
         debe2 = debe2 + saldo;
     |FINSI;
     |SI #1#4 = 3;
         debe3 = debe3 + saldo;
     |FINSI;
     |SI #1#4 = 4;
         debe4 = debe4 + saldo;
     |FINSI;
     |SI #1#71 = "D";
         debe1_p = debe1_p + saldo_p;
     |SINO;
         debe1_p = debe1_p - saldo_p;
     |FINSI;
 |FINSI;
 |SI #1#5 = "H";
     |SI #1#4 = 1;
         haber1 = haber1 + saldo;
     |FINSI;
     |SI #1#4 = 2;
         haber2 = haber2 + saldo;
     |FINSI;
     |SI #1#4 = 3;
         haber3 = haber3 + saldo;
     |FINSI;
     |SI #1#4 = 4;
         haber4 = haber4 + saldo;
     |FINSI;
     |SI #1#71 = "H";
         haber1_p = haber1_p + saldo_p;
     |SINO;
         haber1_p = haber1_p - saldo_p;
     |FINSI;
 |FINSI;
|FPRC;

|| ***********************************************************************
|PROCESO gr_final; || Grabar totales
 saldo_p = 0;
 debe    = 0;
 haber   = 0;
 #1#0 = cue_perdi;
 #1#1 = dig_perdi;
 |LEE 001#1=;
 |SI FSalida ! 0;
     sw_error = 1;
     |MENAV "    ATENCION !!!. NO existe la cuenta de Perdidas y Ganancias ";
     |ACABA;
 |FINSI;
 |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
       y = x + 1;
       debe  = debe  + #1x;
       haber = haber + #1y;
       saldo = debe - haber;
 |SIGUE;
 |PARA x = eldes_p; |SI x [ elhas_p; |HACIENDO x = x + 1;
       saldo_p = saldo_p + #1x;
 |SIGUE;

 debe  = debe1  + debe2  + debe3  + debe4;
 haber = haber1 + haber2 + haber3 + haber4;
 saldo = saldo + ( debe - haber);

 debe_p  = debe1_p;
 haber_p = haber1_p;
 |SI #1#5 = "P"; saldo = - saldo; |FINSI;

 |SI #1#71 = "D"; saldo_p = saldo_p + ( debe_p - haber_p); |FINSI;
 |SI #1#71 = "H"; saldo_p = saldo_p + ( haber_p - debe_p); |FINSI;

 |HAZ grabatra; || ....................... Graba la cuenta 129. como otra mas.
 |SI #1#5 = "A"; activo = activo + saldo; activo_p = activo_p + saldo_p; |FINSI;
 |SI #1#5 = "P"; pasivo = pasivo + saldo; pasivo_p = pasivo_p + saldo_p; |FINSI;
|FPRC;

|| ***************************************************************************
|| *********************  CALCULO DE IMPRESION *******************************
|| ***************************************************************************

|PROCESO impresio;

 |SI sw = 0;
     tipo = #3#0;
     agrupaci = #3#2;
     epigrafe = #3#3;
     |SI #3#0 = "A"; doh = "ACTIVO"; |FINSI;
     |SI #3#0 = "P"; doh = "PASIVO"; |FINSI;
     |HAZ agrupacion;
     sw = 1;
 |FINSI;

 |SI tipo ! #3#0;
     sw_inf = 5;  |PRINT;  |PRINT;
     sw_inf = 1;
     |SI tipo = "A";
         variable = "  TOTAL ACTIVO ....................";
         canti = activo;
         canti_p = activo_p;
         |PRINT;
         doh = "PASIVO";
     |SINO;
         variable = "  TOTAL PASIVO ....................";
         canti = pasivo;
         canti_p = pasivo_p;
         |PRINT;
     |FINSI;
     |PIEINF;
     tipo = #3#0;
     agrupaci = #3#2;
     epigrafe = #3#3;
     |HAZ agrupacion;
 |FINSI;
 |HAZ epigraf;
|FPRC;

|PROCESO epigraf;
 |SI agrupaci ! #3#2;
     sw_inf = 5;  |PRINT;       || linea en blanco
     |HAZ agrupacion;
     agrupaci = #3#2;
 |FINSI;
 |HAZ rotura;
 sw_epi = 0;
|FPRC;

|PROCESO rotura;
 |SI epigrafe ! #3#3; |O sw_epi = 1;
     |SI #3#3 ! 0;
         sw_inf = 2;  |PRINT;        || epigrafe
     |FINSI;
 |FINSI;
 |HAZ partida;                                       || partida
 |HAZ detalle;                                       || cuentas
 epigrafe = #3#3;
|FPRC;

|PROCESO agrupacion;
  sw_epi = 1;                || fuerza la impresion del epigrafe
  |SI #3#3 = 0;                  || epigrafe=0
      sw_inf = 7;  |PRINT;   || agrupacion (con importe)
  |SINO;
      sw_inf = 0;  |PRINT;   || agrupacion (sin importe)
  |FINSI;
|FPRC;

|PROCESO partida;        || imprime titulo de partidas
  |SI #3#4 ! 0;
      sw_inf = 3;
      |PRINT;
  |FINSI;
|FPRC;

|PROCESO cuentas;
 cantidad   = #5#11;
 cantidad_p = #5#12;
 sw_inf = 6;
 |SI cantidad ! 0; |O cantidad_p ! 0;
     |PRINT;
 |FINSI;
|FPRC;

|DEFBUCLE cabasipr4;
 #5#1;
 ;
 #3#0, #3#1, #3#2, #3#3, #3#4, "            ", 0;
 #3#0, #3#1, #3#2, #3#3, #3#4, "zzzzzzzzzzzz", 9;
 e;
 NULL, NULL, NULL, NULL, NULL, cuentas;
 #5#5, #5#6;
|FIN;


|PROCESO detalle;        || imprime detalle de cuentas
  |SI #0#4 = "S";
      |BUCLE cabasipr4;
      sw_inf=5;
      |PRINT;
  |FINSI;
|FPRC;

