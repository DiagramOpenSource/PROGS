|PROCESO grabarecib; ||Calculo registros individuales;
  swERROR = 0;

  |HAZ leecl_y_cobro;
  |SI swERROR = 1; |ACABA; |FINSI;   || Salta este Cobro. NO el resto

  ca1 = libre * 28;

  ca2 = "";               || Referencia Cliente
  |SI #0#4 = 1;
      ca2 = #3#9;         || Cuenta Contable
  |SINO;
      ca2 = #4#0;         || Codigo de Ficha Proveedor
  |FINSI;
  |QBF ca2;
  ca2 = ca2 + libre40;
  ca2 = ca2 % 115;

  ca3 = NifProv;             ||  D.N.I.
  CampoDni = ca3; |HAZ LetraDni; ca3 = CampoDni;

  ca3 = ca3 + libre40;
  ca3 = ca3 % 116;

  ca4 = (NombreProv + libre40) % 140;   || Nombre Cliente

  direcc = DirecProv; || Direccion
  |HAZ separa;
  |SI numero ! 0;
     ca5 = (direcc + libre40) % 134;
     ca6 = numero; |QBT ca6;
     ca6 = (ceros + ca6) % -105;
     ca6 = "," + ca6;
  |SINO;
     ca5 = (direcc + libre40) % 140;
     ca6 = "";
  |FINSI;

  ca7 = (PoblaProv + libre40) % 125;  || Poblacion

  alfa2 = Cp1Prov; |QBF alfa2; alfa2 = (ceros + alfa2) % -102;
  alfa3 = Cp2Prov; |QBF alfa3; alfa3 = (ceros + alfa3) % -103;
  ca8   = alfa2 + alfa3;                                     || Codigo

  ca9   = (TelefProv + libre40) % 110;  || Telefono

  || ca10  = (#3#11 + libre40) % 106;  antes....

  ca10  = (FaxProv + libre40) % 109;  || Fax.

  ca22  = (PaisProv + libre40) % 102; || Pais.

  ccc = #5#19; |HAZ cccBanco;         || Datos del banco del Cobro
  ccc = (ccc + ceros) % 110;
  ca11 = ccc % 104;      || Banco
  ca12 = ccc % 504;      || Sucursal
  ca14 = ccc % 902;      || D.C.

  ccc = #5#18; |HAZ cccBanco;
  ccc = (ceros + ccc) % -110;
  ca13 = ccc;                 || Cuenta corriente  Proveedor

  ca15  = #3#12;

  ca16 = concepto % 115;
  |SI ca16 = "               ";
      alfa1 = #3#2; alfa1 = alfa1 + "-";
      alfa2 = #3#3;
      alfa3 = #3#4; |QBF alfa3;
      |SI alfa3 ! "";
          alfa3 = "/" + alfa3;
      |FINSI;
      alfa4 = #3#5; alfa4 = "-" + alfa4;
      ca16 = (alfa1 + alfa2 + alfa3 + alfa4 + libre40) % 115;
  |FINSI;

  nume1 = #3#8;     nume1 = (#3#8 * 100) ! 0;
  ca17 = nume1; |QBF ca17;
  |SI nume1 ] 0; ca17 = ceros + ca17; ca17 = ca17 % -115; |FINSI;  || Importe
  |SI nume1 < 0; ca17 = "               "+ ca17; ca17 = ca17 % -115; |FINSI;

  alfa2 = #3#6;
  ca18   = alfa2 % 704;
  ca18   = ca18 + (alfa2 % 402);
  ca18   = ca18 + (alfa2 % 102);         || Fecha Emision

  alfa2 = #3#7;
  ca19   = alfa2 % 704;
  ca19   = ca19 + (alfa2 % 402);
  ca19   = ca19 + (alfa2 % 102);         || Fecha Vto.

  ca20   = (#4#8 + libre40) % 109;
  ca20   = (libre * 6) + ca20;
||  ca21   = (#4#5 + libre40) % 116;

  varalfa1 = "";
  || varalfa1 = cabe2    + ca1  + ca2  + ca3  + ca4  + ca5;
  varalfa1 = cabe2    + ca2  + ca3  + ca4  + ca5;
  varalfa1 = varalfa1 + ca6  + ca7  + ca8  + ca9  + ca10 + ca22;
  varalfa2 = "";
  varalfa2 = ca15 + ca11 + ca12 + ca14 + ca13 + "    " + monei + "F";
  varalfa2 = varalfa2 + ca16 + ca17 + ca18 + ca19 + libre16 + ca1;

  contenido = handle + " " + varalfa1;
  |FGRABA contenido;

  contenido = handle + " " + varalfa2 + &#0#7 + &#0#8;
  |FGRABA contenido;

  trecibos = trecibos + 1;
  l = l + 1;
  timporte = timporte + #3#8;
|FPRC;

