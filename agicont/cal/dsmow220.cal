|FICHEROS;
     dsmow220;

     caportad;
|FIN;

|VARIABLES;
     nNume   = 0;
     nNume1  = 0;
     nNume2  = 0;
     nNume3  = 0;
     nPara1  = 0;
     aAlfa   = "";
|FIN;

|PROCESO CargaDatos;

     |SI FSalida = 10;

         nNume = 0; nPara1 = 0;

         |PARA nNume1 = 20; |SI nNume1 [ 25; |HACIENDO nNume1 = nNume1 + 1;
               nNume2 = nNume1 + 12;
               nNume3 = nNume1 + 6;
               aAlfa = #caportad#nNume1#; |QBF aAlfa;
               |SI aAlfa ! "";
                   nNume  = nNume  + 1;   #dsmow220#nPara1# = nNume;
                   nPara1 = nPara1 + 1;   aAlfa =  #caportad#nNume2# - "E"; #dsmow220#nPara1# = aAlfa;
                   nPara1 = nPara1 + 1;   #dsmow220#nPara1# = #caportad#nNume1#;
                   nPara1 = nPara1 + 1;   #dsmow220#nPara1# = #caportad#nNume3#;
                   nPara1 = nPara1 + 1;
               |FINSI;
         |SIGUE;
         |PINDA #0#dsmow220;
     |FINSI;
|FPRC;
