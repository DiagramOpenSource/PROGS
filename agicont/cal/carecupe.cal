|PROCESO procal; |TIPO 3;
  |DELINDEX #999;        || anula cualquier recalculo anterior por fechas o
                         ||/por departamentos
  |NOPARA;
  conforme = #0#0;
  fini = "01.01.0000";
  ffin = "31.12.9999";
