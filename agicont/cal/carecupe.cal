  cfin = 999;
  |HAZ recuperacion;
  |SIPARA;
  |ABRE #8;
  |LEE 111#8p;
  |SI FSalida ! 0;
      |DDEFECTO #8;
      #8#3 = "S";
      |GRABA 011#8n;
  |SINO;
      #8#3 = "S";
      |GRABA 011#8a;
  |FINSI;
  |CIERRA #8;
|FPRC;

|PROGRAMA;
 aParam = PARAMETRO; |QBF aParam;
 |SI aParam = "";
