|PROCESO mayus;  |TIPO 6;
  #0Campo = #0Campo > " ";
  |PINTA #0Campo;
|FPRC;


|PROCESO conase4; |TIPO 7;
|SI sw_conase4 = 1;
        #0#0 = "S";
    |PTEC 802;
    |PTEC 802;
|FINSI;
