|PROCESO LeeDefinicion;
 |SI Campo = 1; |Y #50#1 = #50#0;
     |MENAV "    Error, origen y destino igual";
     |ERROR;
     |ACABA;
 |FINSI;

 |SI Campo = 1;
     enDPlan  = #10#2;
     nBalNum  = #50#1;
     wTipoPat = 0;
     |HAZ eVerEsPatron;
     |SI wPatOfi = 1;
         |CONFI "    NCodigo de Patron Oficial. Continuar S/N";
         |SI FSalida ! 0;
             |ERROR;
             |ACABA;
         |FINSI;
    |FINSI;
 |FINSI;

 |ABRE #1;
 #1#0 = #50Campo;
 |LEE 101#1=;
 |SI FSalida = 0;
     |SI Campo = 0; #50#2 = #1#1; |PINTA #50#2; |FINSI;
