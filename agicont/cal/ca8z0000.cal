|PROCESO NoBorroCtas;
 nSw = 1;
 |ERROR10;
|FPRC;

|DEFBUCLE NoBorroCtas;
 #0#1;
 ;
 2008, nNumero, "     ";
 2099, nNumero, "zzzzz";
 e;
 NULL, NoBorroCtas;
|FIN;

|PROCESO BorroCtas;
 |BORRA 020#0a;
 |LIBERA #0;
|FPRC;

|DEFBUCLE BorroCtas;
 #0#1;
 ;
 #10#2, nNumero, "     ";
 #10#2, nNumero, "zzzzz";
 be;
 NULL, BorroCtas;
|FIN;

|PROCESO Borrar;
 nNumero = #10#0;
 |BUCLE BorroCtas;

 |SI nNumero ! 0; |Y nNumero ! 1;
     |Y nNumero ! 20; |Y nNumero ! 21;
       |Y nNumero ! 30; |Y nNumero ! 31;
      nSw = 0;
      |BUCLE NoBorroCtas;
      |SI nSw = 0;
         |ABRE #1;
         #1#0 = nNumero;
         |LEE 141#1=;
         |SI FSalida = 0;
             |BORRA 020#1a;
         |FINSI;
         |LIBERA #1;
         |CIERRA #1;
      |FINSI;
 |FINSI;
|FPRC;

