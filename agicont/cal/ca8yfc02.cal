|PROCESO Leem805;

  |PDEFECTO #0,10,21;
  aCasilla = "";
  |SI #5#16 ! 0;
      aCasilla = #5#16;
      aCasilla = ("000000" + aCasilla) % -106;
  |FINSI;
  #0#16 = #5#3;
  #0#17 = aCasilla;
  #0#18 = #5#7;
  #0#19 = #5#6;
  #0#24 = #5#5;
  |SI #5#8 ! "A"; nImprimir = 0; |FINSI;

  |SI #5#7 = 0; |Y #5#6 = 0; |Y #5#8 ! "A";
      |SI #0#2 = "N"; |ACABA; |FINSI;
  |FINSI;

  |SI sw = 0;
      sw = 1;
  |FINSI;

  sw_inf = 0;
  |SI #5#4 = "G"; sw_inf = 1; |FINSI;
  |SI #5#8 = "A"; sw_inf = 7; |FINSI;
  |SI #5#16 = 0; sw_inf = 7; |FINSI;
  |SI #5#8 ! "A"; |O nImprimir = 1;
      |PRINT;
      nImprimir = 1;
  |FINSI;
|FPRC;

