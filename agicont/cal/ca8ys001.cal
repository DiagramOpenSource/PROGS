      |C_M #0#30,1,"S";
      |C_V #0#30,1,"S";
      #0#30 = 1; |PINTA #0#30;
  |SINO;
      |PINTA 2003, "                              ";
      |PINTA 2103, "                              ";
      |PINTA 2203, "                              ";
      |PINTA 2303, "                              ";
      |C_M #0#30,1,"N";
      |C_V #0#30,1,"N";
      #0#30 = 1;
  |FINSI;

  |ABRE #6;
  |SI #0#6 = 0;
      |LEE 040#6p;
  |SINO;
      #6#0 = #0#6;
      |LEE 040#6=;
  |FINSI;
