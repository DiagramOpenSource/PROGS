|FICHEROS;
  caepigra #0;
|FIN;

|VARIABLES;
 aAlfa        = "";
 nNume1       = 0;
 &enEjercicio = 0;
|FIN;

|PROCESO Prim_epi;  |TIPO 8;
   nNume1 = #48#26;
   |SI nNume1 < 80;
       enEjercicio = 2000 + nNume1;
   |SINO;
       enEjercicio = 1900 + nNume1;
   |FINSI;
||   |SI enEjercicio ] 2008;
||       |MENAV "    Estructuras Plan Contable 1990. No modificables en este Ejercicio";
||       |PTEC 807;
||   |FINSI;
|FPRC;

|PROCESO Ant_epi; |TIPO 7;
  aAlfa = "    Entre: (A) Activo; (P) Pasivo; (D) Debe; (H) Haber";
  |SI enEjercicio ] 2008;
      aAlfa = "    Entre: (A)Activo; (P) Pasivo; (D) Debe; (H) Haber; (I) Ingreso; (G) Gasto";
  |FINSI;
  |MENSA aAlfa;
|FPRC;

|PROCESO Des_epi;
   |BLIN 4;
   |SI enEjercicio < 2008;
       |SI #0#0 = "I"; |O #0#0 = "G";
           |ERROR;
      |FINSI;
   |FINSI;
|FPRC;
