|CALCULO imprimir;
|SI #1#11 ! "S";
     |SI sw = 0;
        cuenta = #1#1;
        sw = 1;
        debe = 0;
        haber = 0;
     |FINSI;

     |SI cuenta ! #1#1;
         |PIEINF;
         cuenta = #1#1;
         debe = 0;
         haber = 0;
     |FINSI;

     |SI #0#4 = "S";
         |SI #1#9 ! "S";
             |VETE fin;
         |FINSI;
     |SINO;
         |SI #0#4 = "N";
             |SI #1#9 ! "N";
                 |VETE fin;
             |FINSI;
         |FINSI;
     |FINSI;

     |SI #1#6 = "H";
         haber = haber + #1#7;
     |SINO;
         debe  = debe  + #1#7;
     |FINSI;
     |PRINT;
     |ET fin;
|FINSI;
