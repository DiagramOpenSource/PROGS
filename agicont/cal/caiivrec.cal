      giant = #1#9;
      noant = #1#27;
      ciant = #1#30;
      peant = #1#31;
      nuant = #1#35;
      foant = #1#36;
  |FINSI;
|FPRC;

|PROCESO BorraCC12;
  |BORRA 020#512a;
  |LIBERA #512;
|FPRC;

|DEFBUCLE borraCC12;
  #512#1;
  ;
  aTipoIVA, #0#0, 001, 01;
  aTipoIVA, #0#0, 999, 99;
  be;
  NULL, BorraCC12;
|FIN;

|PROCESO tipo1; |TIPO 1;
  nModo = (FEntrada / 100) ! 0;
  orant = 0;
  |BUCLELIN 0verultim#0;
  |SI nModo = 3;
      |BUCLE borraCC12;
  |FINSI;
|FPRC;

|PROCESO tipo3;  |TIPO 3;
  sw_movim = 1;
|FPRC;


|PROCESO salir; |TIPO 9;
 |SI sw_movim = 0; |ACABA; |FINSI;
