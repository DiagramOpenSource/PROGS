  |SI nume1 > #1#4;
      |MENAV "    ANTENCION! Importe Desglose MAYOR a Importe Contable";
  |FINSI;
|FPRC;

|PROCESO salta; |TIPO 7;
  xx = (FEntrada / 100) ! 0;
  |SI xx = 1;|Y FSalida ! 1;|Y FSalida ! 7;|Y #0#2 ! 1;
      #0#2 = #0#2 + 4;
      |PINTA #0#2;
  |FINSI;
|FPRC;
