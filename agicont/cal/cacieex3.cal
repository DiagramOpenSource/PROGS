|PROCESO expl5;         || perdidas y ganacias
  impo = #2#4 - #2#5;   || saldo de la cabecera del asiento de explotacion
  |SI impo = 0;  |ACABA;  |FINSI;
  line = line + 3;
  |DDEFECTO #3;
  #3#00 = #0#11; ||  diario
  #3#01 = #0#0;  ||  fecha
  #3#02 = asie;  ||  documento
  #3#03 = line;  ||  linea
  |LEE 101#3=;
  FEstado = FSalida;
  #3#04 = #6#2;  ||  cuenta
  #3#05 = #6#3;  ||  digito
  #3#06 = #0#10; ||  concepto (explotacion)
  #3#07 = #0#5;  ||  descripcion (explotacion)
  #3#24 = #0#14;
  #3#21 = #0#16;
  |SI impo > 0;  #3#08 = "H";  |FINSI;
  |SI impo < 0;  #3#08 = "D";  impo = impo * -1;  |FINSI;
  |HAZ carga;
  #3#09 = impo;  ||  importe
  |SI FEstado = 0;
      |GRABA 020#3a;
  |SINO;
      |GRABA 020#3n;
  |FINSI;
  |LIBERA #3;
  |HAZ cabecera;
  |HAZ actualiz1;   || actualiza cuentas
  opera = "A";
  |HAZ altatra;     || actualiza niveles
  swMovi = 1;
|FPRC;

|PROCESO Asto917;
        |SI lee = 1;
            #1#0 = LaCuenta;
            #1#1 = ElDigito;
            |LEE 101#1=;
            |SI FSalida ! 0;
                nSwError = 2;
                |ACABA;
            |FINSI;
        |FINSI;

        |DDEFECTO #3;
        #3#00 = #0#11;     ||  diario
        #3#01 = #0#0;      ||  fecha
        #3#02 = asie;      ||  documento
        #3#03 = line;      ||  linea
        |LEE 101#3=;
        FEstado = FSalida;
        #3#04 = LaCuenta;  ||  cuenta
        #3#05 = ElDigito;  ||  digito
        #3#06 = #0#18; ||  concepto
        #3#07 = #0#17; ||  descripcion
        #3#24 = #0#19;
        #3#21 = #0#16;
        |SI impo > 0;  #3#08 = "H";  |FINSI;
        |SI impo < 0;  #3#08 = "D";  impo = impo * -1;  |FINSI;
        |HAZ carga;
        #3#09 = impo;  ||  importe
        |SI FEstado = 0;  |GRABA 020#3a;  |FINSI;
        |SI FEstado ! 0;  |GRABA 020#3n;  |FINSI;
        |LIBERA #3;
        |HAZ cabecera;
        |HAZ actualiz1;   || actualiza cuentas
        opera = "A";
        |HAZ altatra;     || actualiza niveles
        line = line + 3;
        swMovi = 1;
        |SI #3#8 = "D";
            nDebe  = (nDebe + #3#9) ! EURODB;
        |SINO;
            nHaber = (nHaber + #3#9) ! EURODB;
        |FINSI;
|FPRC;

