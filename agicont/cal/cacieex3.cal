  ElDigito = #1#1;
  impo     = #1#53;
  nImport0 = #1#53;

  |SI nSwOp = 1; |HAZ CreaApunte; |FINSI;

  |SI nSwOp = 0;
      |SI line = 1;
          enDiario = #0#11;
          |HAZ contador;
          asie = enContador;
      |FINSI;
      |DDEFECTO #3;
      #3#00 = #0#11; ||  diario
      #3#01 = #0#0;  ||  fecha
      #3#02 = asie;  ||  documento
      #3#03 = line;  ||  linea
      |LEE 101#3=;
      FEstado = FSalida;
      #3#04 = LaCuenta;  ||  cuenta
      #3#05 = ElDigito;  ||  digito
      #3#06 = #0#10; ||  concepto (explotacion)
      #3#07 = #0#5;  ||  descripcion (explotacion)
      #3#24 = #0#14;
      #3#21 = #0#16;
      |SI impo > 0;  #3#08 = "H";  |FINSI;
      |SI impo < 0;  #3#08 = "D";  impo = impo * -1;  |FINSI;
      |HAZ carga;
      #3#09 = impo;  ||  importe
      |SI FEstado = 0;  |GRABA 020#3a;  |FINSI;
      |SI FEstado ! 0;  |GRABA 020#3n;  |FINSI;
      |LIBERA #3;
      |HAZ cabecera;
      |HAZ actualiz1;   || actualiza cuentas
      opera = "A";
      |HAZ altatra;     || actualiza niveles
      line = line + 3;
      swMovi = 1;
  |FINSI;
