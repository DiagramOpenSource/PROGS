|PROCESO CreaApunte;

    #431#0 = LaCuenta;
    #431#5 = ElDigito;
    |LEE 001#431=;
    |SI FSalida ! 0;
        nSwError = 2;
        |ACABA;
    |FINSI;

    |SI line = 1;
        enDiario = #0#11;
        |HAZ contador;
        asie = enContador;
    |FINSI;

    impo = nImport0;
    |HAZ Asto917;

    |SALVA_DATOS #1;
    LaCuenta = #431#2; |QBF LaCuenta;
    ElDigito = #431#6;
    lee = 1;
    |SI LaCuenta ! "";
        impo = -nImport0;
        |HAZ Asto917;
    |SINO;
        |BUCLE LasLineas;
    |FINSI;
    lee = 0;
    |REPON_DATOS #1;
|FPRC;

|PROCESO expl4;   || apuntes para saldar explotacion
  |SI #1#3 ! "S";  |ACABA;  |FINSI;
  |SI #1#53 = 0;   |ACABA;  |FINSI;
  |PINTA 0436, #1#0;
