|RUTINA actualmes;
  ini = dig1;
  fecalf = #0#3;
  mes = fecalf % 402;
  mesfec = mes;
  |SI ini = 1;
      mesac = mesfec;
  |SINO;
      mesac = (mesfec - ini) + 1;
      |SI mesac [ 0;
          mesac = (13 - ini) + mesfec;
      |FINSI;
  |FINSI;
  a = (mesac * 3) + 9;
  b = a + 1;
  c = a + 2;
  |SI signo = "D";
      #2a = #2a + elimporte;
      #2#51 = #2#51 + elimporte;
  |SINO;
      #2b = #2b + elimporte;
      #2#52 = #2#52 + elimporte;
  |FINSI;
  #2c = #2a - #2b;
  #2#53 = #2#51 - #2#52;
|FRUT;

|RUTINA actualiza;
  #2#0 = #3#4;
  #2#1 = #3#5;
  |LEE 101#2=;
  |SI FSalida = 0;
      |HAZ actualmes;
      |SI #2#58 < #0#3;  #2#58 = #0#3;  |FINSI;
      |GRABA 020#2a;
  |FINSI;
  |LIBERA #2;
