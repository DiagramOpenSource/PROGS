|PROCESO contrapartidas;
  |PUSHV 0708 2174;
  total = #0#7;
  sumal = 0;
  |BUCLE hazlasuma;
  difer = total - sumal;
  |SI sumal = 0;
      |ABRE #3;
      #3#0 = #0#0;
      #3#1 = #0#1;
      #3#2 = #0#2;
      #3#3 = 1;
      |ABRE #4;
      #4#0 = #0#0;
      #4#1 = #0#1;
      |LEE 101#4=;
      #3#4 = #4#5;
      #3#5 = #4#6;
      |CIERRA #4;
      #3#6 = #0#10;
      #3#7 = #0#11;
      #3#8 = #0#7;
      |ABRET #2;
      |SI #0#6 = "D";  signo = "H";  |SINO;  signo = "D";  |FINSI;
      elimporte = #3#8;
      |HAZ actualiza;
      |CIERRAT #2;
      |GRABA 101#3n;
      |CIERRA #3;
      sumal = #0#7;
      difer = 0;
  |FINSI;
  |ENTLINEAL #1#3, 4, 1, 20, 1, menorl, mayorl;
  |POPV;
|FPRC;

|RUTINA blanco;
  |ABRET #2;
  #2#0 = #0#8;
  #2#1 = #0#9;
  |LEE 001#2=;
  |SI FSalida ! 0;
      |DDEFECTO #2;
      #2#0 = #0#8;
      #2#1 = #0#9;
      #2#3 = "S";       || pase directo si
      |PARA nume1 = 1; |SI nume1 [ dig3; |HACIENDO nume1 = nume1 + 1;
           |SI nume1 = 1;  #2#1 = dig4;  |FINSI;
           |SI nume1 = 2;  #2#1 = dig5;  |FINSI;
           |SI nume1 = 3;  #2#1 = dig6;  |FINSI;
           |SI nume1 = 4;  #2#1 = dig7;  |FINSI;
           |SI nume1 = 5;  #2#1 = dig8;  |FINSI;
           |SI nume1 = 6;  #2#1 = dig9;  |FINSI;
           |GRABA 040#2n;
      |SIGUE;
      #2#1 = 0;
      |GRABA 040#2n;        || graba con el nivel 0 (por si acaso)
  |FINSI;
  |CIERRAT #2;
|FRUT;

|PROCESO elnivelin;  |TIPO 6;
  salidas = FSalida;
  saliext = FSalida;
  p_alfa1 = #0Campo;
  |HAZ punto4;
  |SI p_error ! 0;  |ERROR;  |ACABA;  |FINSI;
  cta = #0Campo;
  |QBF cta;
  |SI cta = "";
      |HAZ blanco;
      |HAZ contrapartidas;
      |ACABA;
  |FINSI;
  nada = cta % A101;
  |SI nada ! "0";
      ctanum = cta;
      ctaalf = ctanum;
      longnum = ctaalf % A0;
  |SINO;
      ctanum = 1;
      ctaalf = cta;
      longnum = ctaalf % A0;
  |FINSI;
  longalf = cta    % A0;
  |SI longnum ! longalf;|O ctanum = 0;
      |MENAV "     CUENTA ERRONEA !!";
      |PINTA #0#8;
      |ERROR;
      |ACABA;
  |FINSI;
  nivel= 0;
  sw = 0;
  |SI longnum = dig4; sw = 1; nivel = 1; |FINSI;
  |SI longnum = dig5; sw = 1; nivel = 2; |FINSI;
  |SI longnum = dig6; sw = 1; nivel = 3; |FINSI;
  |SI longnum = dig7; sw = 1; nivel = 4; |FINSI;
  |SI longnum = dig8; sw = 1; nivel = 5; |FINSI;
  |SI longnum = dig9; sw = 1; nivel = 6; |FINSI;
  |SI sw = 0;
      |MENAV "     LONGITUD DE CUENTA FUERA DE NIVEL !";
      |PINTA #0#8;
      |ERROR;
      |ACABA;
  |FINSI;
  #0#9 = nivel;
  |SI saliext = 9;
      ext1 = #0#8;
      ext2 = #0#9;
      |SUB_EJECUTA "caextrap"
  |FINSI;
|FPRC;

