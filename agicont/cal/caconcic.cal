|FICHEROS;
   caconcic #0;
   caconcil #1;
   cacuenta #2;
|FIN;

|VARIABLES;
    &entrada  = 1;
    cta       = 0;
    cta_t     = 0;
    dig_t     = 0;
    nModo     = 0;
    sw        = 0;
    swcero    = 0;
    aAlfa     = "";
    aBlancs   = "";
    long      = "";
    &eaCodDep = "";
    &eswLect  = 0;
    &eOpDep   = 0;
|FIN;

|INCLUYE acceso_i;
|INCLUYE puntos_i;

|PROCESO saldo_2;
     |SI #0#5 = "D";
          #0#4 = #0#4 + (#1#6 - #1#7);
     |FINSI;
     |SI #0#5 = "H";
          #0#4 = #0#4 + (#1#7 - #1#6);
     |FINSI;
|FPRC;

|PROCESO saldo_1;
     |SI swcero = 0;
          #0#4 = 0;
     |FINSI;
     |SI swcero = 1;
          |BUCLELIN 2saldo_2#0;
     |FINSI;
     |GRABA 020#0a;
|FPRC;

|DEFBUCLE saldo;
     #0#1;
     ;
     INICIO;
     FINAL;
     e;
     NULL, saldo_1;
|FIN;

|PROCESO tipo20; |TIPO 20;
     |SI FSalida = "OPCION";
          FSalida = "Saldo";
          |ACABA;
     |FINSI;

     |CIERRAT #0;
     |CIERRAT #1;
     swcero = 0;    |BUCLE saldo;
     swcero = 1;    |BUCLE saldo;
     |ABRET #0;
     |ABRET #1;
|FPRC;

|PROCESO mayor;
  #0Campo = #0Campo > " ";
  |PINTA #0Campo;
|FPRC;

|PROCESO cuenta; |TIPO 6;
     salidas = FSalida;
    p_alfa1 = #0Campo;
|HAZ punto;
|SI p_error ! 0;  |ERROR;  |ACABA;  |FINSI;
   cta_t = Campo;
   dig_t = Campo + 1;
   |HAZ cuenta_t;
|FPRC;

|PROCESO cuenta_t;
   cta  = #0cta_t;
   long = cta % 0;
   sw   = 0;
|SI long = dig4; sw = 1; #0dig_t = 1; |FINSI;
|SI long = dig5; sw = 1; #0dig_t = 2; |FINSI;
|SI long = dig6; sw = 1; #0dig_t = 3; |FINSI;
|SI long = dig7; sw = 1; #0dig_t = 4; |FINSI;
|SI long = dig8; sw = 1; #0dig_t = 5; |FINSI;
|SI long = dig9; sw = 1; #0dig_t = 6; |FINSI;

|SI sw = 0;
    #0dig_t = 0;
    |MENAV "    Longitud de cuenta fuera de Nivel !";
    |ERROR;
    |ACABA;
|FINSI;
|FPRC;

|PROCESO cuefin; |TIPO 0;
|ABRE #2;
#2#0 = #0cta_t;
#2#1 = #0dig_t;
|LEE 040#2=;
|SI FSalida ! 0; |CIERRA #2; |ERROR; |ACABA; |FINSI;
|SI #2#3 = "N";
    |CIERRA #2;
    |MENAV "      La cuenta no tiene pase DIRECTO !";
    |ERROR;
    |ACABA;
|FINSI;
#0#2 = #2#2;
|PINTA #0#2;
|CIERRA #2;
|FPRC;

|PROCESO c_departa; |TIPO 7;
  |C_M #caconcil#9, 1, eaDepar;
|FPRC;

|PROCESO pActividad; |TIPO 0;

   |C_M #caconcil#9,0,aAlfa;
   |SI aAlfa = "N"; |ACABA; |FINSI;

   aBlancs = " "* 78; |PINTA 2302, aBlancs;

   eOpDep = 0; || 1 = Consulta pantalla
   |SI FSalida = 10;  eOpDep = 1; |FINSI;
   eaCodDep    = #caconcil#9;
   |HAZ Actividad;
   |SI eaCodDep = ""; eswLect = 0; |FINSI;
   |SI eswLect = 1;
       |MENAV "    Error.! No Existe Departamento";
       |ERROR;
   |SINO;
       #caconcil#9 = eaCodDep; |PINTA #caconcil#9;
   |FINSI;
|FPRC;
