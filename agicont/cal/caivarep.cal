     |SI SerieE ! "";
         |SI nEstado = 0; |GRABA 020#120a; |FINSI;
         |SI nEstado ! 0; |GRABA 020#120b; |FINSI;
     |SINO;
         |SI nEstado = 0; |BORRA 020#120a; |FINSI;
     |FINSI;
 |FINSI;
 |SI nOpera = 1;           || Baja
     |SI nEstado = 0; |BORRA 020#120a; |FINSI;
 |FINSI;
 |LIBERA #120;
 |CIERRA #120;
|FPRC;

