|PROCESO nivel6;  |TIPO 6;
  p_alfa1 = #0Campo;
  |HAZ punto;
  |SI p_error ! 0;  |ERROR;  |ACABA;  |FINSI;
  cta = #0Campo;
  |QBF cta;
  nada = cta % A101;
  |SI nada ! "0";
      ctanum = cta;
      ctaalf = ctanum;
      longnum = ctaalf % A0;
  |SINO;
      ctanum = 1;
      ctaalf = cta;
      longnum = ctaalf % A0;
  |FINSI;
  longalf = cta    % A0;
  |SI longnum ! longalf;|O ctanum = 0;
      |MENAV "     CUENTA ERRONEA !!";
      |PINTA #0Campo;
      |ERROR;
      |ACABA;
  |FINSI;

  nivel= 0;
  sw = 0;
  |SI longnum = dig4; sw = 1; nivel = 1; |FINSI;
  |SI longnum = dig5; sw = 1; nivel = 2; |FINSI;
  |SI longnum = dig6; sw = 1; nivel = 3; |FINSI;
  |SI longnum = dig7; sw = 1; nivel = 4; |FINSI;
  |SI longnum = dig8; sw = 1; nivel = 5; |FINSI;
  |SI longnum = dig9; sw = 1; nivel = 6; |FINSI;
  |SI sw = 0;
      |MENAV "     LONGITUD DE CUENTA FUERA DE NIVEL !";
      |PINTA #0Campo;
      |ERROR;
      |ACABA;
  |FINSI;
  |SI Campo = 0;
      #0#1 = nivel;
  |SINO;
      #0#6 = nivel;
  |FINSI;
|FPRC;

|PROCESO nivel0; |TIPO 0;
  |ABRET #1;
  #1#0 = #0Campo;
  #1#1 = nivel;
  |LEE 101#1=;
  |CIERRAT #1;
  |SI #1#3 ! "S";
      |MENAV "     LA CUENTA NO ES DE PASO DIRECTO";
      |ERROR;
      |ACABA;
  |FINSI;
|FPRC;
