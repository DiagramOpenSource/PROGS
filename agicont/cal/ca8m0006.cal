|FICHEROS;
  :/agicont/def/ca8m0000 #0;
  :/agicont/def/ca8m0006 #1,MANTE;
  :/agicont/def/ca8m0007 #2,MANTE;

  :/agicont/def/ca8mtlin #11;
  :/agicont/def/ca8mtcol #12;

  ca8zso02 #10;
  :/agicont/def/ca8cumay #20;
|FIN;

|VARIABLES;
  &entrada  = 1;
  &nBalance = 0;

  inkey     = 0;
  esalta    = 0;
  esaltaL   = 0;
  nSwSal    = 0;

  aAlfa1    = "";
  aAlfa2    = "";
  nNume1    = 0;
  nNume2    = 0;
  nNume3    = 0;
  nBorro    = 0;
  aDesde    = "";
  aHasta    = "";
  nDesde    = 0;
  nHasta    = 0;
  nColum    = 0;
  nDColum    = 0;
  nHColum    = 0;
|FIN;

|INCLUYE varnue_i;

**************************************************************************
|RUTINA ClaveBajaL;
     #2#7  = #1#34;
     #2#0  = #1#0;
     #2#1  = #1#1;
     #2#2  = nColum;
     #2#3  = 001;
|FRUT;

|RUTINA ClaveAltaL;
     #2#7  = #1#34;
     #2#0  = #1#0;
     #2#1  = #1#1;
     #2#2  = nColum;
     #2#3  = 999;
|FRUT;

**************************************************************************
|PROCESO BorraLineas;
     |BORRA 140#2a;
     |LIBERA #2;
|FPRC;

|DEFBUCLE ca8m0007;
     #2#1;
     ;
     #1#34, #1#0, #1#1, nDColum, 001;
     #1#34, #1#0, #1#1, nHColum, 999;
     be;
     NULL, BorraLineas;
|FIN;

|CALCULO Tipo2_1;  |TIPO 2;
     esalta  = (FEntrada / 100) ! 0;
     |SI esalta ! 3;  |ACABA;  |FINSI;
     nDColum = 1;
     nHColum = 99;
     |BUCLE ca8m0007;
|FCAL;

||************************************************************************
|CALCULO TipoLinea;
 esalta = (FEntrada / 100) ! 0;
 aAlfa1 = "S"; aAlfa2 = "M";
 |SI #1#5 = "S"; aAlfa2 = "T"; |FINSI;
 |SI #1#5 = "T"; aAlfa1 = "N"; aAlfa2 = "X"; |FINSI;
 |PARA nNume1 = 7; |SI nNume1 [ 18; |HACIENDO nNume1 = nNume1 + 1;
       |C_M #1nNume1, 1, aAlfa1;
       |SI esalta = 1;
           #1nNume1 = aAlfa2; |PINTA #1nNume1;
       |FINSI;
 |SIGUE;
 |HAZ Campo5;
|FCAL;

|PROCESO Campo5;
 |SI #1#5 = "S";
     |C_M #1#20, 1, "S";
     |C_V #1#20, 1, "S";
     |PINTA 0868, "Sumar:     ";
     |PINTA #1#20;
 |SINO;
     |C_M #1#20, 1, "N"; #1#20 = 0;
     |C_V #1#20, 1, "N"; #1#20 = 0;
     |PINTA 0868, "           ";
 |FINSI;
|FPRC;

|CALCULO elTipo0; |TIPO 0;
  esalta  = (FEntrada / 100) ! 0;
  |SI esalta = 1;
      |HAZ LeeLinea;
      #1#2 = #11#6; |PINTA #1#2;
      #1#3 = #11#7; |PINTA #1#3;
      #1#6 = #11#2; |PINTA #1#6;
  |FINSI;
|FCAL;

|CALCULO elTipo3; |TIPO 3;
  #1#34 = #10#4;
  #1#0  = #10#2;

  esalta  = (FEntrada / 100) ! 0;
  |SI FSalida = 0;
     |SI #1#5 ! "I";
         nBorro = 0;
         |ABRE #2;
         #2#7  = #1#34;
         #2#0  = #1#0; #2#1 = #1#1;
         #2#2  = 1;    #2#3 = 1;
         |LEE 041#2];
         |SI FSalida = 0; |Y #2#0 = #1#0; |Y #2#1 = #1#1; |Y #2#7 = #1#34;
             |CONFI "    SBorro Relacion de Ctas posibles ";
             |SI FSalida = 0;
                 nBorro = 1;
             |FINSI;
         |FINSI;
         |CIERRA #2;
         |SI nBorro = 1;
             nDColum =  1;
             nHColum = 99;
             |BUCLE ca8m0007;
         |FINSI;
     |FINSI;
 |FINSI;
|FCAL;

|CALCULO elTipo3Col;
    inkey = FSalida;
    |BLIN 4;
    |SI #1#5 = "S";
        |SI #1Campo !  "T"; |Y #1Campo ! "X";
            #1Campo = "T"; |PINTA #1Campo;
            |ERROR;
            |ACABA;
        |FINSI;
    |FINSI;
    |SI #1Campo = "T";
        |SI #1#5 ! "S"; |ERROR; |ACABA; |FINSI;
    |FINSI;

    nColum = Campo - 6;

    |SI #1Campo = "C";
         |SI inkey ! 2; |Y inkey ! 3;
             nNume1 = Campo + 14;
             |C_P #1nNume1, 0, nNume2;
             |C_P #1Campo, 0, nNume3;
             |ATRI R; |PINTA nNume2, #1nNume1;
             |PINTA nNume3, #1Campo; |ATRI 0;
             nNume2 = Campo;
             nSwSal = 0;
             |ENTLINEAL #1#2, -5, 2, 22, 0, ClaveBajaL, ClaveAltaL;
             |PINTA #1nNume2;
             |PINTA #1nNume1;
         |FINSI;
    |SINO;
         nBorro = 0;
         |ABRE #2;
         #2#7  = #1#34;
         #2#0  = #1#0; #2#1 = #1#1;
         #2#2  = nColum;  #2#3 = 1;
         |LEE 041#2];
         |SI FSalida = 0; |Y #2#0 = #1#0; |Y #2#1 = #1#1; |Y #2#2 = nColum;
                 |Y #2#7  = #1#34;
             |CONFI "    SBorro Relacion de Ctas posibles ";
             |SI FSalida = 0;
                 nBorro = 1;
             |FINSI;
         |FINSI;
         |CIERRA #2;
         |SI nBorro = 1;
             nDColum = nColum;
             nHColum = nColum;
             |BUCLE ca8m0007;
         |FINSI;
    |FINSI;
|FCAL;

|CALCULO PresentaLinea;  |TIPO 7;
    nColum = Campo - 6;
    |SI Campo = 1;
        nColum = 1;
    |SINO;
        |SI #1#5 = "S"; aAlfa1 = "    T=Total Columna,X=Columna no Activa"; |FINSI;
        |SI #1#5 = "I"; aAlfa1 = "    C=Cuentas Contables, M=Importe Manual, X=Columna no Activa"; |FINSI;
        |MENSA aAlfa1;
    |FINSI;

    |HAZ LeeColumna;
    |C_A #12#3, 1, 14; |PINTA 0966, #12#3; |C_A #12#3, 1, 80;
    |C_A #12#4, 1, 14; |PINTA 1066, #12#4; |C_A #12#4, 1, 80;
    |HAZ Campo5;

    nSwSal = 1;
    |ENTLINEAL #1#2, -5, 7, 22, 0, ClaveBajaL, ClaveAltaL;
|FCAL;

|PROCESO LeeColumna;
 |ABRE #12;
 |DDEFECTO #12;
 #12#5 = #1#34;
 #12#0 = #1#0;
 #12#1 = nColum;
 |LEE 001#12=;
 |CIERRA #12;
|FPRC;

|PROCESO LeeLinea;
 |ABRE #11;
 |DDEFECTO #11;
 #11#8 = #1#34;
 #11#0 = #1#0;
 #11#1 = #1#1;
 |LEE 001#11=;
 |CIERRA #11;
|FPRC;


|| ************************************************************************
|CALCULO SalgoYa;  |TIPO 7;
   |SI nSwSal = 1;  |PTEC 807;  |FINSI;
|FCAL;

|RUTINA min20;
  #20#12 = #10#4;
  #20#10 = 0;
  #20#0  = "     ";
  #20#2 = nDesde;
  #20#3 = aDesde;
  #20#5 = 00;
|FRUT;

|RUTINA max20;
  #20#12 = #10#4;
  #20#10 = 0;
  #20#0 = "zzzzz";
  #20#2 = nHasta;
  #20#3 = aHasta;
  #20#5 = 99;
|FRUT;

|CALCULO Tipo0_L; |TIPO 0;
  inkey = FSalida;
  |SI inkey = 10;
      nDesde = 0;   nHasta = 9;
      aDesde = " "; aHasta = "Z";
      |ABRE #20;
      |CONSULTA_F_CLAVE #4#20, min20, max20;
      |CIERRA #20;
      #2#4  = #20#0;  |PINTA #2#4;
  |FINSI;

  aAlfa1 = #2#4; |QBF aAlfa1;
  |SI aAlfa1 = ""; |ERROR; |FINSI;

  aAlfa1 = aAlfa1 % 101;
  |SI aAlfa1 = "7"; #2#5 = "-"; |FINSI;
  |SI aAlfa1 = "9"; #2#5 = "-"; |FINSI;
  |PINTA #2#5;
|FCAL;
