|FICHEROS;
    caxinfim #0;
    caxcontr #01;   || control expor./impor.
    exivarep #05;   || repercutido (exportacion)
    exivasop #07;   || soportado (exportacion)
    exmancob #09;   || cobros (exportacion)
    exmanpag #11;   || pagos (exportacion)
    excuenta #13;   || plan contable (exportacion)
    exmaasic #15;   || apuntes cabs. (exportacion)
|FIN;

|VARIABLES;
    alfa1 = "";
    alfa2 = "";
    alfa3 = "";
    alfa4 = "";
    nume1 = 0;
    nume2 = 0;
    nume3 = 0;
    nume4 = 0;
    para1 = 0;
    para2 = 0;
    para3 = 0;
    para4 = 0;
    fech1 = @;
    fech2 = @;
    fech3 = @;
    fech4 = @;
    FEstado = 0;
    seleccio = 0;
    sel_apun = 0;
    sel_repe = 0;
    sel_sopo = 0;
    sel_cobr = 0;
    sel_pago = 0;
    sel_cuen = 0;

    &tb = 0;
    &tc = 0;
    &tr = 0;
    feini = @;
    fefin = @;
|FIN;
____________________________________/ CALCULOS PANTALLA
|PROCESO ver_tipo; |TIPO 8;
|ABRE #1;
|LEE 001#1p;
|SI FSalida = 0;
    |SI #1#1 = "E";
        |MENAV "    Control configurado para la Exportacion ...";
        |PTEC 807;
    |FINSI;
|SINO;
    |MENAV "    Debera definir el Control de Exportaciones/Importaciones ...";
    |PTEC 807;
|FINSI;
|CIERRA #1;
|FPRC;

|PROCESO comprueba; |TIPO 3;
|SI #0#0 = "SI";
    |IMPRE 0;
    |SI FSalida = 0;
        |SI #1#2 = "S";  |HAZ apuntes0;                  |FINSI;
        |SI #1#3 = "S";  |HAZ repercu0;  |HAZ soporta0;  |FINSI;
        |SI #1#4 = "S";  |HAZ cobros0;   |HAZ pagos0;    |FINSI;
        |SI #1#5 = "S";  |HAZ cuentas0;                  |FINSI;
        |SI sel_apun = 0;|Y sel_repe = 0;|Y sel_sopo = 0;|Y sel_cobr = 0;
                         |Y sel_pago = 0;|Y sel_cuen = 0;
            |MENAV "    Seleccion vacia ...";
        |FINSI;
    |SINO;
        |MENAV "    Proceso abortado ...";
    |FINSI;
    |FINIMP;
|FINSI;
|FPRC;
____________________________________/ COMPROBACION APUNTES
|PROCESO apuntes2;
    sel_apun = 1;
|PRINT;
|FPRC;

|DEFBUCLE apuntes1;
 #15#1;
 ;
  0, feini,      0;
 99, fefin, 999999;
 e;
 NULL, apuntes2;
|FIN;

|PROCESO apuntes0;
  |INFOR "exmaasic";
  feini = "01.01.0001";
  fefin = "31.12.8999";
  |BUCLE apuntes1;
  |PIEINF;
  |FININF;
|FPRC;
____________________________________/ COMPROBACION REPERCUTIDO
|PROCESO repercu2;
    tb = #5#08 + #5#09 + #5#10 + #5#52 + #5#53;   || base total
    tc = #5#14 + #5#15 + #5#16 + #5#56 + #5#57;   || cuota iva total
    tr = #5#20 + #5#21 + #5#22 + #5#60 + #5#61;   || cuota recargo total
    sel_repe = 1;
|PRINT;
|FPRC;

|DEFBUCLE repercu1;
 #5#3;
 ;
  0, feini,      0, "  ";
 99, fefin, 999999, "zz";
 e;
 NULL, repercu2;
|FIN;

|PROCESO repercu0;
  |INFOR "exivarep";
  feini = "01.01.0001";
  fefin = "31.12.8999";
  |BUCLE repercu1;
  |PIEINF;
  |FININF;
|FPRC;
____________________________________/ COMPROBACION SOPORTADO
|PROCESO soporta2;
    tb = #7#08 + #7#09 + #7#10 + #7#52 + #7#53;   || base total
    tc = #7#14 + #7#15 + #7#16 + #7#56 + #7#57;   || cuota iva total
    tr = #7#20 + #7#21 + #7#22 + #7#60 + #7#61;   || cuota recargo total
    sel_sopo = 1;
|PRINT;
|FPRC;

|DEFBUCLE soporta1;
 #7#3;
 ;
  0, feini,      0;
 99, fefin, 999999;
 e;
 NULL, soporta2;
|FIN;

|PROCESO soporta0;
  |INFOR "exivasop";
  feini = "01.01.0001";
  fefin = "31.12.8999";
  |BUCLE soporta1;
  |PIEINF;
  |FININF;
|FPRC;
____________________________________/ COMPROBACION COBROS
|PROCESO cobros2;
    sel_cobr = 1;
|PRINT;
|FPRC;

|DEFBUCLE cobros1;
 #9#1;
 ;
  0,      0, "  ",   0;
 99, 999999, "zz", 999;
 e;
 NULL, NULL, NULL, NULL, NULL, cobros2;
 #9#6, #9#0, #9#1, #9#2, #9#3;
|FIN;

|PROCESO cobros0;
|INFOR "exmancob";
|BUCLE cobros1;
|PIEINF;
|FININF;
|FPRC;
____________________________________/ COMPROBACION PAGOS
|PROCESO pagos2;
    sel_pago = 1;
|PRINT;
|FPRC;

|DEFBUCLE pagos1;
 #11#1;
 ;
  0,      0, "  ",   0;
 99, 999999, "zz", 999;
 e;
 NULL, NULL, NULL, NULL, NULL, pagos2;
 #11#6, #11#0, #11#1, #11#2, #11#3;
|FIN;

|PROCESO pagos0;
|INFOR "exmanpag";
|BUCLE pagos1;
|PIEINF;
|FININF;
|FPRC;
____________________________________/ COMPROBACION CUENTAS
|PROCESO cuentas2;
    sel_cuen = 1;
|PRINT;
|FPRC;

|DEFBUCLE cuentas1;
 #13#1;
 ;
 "            ", 0;
 "zzzzzzzzzzzz", 9;
 e;
 NULL, cuentas2;
|FIN;

|PROCESO cuentas0;
|INFOR "excuenta";
|BUCLE cuentas1;
|PIEINF;
|FININF;
|FPRC;
