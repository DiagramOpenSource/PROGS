|PROCESO Calculo;
     |SI #660#3 ] "31.12.1994";  |ACABA;  |FINSI;

     |ABRE #999;
     |SELECT #2#999;
     #999#1 = #20#2;
     |LEE 000#999=;
     |SI FSalida ! 0;  |DDEFECTO #999;  |FINSI;

     |SELECT #1#999;
     |CIERRA #999;

     |SI #999#5 = "S";
         nValor1 = #660#7;
         |ACABA;
     |FINSI;

     |SI #660#12 < #660#5;
         nValor1 = #660#12 - #660#6;
     |SINO;
         nValor1 = #660#7;
     |FINSI;
|FPRC;

|PROCESO Totalw660;
     #660#7 = #660#5 - #660#6;
     |SI #660#7 < 0;
         #660#8 = -#660#7;
         #660#9 = 0;
     |SINO;
         aAlfa1      = #660#3 % -104;
         aAlfa3      = #660#3 % 0105;
         aAlfa2      = #660#3 % -104;
         nAnyoCompra = aAlfa1;
         nAnyoVenta  = aAlfa2;
         nDiferencia = nAnyoVenta - nAnyoCompra;

         |SI aAlfa3 = "31.12";  nAnyoCompra = nAnyoCompra + 1;  |FINSI;

         |SI #20#11 = "F1";   |O #20#11 = "G2";  |O #20#11 = "H1";
             nPorce = 25;
         |SINO;
             nPorce = 14.28;
         |FINSI;

         nAnyos = (1996 - nAnyoCompra) + 1;
         nCoefi = 0;
         |SI nAnyos ] 3;
             nCoefi = (nAnyos - 2) * nPorce;
         |FINSI;
         |SI nCoefi > 99; nCoefi = 100; |FINSI;

         |HAZ Calculo;

         |SI #660#3 ] "31.12.1994";
             nValor1 = 0;
         |FINSI;

         |SI nValor1 < 0;  nValor1 = 0;  |FINSI;

         nValor1 = nValor1 % nCoefi;
         #660#9   = #660#7 - nValor1;
         #660#8   = 0;
     |FINSI;

     |PINTA #660#7;
     |PINTA #660#8;
     |PINTA #660#9;

     |C_M #660#10, 1, "S";
     |C_M #660#11, 1, "S";
     |SI #660#7 = 0;  |O #660#7 < 0;
         |C_M #660#10, 1, "N";  #660#10 = 0;  |PINTA #660#10;
         |C_M #660#11, 1, "N";  #660#11 = 0;  |PINTA #660#11;
     |FINSI;
|FPRC;

