|PROCESO rem00060;
     |SI #60#5 = 0;  |Y #60#6 = 0;  |ACABA;  |FINSI;

     aAlfa1      = #60#3 % -104;
     aAlfa2      = #60#4 % -104;
     nDiferencia = (#60#4 - #60#3) + 1;
     nLinea      = nLinea1;

     |SI nDiferencia > 365;
         nLinea = nLinea2;
     |FINSI;

     |DDEFECTO #36;
     #36#0  = #60#0;
     #36#1  = nLinea;
     |LEE 101#36=;
     |SI FSalida ! 0;
         |DDEFECTO #36;
         #36#0  = #60#0;
         #36#1  = nLinea;
         |PARA nCampo = 2;  |SI nCampo [ 10;  |HACIENDO nCampo = nCampo + 1;
               #36nCampo = #20nCampo;
         |SIGUE;
         #36#3  = "R";
         |SI #20#3 = "X";  #36#3 = "X";  |FINSI;

         #36#11 = #20#17;
         #36#16 = "C5";
         #36#17 = #60#1;
         #36#18 = #20#16;
         |GRABA 020#36b;
     |FINSI;

     #36#12 = #36#12 + #60#9;
     #36#13 = #36#13 + #60#8;
     #36#19 = #36#19 + #60#11;

     #36#21 = "1";
     |SI nDiferencia > 365;
         #36#21 = "2";
     |FINSI;

     |GRABA 020#36a;
     |LIBERA #36;
|FPRC;

|DEFBUCLE rem00060;
     #60#1;
     ;
     #20#0, #20#1, 01;
     #20#0, #20#1, 99;
     ;
     NULL, rem00060;
|FIN;

