|FICHEROS;
     reconfac #0;
     :/facges/def/facemp #1;      || empresas facturacion
|FIN;

|VARIABLES;
     alfa1 = "";
     alfa2 = "";
     alfa3 = "";
     alfa4 = "";
     nume1 = 0;
     nume2 = 0;
     nume3 = 0;
     nume4 = 0;
     fech1 = @;
     fech2 = @;
     fech3 = @;
     fech4 = @;
     primera = 0;
     cuarenta = " ";
     putada = "";
     factu = "";
     aOrigen  = "";
     aDestino = "";
     aAplicas = "";
     aRen12   = "";
     aRen13   = "";
     aFicFac = "";
|FIN;

|PROCESO LeeEmpre; |TIPO 0;
     |ABRE #1;
     #1#0 = #0#1;
     |LEE 001#1=;
     |SI FSalida ! 0;
         |MENAV "    ATENCION!!! Empresa inexistente en Facturacion ...";
         |ERROR;
     |FINSI;
     |CIERRA #1;

     |PINTA 0732, #1#1;
|FPRC;

|PROCESO TraeEjerAnt;
     |DBASS aAplicas;   |QBT aAplicas;
     aRen12 = aAplicas + "agiren12/fich/";
     |DFICE aRen13;     |QBT aRen13;

     aOrigen  = aRen12 + "reconfac.dat";
     aDestino = aRen13 + "reconfac.dat";
     |COPIA_FICHERO aOrigen, aDestino;

     aOrigen  = aRen12 + "recontar.dat";
     aDestino = aRen13 + "recontar.dat";
     |COPIA_FICHERO aOrigen, aDestino;

     aOrigen  = aRen12 + "reconcod.dat";
     aDestino = aRen13 + "reconcod.dat";
     |COPIA_FICHERO aOrigen, aDestino;

     aOrigen  = aRen12 + "reempres.dat";
     aDestino = aRen13 + "reempres.dat";
     |COPIA_FICHERO aOrigen, aDestino;
|FPRC;

|PROGRAMA;
     |DBASS aFicFac;
     aFicFac = aFicFac + "facges/fich/";
     |PATH_DAT #1 aFicFac;

     |CLS;
     |CABEZA "Control Pase";
     |ABRE #0;
     |LEE 001#0p;
     |SI FSalida ! 0;
         |HAZ TraeEjerAnt;
     |FINSI;
     |CIERRA #0;

     |ABRE #0;
     |LEE 001#0p;
     |SI FSalida ! 0;
        |DDEFECTO #0;
        |GRABA 020#0b;
     |FINSI;

     |PINPA #0#0;
     |PINDA #0#0;

     |HAZ LeeEmpre;

     |ENDATOS #1#0;
     |SI FSalida = 0;
        |GRABA 020#0a;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;
