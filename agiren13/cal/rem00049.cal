|| ---- PROCESOS DEL REW00649

|PROCESO FinVentana;
     |SI nVentana ! -1;
         |FINVENTANA nVentana;
         nVentana = -1;
     |FINSI;

     |PULSATECLA;
|FPRC;

|PROCESO Tipo12Fw649;  |TIPO 12;
     |SI FSalida = 7;
         |HAZ FinVentana;
         |ACABA;
     |FINSI;

     |HAZ FinVentana;

     aAlfa = #649#2;  |QBF aAlfa;
     |SI aAlfa = "";
         |MENAV "0000No se puede validar la ficha, no hay contribuyente introducido";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO TotalFw649;
     #649#15 = #649#13 % #649#14;

     |SI #649#15 > 9015;
         #649#15 = 9015;
     |FINSI;
     |PINTA #649#15;
|FPRC;

|PROCESO Tipo0C12Fw649;
     |SI #649#12 < "27.04.2003";  |O #649#12 > "31.12.2013";
         |MENAV "0000 Fecha Erronea. Limite del 27.04.2003 al 31.12.2013";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Tipo0C13Fw649;
     |SI #649#13 > 9000;
         |MENAV "     Limite maximo 9.000 Euros.";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Tipo0C1Fw649;
     |C_M #649#1, 0, aAlfa;
     |SI aAlfa = "N";  |ACABA;  |FINSI;

     #20#0 = #649#0;
     #20#1 = #649#1;
     |LEE 000#20=;
     |SI FSalida = 0;
         |MENAV "0000El registro ya existe.";
         |ERROR;
         |ACABA;
     |FINSI;
|FPRC;

|PROCESO CargaFantasma;
     |C_M #649#12, 1, "S";
     |C_M #649#13, 1, "S";
     |C_M #649#14, 1, "S";
     |C_M #649#15, 1, "S";
     |C_M #649#20, 1, "S";
     |C_M #649#21, 1, "S";
     |C_M #649#22, 1, "S";
     |C_M #649#23, 1, "S";

     |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
           #649nCampo = #20nCampo;

           |C_V #649nCampo, 0, aAlfa;
           |SI aAlfa = "S";  |PINTA #649nCampo;  |FINSI;
     |SIGUE;
|FPRC;

|PROCESO DescargaFantasma;
     |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
           |SI enSwLiquida = 1;
               |SI #20nCampo = #649nCampo;
                   enSwRetorno = 1;
               |FINSI;
           |FINSI;

           #20nCampo = #649nCampo;
     |SIGUE;

     #20#11 = #20#20 + #20#21 + #20#22 + #20#23;

                        #20#24 = "";
     |SI #20#3  = "X";  #20#24 = eaRojo;  |FINSI;

     |HAZ PintaDatosPantalla;
|FPRC;

|PROCESO Alta;
     |ESTADO_CONTROL nBotonAlta,   "DISABLE";
     |ESTADO_CONTROL nBotonModi,   "DISABLE";
     |ESTADO_CONTROL nBotonBaja,   "DISABLE";

     |HAZ QuitaBtnSalir;

     |ESTADO_CONTROL nGrid, "DISABLE";

     |PULSATECLA;

     #20#0 = enEmpresa;
     #20#1 = 99;
     |LEE 000#20];
     |SI FSalida = 0;
         |LEE 000#20a;
     |SINO;
         |LEE 000#20u;
     |FINSI;

     |DDEFECTO #649;
     #649#0 = enEmpresa;
     #649#1 = 1;
     |SI FSalida = 0;  |Y  #20#0 = enEmpresa;
         #649#1 = #20#1 + 1;
     |FINSI;

     |DDEFECTO #20;

     #20#0 = #649#0;
     #20#1 = #649#1;

     enModoReg = 1;
     |HAZ CargaFantasma;

     |C_M #649#1, 1, "S";
     |ENDATOS #1#649;
     |SI FSalida = 0;
         |HAZ DescargaFantasma;

         |GRABA 020#20n;
         |LIBERA #20;
     |FINSI;

     |SI enModo ! 4;
         |ESTADO_CONTROL nBotonAlta, "ENABLE";
     |FINSI;

     |HAZ PonBtnSalir;

     enModoReg = 4;
     |ESTADO_CONTROL nGrid, "ENABLE";
     |REFRESCACONTROL nGrid;

     |FOCOTECLADO nGrid;
|FPRC;

|PROCESO Modificar;
     |SI nEntrar = 0;  |ACABA;  |FINSI;

     |LEE 101#20=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |ESTADO_CONTROL nBotonAlta,  "DISABLE";
     |ESTADO_CONTROL nBotonModi,  "DISABLE";
     |ESTADO_CONTROL nBotonBaja,  "DISABLE";

     |HAZ QuitaBtnSalir;

     |ESTADO_CONTROL nGrid, "DISABLE";

     enModoReg = 2;
     |C_M #649#1, 1, "N";

     |HAZ CargaFantasma;
     |ENDATOS #2#649;
     |SI FSalida = 0;
         |HAZ DescargaFantasma;

         |GRABA 020#20a;
     |FINSI;
     |LIBERA #20;

     |ESTADO_CONTROL nBotonAlta, "ENABLE";
     |ESTADO_CONTROL nBotonModi, "ENABLE";
     |ESTADO_CONTROL nBotonBaja, "ENABLE";

     |HAZ PonBtnSalir;

     enModoReg = 4;
     |ESTADO_CONTROL nGrid, "ENABLE";
     |REFRESCACONTROL nGrid;
     |FOCOTECLADO nGrid;
|FPRC;

|PROCESO Baja;
     |SI nEntrar = 0;  |ACABA;  |FINSI;

     |CONFI "0000 Confirma la baja del registro seleccionado";
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |LEE 101#20=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |BORRA 020#20a;
     |LIBERA #20;

     nEntrar     = 0;
     enSwRetorno = 1;

     |ESTADO_CONTROL nBotonModi,  "DISABLE";
     |ESTADO_CONTROL nBotonBaja,  "DISABLE";

     |REFRESCACONTROL nGrid;
     |FOCOTECLADO nGrid;
|FPRC;

|PROCESO BotonCntr;
     enComunes   = 1;
     enChequeo   = 0;

     #rew00003#0 = #649#2;
     #rew00003#1 = #649#3;
     #rew00003#2 = #649#4;
     #rew00003#3 = #649#5;
     #rew00003#4 = #649#6;
     #rew00003#5 = #649#7;
     #rew00003#6 = #649#8;
     #rew00003#7 = #649#9;
     #rew00003#8 = #649#10;

     |HAZ PantaREW00003;

     #649#2  = #rew00003#0;    |PINTA #649#2;
     #649#3  = #rew00003#1;
     #649#4  = #rew00003#2;
     #649#5  = #rew00003#3;
     #649#6  = #rew00003#4;
     #649#7  = #rew00003#5;
     #649#8  = #rew00003#6;
     #649#9  = #rew00003#7;
     #649#10 = #rew00003#8;

     aAlfa = "|C016WID";
     aID   = #649aAlfa;
     nID   = aID;
     |FOCOTECLADO nID;

     |SI #649#3 = "X";  |Y enModoReg ! 4;
         |PTEC 806;
     |FINSI;
|FPRC;

|PROCESO Tipo7C2Fw649;  |TIPO 7;
     aAlfa = #649#2;  |QBF aAlfa;
     |SI aAlfa ! "";  |Y #649#3 ! "X";
         |ACABA;
     |FINSI;

     |HAZ BotonCntr;
|FPRC;

|PROCESO PintaDatosPantalla;
     |DDEFECTO #649;
     |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
           #649nCampo = #20nCampo;

           |C_V #649nCampo, 0, aAlfa;
           |SI aAlfa = "S";  |PINTA #649nCampo;  |FINSI;

           |C_M #649nCampo, 1, "N";
     |SIGUE;
|FPRC;

|PROCESO Evento20;
     |SI enModoReg ! 4;  |ACABA;  |FINSI;

     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#20=;

         |SI enModo = 4;
             |ESTADO_CONTROL nBotonAlta, "DISABLE";
             |ESTADO_CONTROL nBotonModi, "DISABLE";
             |ESTADO_CONTROL nBotonBaja, "DISABLE";
         |SINO;
             |ESTADO_CONTROL nBotonModi, "ENABLE";
             |ESTADO_CONTROL nBotonBaja, "ENABLE";
         |FINSI;

         |PULSATECLA;

         nEntrar   = 1;
         enModoReg = 4;
         |HAZ PintaDatosPantalla;
     |FINSI;

     |SI FSalida = 4;
         |SI enModo = 4;  |ACABA;  |FINSI;

         |HAZ Modificar;
     |FINSI;
|FPRC;

|PROCESO BotonesPanta;
     |CONTROL_SIMPLE nPanta, "BOTON,{{193}}&Alta ", 2164, 2174, Alta;
     nBotonAlta = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{197}}&Modificar ", 2176, 2186, Modificar;
     nBotonModi = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{199}}&Borrar", 2188, 2198, Baja;
     nBotonBaja = FSalida;

     |CONTROL_SIMPLE nPanta, "BOTON,{{197}} &Y", 2422, 2426, BotonCntr;
     nBotonCntr = FSalida;

     nEntrar = 0;

     |ESTADO_CONTROL nBotonModi,   "DISABLE";
     |ESTADO_CONTROL nBotonBaja,   "DISABLE";

     |SI enModo = 4;
         |ESTADO_CONTROL nBotonAlta, "DISABLE";
     |FINSI;
|FPRC;

|PROCESO Baja20;
     #20#0 = enEmpresa;
     #20#1 = 1;
|FPRC;

|PROCESO Alta20;
     #20#0 = enEmpresa;
     #20#1 = 99;
|FPRC;

