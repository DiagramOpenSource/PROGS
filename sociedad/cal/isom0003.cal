|PROCESO Tipo3;  |TIPO 3;
     |C_M #isom0003#5, 0, aAlfa;
     |SI aAlfa = "N";  |ACABA;  |FINSI;

     |GRABA 020#isom0003.a;
     |LIBERA #isom0003;

     nLinea = #isom0003#2;

     |ABRE #isom0001;
     |ABRE #isom0004;
     |HAZ TotalFich103;
     |CIERRA #isom0001;
     |CIERRA #isom0004;

     #isom0003#2 = nLinea;
