     |SI #isom0003#6 = "N";
         |SI #isom0003#7 = "I";
             |MENAV "0000 Casilla no operativa, no procede por la configuraci¢n de la empresa.";
         |FINSI;
     |SINO;
         |SI #isom0003#7 = "I";
             |C_M #isom0003#5, 1, "S";
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO Evento103;
     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#isom0003.=;
     |FINSI;
|FPRC;

|PROCESO isom0502;
