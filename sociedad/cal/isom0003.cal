     |SI #cuaccpar#3 = "S";  |Y enModoConsulta = 0;
         nRango = 1 + 4 + 8 + 16 + 128 + 256;
     |FINSI;

     |LINEAL_SIMPLE #1#isom0003, nRango, 0502, 3498, Baja103, Alta103, Evento103;

     |FIN_CONTROL_WINDOWS nBotonSal;

