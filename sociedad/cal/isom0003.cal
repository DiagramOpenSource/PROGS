         #isom0003#0  = enEmpresa;
         #isom0003#1  = #isom0502#0;
         #isom0003#2  = #isom0502#1;
         |GRABA 020#isom0003.b;
     |FINSI;

     #isom0003#3  = #isom0502#2;
     #isom0003#4  = #isom0502#3;
     #isom0003#6  = "N";

     |SI #ca8m0000#2 = 1;  #isom0003#6 = #isom0502#4;  #isom0003#7 = #isom0502#7;  |FINSI;
     |SI #ca8m0000#2 = 2;  #isom0003#6 = #isom0502#5;  #isom0003#7 = #isom0502#8;  |FINSI;
     |SI #ca8m0000#2 = 3;  #isom0003#6 = #isom0502#6;  #isom0003#7 = #isom0502#9;  |FINSI;

     eaTipoColor = #isom0502#10;
     |HAZ CogeColor;
     #isom0003#8      = eaColor;

     |GRABA 020#isom0003.a;
     |LIBERA #isom0003;
|FPRC;

