     |SI nAcum = 0;    |ACABA;  |FINSI;
     |SI nLimiteT [ 0; |ACABA;  |FINSI;
     |SI nTope    = 0; |ACABA;  |FINSI;

     |SI nDeducido > nTope;
         nDeducido = nTope;
         nTope     = 0;
     |SINO;
         nTope = nTope - nDeducido;
     |FINSI;

     nAcum = nAcum - nDeducido;
     |SI nAcum ] 0
         nAplicado = nDeducido;
     |SINO;
         nAplicado = nDeducido + nAcum;
         nAcum     = 0;
     |FINSI;

     nLimiteT = nLimiteT - nAplicado;
     |SI nLimiteT < 0;
         nAplicado = nAplicado + nLimiteT;
         nLimiteT  = 0;
     |FINSI;
|FPRC;

