|FICHEROS;
     isom0000;
     isom0001;
     isom0010;
     isom0507;

     maempr10;
|FIN;

|VARIABLES;
      nRango         = 0;
      nBotonSal      = 0;
      nBotonAnota    = -1;
      nTree          = 0;
      nHandle        = 0;
      nPanta         = 0;
      nBotonLabel    = 0;
      nBotonPorce    = 0;
      nVentana       = 0;
      nVent          = 0;
      nGrid          = 0;
      nModo          = 0;
      nLinea         = 0;
      nBase          = 0;
      nBase1         = 0;
      nCas560        = 0;
      nCas593        = 0;
      nPorce         = 0;
      nVal           = 0;
      nDiasAnyo      = 0;
      nDias          = 0;

      aContenido     = "";
      aTexto         = "";
      aBase          = "";
      aAlfa          = "";
      aAbre          = "";
      aEsc1          = "";
      aEsc2          = "";
      aRetu          = "";
      aTecla         = "";

      fFecha1        = @;
      fFecha2        = @;

     {-1}sTree       = 0;
     sT_nID          = 0;
     sT_nOper        = 0;
     sT_aData        = "";

     {-1}aVent       = "";
         aVent1      = 0;
         aVent2      = 0;
         aVent3      = 0;
         aVent4      = 0;
         aVent5      = "";

     &enEmpresa      = 0;
     &enAnyo         = 0;
     &enVal1510      = 0;
     &eaCasilla      = "";
     &eaTitCasilla   = "";
     &eaRutaOrigen   = "";
     &eaRutaDestino  = "";
     &eaFicheroTra   = "";
     &eaFicheroTxt   = "";
     &eaFicheroTgz   = "";
     &eaFicheroTar   = "";
     &eaOrigen       = "";
     &eaDestino      = "";
     &eaAlfa         = "";
     &eaTipoColor    = "";
     &eaColor        = "";
     &enModoConsulta = 0;
|FIN;

|PROCESO Tipo1;  |TIPO 1;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;  |O nModo = 3;
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Tipo7C8m10;  |TIPO 7;
     |SI #isom0010#7 = "     ";
         |ACABA;
     |FINSI;

     |SI #isom0010#6 < nBase;
         #isom0010#8 = #isom0010#6;
     |SINO;
         #isom0010#8 = nBase;
     |FINSI;

     |PINTA #isom0010#8;
|FPRC;

|PROCESO Tipo0C8m10;  |TIPO 0;
     |SI #isom0010#7 = "     ";
         |ACABA;
     |FINSI;

     |SI nBase [ 0;
         |SI #isom0010#8 ! 0;
             |MENAV "0000 Lo aplicado supera a la casilla 560";
             |ERROR;
             |ACABA;
         |FINSI;
     |FINSI;

     |SI #isom0010#8 > nBase;
         |MENAV "0000 Lo aplicado supera a la casilla 560";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Totalm10;
     #isom0010#10 = 0;

     |SI #isom0010#7 ! "     ";
         |SI #isom0010#8 > #isom0010#6;  #isom0010#8 = #isom0010#6;   |FINSI;
     |FINSI;

     |SI #isom0010#9 ! "     ";
         #isom0010#10 = #isom0010#6 - #isom0010#8;
     |FINSI;

     |PINTA #isom0010#8;
     |PINTA #isom0010#10;
|FPRC;

|PROCESO Tipo2;  |TIPO 2;
     |HAZ Tipo1;

     nBase = (nBase -. #isom0010#8) ! 2;
|FPRC;

|PROCESO Tipo3;  |TIPO 3;
     #isom0507#0 = #isom0010#1;
     #isom0507#1 = #isom0010#2;
     |LEE 000#isom0507.=;
     |SI FSalida ! 0;  |DDEFECTO #isom0507;  |FINSI;

     |SI #isom0507#8 ! "I";  |ACABA;  |FINSI;

     |GRABA 020#isom0010.a;
     |LIBERA #isom0010;

     nLinea = #isom0010#2;

     |HAZ TotalFich110;

     #isom0010#0 = #maempr10#0;
     #isom0010#1 = #maempr10#1;
     #isom0010#2 = nLinea;
     |LEE 101#isom0010.=;

     aAlfa = "|IDGRID";
     aAlfa = #isom0010#aAlfa#;
     nGrid = aAlfa;
     |REFRESCACONTROL nGrid;
|FPRC;

|PROCESO Tipo0;  |TIPO 0;
     |C_M #isom0010#4,  1, "N";
     |C_M #isom0010#6,  1, "N";
     |C_M #isom0010#8,  1, "N";
     |C_M #isom0010#10, 1, "N";

     #isom0507#0 = #isom0010#1;
     #isom0507#1 = #isom0010#2;
     |LEE 000#isom0507.=;
     |SI FSalida ! 0;  |DDEFECTO #isom0507;  |FINSI;

     |SI #isom0507#8 ! "I";  |ACABA;  |FINSI;

     |SI enAnyo = 2008;   |C_M #isom0010#4,  1, "S";  |FINSI;

     |SI #isom0010#5 ! "     ";  |C_M #isom0010#6,  1, "S";  |FINSI;
     |SI #isom0010#7 ! "     ";  |C_M #isom0010#8,  1, "S";  |FINSI;
     |SI #isom0010#9 ! "     ";  |C_M #isom0010#10, 1, "S";  |FINSI;
|FPRC;

|PROCESO Evento110;
     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#isom0010.=;
     |FINSI;
|FPRC;

|PROCESO isom0507;
     #isom0010#0  = enEmpresa;
     #isom0010#1  = #isom0507#0;
     #isom0010#2  = #isom0507#1;
     |LEE 101#isom0010.=;
     |SI FSalida ! 0;
         |DDEFECTO #isom0010;
         #isom0010#0  = enEmpresa;
         #isom0010#1  = #isom0507#0;
         #isom0010#2  = #isom0507#1;
         |GRABA 020#isom0010.b;
     |FINSI;

     |SI enAnyo ] 2009;
         #isom0010#4  = #isom0507#2;
     |FINSI;

     #isom0010#3  = #isom0507#7;
     #isom0010#5  = #isom0507#4;
     #isom0010#7  = #isom0507#5;
     #isom0010#9  = #isom0507#6;

     eaTipoColor = #isom0507#9;
     |HAZ CogeColor;
     #isom0010#11     = eaColor;

     |GRABA 020#isom0010.a;
     |LIBERA #isom0010;
|FPRC;

|DEFBUCLE isom0507;
     #isom0507#1;
     ;
     enAnyo, 1000;
     enAnyo, 9999;     || Del 1000 al 9999 es el isom0010
     ;
     NULL, isom0507;
|FIN;

|PROCESO MontaEstructura507;
     |BUCLE isom0507;
|FPRC;

|PROCESO isom0010;
     |SI #isom0010#6  ! 0;  |ACABA;  |FINSI;
     |SI #isom0010#8  ! 0;  |ACABA;  |FINSI;
     |SI #isom0010#10 ! 0;  |ACABA;  |FINSI;

     |BORRA 020#isom0010.a;
     |LIBERA #isom0010;
|FPRC;

|DEFBUCLE isom0010;
     #isom0010#1;
     ;
     enEmpresa, enAnyo, 0000;
     enEmpresa, enAnyo, 9999;
     be;
     NULL, isom0010;
|FIN;

|PROCESO BotonSalir;
     |PTEC 807;
|FPRC;

|PROCESO BotonAnota;
     eaCasilla    = #isom0010#5;     |QBF eaCasilla;
     eaTitCasilla = #isom0010#3;     |QBF eaTitCasilla;
     eaTitCasilla = eaTitCasilla + " " + #isom0010#4;

     |SUB_EJECUTA "isom0499";
|FPRC;

|PROCESO Baja110;
     #isom0010#0  = enEmpresa;
     #isom0010#1  = enAnyo;
     #isom0010#2  = 1;
|FPRC;

|PROCESO Alta110;
     #isom0010#0  = enEmpresa;
     #isom0010#1  = enAnyo;
     #isom0010#2  = 9999;
|FPRC;

|PROCESO isom0010R;
     |SI #isom0010#6  = 0;  |ACABA;  |FINSI;

     |SI nBase = 0;
         #isom0010#8  = 0;
         #isom0010#10 = 0;
         |SI #isom0010#9 ! "     ";
             #isom0010#10 = #isom0010#6 - #isom0010#8;
         |FINSI;
         |GRABA 020#isom0010.a;
         |LIBERA #isom0010;
         |ACABA;
     |FINSI;

     nBase1 = nBase1 + #isom0010#8;

     |SI nBase1 > nBase;
         nBase1 = nBase1 - #isom0010#8;
         #isom0010#8 = nBase - nBase1;
         nBase1 = nBase1 + #isom0010#8;

         #isom0010#10 = 0;
         |SI #isom0010#9 ! "     ";
             #isom0010#10 = #isom0010#6 - #isom0010#8;
         |FINSI;

         |GRABA 020#isom0010.a;
         |LIBERA #isom0010;
     |FINSI;
|FPRC;

|DEFBUCLE isom0010R;
     #isom0010#1;
     ;
     enEmpresa, enAnyo, 0000;
     enEmpresa, enAnyo, 9999;
     be;
     NULL, isom0010R;
|FIN;

|PROCESO RecogeBase2015;
     #isom0001#0 = #maempr10#0;
     #isom0001#1 = #maempr10#1;
     #isom0001#3 = "560";
     |LEE 000#isom0001.=;
     |SI FSalida = 0;
         nBase       = #isom0001#5;
         nCas560 = nBase;
     |FINSI;

     nBase = (nBase % nPorce) ! 2;
|FPRC;

|PROCESO RecogeBase2016;
     #isom0001#0 = #maempr10#0;
     #isom0001#1 = #maempr10#1;
     #isom0001#3 = "560";
     |LEE 000#isom0001.=;
     |SI FSalida = 0;
         nCas560 = #isom0001#5;
     |FINSI;

     #isom0001#0 = #maempr10#0;
     #isom0001#1 = #maempr10#1;
     #isom0001#3 = "593";
     |LEE 000#isom0001.=;
     |SI FSalida = 0;
         nCas593 = #isom0001#5;
     |FINSI;

                        nDiasAnyo = 365;
     |SI enAnyo = 2008; nDiasAnyo = 366;  |FINSI;
     |SI enAnyo = 2012; nDiasAnyo = 366;  |FINSI;
     |SI enAnyo = 2016; nDiasAnyo = 366;  |FINSI;

     fFecha1 = #maempr10#31;
     fFecha2 = #maempr10#32;
     nDias   = (fFecha2 - fFecha1) + 1;

     nBase = ((nCas560 - nCas593 - enVal1510) % nPorce) + (nCas593 + enVal1510);
     |SI nBase [ 1000000;
         nBase = 1000000;
     |FINSI;

     nBase = ((nBase * nDiasAnyo) / nDias) ! 2;

     nVal  = nCas560;
     |SI nBase > nVal;
         nBase = nVal;
     |FINSI;
|FPRC;

|PROCESO RecogeBase;
     nBase = 0;
     |ABRE #isom0001;  |SELECT #2#isom0001;

     |SI #maempr10#1 [ 2015;
         |HAZ RecogeBase2015;
     |FINSI;

     |SI #maempr10#1 ] 2016;
         |HAZ RecogeBase2016;
     |FINSI;

     |SELECT #1#isom0001;
     |CIERRA #isom0001;
|FPRC;

|PROCESO TopeCompensa;
     |ABRE #isom0000;
     #isom0000#0 = enEmpresa;
     #isom0000#1 = enAnyo;
     |LEE 000#isom0000.=;
     |SI FSalida ! 0;  |DDEFECTO #isom0000;  |FINSI;
     |CIERRA #isom0000;

                               nPorce = 100;
     |SI #isom0000#120 = "2";  nPorce = 50;  |FINSI;
     |SI #isom0000#120 = "3";  nPorce = 25;  |FINSI;

     |SI enAnyo = 2016;
                                   nPorce = 60;
         |SI #isom0000#120 = "2";  nPorce = 50;  |FINSI;
         |SI #isom0000#120 = "3";  nPorce = 25;  |FINSI;
     |FINSI;

     |SI enAnyo ] 2017;
                                   nPorce = 70;
         |SI #isom0000#120 = "2";  nPorce = 50;  |FINSI;
         |SI #isom0000#120 = "3";  nPorce = 25;  |FINSI;
     |FINSI;

     |HAZ RecogeBase;

     |SELECT #2#isom0010;
     #isom0010#0 = enEmpresa;
     #isom0010#1 = enAnyo;
     #isom0010#5 = "694";
     |LEE 000#isom0010.=;
     |SI FSalida = 0;
         nBase = (nBase - #isom0010#8) ! 2;
     |FINSI;
     |LIBERA #isom0010;
     |SELECT #1#isom0010;

     |SI nBase < 0;  |O nCas560 [ 0;
         |HAZ RecogeBase;
         |SI nBase < 0;  nBase = 0;  |FINSI;

         nBase1 = 0;  |BUCLE isom0010R;
         |HAZ TotalFich110;

         nBase = 0;
     |FINSI;
|FPRC;

|PROGRAMA;
     |ABRE #maempr10;
     #maempr10#0 = enEmpresa;
     #maempr10#1 = enAnyo;
     |LEE 000#maempr10.=;
     |SI FSalida ! 0;  |DDEFECTO #maempr10;  |FINSI;
     |CIERRA #maempr10;

     |HAZ MontaEstructura507;

     nCas560 = 0;

     |HAZ TopeCompensa;

     #isom0010#0 = enEmpresa;
     #isom0010#1 = enAnyo;
     #isom0010#2 = 5;
     |LEE 000#isom0010.];

     |PINPA #0#isom0010;
     nPanta = FSalida;

     |CONTROL_SIMPLE 0, "BOTON,{{197}} sali&R", 3488, 3498, BotonSalir;
     nBotonSal = FSalida;

     |CONTROL_SIMPLE 0, "BOTON,{{197}} a&Notaciones de la compensaci¢n", 3302, 3425, BotonAnota;
     nBotonAnota = FSalida;

     |SI enModoConsulta = 0;
         nRango = 1 + 2 + 4 + 8 + 16 + 128 + 256;
     |SINO;
         nRango = 4 + 8 + 16 + 128 + 256;
     |FINSI;

     |LINEAL_SIMPLE #1#isom0010, nRango, 0502, 3498, Baja110, Alta110, Evento110;

     |FIN_CONTROL_WINDOWS nBotonSal;
     |FIN_CONTROL_WINDOWS nBotonAnota;

     |BUCLE isom0010;

     |PULSATECLA;
|FPRO;

|PROCESO Tipo80m10;  |TIPO 80;
     |ABRET #isom0010;
     |ABRET #isom0507;

     FSalida = 3499;
|FPRC;

|PROCESO Tipo90m10;  |TIPO 90;
     |CIERRAT #isom0010;
     |CIERRAT #isom0507;
|FPRC;
