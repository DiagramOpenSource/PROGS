|PROCESO AbreSecuencial;
     |SI #0#40 = "I";
         aAlfa      = "/AEAT";                        |RMKDIR aAlfa;
         aAlfa      = "/AEAT/INTERNET";               |RMKDIR aAlfa;
         aAlfa      = "/AEAT/INTERNET/201";           |RMKDIR aAlfa;
         aAlfa      = "/AEAT/INTERNET/201/" + #0#36;  |RMKDIR aAlfa;

         aAbre = aAlfa + "/201" + (("00000" + #99#0) % -105) + ".TXT";
     |FINSI;

     |SI #0#40 = "L";
         aAlfa      = "/MODELOS";                         |RMKDIR aAlfa;
         aAlfa      = "/MODELOS/SOCIEDAD";                |RMKDIR aAlfa;
         aAlfa      = "/MODELOS/SOCIEDAD/201";            |RMKDIR aAlfa;
         aAlfa      = "/MODELOS/SOCIEDAD/201/" + #0#36;   |RMKDIR aAlfa;

         aFichero   = "201" + (("00000" + #99#0) % -105);
         aAbre      = aAlfa + "/" + aFichero;

         aAlfa = "WAIT 20101PDF /E:" + aFichero + " /R:" + aFichero + ".TXT ";
         aAlfa = aAlfa + "/I:" + eaImpresora + " /C:" + eaCopia + " /H:" + eaTipoCola + &13 + &10;
         |XGRABA nHandle1, aAlfa;

         aAlfa = "DEL " + aFichero + &13 + &10;
         |XGRABA nHandle1, aAlfa;
     |FINSI;

     |XABRE "CBW", aAbre, nHandle;
|FPRC;

|PROCESO AbreImprime;
     aAlfa      = "/MODELOS";                         |RMKDIR aAlfa;
     aAlfa      = "/MODELOS/SOCIEDAD";                |RMKDIR aAlfa;
