     nConta = 0;
     nEnContagen = 0;
     |BUCLE laempresa;
     |SI nEnContagen = 1;
         |ABRE #400;
         |DDEFECTO #400;
         #400#0 = #0#0;
         #400#1 = nPerConta;
         |LEE 001#400=;
