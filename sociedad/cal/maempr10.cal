|PROCESO pidedato;
  #0#0 = empresa;
  #0#1 = anyo;
  |LEE 110#0=;
  FEstado = FSalida;
  |PINDA #0#0;
  |SI anyo ] 2008; |HAZ LeeEmpresa; |FINSI;

  |SI modo = 1; |ENDATOS #1#0;  |FINSI;
  |SI modo = 2; |ENDATOS #2#0;  |FINSI;
  |SI FSalida = 0;
      #0#0 = empresa;
      #0#1 = anyo;
      #0#2 = nombre;
      #0#3 = nif;
      |SI FEstado = 0; |GRABA 020#0a; |SINO; |GRABA 020#0n; |FINSI;
      |LIBERA #0;
  |FINSI;
|FPRC;

|PROCESO consulta;
  avance = &255 + avance;
