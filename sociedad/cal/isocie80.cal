|FICHEROS;
     isocie80 #0;
     maempr11 #1;
|FIN;

|VARIABLES;
     nNegocio   = 0;
     nNumero    = 0;
     nOpcion    = 0;

     &empresa   = 0;
     &anyo      = 0;
     &modo      = 0;
     aNifQbf1   = "";
     aNifQbf2   = "";

     &eaVarDni  = "";
     &enCalcCif = 0;
|FIN;

|PROCESO LetraNif;
     |SI FSalida = 9;
         eaVarDni   = #0Campo;
         enCalcCif  = 1;
         |HAZ CalculoDNI;
         #0Campo = eaVarDni;
         |PINTA #0Campo;
         |ERROR;
         |ACABA;
     |FINSI;

     eaVarDni  = #0Campo;
     enCalcCif = 0;
     |HAZ CalculoDNI;

     aNifQbf1 = #0Campo;   |QBF aNifQbf1;
     aNifQbf2 = eaVarDni;  |QBF aNifQbf2;

     |SI aNifQbf1 ! aNifQbf2; |Y aNifQbf1 ! "";
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #0Campo = eaVarDni;  |PINTA #0Campo;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo7;  |TIPO 7;
     #0#4 = nNegocio;
     #0#5 = nNumero;
     |PINTA #0#4;
     |PINTA #0#5;
|FPRC;

|RUTINA inferior;
     #0#0 = empresa;
     #0#1 = anyo;
     #0#2 = 1;
|FRUT;

|RUTINA superior;
     #0#0 = empresa;
     #0#1 = anyo;
     #0#2 = 99;
|FRUT;

|PROGRAMA;
     nOpcion = 1;

     |ABRE #1;
     #1#0 = empresa;
     #1#1 = anyo;
     #1#2 = 21;
     |LEE 040#1=;
     |SI FSalida = 0;  nOpcion = 0;  |FINSI;
     |CIERRA #1;

     |SI nOpcion = 1;
         |MENAV "     La Empresa no tiene activada el Caracter de la declaracion 21";
         |ACABA;
     |FINSI;

     |ABRET #0;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENTLINEAL #1#0, -3, 7, 22, 1, inferior, superior;

     #0#0 = empresa;
     #0#1 = anyo;
     #0#2 = 0;
     |LEE 101#0=;
     |SI FSalida ! 0;
         |DDEFECTO #0;
         #0#0 = empresa;
         #0#1 = anyo;
         #0#2 = 0;
         |GRABA 020#0b;
     |FINSI;

     |PINTA #0#4;
     |PINTA #0#5;

     |C_M #0#4, 1, "S";
     |C_M #0#5, 1, "S";

     |ENCAMPO #4#0;
     |ENCAMPO #5#0;

     nNegocio = #0#4;
     nNumero  = #0#5;

     |C_M #0#4, 1, "N";
     |C_M #0#5, 1, "N";

     |GRABA 020#0a;
     |LIBERA #0;

     |ENTLINEAL #1#0, -3, modo, 22, 1, inferior, superior;

     |CIERRAT #0;
|FPRO;
