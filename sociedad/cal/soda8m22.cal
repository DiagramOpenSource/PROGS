      #1#2 = #11#6; |PINTA #1#2;
      #1#3 = #11#7; |PINTA #1#3;
      #1#6 = #11#2; |PINTA #1#6;
  |FINSI;
|FCAL;

|CALCULO elTipo3; |TIPO 3;
  #1#0 = #10#2;

  esalta  = (FEntrada / 100) ! 0;
  |SI FSalida = 0;
     |SI #1#5 ! "I";
         nBorro = 0;
         |ABRE #2;
         #2#7  = #1#34;
         #2#0  = #1#0; #2#1 = #1#1;
         #2#2  = 1;    #2#3 = 1;
         |LEE 041#2];
         |SI FSalida = 0; |Y #2#0 = #1#0; |Y #2#1 = #1#1; |Y #2#7 = #1#34;
             |CONFI "    SBorro Relacion de Ctas posibles ";
             |SI FSalida = 0;
                 nBorro = 1;
             |FINSI;
         |FINSI;
         |CIERRA #2;
         |SI nBorro = 1;
             nDColum =  1;
             nHColum = 99;
             |BUCLE soda8m03;
         |FINSI;
     |FINSI;
 |FINSI;
|FCAL;

|CALCULO elTipo3Col;
    inkey = FSalida;
    |BLIN 4;
    |SI #1#5 = "S";
        |SI #1Campo !  "T"; |Y #1Campo ! "X";
            #1Campo = "T"; |PINTA #1Campo;
            |ERROR;
            |ACABA;
        |FINSI;
    |FINSI;
    |SI #1Campo = "T";
        |SI #1#5 ! "S"; |ERROR; |ACABA; |FINSI;
    |FINSI;

    nColum = Campo - 6;

    |SI #1Campo = "C";
         |SI inkey ! 2; |Y inkey ! 3;
             nNume1 = Campo + 14;
             |C_P #1nNume1, 0, nNume2;
             |C_P #1Campo, 0, nNume3;
             |ATRI R; |PINTA nNume2, #1nNume1;
             |PINTA nNume3, #1Campo; |ATRI 0;
             nNume2 = Campo;
             nSwSal = 0;
             |ENTLINEAL #1#2, -5, 2, 22, 0, ClaveBajaL, ClaveAltaL;
             |PINTA #1nNume2;
             |PINTA #1nNume1;
         |FINSI;
    |SINO;
         nBorro = 0;
         |ABRE #2;
         #2#7  = #1#34;
         #2#0  = #1#0; #2#1 = #1#1;
         #2#2  = nColum;  #2#3 = 1;
         |LEE 041#2];
         |SI FSalida = 0; |Y #2#0 = #1#0; |Y #2#1 = #1#1; |Y #2#2 = nColum; |Y #2#7  = #1#34;
             |CONFI "    SBorro Relacion de Ctas posibles ";
             |SI FSalida = 0;
                 nBorro = 1;
             |FINSI;
         |FINSI;
         |CIERRA #2;
         |SI nBorro = 1;
             nDColum = nColum;
             nHColum = nColum;
             |BUCLE soda8m03;
         |FINSI;
    |FINSI;
|FCAL;

|CALCULO PresentaLinea;  |TIPO 7;
    nColum = Campo - 6;
    |SI Campo = 1;
        nColum = 1;
    |SINO;
        |SI #1#5 = "S"; aAlfa1 = "    T=Total Columna,X=Columna no Activa"; |FINSI;
        |SI #1#5 = "I"; aAlfa1 = "    C=Cuentas Contables, M=Importe Manual, X=Columna no Activa"; |FINSI;
        |MENSA aAlfa1;
    |FINSI;

    |HAZ LeeColumna;
    |C_A #12#3, 1, 14; |PINTA 0966, #12#3; |C_A #12#3, 1, 80;
    |C_A #12#4, 1, 14; |PINTA 1066, #12#4; |C_A #12#4, 1, 80;
    |HAZ Campo5;

    nSwSal = 1;
    |ENTLINEAL #1#2, -5, 7, 22, 0, ClaveBajaL, ClaveAltaL;

    |ENTLINEAL #1#421, -3, 7, 22, 0, min9, max9;
|FCAL;

