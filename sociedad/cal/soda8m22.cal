|CALCULO TipoLinea;
 esalta = (FEntrada / 100) ! 0;
 aAlfa1 = "S"; aAlfa2 = "M";
 |SI #1#5 = "S"; aAlfa2 = "T"; |FINSI;
 |SI #1#5 = "T"; aAlfa1 = "N"; aAlfa2 = "X"; |FINSI;
 |PARA nNume1 = 7; |SI nNume1 [ 18; |HACIENDO nNume1 = nNume1 + 1;
       |C_M #1nNume1, 1, aAlfa1;
       |SI esalta = 1;
           #1nNume1 = aAlfa2; |PINTA #1nNume1;
       |FINSI;
 |SIGUE;
 |HAZ Campo5;
|FCAL;

|PROCESO Campo5;
 |SI #1#5 = "S";
     |C_M #1#20, 1, "S";
     |C_V #1#20, 1, "S";
     |PINTA 0868, "Sumar:     ";
     |PINTA #1#20;
 |SINO;
     |C_M #1#20, 1, "N"; #1#20 = 0;
     |C_V #1#20, 1, "N"; #1#20 = 0;
     |PINTA 0868, "           ";
 |FINSI;
|FPRC;

