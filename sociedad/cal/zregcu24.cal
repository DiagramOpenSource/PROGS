     nNume  = enEjerCuadro - 1;
     aAlfa1 = enEjerCuadro;
     aAlfa2 = nNume;

     aAlfa1 = "LABEL,{{2}}" + aAlfa1;
     aAlfa2 = "LABEL,{{2}}" + aAlfa2;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0566, 0670, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0589, 0593, NULL;

     |PINDA #0#1024;
     |ENDATOS #1#1024;
     |SI FSalida = 0;
         |GRABA 020#1024a;
     |FINSI;
     |LIBERA #1024;
     |CIERRA #1024;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
