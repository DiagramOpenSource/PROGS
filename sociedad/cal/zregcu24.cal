     #1024#1 = enEjerCuadro;
     |LEE 101#1024=;
     |SI FSalida ! 0;
         |DDEFECTO #1024;
         #1024#0 = enEmpresa;
         #1024#1 = enEjerCuadro;
         |GRABA 020#1024b;
     |FINSI;

     |PINPA #0#1024;
