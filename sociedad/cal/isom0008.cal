|FICHEROS;
     isom0008;
     isom0506;

     maempr10;
|FIN;

|VARIABLES;
      nRango         = 0;
      nBotonSal      = 0;
      nBotonAnota    = -1;
      nTree          = 0;
      nHandle        = 0;
      nPanta         = 0;
      nBotonLabel    = 0;
      nVentana       = 0;
      nVent          = 0;
      nGrid          = 0;
      nModo          = 0;
      nLinea         = 0;

      aContenido     = "";
      aTexto         = "";
      aBase          = "";
      aAlfa          = "";
      aAbre          = "";
      aEsc1          = "";
      aEsc2          = "";
      aRetu          = "";
      aTecla         = "";

     {-1}sTree       = 0;
     sT_nID          = 0;
     sT_nOper        = 0;
     sT_aData        = "";

     {-1}aVent       = "";
         aVent1      = 0;
         aVent2      = 0;
         aVent3      = 0;
         aVent4      = 0;
         aVent5      = "";

     &enEmpresa      = 0;
     &enAnyo         = 0;
     &eaCasilla      = "";
     &eaTitCasilla   = "";
     &eaRutaOrigen   = "";
     &eaRutaDestino  = "";
     &eaFicheroTra   = "";
     &eaFicheroTxt   = "";
     &eaFicheroTgz   = "";
     &eaFicheroTar   = "";
     &eaOrigen       = "";
     &eaDestino      = "";
     &eaAlfa         = "";
     &eaTipoColor    = "";
     &eaColor        = "";
     &enModoConsulta = 0;
|FIN;

|PROCESO Tipo1;  |TIPO 1;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;  |O nModo = 3;
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Tipo2;  |TIPO 2;
     |HAZ Tipo1;
|FPRC;

|PROCESO Tipo3;  |TIPO 3;
     #isom0506#0 = #isom0008#1;
     #isom0506#1 = #isom0008#2;
     |LEE 000#isom0506.=;
     |SI FSalida ! 0;  |DDEFECTO #isom0506;  |FINSI;

     |SI #isom0506#5 ! "I";  |ACABA;  |FINSI;

     |GRABA 020#isom0008.a;
     |LIBERA #isom0008;

     nLinea = #isom0008#2;

     |HAZ TotalFich108;

     #isom0008#0 = #maempr10#0;
     #isom0008#1 = #maempr10#1;
     #isom0008#2 = nLinea;
     |LEE 101#isom0008.=;

     aAlfa = "|IDGRID";
     aAlfa = #isom0008#aAlfa#;
     nGrid = aAlfa;
     |REFRESCACONTROL nGrid;
|FPRC;

|PROCESO Tipo0;  |TIPO 0;
     |C_M #isom0008#5, 1, "N";
     |C_M #isom0008#7, 1, "N";

     #isom0506#0 = #isom0008#1;
     #isom0506#1 = #isom0008#2;
     |LEE 000#isom0506.=;
     |SI FSalida ! 0;  |DDEFECTO #isom0506;  |FINSI;

     |SI #isom0506#5 ! "I";  |ACABA;  |FINSI;

     |SI #isom0008#4 ! "     ";  |C_M #isom0008#5, 1, "S";  |FINSI;
     |SI #isom0008#6 ! "     ";  |C_M #isom0008#7, 1, "S";  |FINSI;
|FPRC;

|PROCESO Evento108;
     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#isom0008.=;
     |FINSI;
|FPRC;

|PROCESO isom0506;
     #isom0008#0  = enEmpresa;
     #isom0008#1  = #isom0506#0;
     #isom0008#2  = #isom0506#1;
     |LEE 101#isom0008.=;
     |SI FSalida ! 0;
         |DDEFECTO #isom0008;
         #isom0008#0  = enEmpresa;
         #isom0008#1  = #isom0506#0;
         #isom0008#2  = #isom0506#1;
         |GRABA 020#isom0008.b;
     |FINSI;

     #isom0008#3  = #isom0506#4;
     #isom0008#4  = #isom0506#2;
     #isom0008#6  = #isom0506#3;

     eaTipoColor = #isom0506#6;
     |HAZ CogeColor;
     #isom0008#8 = eaColor;

     |GRABA 020#isom0008.a;
     |LIBERA #isom0008;
|FPRC;

|DEFBUCLE isom0506;
     #isom0506#1;
     ;
     enAnyo, 1000;
     enAnyo, 9999;     || Del 1 al 999 es el isom0007
     ;
     NULL, isom0506;
|FIN;

|PROCESO MontaEstructura506;
     |BUCLE isom0506;
|FPRC;

|PROCESO isom0008;
     |SI #isom0008#5 ! 0;  |ACABA;  |FINSI;
     |SI #isom0008#7 ! 0;  |ACABA;  |FINSI;

     |BORRA 020#isom0008.a;
     |LIBERA #isom0008;
|FPRC;

|DEFBUCLE isom0008;
     #isom0008#1;
     ;
     enEmpresa, enAnyo, 0000;
     enEmpresa, enAnyo, 9999;
     be;
     NULL, isom0008;
|FIN;

|PROCESO BotonSalir;
     |PTEC 807;
|FPRC;

|PROCESO BotonAnota;
     eaCasilla = #isom0008#4;  |QBF eaCasilla;
     |SI eaCasilla = "";
         eaCasilla = #isom0008#6;  |QBF eaCasilla;
     |FINSI;

     |SI eaCasilla = "";   |ACABA;  |FINSI;

     eaTitCasilla = #isom0008#3;  |QBF eaTitCasilla;

     |SUB_EJECUTA "isom0499";
|FPRC;

|PROCESO Baja108;
     #isom0008#0  = enEmpresa;
     #isom0008#1  = enAnyo;
     #isom0008#2  = 1;
|FPRC;

|PROCESO Alta108;
     #isom0008#0  = enEmpresa;
     #isom0008#1  = enAnyo;
     #isom0008#2  = 9999;
|FPRC;


|PROGRAMA;
     |ABRE #maempr10;
     #maempr10#0 = enEmpresa;
     #maempr10#1 = enAnyo;
     |LEE 000#maempr10.=;
     |SI FSalida ! 0;  |DDEFECTO #maempr10;  |FINSI;
     |CIERRA #maempr10;

     |HAZ MontaEstructura506;

     #isom0008#0 = enEmpresa;
     #isom0008#1 = enAnyo;
     #isom0008#2 = 5;
     |LEE 000#isom0008.];

     |PINPA #0#isom0008;
     nPanta = FSalida;

     |CONTROL_SIMPLE 0, "BOTON,{{197}} sal&Ir", 3488, 3498, BotonSalir;
     nBotonSal = FSalida;

     |CONTROL_SIMPLE 0, "BOTON,{{197}} a&Notaciones del detalle", 3302, 3425, BotonAnota;
     nBotonAnota = FSalida;

     |SI enModoConsulta = 0;
         nRango = 1 + 2 + 4 + 8 + 16 + 128 + 256;
     |SINO;
         nRango = 4 + 8 + 16 + 128 + 256;
     |FINSI;

     |LINEAL_SIMPLE #1#isom0008, nRango, 0502, 3498, Baja108, Alta108, Evento108;

     |FIN_CONTROL_WINDOWS nBotonSal;
     |FIN_CONTROL_WINDOWS nBotonAnota;

     |BUCLE isom0008;

     |PULSATECLA;
|FPRO;

|PROCESO Tipo80m8;  |TIPO 80;
     |ABRET #isom0008;
     |ABRET #isom0506;

     FSalida = 3499;
|FPRC;

|PROCESO Tipo90m8;  |TIPO 90;
     |CIERRAT #isom0008;
     |CIERRAT #isom0506;
|FPRC;
