|FICHEROS;
     maempr05,MANTE;
     maempr02;
|FIN;

|VARIABLES;
     &empresa   = 0;
     &anyo      = 0;
     &modo      = 0;
     &enCalcCif = 0;
     &eaVarDni  = "";
|FIN;

|PROCESO maempr02;
     |DDEFECTO #maempr05;
     #maempr05#0  = #maempr02#0;
     #maempr05#1  = #maempr02#1;
     #maempr05#2  = #maempr02#2;
     #maempr05#3  = #maempr02#3;
     #maempr05#4  = #maempr02#4;
     #maempr05#6  = #maempr02#5;
     #maempr05#7  = #maempr02#6;
     #maempr05#10 = #maempr02#9;
     |GRABA 020#maempr05.n;
     |LIBERA #maempr05;
|FPRC;

|DEFBUCLE maempr02;
     #maempr02#1;
     ;
     empresa, anyo, 01;
     empresa, anyo, 99;
     ;
     NULL, maempr02;
|FIN;

|PROCESO inferior;
     #maempr05#0 = empresa;
     #maempr05#1 = anyo;
     #maempr05#2 = 1;
|FPRC;

|PROCESO superior;
     #maempr05#0 = empresa;
     #maempr05#1 = anyo;
     #maempr05#2 = 99;
|FPRC;

|PROGRAMA;
     |ABRE #maempr05;
     #maempr05#0 = empresa;
     #maempr05#1 = anyo;
     #maempr05#2 = 1;
     |LEE 000#maempr05.];
     |SI FSalida ! 0;  |O  #maempr05#0 ! empresa;   |O #maempr05#1 ! anyo;
         |CONFI "0000 No existen socios en Transparencia Fiscal, quiere recogerlos del fichero de Socios";
         |SI FSalida = 0;
             |BUCLE maempr02;
         |FINSI;
     |FINSI;
     |CIERRA #maempr05;

     |ENTLINEAL #1#maempr05, -3, modo, 22, 1, inferior, superior;
|FPRO;
