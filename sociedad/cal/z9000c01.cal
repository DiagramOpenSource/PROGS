|FICHEROS;
     isom9000;

     isow9000,MANTE;
|FIN;

|VARIABLES;
     n1Vent            = 0;
     nCampo            = 0;

     aAlfa             = "";

     {-1}aVent         = "";
         aVent1        = 0;
         aVent2        = 0;
         aVent3        = 0;
         aVent4        = 0;
         aVent5        = "";

     &enModo           = 0;
     &enFicha          = 0;
     &eaTipo           = "";
|FIN;

|PROCESO z9000c01;
     aAlfa = "Empresa: " + (("00000" + #isom9000#0) % -105) + " " + #isom9000#5;  |QBF aAlfa;
     aAlfa = aAlfa + " Ejercicio: " + #isom9000#1 + " Periodo: " + #isom9000#2 + " Compl.: " + #isom9000#3;

     |VENTANA 0501, 3299, -1, 16, aAlfa;
     n1Vent = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = n1Vent;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PTEC 802;  |PAUSA;

     |PARA nCampo =  0;  |SI nCampo [ 38;  |HACIENDO nCampo = nCampo + 1;
           #isow9000#nCampo# = #isom9000#nCampo#;
     |SIGUE;

     |SI #isom9000#6 = "A ";  #isow9000#6 = "1";  |FINSI;
     |SI #isom9000#6 = "B1";  #isow9000#6 = "2";  |FINSI;
     |SI #isom9000#6 = "B2";  #isow9000#6 = "3";  |FINSI;

     |PINPA #0#isow9000;
     |PINDA #0#isow9000;

     |SI enModo ! 4;
         |SI enModo = 1;
             |C_M #isow9000#6, 1, "S";
         |SINO;
             |C_M #isow9000#6, 1, "N";
         |FINSI;

         |ENDATOS #1#isow9000;
         |SI FSalida = 0;
             |PARA nCampo =  0;  |SI nCampo [ 38;  |HACIENDO nCampo = nCampo + 1;
                   #isom9000#nCampo# = #isow9000#nCampo#;
             |SIGUE;

             |SI #isow9000#6 = "1";  #isom9000#6 = "A";   |FINSI;
             |SI #isow9000#6 = "2";  #isom9000#6 = "B1";  |FINSI;
             |SI #isow9000#6 = "3";  #isom9000#6 = "B2";  |FINSI;

             |GRABA 020#isom9000.a;

             |SI enModo = 1;
                 eaTipo = #isow9000#6;
                 |HAZ GrabaFicha;
             |FINSI;
         |SINO;
             |SI enModo = 1;
                 |BORRA 020#isom9000.a;
             |FINSI;
         |FINSI;

         |LIBERA #isom9000;
     |SINO;
         |PAUSA;
     |FINSI;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = n1Vent;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |FINVENTANA n1Vent;
|FPRC;
