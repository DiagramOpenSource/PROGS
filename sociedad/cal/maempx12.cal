|FICHEROS;
     maempr12;
     maempr13;
     maempr10;

     maempw02,MANTE;
|FIN;

|VARIABLES;
     n0Vent         = 0;
     n1Vent         = 0;
     nPan           = 0;
     nFSalida       = 0;
     nRango         = 0;
     nGrid12        = 0;
     nGrid13        = 0;

     nBtnAlt12      = 0;
     nBtnMod12      = 0;
     nBtnBor12       = 0;
     nBtnAlt13      = 0;
     nBtnMod13      = 0;
     nBtnBor13       = 0;

     aAlfa          = "";
     aEsc1          = "";
     aEsc2          = "";
     aRetu          = "";
     aTecla         = "";

     {-1}aVent      = "";
         aVent1     = 0;
         aVent2     = 0;
         aVent3     = 0;
         aVent4     = 0;
         aVent5     = "";

     &enEmpresa = 0;
     &enAnyo    = 0;
     &enModoConsulta = 0;
     &enModoX        = 1;
|FIN;

|PROCESO Tipo12;  |TIPO 12;
|FPRC;

|PROCESO Baja13;
     #maempr13#0  = enEmpresa;
     #maempr13#1  = enAnyo;
     #maempr13#2  = #maempr12#2;
     #maempr13#3  = 1;
|FPRC;

|PROCESO Alta13;
     #maempr13#0  = enEmpresa;
     #maempr13#1  = enAnyo;
     #maempr13#2  = #maempr12#2;
     #maempr13#3  = 99;
|FPRC;

|PROCESO Baja12;
     #maempr12#0  = enEmpresa;
     #maempr12#1  = enAnyo;
     #maempr12#2  = 1;
|FPRC;

|PROCESO Alta12;
     #maempr12#0  = enEmpresa;
     #maempr12#1  = enAnyo;
     #maempr12#2  = 99;
|FPRC;

|PROCESO Evnt12;
     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#maempr12.=;

         |ESTADO_CONTROL nBtnMod12, "ENABLE";
         |ESTADO_CONTROL nBtnBor12, "ENABLE";
         |ESTADO_CONTROL nBtnAlt13, "ENABLE";
         |ESTADO_CONTROL nBtnMod13, "DISABLE";
         |ESTADO_CONTROL nBtnBor13, "DISABLE";

         |REFRESCACONTROL nGrid13;
     |FINSI;

     |SI FSalida = 4;
         |HAZ Modificar12;
     |FINSI;
|FPRC;

|PROCESO Evnt13;
     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#maempr13.=;

         |ESTADO_CONTROL nBtnMod13, "ENABLE";
         |ESTADO_CONTROL nBtnBor13, "ENABLE";
     |FINSI;

     |SI FSalida = 4;
         |HAZ Modificar13;
     |FINSI;
|FPRC;

|PROCESO PideDatos;
     |VENTANA 0501, 1199, -1, 16, "Entrada de datos";
     n1Vent = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = n1Vent;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PINPA #0#maempw02;
     |PINDA #0#maempw02;
     |ENDATOS #1#maempw02;
     nFSalida = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = n1Vent;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;
     |FINVENTANA n1Vent;

     |PULSATECLA;
|FPRC;

|PROCESO Nuevo12;
     |DDEFECTO #maempw02;

     #maempr12#0 = enEmpresa;
     #maempr12#1 = enAnyo;
     #maempr12#2 = 99;
     |LEE 000#maempr12.];
     |SI FSalida = 0;
         |LEE 000#maempr12.a;
     |SINO;
         |LEE 000#maempr12.u;
     |FINSI;

     |SI FSalida = 0;  |Y  #maempr12#0 = enEmpresa;  |Y  #maempr12#1 = enAnyo;
          #maempw02#0 = #maempr12#2 + 1;
     |FINSI;

     |C_M #maempw02#4, 1, "N";
     |HAZ PideDatos;
     |SI nFSalida = 0;
         #maempr12#0 = enEmpresa;
         #maempr12#1 = enAnyo;
         #maempr12#2 = #maempw02#0;
         #maempr12#3 = #maempw02#1;
         #maempr12#4 = #maempw02#2;
         #maempr12#5 = #maempw02#3;
         #maempr12#6 = #maempw02#4;

         |GRABA 020#maempr12.n;
         |LIBERA #maempr12;

         FSalida = "SKIPDEFAULT";
         |REFRESCACONTROL nGrid12;  |PULSATECLA;
         |FOCOTECLADO nGrid12;
         |PULSATECLA;
     |FINSI;
|FPRC;

|PROCESO Modificar12;
     |LEE 101#maempr12.=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     #maempw02#0 = #maempr12#2;
     #maempw02#1 = #maempr12#3;
     #maempw02#2 = #maempr12#4;
     #maempw02#3 = #maempr12#5;
     #maempw02#4 = #maempr12#6;

     |C_M #maempw02#4, 1, "N";
     |HAZ PideDatos;
     |SI nFSalida = 0;
         #maempr12#2 = #maempw02#0;
         #maempr12#3 = #maempw02#1;
         #maempr12#4 = #maempw02#2;
         #maempr12#5 = #maempw02#3;
         #maempr12#6 = #maempw02#4;

         |GRABA 020#maempr12.a;
         |LIBERA #maempr12;

         FSalida = "SKIPDEFAULT";
         |REFRESCACONTROL nGrid12;  |PULSATECLA;
         |FOCOTECLADO nGrid12;
         |PULSATECLA;
     |FINSI;
|FPRC;

|PROCESO maempr13B;
     |BORRA 020#maempr13.a;
     |LIBERA #maempr13;
|FPRC;

|DEFBUCLE maempr13B;
     #maempr13#1;
     ;
     #maempr12#0, #maempr12#1, #maempr12#2, 00;
     #maempr12#0, #maempr12#1, #maempr12#2, 99;
     be;
     NULL, maempr13B;
|FIN;

|PROCESO Borrar12;
     |CONFI "0000NSeguro que quiere BORRAR el registro";
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |LEE 101#maempr12.=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |BUCLE maempr13B;

     |BORRA 020#maempr12.a;
     |LIBERA #maempr12;

     |ESTADO_CONTROL nBtnMod12,   "DISABLE";
     |ESTADO_CONTROL nBtnBor12,   "DISABLE";

     FSalida = "SKIPDEFAULT";
     |REFRESCACONTROL nGrid12;  |PULSATECLA;
     |FOCOTECLADO nGrid12;
     |PULSATECLA;
|FPRC;

|PROCESO Nuevo13;
     |DDEFECTO #maempw02;

     #maempr13#0 = enEmpresa;
     #maempr13#1 = enAnyo;
     #maempr13#2 = #maempr12#2;
     #maempr13#3 = 99;
     |LEE 000#maempr13.];
     |SI FSalida = 0;
         |LEE 000#maempr13.a;
     |SINO;
         |LEE 000#maempr13.u;
     |FINSI;

     |SI FSalida = 0;  |Y  #maempr13#0 = enEmpresa;  |Y  #maempr13#1 = enAnyo;
                       |Y  #maempr13#2 = #maempr12#2;
          #maempw02#0 = #maempr13#3 + 1;
     |FINSI;

     |C_M #maempw02#4, 1, "S";
     |HAZ PideDatos;
     |SI nFSalida = 0;
         #maempr13#0 = enEmpresa;
         #maempr13#1 = enAnyo;
         #maempr13#2 = #maempr12#2;
         #maempr13#3 = #maempw02#0;
         #maempr13#4 = #maempw02#1;
         #maempr13#5 = #maempw02#2;
         #maempr13#6 = #maempw02#3;
         #maempr13#7 = #maempw02#4;

         |GRABA 020#maempr13.n;
         |LIBERA #maempr13;

         |LEE 101#maempr12.=;
         |SI FSalida = 0;
             #maempr12#6 = #maempr12#6 + #maempr13#7;
             |GRABA 020#maempr12.a;
             |LIBERA #maempr12;

             |PINTA #maempr12#6;
         |FINSI;

         FSalida = "SKIPDEFAULT";
         |REFRESCACONTROL nGrid13;  |PULSATECLA;
         |FOCOTECLADO nGrid13;
         |PULSATECLA;
     |FINSI;
|FPRC;

|PROCESO Modificar13;
     |LEE 101#maempr13.=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     #maempw02#0 = #maempr13#3;
     #maempw02#1 = #maempr13#4;
     #maempw02#2 = #maempr13#5;
     #maempw02#3 = #maempr13#6;
     #maempw02#4 = #maempr13#7;

     |C_M #maempw02#4, 1, "S";
     |HAZ PideDatos;
     |SI nFSalida = 0;
         #maempr13#3 = #maempw02#0;
         #maempr13#4 = #maempw02#1;
         #maempr13#5 = #maempw02#2;
         #maempr13#6 = #maempw02#3;
         #maempr13#7 = #maempw02#4;

         |GRABA 020#maempr13.a;
         |LIBERA #maempr13;

         |LEE 101#maempr12.=;
         |SI FSalida = 0;
             #maempr12#6 = #maempr12#6 + #maempr13#7;
             |GRABA 020#maempr12.a;
             |LIBERA #maempr12;

             |PINTA #maempr12#6;
         |FINSI;

         FSalida = "SKIPDEFAULT";
         |REFRESCACONTROL nGrid13;  |PULSATECLA;
         |FOCOTECLADO nGrid13;
         |PULSATECLA;
     |FINSI;
|FPRC;

|PROCESO Borrar13;
     |CONFI "0000NSeguro que quiere BORRAR el registro";
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |LEE 101#maempr13.=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |BORRA 020#maempr13.a;
     |LIBERA #maempr13;

     |LEE 101#maempr12.=;
     |SI FSalida = 0;
         #maempr12#6 = #maempr12#6 - #maempr13#7;
         |GRABA 020#maempr12.a;
         |LIBERA #maempr12;

         |PINTA #maempr12#6;
     |FINSI;

     |ESTADO_CONTROL nBtnMod13,   "DISABLE";
     |ESTADO_CONTROL nBtnBor13,   "DISABLE";

     FSalida = "SKIPDEFAULT";
     |REFRESCACONTROL nGrid13;  |PULSATECLA;
     |FOCOTECLADO nGrid13;
     |PULSATECLA;
|FPRC;

|PROGRAMA;
     aAlfa = "";
     |SI enModoConsulta = 1;
         aAlfa = "(MODO CONSULTA)   ";
     |FINSI;

     aAlfa = aAlfa + "R‚gimen especial de transparencia fiscal internacional";
     |VENTANA 0501, 3399, -1, 16, aAlfa;
     n0Vent = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = n0Vent;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |CONTROL_SIMPLE 0, "LABEL,{{15}}Administradores", 1902, 1980, NULL;

     nRango = 4 + 8 + 16 + 32 + 128;

     |SI enModoConsulta = 0;
         |CONTROL_SIMPLE nPan, "BOTON,{{193}}&Alta ", 1864, 1874, Nuevo12;
         nBtnAlt12  = FSalida;

         |CONTROL_SIMPLE nPan, "BOTON,{{197}}&Modificar ", 1876, 1886, Modificar12;
         nBtnMod12  = FSalida;

         |CONTROL_SIMPLE nPan, "BOTON,{{199}}&Borrar", 1888, 1898, Borrar12;
         nBtnBor12  = FSalida;

         |CONTROL_SIMPLE nPan, "BOTON,{{193}}a&Lta ", 3164, 3174, Nuevo13;
         nBtnAlt13  = FSalida;

         |CONTROL_SIMPLE nPan, "BOTON,{{197}}mo&Dificar ", 3176, 3186, Modificar13;
         nBtnMod13  = FSalida;

         |CONTROL_SIMPLE nPan, "BOTON,{{199}}bo&Rrar", 3188, 3198, Borrar13;
         nBtnBor13  = FSalida;

         |ESTADO_CONTROL nBtnMod12, "DISABLE";
         |ESTADO_CONTROL nBtnBor12, "DISABLE";

         |ESTADO_CONTROL nBtnAlt13, "DISABLE";
         |ESTADO_CONTROL nBtnMod13, "DISABLE";
         |ESTADO_CONTROL nBtnBor13, "DISABLE";
     |FINSI;

     |LINEAL_SIMPLE #1#maempr12, nRango, 0502, 1798, Baja12, Alta12, Evnt12;
     nGrid12 = FSalida;

     |LINEAL_SIMPLE #1#maempr13, nRango, 2002, 3098, Baja13, Alta13, Evnt13;
     nGrid13 = FSalida;

     |PULSATECLA;
     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
         FSalida = "::{{001}}Salir," + nGrid12;
         |LEETECLA aTecla;
         |SI aTecla = aEsc1; |SAL;  |FINSI;
         |SI aTecla = aEsc2; |SAL;  |FINSI;
         |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;

     |DESTRUYECONTROL nGrid12;
     |DESTRUYECONTROL nGrid13;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = n0Vent;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA n0Vent;
|FPRO;

|PROCESO Tipo80;  |TIPO 80;
     aAlfa  = PARAMETRO % 104;  enAnyo    = aAlfa;
     aAlfa  = PARAMETRO % 505;  enEmpresa = aAlfa;

     |ABRE #maempr10;
     #maempr10#0 = enEmpresa;
     #maempr10#1 = enAnyo;
     |LEE 000#maempr10.=;
     |SI FSalida ! 0;  |DDEFECTO #maempr10;  |FINSI;
     |CIERRA #maempr10;

     |ABRET #maempr12;
     |ABRET #maempr13;
|FPRC;

|PROCESO Tipo90;  |TIPO 90;
     |CIERRAT #maempr12;
     |CIERRAT #maempr13;
|FPRC;
