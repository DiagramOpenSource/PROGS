|FICHEROS;
   isocie04 #0;
|FIN;

|VARIABLES;
   &empresa   = 0;
   &anyo      = 0;
   &modo      = 0;
   &modelito  = 0;
   &numerica  = 0;
   &tecla     = 0;
|FIN;

|RUTINA inferior;
  #0#0 = empresa;
  #0#1 = anyo;
  #0#2 = 1;
|FRUT;

|RUTINA superior;
  #0#0 = empresa;
  #0#1 = anyo;
  #0#2 = 99;
|FRUT;

|PROGRAMA;
|SI tecla ! 9; |HAZ numerica; |ACABA; |FINSI;

|PINPA #0#0;
|SI modo = 1;
    |ENTLINEAL #1#0,3,1,17,0,inferior,superior;
|FINSI;
|SI modo = 2;
    |ENTLINEAL #1#0,3,2,17,0,inferior,superior;
|FINSI;
|SI modo = 4;
    |ENTLINEAL #1#0,3,4,17,0,inferior,superior;
|FINSI;
|HAZ numerica;
|FPRO;

|PROCESO calculo;
#0#7 = #0#5 - #0#6;
|PINTA #0#7;
|FPRC;

|PROCESO sumar;
numerica = numerica + #0#9;
|FPRC;

|DEFBUCLE lineas;
#0#1;
;
empresa, anyo, 01;
empresa, anyo, 99;
e;
NULL, sumar;
|FIN;

|PROCESO numerica;
numerica = 0;
|BUCLE lineas;
|FPRC;
