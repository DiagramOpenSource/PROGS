|FICHEROS;
   meimpact #0;
   meficact #1;
   meempmem #2;
|FIN;

|VARIABLES;
     &acabalo  = 0;
     &eImpresora = "";
     x = 0;
|FIN;

|PROCESO imprimelo;
    #2#0 = #1#0;
    |LEE 040#2=;
    |SI FSalida ! 0; |DDEFECTO #2; |FINSI;

    |SI #1#2 = 1; |Y #1#3 = "G"; |INFOR "modelo1g"; |FINSI;
    |SI #1#2 = 1; |Y #1#3 = "P"; |INFOR "modelo1p"; |FINSI;
    |SI #1#2 = 1; |Y #1#3 = "0"; |INFOR "modelo10"; |FINSI;
    |SI #1#2 = 2; |Y #1#3 = "G"; |INFOR "modelo2g"; |FINSI;
    |SI #1#2 = 2; |Y #1#3 = "P"; |INFOR "modelo2p"; |FINSI;
    |SI #1#2 = 2; |Y #1#3 = "0"; |INFOR "modelo20"; |FINSI;
    |SI #1#2 = 3; |Y #1#3 = "G"; |INFOR "modelo3g"; |FINSI;
    |SI #1#2 = 3; |Y #1#3 = "P"; |INFOR "modelo3p"; |FINSI;
    |SI #1#2 = 3; |Y #1#3 = "0"; |INFOR "modelo30"; |FINSI;
    |SI #1#2 = 4; |Y #1#3 = "G"; |INFOR "modelo4g"; |FINSI;
    |SI #1#2 = 4; |Y #1#3 = "P"; |INFOR "modelo4p"; |FINSI;
    |SI #1#2 = 4; |Y #1#3 = "0"; |INFOR "modelo40"; |FINSI;
    |SI #1#2 = 5; |Y #1#3 = "G"; |INFOR "modelo5g"; |FINSI;
    |SI #1#2 = 5; |Y #1#3 = "P"; |INFOR "modelo5p"; |FINSI;
    |SI #1#2 = 5; |Y #1#3 = "0"; |INFOR "modelo50"; |FINSI;
    |SI #1#2 = 6; |Y #1#3 = "G"; |INFOR "modelo6g"; |FINSI;
    |SI #1#2 = 6; |Y #1#3 = "P"; |INFOR "modelo6p"; |FINSI;
    |SI #1#2 = 6; |Y #1#3 = "0"; |INFOR "modelo60"; |FINSI;

    |SI FSalida = 0; |PRINT; |PIEINF; |FININF; |FINSI;
|FPRC;

|PROCESO impre; |TIPO 3;
|SI acabalo = 1;
    Impresora = eImpresora;
    |IMPRE -1;
|SINO;
    |IMPRE 0;
|FINSI;
|SI FSalida = 0;
    |ABRE #1;
    |PARA x = 0; |SI x [ 9; |HACIENDO x = x + 1;
       |SI #0x ! 0;
           #1#0 = #0x;
           #1#1 = #0#10;
           |LEE 040#1=;
           |SI FSalida = 0;
               |ABRE #2; |HAZ imprimelo; |CIERRA #2;
           |FINSI;
       |FINSI;
    |SIGUE;
    |CIERRA #1;
    |FINIMP;
|FINSI;
|FPRC;
