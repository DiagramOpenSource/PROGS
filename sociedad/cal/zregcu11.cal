|FICHEROS;
     zregcu11 #1011;
|FIN;

|VARIABLES;
     nVentana         = 0;
     nSubPanta        = 0;
     nNume            = 0;

     aAlfa1           = "";
     aAlfa2           = "";

     {-1}aVent        = "";
         aVent1       = 0;
         aVent2       = 0;
         aVent3       = 0;
         aVent4       = 0;
         aVent5       = "";

     &enEmpresa       = 0;
     &enEjerCuadro    = 0;
|FIN;

|PROCESO Tipo12Cuadros;  |TIPO 12;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 1999, -1, 16, "Sueldos, dietas y remuneraciones";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #1011;
     #1011#0 = enEmpresa;
     #1011#1 = enEjerCuadro;
     |LEE 101#1011=;
     |SI FSalida ! 0;
         |DDEFECTO #1011;
         #1011#0 = enEmpresa;
         #1011#1 = enEjerCuadro;
         |GRABA 020#1011b;
     |FINSI;

     |PINPA #0#1011;
     nSubPanta = FSalida;

     nNume  = enEjerCuadro - 1;
     aAlfa1 = enEjerCuadro;
     aAlfa2 = nNume;

     aAlfa1 = "LABEL,{{2}}" + aAlfa1;
     aAlfa2 = "LABEL,{{2}}" + aAlfa2;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0645, 0649, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0661, 0665, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0676, 0680, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0691, 0695, NULL;

     |PINDA #0#1011;
     |ENDATOS #1#1011;
     |SI FSalida = 0;
         |GRABA 020#1011a;
     |FINSI;
     |LIBERA #1011;
     |CIERRA #1011;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
