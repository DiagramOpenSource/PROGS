|PROCESO Totalcu15;
     #1015#4  = #1015#02 + #1015#03;
     #1015#6  = (#1015#02 * 100) / #1015#4;
     #1015#7  = 0;
     |SI #1015#3 ! 0;
         #1015#7  = 100 - #1015#6;
     |FINSI;
     #1015#8  = #1015#6 + #1015#7;

     #1015#11 = #1015#09 + #1015#10;
     #1015#13 = (#1015#09 * 100) / #1015#11;
     #1015#14 = 0;
     |SI #1015#10 ! 0;
         #1015#14 = 100 - #1015#13;
     |FINSI;
     #1015#15 = #1015#13 + #1015#14;

     |PINTA #1015#4;
     |PINTA #1015#6;
     |PINTA #1015#7;
     |PINTA #1015#8;
     |PINTA #1015#11;
     |PINTA #1015#13;
     |PINTA #1015#14;
     |PINTA #1015#15;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 1599, -1, 16, "Informaci¢n sobre aplazamientos de pago efectuados a proveedores";
