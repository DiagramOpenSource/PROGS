     pepe =  #3#0;
     variable = "                    " + #10#1; |QBF variable;
     |SI variable ! "";
         sw_print = 1;  |PRINT;
         sw_print = 0;
     |FINSI;
  |FINSI;

  alfa1 = #3#0;            || Recoge los dos primeros digitos de la clave;
