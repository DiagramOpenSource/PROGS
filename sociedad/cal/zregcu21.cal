|FICHEROS;
     zregcu21 #1021;
|FIN;

|VARIABLES;
     nVentana         = 0;
     nSubPanta        = 0;
     nNume            = 0;

     aAlfa1           = "";
     aAlfa2           = "";

     {-1}aVent        = "";
         aVent1       = 0;
         aVent2       = 0;
         aVent3       = 0;
         aVent4       = 0;
         aVent5       = "";

     &enEmpresa       = 0;
     &enEjerCuadro    = 0;
|FIN;

|PROCESO Tipo12Cuadros;  |TIPO 12;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 1999, -1, 16, "Detalle de subvenciones, donaciones y legados";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #1021;
     #1021#0 = enEmpresa;
     #1021#1 = enEjerCuadro;
     |LEE 101#1021=;
     |SI FSalida ! 0;
         |DDEFECTO #1021;
         #1021#0 = enEmpresa;
         #1021#1 = enEjerCuadro;
         |GRABA 020#1021b;
     |FINSI;

     |PINPA #0#1021;
     nSubPanta = FSalida;

     nNume  = enEjerCuadro - 1;
     aAlfa1 = enEjerCuadro;
     aAlfa2 = nNume;

     aAlfa1 = "LABEL,{{2}}" + aAlfa1;
     aAlfa2 = "LABEL,{{2}}" + aAlfa2;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0575, 0779, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0591, 0995, NULL;

     |PINDA #0#1021;
     |ENDATOS #1#1021;
     |SI FSalida = 0;
         |GRABA 020#1021a;
     |FINSI;
     |LIBERA #1021;
     |CIERRA #1021;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
