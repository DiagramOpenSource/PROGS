|FICHEROS;
     zregcu12 #1012;
|FIN;

|VARIABLES;
     nVentana         = 0;
     nSubPanta        = 0;
     nNume            = 0;

     aAlfa1           = "";
     aAlfa2           = "";

     {-1}aVent        = "";
         aVent1       = 0;
         aVent2       = 0;
         aVent3       = 0;
         aVent4       = 0;
         aVent5       = "";

     &enEmpresa       = 0;
     &enEjerCuadro    = 0;
|FIN;

|PROCESO Tipo12Cuadros;  |TIPO 12;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 3099, -1, 30, "Actuaciones medioambientales";
     nVentana = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |ABRE #1012;
     #1012#0 = enEmpresa;
     #1012#1 = enEjerCuadro;
     |LEE 101#1012=;
     |SI FSalida ! 0;
         |DDEFECTO #1012;
         #1012#0 = enEmpresa;
         #1012#1 = enEjerCuadro;
         |GRABA 020#1012b;
     |FINSI;

     |PINPA #0#1012;
     nSubPanta = FSalida;

     nNume  = enEjerCuadro - 1;
     aAlfa1 = enEjerCuadro;
     aAlfa2 = nNume;

     aAlfa1 = "LABEL,{{2}}" + aAlfa1;
     aAlfa2 = "LABEL,{{2}}" + aAlfa2;

     |CONTROL_SIMPLE nSubPanta, aAlfa1, 0575, 0779, NULL;
     |CONTROL_SIMPLE nSubPanta, aAlfa2, 0591, 0995, NULL;

     |PINDA #0#1012;
     |ENDATOS #1#1012;
     |SI FSalida = 0;
         |GRABA 020#1012a;
     |FINSI;
     |LIBERA #1012;
     |CIERRA #1012;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVentana;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PULSATECLA;

     |FINVENTANA nVentana;
|FPRO;
