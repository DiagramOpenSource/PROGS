|FICHEROS;
     dspase18 #18;
|FIN;

|VARIABLES;
     nCampo = 0;
|FIN;

|PROCESO Renumera;
     |C_M #18#1, 1, "S";
     |C_M #18#2, 1, "S";
     |C_M #18#3, 1, "S";
     |C_M #18#4, 1, "S";
     |C_M #18#5, 1, "S";
     |C_M #18#6, 1, "S";
     |C_M #18#7, 1, "S";

     |SI #18#0 = "S";
         |C_M #18#1, 1, "N";  #18#1 = 0;  |PINTA #18#1;
         |C_M #18#2, 1, "N";  #18#2 = 0;  |PINTA #18#2;
         |C_M #18#3, 1, "N";  #18#3 = 0;  |PINTA #18#3;
         |C_M #18#4, 1, "N";  #18#4 = 0;  |PINTA #18#4;
         |C_M #18#5, 1, "N";  #18#5 = 0;  |PINTA #18#5;
         |C_M #18#6, 1, "N";  #18#6 = 0;  |PINTA #18#6;
         |C_M #18#7, 1, "N";  #18#7 = 0;  |PINTA #18#7;
     |FINSI;
|FPRC;

|PROCESO MiraNumero;
     |SI #18#0 = "S";  |ACABA;  |FINSI;

     |SI #18Campo = 0;
         |MENAV "      Introduzca un numero de Orden";
         |ERROR;
         |ACABA;
     |FINSI;

     |PARA nCampo = 1;  |SI nCampo [ 7;  |HACIENDO nCampo = nCampo + 1;
           |SI nCampo ! Campo;
               |SI #18nCampo ! 0;
                   |SI #18Campo = #18nCampo;
                       |MENAV "     Orden ya introducida.";
                       |ERROR;
                       |SAL;
                   |FINSI;
               |FINSI;
           |FINSI;
     |SIGUE;
|FPRC;
