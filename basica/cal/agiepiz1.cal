|PROGRAMA;
     |HAZ Version;

     |SI eaEpigrafe ! "     ";
         |SI eaTipoEpi = "E";   #0#0 = "X";   |FINSI;
         |SI eaTipoEpi = "P";   #0#1 = "X";   |FINSI;
         |SI eaTipoEpi = "A";   #0#2 = "X";   |FINSI;
         |SI eaTipoEpi = "O";
             #0#3 = "X";
             #0#4 = eaNombEpi;
             |C_V #0#4, 1, "S";
         |FINSI;

         |CLS;
         |CABEZA "Seleccionar Epigrafes";
         |PINPA #0#0;
         |PINDA #0#0;
         |HAZ Entrada;
         |ENDATOS #1#0;
     |SINO;
         |CLS;
         |CABEZA "Gestion de Epigrafes";
         |PINPA #0#0;
         |PINDA #0#0;
         |ENDATOS #1#0;
     |FINSI;

     |SI eaTipoEpi = "O";
         eaEpigrafe = 9999;
         eaNombEpi  = #0#4;
         enAgrupa   = 0;
     |FINSI;

