|PROCESO LeePorNif;
     |C_M #agim0058#Campo#, 0, aAlfa;
     |SI aAlfa = "N"; |ACABA; |FINSI;

     aAlfa1 = Contenido;
     Modo1  = (FEntrada / 100) ! 0;
     aAlfa  = #agim0058#6; |QBF aAlfa;

     |SI Modo1 = 1; |O aAlfa = ""; |O aAlfa1 ! #agim0058#5;
         |ABRE #709;
         #709#0 = #agim0058#5;
         |LEE 040#709=;
         |SI FSalida ! 0; |DDEFECTO #709; |FINSI;
         |CIERRA #709;

         #agim0058#12 = "01.01.0000";
         #agim0058#6  = #709#1;    |PINTA #agim0058#6;

         #agim0058#10 = #709#12;   |PINTA #agim0058#10;
         #agim0058#11 = #709#13;   |PINTA #agim0058#11;
     |FINSI;

     |HAZ Tipo0FecNac;
|FPRC;

|PROCESO Tipo0FecNac;
     |C_M #agim0058#12, 0, aAlfa;
     |SI aAlfa = "N"; |ACABA; |FINSI;

     |SI #agim0058#12 [ "01.01.0001";
         |ABRE #agidanif;
         #agidanif#0  = #agim0058#5;
         #agidanif#1  = 1;
         |LEE 040#agidanif.=;
         |SI FSalida = 0; |Y #agidanif#13 > "01.01.0000";
             #agim0058#12 = #agidanif#13; |PINTA #agim0058#12;
         |FINSI;
         |CIERRA #agidanif;
     |FINSI;

     fHoy = ~;
     |SI #agim0058#12 > fHoy;    || nacimientos futuros como que no
          |HAZ PonSiglo20;       ||/se cambia sin preguntar
     |FINSI;

     |SI #agim0058#12 ] "01.01.2001"; |Y #agim0058#12 [ fHoy;  || presuntos ni¤os,
                                                                       ||/se pregunta
         |HAZ CumpleAnyos;
         |SI nEdad < 18;
             aAlfa = "0000SLa fecha puede ser incorrecta. Ha introducido un contribuyente menor de edad. Corregir fecha?";
             |CONFI aAlfa;
             |SI FSalida = 0;
                 |ERROR;
                 || |HAZ PonSiglo20;
             |FINSI;
         |FINSI;
     |FINSI;

|FPRC;

|PROCESO CumpleAnyos;
     aAlfa1 = fHoy;         aAlfa1 = aAlfa1 % -104; nAny1 = aAlfa1;
     aAlfa1 = #agim0058#12; aAlfa1 = aAlfa1 % -104; nAny2 = aAlfa1;
     nEdad  = nAny1 - nAny2;

     |SI nEdad ! 18; |ACABA; |FINSI;

     aAlfa1 = fHoy;         aAlfa1 = aAlfa1 % 402; nMes1 = aAlfa1;
     aAlfa1 = #agim0058#12; aAlfa1 = aAlfa1 % 402; nMes2 = aAlfa1;

     |SI nMes2 > nMes1;  nEdad = nEdad - 1; |FINSI;
     |SI nMes1 ! nMes2;  |ACABA;            |FINSI;

     aAlfa1 = fHoy;         aAlfa1 = aAlfa1 % 102; nDia1 = aAlfa1;
     aAlfa1 = #agim0058#12; aAlfa1 = aAlfa1 % 102; nDia2 = aAlfa1;

     |SI nDia2 > nDia1;  nEdad = nEdad - 1; |FINSI;
|FPRC;

|PROCESO PonSiglo20;
     aAlfa1    = (#agim0058#12 % 106) + "19" + (#agim0058#12 % -102);
     #agim0058#12 = aAlfa1;
     |PINTA #agim0058#12;
|FPRC;

