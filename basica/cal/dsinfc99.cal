|VARIABLES;
   aAlfa1      = "";
   aAlfa2      = "";
   aAlfa3      = "";
   aAlfa4      = "";
   nNume1      = 0;
   nNume2      = 0;
   &nSwMal     = 0;
   &eaFecha    = "";
   &NumMes     = 0;
   &NombreMes  = "";
   &eaCuenta   = "";
   &salidas    = 0;

   aGuarda     = "";
   aPunto      = "";
   nPos        = 0;
|FIN;

|PROCESO VerLaFecha;
  nSwMal = 0;
  aAlfa1 = eaFecha;
  |SI aAlfa1 = "";
      nSwMal = 1;
  |SINO;
      aAlfa2 = aAlfa1 - ".";
      |SI FSalida = 0;
          nSwMal = 1;
      |SINO;
          aAlfa2 = aAlfa1 % 0102;
          aAlfa3 = aAlfa1 % 0402;
          nNume1 = aAlfa2;
          nNume2 = aAlfa3;
          |SI nNume1 < 1; |O nNume1 > 31; nSwMal = 1; |FINSI;
          |SI nNume2 < 1; |O nNume2 > 12; nSwMal = 1; |FINSI;
          |SI nSwMal = 0;
              |SI nNume2 = 2; |Y nNume1 > 29; nSwMal = 1; |FINSI;
              |SI nNume2 = 4; |O nNume2 = 6; |O nNume2 = 9; |O nNume2 = 11;
                  |SI nNume1 > 30; nSwMal = 1; |FINSI;
              |FINSI;
          |FINSI;
      |FINSI;
  |FINSI;
|FPRC;

|PROCESO NombreMes;
  |SI NumMes = 0;  NombreMes = "Apertura  "; |FINSI;
  |SI NumMes = 1;  NombreMes = "Enero     "; |FINSI;
  |SI NumMes = 2;  NombreMes = "Febrero   "; |FINSI;
  |SI NumMes = 3;  NombreMes = "Marzo     "; |FINSI;
  |SI NumMes = 4;  NombreMes = "Abril     "; |FINSI;
  |SI NumMes = 5;  NombreMes = "Mayo      "; |FINSI;
  |SI NumMes = 6;  NombreMes = "Junio     "; |FINSI;
  |SI NumMes = 7;  NombreMes = "Julio     "; |FINSI;
  |SI NumMes = 8;  NombreMes = "Agosto    "; |FINSI;
  |SI NumMes = 9;  NombreMes = "Septiembre"; |FINSI;
  |SI NumMes = 10; NombreMes = "Octubre   "; |FINSI;
  |SI NumMes = 11; NombreMes = "Noviembre "; |FINSI;
  |SI NumMes = 12; NombreMes = "Diciembre "; |FINSI;
  |SI NumMes = 13; NombreMes = "Cierre    "; |FINSI;
|FPRC;

|PROCESO FiltraPuntos;
     aGuarda = FSalida;
     |SI eaCuenta = "            "; |ACABA; |FINSI;

     aPunto = eaCuenta - ".";
     |SI FSalida ! 0;
         |HAZ PorPuntos;
     |FINSI;

     FSalida = aGuarda;
|FPRC;

|PROCESO PorPuntos;
     nNume1 = FSalida;
     nPos   = 100 + (((nNume1 / 100) ! 0) -1);
     aAlfa2 = eaCuenta % nPos;   |QBF aAlfa2;
     nPos   = (((nNume1 / 100) ! 0) * 100) + 199;
     aAlfa3 = eaCuenta % nPos;   |QBF aAlfa3;

     FSalida = nNume1;
     |PARA; |SI FSalida ! 0; |HACIENDO;
           aAlfa3 = aAlfa3 - "."; || limpiar mas puntos (los ignoramos)
     |SIGUE;

     nPos   = 12 - ( (A \ aAlfa2 % 0) + (A \ aAlfa3 % 0));
     aAlfa4 = (A\ "0" * nPos);
     eaCuenta  = (A\ aAlfa2 + aAlfa4 + aAlfa3);
|FPRC;

|PROCESO Tipo12Sel; |TIPO 12;
|FPRC;
