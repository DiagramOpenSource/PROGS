|PROCESO SacaCali43;  |TIPO 7;
     eaTabla = "txtn0005.txt";
     |HAZ CargaTabla;
|FPRC;

|PROCESO CompCali43;  |TIPO 0;
     |SI Campo = 61; nCampo = 102; |FINSI;
     |SI Campo = 70; nCampo = 103; |FINSI;
     |SI FSalida = 10;
         enPosicion = 55;
         |HAZ SacaTabla;

         aAlfa1   = eaDestino % 102;
         #0Campo  = aAlfa1;
         |PINTA #0Campo;
     |FINSI;

     enComp = 2;
     eaAlfa = #0Campo;
     |HAZ CompruebaTabla;

     |SI enError = 1;
         |MENAV "0000 Contenido Incorrecto. Consulta la Tabla con F2";
         |ERROR;
         |ACABA;
     |FINSI;

     aAlfa1   = eaDestino % 102;
     aAlfa2   = eaDestino % 415;
     #0Campo  = aAlfa1;
     #0nCampo = aAlfa2;

     |PINTA #0Campo;
     |PINTA #0nCampo;

     |HAZ FinTabla;
|FPRC;

|PROCESO SacaSg43;  |TIPO 7;
     |SI eaPathIntegral = "";
         eaTabla = "txtn0001.txt";
         |HAZ CargaTabla;
     |FINSI;
|FPRC;

|PROCESO inf115_43;
  #115#2 = "     ";
|FPRC;

|PROCESO sup115_43;
  #115#2 = "zzzzz";
|FPRC;

|PROCESO CompSg43;  |TIPO 0;

  |SI eaPathIntegral = "";
     |SI FSalida = 10;
         enPosicion = 55;
         |HAZ SacaTabla;
         #0Campo = eaDestino % 105;
         |PINTA #0Campo;
     |FINSI;

     enComp = 5;
     eaAlfa = #0Campo;
     |HAZ CompruebaTabla;

     |SI enError = 1; |Y Campo = 12;
         |MENAV "0000 Contenido Incorrecto. Consulta la Tabla con F2";
         || |ERROR;
     |FINSI;

     |HAZ FinTabla;
 |SINO;

     |SI FSalida = 10;
         |ABRE #115;
         |CONSULTA_F_CLAVE #3#115, inf115_43, sup115_43;
         |CIERRA #115;
         #0Campo = #115#2; |PINTA #0Campo;
     |FINSI;

     |SI #0Campo = "     "; |Y Campo = 12;
         || |MENAV "0000 Contenido Incorrecto. Consulta el Fichero de Siglas con F2";
         || |ERROR;  tique 475_1346 que informen lo que quieran
         || |ACABA;
     |FINSI;

     |SI #0Campo ! "     ";
         |ABRE #115;
         |SELECT #3#115;
         #115#2 = #0Campo;
         |LEE 040#115=
         |SI FSalida ! 0;
             |MENAV "0000 Contenido Incorrecto. Consulta el Fichero de Siglas con F2";
             ||  |ERROR;  tique 475_1346 que informen lo que quieran
         |FINSI;
         |SELECT #1#115;
         |CIERRA #115;
     |FINSI;
 |FINSI;
|FPRC;

|PROCESO SacaPais43;  |TIPO 7;
     eaTabla = "txtn0006.txt";
     |HAZ CargaTabla;
|FPRC;

|PROCESO CompPais43;  |TIPO 0;
     |SI Campo = 4;
          nCampo = 10;
     |FINSI;
     |SI Campo = 38;
          nCampo = 37;
     |FINSI;

     |SI Campo = 120;
          nCampo = -1;
     |FINSI;

     |SI FSalida = 10;
         enPosicion = 55;
         |HAZ SacaTabla;

         aAlfa1   = eaDestino % 102;
         #0Campo  = aAlfa1;
         |PINTA #0Campo;
     |FINSI;

     enComp = 3;
     eaAlfa = #0Campo;
     |HAZ CompruebaTabla;

     |SI enError = 1;
         |MENAV "0000 Contenido Incorrecto. Consulta la Tabla con F2";
         |ERROR;
         |ACABA;
     |FINSI;

     aAlfa1   = eaDestino % 102;
     aAlfa2   = eaDestino % 420;
     #0Campo  = aAlfa1; |PINTA #0Campo;
     |SI nCampo ! -1;
         #0nCampo = aAlfa2;
         |PINTA #0nCampo;
     |FINSI;

     |HAZ FinTabla;
|FPRC;

|PROCESO SacaMuni43;
     || nCampo = Cod. Postal
     || nCampo1 = Nombre Municipio
     || nCampo2 = Codigo Provincia
     || nCampo3 = Nombre Provincia

     nCampo1 = Campo + 1;
     nCampo2 = Campo + 2;
     |SI Campo = 24;
          nCampo3 = 11;
     |FINSI;
     |SI Campo = 54;
          nCampo3 = 101;
     |FINSI;
     |SI Campo = 64;
          nCampo1 = 63;
          nCampo2 = 65;
          nCampo3 = 104;
     |FINSI;
     |SI Campo = 84;
          nCampo3 = 105;
     |FINSI;
     |SI FSalida = 10;
          |SUB_EJECUTA "agimunic";
          |ABRE #100;
          |CONSULTA_CLAVE #1#100;
          |SI FSalida = 0;
               nCampo = Campo;
               #0nCampo = #100#0;     |PINTA #0nCampo;
               #0nCampo1 = #100#3;     |PINTA #0nCampo1;
               #0nCampo2 = (#100#0 % 102);     |PINTA #0nCampo2;
               #0nCampo3 = #100#2;     |PINTA #0nCampo3;
               |ACABA;
          |FINSI;
          |CIERRA #100;
     |FINSI;
     |ABRE #100;
     |SELECT #4#100;
     #100#0 = #0Campo;
     |LEE 000#100=;
     |SI FSalida = 0;
          |SI #0nCampo ! Contenido;
               nCampo = Campo;
               #0nCampo = #100#0;     |PINTA #0nCampo;
               #0nCampo1 = #100#3;     |PINTA #0nCampo1;
               #0nCampo2 = (#100#0 % 102);     |PINTA #0nCampo2;
               #0nCampo3 = #100#2;     |PINTA #0nCampo3;
          |FINSI;
     |SINO;
          |SI #0Campo ! "     ";
               |MENAV "    El Código Postal no existe. Pulse F2 para ver la lista."
               |ERROR;
          |FINSI;
               nCampo = Campo;
               #0nCampo = "";     |PINTA #0nCampo;
               #0nCampo1 = "";     |PINTA #0nCampo1;
               #0nCampo2 = "";     |PINTA #0nCampo2;
               #0nCampo3 = "";     |PINTA #0nCampo3;
     |FINSI;
     |CIERRA #100;
|FPRC;

|PROCESO Emprendedor;
     nNume1 = 116;
     |SI Campo = 116; nNume1 = 115; |FINSI;

     |SI #0Campo = "S";
         #0nNume1 = "";
         |PINTA #0nNume1;
     |FINSI;

     |C_M #0#117,1,"S";
     |SI #0#115 = " "; |Y #0#116 = " ";
         |C_M #0#117,1,"N";
         #0#117 = "01.01.0000"; |PINTA #0#117;
     |FINSI;
|FPRC;

|PROCESO LaBaja43; |TIPO 7;
     |SI #0Campo ! "S"; |Y #0Campo ! " ";
         #0Campo = " "; |PINTA #0Campo;
     |FINSI;
|FPRC;
