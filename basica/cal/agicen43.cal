|PROCESO PonDatos43;
     eaCodNif  = #0#5;
     |HAZ LeeNifs;

     || #0#9  = #709#1;  |PINTA #0#9;

     aAlfa = #709#1;
     |HAZ SeparaNombre;
     #0#6 = aApel1;
     #0#7 = aApel2;
     #0#8 = aNom;
     |PINTA #0#6;
     |PINTA #0#7;
     |PINTA #0#8;

     eaSigla5 = #709#9; |HAZ Lee5DigSg;
     #0#12 = eaSigla5;    || tipo via
     #0#13 = #709#2;      || nombre via
     #0#15 = #709#3;      || numero
     #0#19 = #709#16;      || escalera
     #0#20 = #709#10;      || planta
     #0#21 = #709#11;      || puerta
     aAlfa1 = #709#4; |QBF aAlfa1; aAlfa1 = ("00" + aAlfa1) % -102;
     #0#26 = aAlfa1;    || codigo provincia
     aAlfa2 = #709#5; |QBF aAlfa2; aAlfa2 = ("000" + aAlfa2) % -103;
     aAlfa = aAlfa1 + aAlfa2;
     #0#24 = aAlfa;
     #0#25 = #709#6;     || nombre municipio
     #0#11 = #709#7;     || nombre provincia
     #0#27 = #709#8;     || fijo

     |PINTA #0#12;
     |PINTA #0#13;
     |PINTA #0#15;
     |PINTA #0#19;
     |PINTA #0#20;
     |PINTA #0#21;
     |PINTA #0#26;
     |PINTA #0#24;
     |PINTA #0#25;
     |PINTA #0#11;
     |PINTA #0#27;
     |PINTA #0#29;
|FPRC;

|PROCESO SacaTipo43;  |TIPO 7;
