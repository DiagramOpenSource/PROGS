|PROCESO DesFichero;
     |DBASE aBase;  |QBF aBase;

     aDirWord   = aBase + "word";   |MKDIR aDirWord;

     aDirPatron = aBase + "patrones/";
     aFichero   = aDirPatron + "zdocumen.tgz";
     |DESCOMPRIME aFichero;

     aFichero   = aDirPatron + "zdocumen.tar";
     |DETAR aFichero, aDirPatron;

     |FBORRA aFichero;

     aOrigen     = aDirPatron;

     |_PDIR aOrigen;
     |PARA; |SI FSalida = 0; |HACIENDO;
            aCadena1 = aOrigen    > " ";
            aAlfa    = aCadena1 - ".DOC";
            |SI aAlfa ! aCadena1;
                aCadena1    = aOrigen > " ";
                aCadena2    = aDirPatron > " ";  |QBF aCadena2
                aNombreFich = aCadena1 - aCadena2;
                |HAZ LeeTexto;
            |FINSI;

            aAlfa    = aCadena1 - ".XLS";
            |SI aAlfa ! aCadena1;
                aCadena1    = aOrigen > " ";
                aCadena2    = aDirPatron > " ";  |QBF aCadena2
                aNombreFich = aCadena1 - aCadena2;
                |HAZ LeeTexto;
            |FINSI;

            |_SDIR aOrigen;
     |SIGUE;

|FPRC;

