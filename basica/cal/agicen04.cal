     #4#1  = enContador;
     |LIBERA #4;
     |LEE 140#4=;
     |SI FSalida = 0;
         |HAZ Campo04;
         ||#4#28 = efFechaBaja;
         |PINDA #0#4;
         |ENDATOS #2#4;
         |SI FSalida = 0; |GRABA 140#4a; |FINSI;
     |SINO;
         |DDEFECTO #4;
         #4#0  = enEmpresa;
         #4#1  = enContador;
         #4#2  = eaNombreEmp;
         #4#3  = eaNombreAct;
         #4#28 = efFechaBaja;
         |HAZ Campo04;
         |PINDA #0#4;
         |ENDATOS #1#4;
         |SI FSalida = 0; |GRABA 140#4n; |FINSI;
