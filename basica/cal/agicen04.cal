         |C_M #4#26, 1, "N";  |C_M #4#27, 1, "N";

         #4#07 = "";  #4#08 = "";  #4#13 = "";  #4#14 = "";  #4#15 = "";
         #4#16 = "";  #4#17 = "";  #4#18 = "";  #4#19 = "";  #4#25 = "";
         #4#26 = "";  #4#27 = "";
     |SINO;
         |C_M #4#07, 1, "S";  |C_M #4#08, 1, "S";
         |C_M #4#13, 1, "S";  |C_M #4#14, 1, "S";
         |C_M #4#15, 1, "S";  |C_M #4#16, 1, "S";
