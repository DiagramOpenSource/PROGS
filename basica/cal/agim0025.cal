|PROCESO Titularidad;
    |SI FSalida = 2; |ACABA; |FINSI;

    |HAZ BuscoIguales;
    nTotalTPC = nTPC + #agim0025#13;

    |SI nTotalTPC [ 25; |Y nExiste = 1; |Y #agim0025#12 = "J";
        |CONFI "0000NPorcentaje inferior o igual a 25%, se borraran datos Titularidad real. Continuar";
        |SI FSalida ! 0;
            |ERROR;
            |ACABA;
        |FINSI;
    |FINSI;

     |SI nTotalTPC > 25; |Y #agim0025#12 = "J";
         enModo    = 2;
         |HAZ AltaAuxiliar;
     |FINSI;
|FPRC;

|PROCESO Parrilla;
     |SI FSalida = 2; |ACABA; |FINSI;

     Cliente   = #agim0025#0;
     Registro  = #agim0025#1;
     Programa  = "agim0025";
     TipoLinea = #agim0025#2;

     |SUB_EJECUTA_NP "agim0037";

     |ABRE #agim0037;
     #agim0037#0 = Programa;
     #agim0037#1 = Cliente;
     #agim0037#2 = Registro;
     #agim0037#3 = TipoLinea;
     #agim0037#4 = 1;
     |LEE 040#agim0037.];
     |SI FSalida ! 0; |O #agim0037#0 ! Programa; |O #agim0037#1 ! Cliente;
                      |O #agim0037#2 ! Registro; |O #agim0037#3 ! TipoLinea;
        |DDEFECTO #agim0037;
     |FINSI;

     #agim0025#15 = #agim0037#6;   |PINTA #agim0025#15;
     #agim0025#16 = #agim0037#7;   |PINTA #agim0025#16;

     #agim0025#20 = #agim0025#14 * enVAccion;
     |PINTA #agim0025#20;
|FPRC;

|PROCESO GrabaImporte;
     #agim0019#0 = #agim0025#0;
     #agim0019#1 = #agim0025#1;
     |LEE 040#agim0019.=;
     |SI FSalida ! 0;  |DDEFECTO #agim0019;  |FINSI;

                     nValor = 0;
    |SI #agim0019#16 ! 0;  nValor = #agim0019#16;  |FINSI;
    |SI #agim0019#20 ! 0;  nValor = #agim0019#20;  |FINSI;
    |SI #agim0019#24 ! 0;  nValor = #agim0019#24;  |FINSI;

    nSuma = #agim0019#16 + #agim0019#20 + #agim0019#24;
    |SI nSuma ! nValor;
        nValor = 0;
    |FINSI;

     #agim0025#20 = #agim0025#14 * nValor;
     |GRABA 020#agim0025.a;
     |LIBERA #agim0025;
|FPRC;

|DEFBUCLE agim0025;
     #agim0025#1;
     ;
     00000, 00, 000;
     99999, 99, 999;
     be;
     NULL, GrabaImporte;
|FIN;

|PROCESO Tipo11;  |TIPO 11;
     nInkey = FSalida;

     nTotalTPC = nTPC + #agim0025#13;
     |SI nInkey = 10;
         |SI #agim0025#12 = "J"; |Y nTotalTPC > 25;
             enModo = 4;
             |HAZ AltaAuxiliar;
         |FINSI;
         |ERROR;
     |FINSI;

     |SI nInkey = 17;
         |ABRE #agim0019;
         |BUCLE agim0025;
         |CIERRA #agim0019;
         |PTEC 807;
     |FINSI;

|FPRC;

|PROCESO Tipo3; |TIPO 3;

     nTotalTPC = nTPC + #agim0025#13;
     |SI #agim0025#12 ! "J"; |O nTotalTPC [ 25;
         nExiste = 0;
         |BUCLE Borram058;
         |SI nExiste = 1;
             enModo = 3;
             |HAZ AltaAuxiliar;
         |FINSI;
     |FINSI;

