  aAlfa = ("          " + nCampo1) % -110;
  |PINTA 2345, aAlfa;

  aAlfa = ("      " + nCampo2) % -106;
  |PINTA 2356, aAlfa;
  |ATRI s;

  |ATRI I;
  |PINTA 2363, "CAPITAL";
  |ATRI i;

  |ATRI S;
  aAlfa = ("          " + nCampo3) % -110;
  |PINTA 2370, aAlfa;
  |ATRI s;
