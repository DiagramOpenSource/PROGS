|PROGRAMA;

  |DBASE aPath;
  aMas = aPath + "def/agim0025.mas";
  |CARGA_DEF aMas, imagen@;
  |SI FSalida ! 0;
      |MENAV  "         Error en cargar el Def ... Proceso Abortado.";
      |ACABA;
  |FINSI;

  nCampo1 = 0;
  nCampo2 = 0;
  nCampo3 = 0;
  |BUCLE agim0025R;

  |PINPA #0#agim0025;

  |HAZ PintaTotales;

  |SI Modo ! 4;
      |ENTLINEAL #1#agim0025, -3, 2, 22, 0, ClaveBaja0, ClaveAlta0;
  |SINO;
      |ENTLINEAL #1#agim0025, -3, 4, 22, 0, ClaveBaja0, ClaveAlta0;
  |FINSI;

  |DESCARGA_DEF #imagen@;
|FPRO;
