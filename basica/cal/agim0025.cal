     nCampo1 = nCampo1 +. #agim0025#14;
     nCampo2 = nCampo2 +. #agim0025#13;
     nCampo3 = nCampo3 +. #agim0025#20;
     |HAZ PintaTotales;

     Modo1 = (FEntrada / 100) ! 0;
