|FICHEROS;
     agicen51 #0;
|FIN;

|VARIABLES;
     nCampo = 0;
|FIN;

|PROCESO FechaA51;
     nCampo = Campo + 20;
     |CAMPO_MODIFICA #0nCampo, 1, "S";
     |SI #0Campo = " ";
          |CAMPO_MODIFICA #0nCampo, 1, "N";
          #0nCampo = "01.01.0000";
          |PINTA #0nCampo;
     |FINSI;
|FPRC;
