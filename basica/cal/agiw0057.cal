     |C_M #0nCampo1, 1, "S";
     |SI #0Campo = " ";
         |C_M #0nCampo1, 1, "N";  #0nCampo1 = "01.01.0000";  |PINTA #0nCampo1;
     |FINSI;
|FPRC;
