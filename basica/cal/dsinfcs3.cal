|FICHEROS;
   dsinfcs3 #13,MANTE;
|FIN;

|VARIABLES;
   aAlfa1    = "";
   aAlfa2    = "";
   &eaCuenta = "";
   &salidas  = 0;
   &eaFecha  = "";
   &nSwMal   = 0;
|FIN;

|CALCULO ElTipo;
   aAlfa1 = "S";
   aAlfa2 = "S";
   |SI #dsinfcs3#9 = "G";
       aAlfa1 = "N";
       aAlfa2 = "N";
   |FINSI;
   |SI #dsinfcs3#9 = "C";
       aAlfa2 = "N";
   |FINSI;
   |SI #dsinfcs3#9 = "A";
       aAlfa1 = "N";
   |FINSI;
   |C_M #dsinfcs3#7,  1, aAlfa1;
   |C_M #dsinfcs3#8,  1, aAlfa1;
   |C_M #dsinfcs3#0,  1, aAlfa2;
   |C_M #dsinfcs3#1,  1, aAlfa2;
   |C_M #dsinfcs3#2,  1, aAlfa2;
   |C_M #dsinfcs3#3,  1, aAlfa2;
   |C_M #dsinfcs3#4,  1, aAlfa2;
   |C_M #dsinfcs3#5,  1, aAlfa2;
|FCAL;

|CALCULO cuentaS3; |TIPO 6;
     |C_M #dsinfcs3#Campo#,0,aAlfa1;
     |SI aAlfa1 = "N"; |ACABA; |FINSI;

     eaCuenta = #dsinfcs3#Campo#;
     salidas  = FSalida;
     |HAZ FiltraPuntos;
     #dsinfcs3#Campo# = eaCuenta;
     |PINTA #dsinfcs3#Campo#;
|FCAL;

|PROCESO VerFechaS3; |TIPO 0;

  eaFecha = #dsinfcs3#Campo#;
  |HAZ VerLaFecha;

  |SI nSwMal = 1;
      |MENAV "0000Fecha Erronea";
      |ERROR;
  |FINSI;
|FPRC;
