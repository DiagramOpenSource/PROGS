|CALCULO SelUsuario;
 aAlfa1 = #0#7;
 aAlfa2 = "N";
 |SI aAlfa1 = "N"; aAlfa2 = "S"; |FINSI;
 |C_M #0#8,  1, aAlfa1; |C_M #0#9,  1, aAlfa1;
 |C_M #0#10, 1, aAlfa2; |C_M #0#11, 1, aAlfa2;
 |C_M #0#12, 1, aAlfa2; |C_M #0#13, 1, aAlfa2;
 |C_M #0#14, 1, aAlfa2; |C_M #0#15, 1, aAlfa2;
 |C_M #0#16, 1, aAlfa2; |C_M #0#17, 1, aAlfa2;
 |SI aAlfa1 = "S";
     #0#10 = ""; |PINTA #0#10;
     #0#11 = ""; |PINTA #0#11;
     #0#12 = ""; |PINTA #0#12;
     #0#13 = ""; |PINTA #0#13;
     #0#14 = ""; |PINTA #0#14;
     #0#15 = ""; |PINTA #0#15;
