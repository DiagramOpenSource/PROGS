     #0#9 = "zzzzzzzzzz"; |PINTA #0#9;
 |FINSI;
|FCAL;

|PROCESO Imprimelo;
  nSwUs = 0;
  |SI #0#7 = "N";
      nSwUs = 1;
      |PARA nPara1 = 10; |SI nPara1 [ 17; |HACIENDO nPara1 = nPara1 + 1;
            aAlfa1 = #0nPara1; |QBF aAlfa1;
            |SI #0nPara1 = #1#33; |Y aAlfa1 ! "";
                nSwUs = 0;
                |SAL;
            |FINSI;
      |SIGUE;
  |FINSI;

  |SI nSwUs = 0;
      |PRINT;
      |PIEINF;
  |FINSI;
|FPRC;

|DEFBUCLE agim0031;
  #1#1;
  1, 9, 20, 33;
  #0#2, #0#0, #0#4, aDJus, #0#8;
  #0#3, #0#1, #0#5, aHJus, #0#9;
  ;
  NULL, Imprimelo;
|FIN;

|PROCESO Tipo3;  |TIPO 3;

  aDJus = " ";
  aHJus = "z";
  |SI #0#6 ! "A";
      aDJus = #0#6;
      aHJus = #0#6;
  |FINSI;

  |IMPRE 0;        || Abre impresora
  |SI FSalida ! 0;  |ACABA;  |FINSI;

  |INFOR "agiy0027";
