|PROGRAMA;
     |ABRE #2;
     |PINPA #0#2;
     #2#0  = enEmpresa;
     #2#1  = enContador
     |LIBERA #2;
     |LEE 140#2=;
     |SI FSalida = 0;
         |HAZ Campo02;
         |PINDA #0#2;
         |ENDATOS #2#2;
         |SI FSalida = 0; |GRABA 140#2a; |FINSI;
     |SINO;
         |DDEFECTO #2;
         #2#0  = enEmpresa;
         #2#1  = enContador
         #2#2  = eaNombreEmp;
         #2#3  = eaNombreAct;
         |HAZ Campo02;
         |HAZ CogeCliente;
         |PINDA #0#2;
         |ENDATOS #1#2;
