     |SI enSwDeDonde = 1;
         |ABRE #5;
         #5#0 = enEmpresa;
         #5#1 = 1;
         |LEE 040#5=;
         |SI FSalida ! 0; |DDEFECTO #5; |FINSI;
         |CIERRA #5;

         #2#15 = #5#3;     || SG
         #2#16 = #5#4;     || Domicilio
