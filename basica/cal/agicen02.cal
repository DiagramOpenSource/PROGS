|PROCESO Campo02;
     |SI enTipo = 2;
         |CAMPO_MODIFICA #2#14, 1, "N";
         |CAMPO_MODIFICA #2#49, 1, "N";
         |CAMPO_MODIFICA #2#50, 1, "N";
         #2#14 = 0;
     |SINO;
