|FICHEROS;
     dspase96 #96,MANTE;
     dspase97 #97,MANTE;

     &referen@ #999;
|FIN;

|VARIABLES;
|FIN;

|PROCESO Libro;

     |SI #97#3 = 0;
         |CONFI "    SConcepto No IVA No IRPF. Seguro ? ";
         |SI FSalida ! 0;
             |ERROR;
         |FINSI;
         |ACABA;
     |FINSI;

     |ABRE #999;
     #999#0 = #97#3;
     |LEE 040#999=;
     |SI FSalida ! 0;
         |MENAV "     Libro Inexistente en la Contabilidad";
         |ERROR;
     |FINSI;
     |CIERRA #999;
|FPRC;

|RUTINA ClaveBaja97;
      #97#0 = #96#0;
      #97#1 = #96#1;
      #97#2 = #96#2;
      #97#3 = 00;
|FRUT;

|RUTINA ClaveAlta97;
      #97#0 = #96#0;
      #97#1 = #96#1;
      #97#2 = #96#2;
      #97#3 = 99;
|FRUT;

|PROCESO Tipo7w96;  |TIPO 7;
     |ENTLINEAL #1#97, -4, 7, 22, 0, ClaveBaja97, ClaveAlta97;
|FPRC;

|PROCESO Tipo11w96;  |TIPO 11;
     |SI FSalida = 10;
         |ENTLINEAL #1#97, -4, 2, 22, 0, ClaveBaja97, ClaveAlta97;
     |FINSI;
|FPRC;
