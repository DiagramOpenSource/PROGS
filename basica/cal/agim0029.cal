|FICHEROS;
     agim0029 #29;
|FIN;

|VARIABLES;
     Modo     = 0;
     aBase    = "";
     aOrigen  = "";
     aDestino = "";
     nEntrada = 0;
     &enCargo = 0;
|FIN;

|PROCESO Tipo8;  |TIPO 88;
  |ABRE #29;
  |LEE 040#29p;
  |SI FSalida ! 0;
      |DBASE aBase;  |QBF aBase;
      aOrigen  = aBase + "def/agim0029.dat";
      aDestino = aBase + "fich/agim0029.dat";
      |RENOMBRA_FICHERO aOrigen, aDestino;

      aOrigen  = aBase + "def/agim0029.ddx";
      aDestino = aBase + "fich/agim0029.ddx";
      |RENOMBRA_FICHERO aOrigen, aDestino;

      aOrigen  = aBase + "def/agim0029.ixx";
      aDestino = aBase + "fich/agim0029.ixx";
      |RENOMBRA_FICHERO aOrigen, aDestino;
  |FINSI;

  |LEE 040#29p;
  |SI FSalida ! 0;
      #29#0 = 01;  #29#1 = "PRESIDENTE     ";  |GRABA 140#29n;  |LIBERA #29;
      #29#0 = 02;  #29#1 = "VICEPRESIDENTE ";  |GRABA 140#29n;  |LIBERA #29;
      #29#0 = 03;  #29#1 = "SECRETARIO     ";  |GRABA 140#29n;  |LIBERA #29;
      #29#0 = 04;  #29#1 = "VICESECRETARIO ";  |GRABA 140#29n;  |LIBERA #29;
      #29#0 = 05;  #29#1 = "CONSEJERO DELEG";  |GRABA 140#29n;  |LIBERA #29;
      #29#0 = 06;  #29#1 = "ADMINISTRADOR  ";  |GRABA 140#29n;  |LIBERA #29;
      #29#0 = 07;  #29#1 = "ORG.ADMINISTRA.";  |GRABA 140#29n;  |LIBERA #29;
      #29#0 = 08;  #29#1 = "GERENTE        ";  |GRABA 140#29n;  |LIBERA #29;
      #29#0 = 09;  #29#1 = "APODERADO      ";  |GRABA 140#29n;  |LIBERA #29;
  |FINSI;
  |CIERRA #29;
  #29#0 = enCargo; |SI #29#0 = 0; #29#0 = 1; |FINSI;
|FPRC;

|PROCESO Cargo;  |TIPO 0;
  |C_M #29#1, 1, "S";
  Modo = (FEntrada / 100) ! 0;
  |SI Modo = 1; |Y #29#0 [ 9;
      |HAZ Tipo8;
      |ABRE #29;
  |FINSI;

  |SI Modo = 3; |Y #29#0 [ 9;
      |MENAV "     No se puede dar de baja uno fijo.";
      |ERROR;
  |FINSI;

  |SI #29#0 [ 9;
     |C_M #29#1, 1, "N";
  |FINSI;
|FPRC;
