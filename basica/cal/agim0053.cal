|FICHEROS;
     agim0053 #0;
|FIN;

|VARIABLES;
     aPrograma = "";
     &enRecibo = 0;
|FIN;

|PROCESO min;
     #0#8 = 999999;
|FPRC;

|PROCESO max;
     #0#8 = 000001;
|FPRC;

|PROGRAMA;
     |CLS;
     |ABRE #0;
     |ENTLINEAL #2#0, -1, 2, 18, 1, min, max;
     |SI FSalida=0;

     |FINSI;
     |CIERRA #0;
|FPRO;

|PROCESO Impresion; |TIPO 11;
     |SI FSalida = 11;        ||F3. Imprimir
          aPrograma = "agim0052;H";     ||Impresion desde el historico;
          enRecibo = #agim0053#8;
          |SUB_EJECUTA aPrograma;
     |FINSI;
|FPRC;
