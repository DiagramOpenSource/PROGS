  |LEE 040#1=;
  |SI FSalida = 0;
      |HAZ existe;
  |SINO;
      |HAZ noexiste;
  |FINSI;
|FPRC;

|DEFBUCLE maempr01;
  #2#1;
  ;
  #0#0, 1996;
  #0#1, 1996;
  be;
