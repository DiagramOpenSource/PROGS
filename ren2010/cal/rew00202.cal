|FICHEROS;
     rew00202 #202;
|FIN;

|VARIABLES;
     nCampo1      = 0;

     aAlfa        = "";
     aAlfa1       = "";
     aAlfa2       = "";
     aAlfa3       = "";
     aComa        = "";

     &eaVarDni    = "";
|FIN;

|PROCESO Tipo12Fw202;  |TIPO 12;
     |HAZ Tipo7C5Fw202;

     |SI #202#14 = "N";
         aAlfa = #202#16;
     |SINO;
         aAlfa = #202#31;
     |FINSI;

     |QBF aAlfa;

     |SI aAlfa = "";
         |MENAV "0000 Introduzca datos del domicilio habitual";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Tipo6C0Fw202;  |TIPO 6;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     eaVarDni = #202Campo;
     |HAZ CalculoDNI;
     #202Campo = eaVarDni;
     |PINTA #202Campo;
     |ERROR;
|FPRC;

|PROCESO Tipo7C5Fw202;  |TIPO 7;
     aAlfa1 = #202#2;  |QBF aAlfa1;
     aAlfa2 = #202#3;  |QBF aAlfa2;
     aAlfa3 = #202#4;  |QBF aAlfa3;
     |SI aAlfa1 ! "";  |O aAlfa2 ! "";  |O aAlfa3 ! "";
         aComa = ",";
     |SINO;
         aComa = "";
     |FINSI;

     aAlfa  = aAlfa1 + " " + aAlfa2 + aComa + " " + aAlfa3;
     #202#1 = aAlfa;
     |PINTA #202#1;
|FPRC;

|PROCESO Tipo0C6Fw202;
     |C_M #202#10, 1, "S";
     |SI #202#6 ! "L"; |Y #202#6 ! "S";
         #202#10 = " ";  |PINTA #202#10;
         |C_M #202#10, 1, "N";
     |FINSI;
|FPRC;

|PROCESO Tipo0C7Fw202;
     |SI #202Campo = "01.01.0000";  |ACABA;  |FINSI;

     |SI #202Campo ] "01.01.2000";
         aAlfa = "     La fecha puede ser incorrecta. Ha introducido un contribuyente menor de edad.";
         |MENAV aAlfa;

         aAlfa = "     Si la fecha es incorrecta, cambie los dos primeros digitos del A" + &209 + "O (20) por (19)";
         |MENAV aAlfa;

         |CONFI "0000NContinuar";
         |SI FSalida ! 0;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo0C8Fw202;
     nCampo1 = Campo + 1;
     |C_M #202nCampo1, 1, "S";
     |SI #202Campo = 0;
         |C_M #202nCampo1, 1, "N";
         #202nCampo1 = "N";  |PINTA #202nCampo1;
     |FINSI;
|FPRC;

|PROCESO Tipo0C11Fw202;
     |C_M #202#13, 1, "S";

     aAlfa = #202#11;  |QBF aAlfa;
     |SI aAlfa = "";
         |C_M #202#13, 1, "N";  #202#13 = "N";  |PINTA #202#13;
     |FINSI;
|FPRC;
