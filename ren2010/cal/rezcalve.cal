|PROCESO CogeVersion;
     aVersionMenu = "";
     aMenu        = aPathBase + "ren2010.men";
     |XABRE "U", aMenu, nHandle;
     |SI FSalida ] 0;
         |XLEE nHandle, 250, aAlfa;
         aVersionMenu = aAlfa % -109;
     |FINSI;
     |XCIERRA nHandle;

     |QBF aVersionMenu;

     |SI aVersionMenu = "30.00.043"; aVersionMenu = aVersionPrograma;  |FINSI;

     aVersionMenu = aVersionPrograma;
|FPRC;

