|FICHEROS;
    ver00000 #39;         || cambios de version AGIREN94
|FIN;

|VARIABLES;
    v_switch = 0;
    v_ultimo = 0;
    v_puedes = 0;
    v_anteri = "";
    v_versio = "AGIREN94 04.01******";
|FIN;

|CALCULO lee_version2;
|SI #39#1 = #0#2;
        alfa1 = "    ATENCION!!! Cambio de Version YA Realizado!!!: " + #39#2;
    |MENAV alfa1;
    |ERROR10;
        v_switch = 1;
|SINO;
        v_anteri = #39#1;
|FINSI;
     v_ultimo = #39#0;
|FCAL;

|DEFBUCLE lee_version1;
 #39#1;
 ;
   1;
 999;
 e;
 NULL, lee_version2;
|FIN;

|PROCESO lee_version; |TIPO 8;
    v_puedes = 0;
    v_ultimo = 0;
    v_switch = 0;
    v_anteri = "";
|BUCLE lee_version1;
|SI v_switch = 1;
    |PTEC 807;
|SINO;
    |SI v_anteri ! v_versio; |Y #0#2 ! v_versio;
        |MENAV "    Quedan Cambios de Version Pendientes ...";
        |PTEC 807;
    |FINSI;
|FINSI;
|FPRC;

|CALCULO gra_version;
|SI v_switch = 0;
    |ABRE #39;
        #39#0 = v_ultimo + 1;
        #39#1 = #0#2;
        #39#2 = ~;
    |GRABA 020#39n;
    |CIERRA #39;
|FINSI;
|FCAL;
