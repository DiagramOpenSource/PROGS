|FICHEROS;
   rexvalor #0;
   reyvalor #1;
   reyvalon #2;
|FIN;

|VARIABLES;
   &empresa    = 0;
   &valorcillo = 0;
   lin         = 0;
   fich        = "";
   copy        = "";
   copiar      = "";
   systema     = "";
   dire        = "";
|FIN;

|PROGRAMA;
|PINPA #0#0;
|HAZ base;
|HAZ impresio;
|DELINDEX #1;
|DELINDEX #2;
|FPRO;

|PROCESO base;
|DFICE fich;
|QUE_SISTEMA systema;
|SI systema = "ESDOS";
    copy = "copy ";
|SINO;
    copy = "cp ";
|FINSI;

dire = "";
|DIRECTORIO dire;
|SI valorcillo = 1;
    copiar = dire + "bin/agicopy " + fich + "revalore " + fich + "reyvalor";
|SINO;
    copiar = dire + "bin/agicopy " + fich + "revalono " + fich + "reyvalon";
|FINSI;
|SYSTEM copiar;
|FPRC;

|| *************************************************************************
||                        Impresion
|| *************************************************************************
|PROCESO pintael1;
#0#0 = #1#13;
#0#1 = #1#14;
#0#2 = #1#1;
#0#3 = #1#18;
#0#4 = #1#16;
#0#5 = #1#17;

Pos = lin;
|PINDA #0#0;
Pos = -1;

lin = lin + 100;
|SI lin > 1000;
    lin = 100;
    |PAUSA;
    |PINPA #0#0;
|FINSI;
|FPRC;

|PROCESO pintael2;
#0#0 = #2#13;
#0#1 = #2#14;
#0#2 = #2#1;
#0#3 = #2#18;
#0#4 = #2#16;
#0#5 = #2#17;

Pos = lin;
|PINDA #0#0;
Pos = -1;

lin = lin + 100;
|SI lin > 1000;
    lin = 100;
    |PAUSA;
    |PINPA #0#0;
|FINSI;
|FPRC;

|DEFBUCLE valores;
#1#1;
;
empresa,01;
empresa,99;
e;
NULL,NULL,NULL,NULL,NULL,pintael1;
#1#13;
|FIN;

|DEFBUCLE novalores;
#2#1;
;
empresa,01;
empresa,99;
e;
NULL,NULL,NULL,NULL,NULL,pintael2;
#2#13;
|FIN;

|PROCESO impresio;
lin = 100;
|SI valorcillo = 1;
    |BUCLE valores;
|SINO;
    |BUCLE novalores;
|FINSI;
|PAUSA;
|FPRC;
