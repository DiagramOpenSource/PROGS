|FICHEROS;
   reincpen #0;
   reparame #1;
|FIN;

|VARIABLES;
   pantalla     = 0;

   &empresa     = 0;
   &modo        = 0;
   &modelito    = 0;
   &errorlin    = 0;
   &indivi      = "";

   &numerica1   = 0;  &numerica2  = 0;   &numerica3  = 0;
   &numerica4   = 0;  &numerica5  = 0;   &numerica6  = 0;
   &numerica7   = 0;  &numerica8  = 0;   &numerica9  = 0;
   &numerica10  = 0;  &numerica11 = 0;   &numerica12 = 0;
   &numerica13  = 0;  &numerica14 = 0;   &numerica15 = 0;
   &numerica16  = 0;

   &alfanumer1  = ""; &alfanumer2  = ""; &alfanumer3  = "";
   &alfanumer4  = ""; &alfanumer5  = ""; &alfanumer6  = "";
|FIN;

|INCLUYE i_activo;

|RUTINA inferior;
  #0#0 = empresa;
  #0#1 = 1;
|FRUT;

|RUTINA superior;
  #0#0 = empresa;
  #0#1 = 99;
|FRUT;

|PROGRAMA;
|ABRE #0;
|PINPA #0#0;
|SI modo = 1;
    |ENTLINEAL #1#0,2,1,15,0,inferior,superior;
|FINSI;
|SI modo = 2;
    |ENTLINEAL #1#0,2,2,15,0,inferior,superior;
|FINSI;
|SI modo = 4;
    |ENTLINEAL #1#0,2,4,15,0,inferior,superior;
|FINSI;
|FPRO;

|PROCESO ayuda; |TIPO 2;
pantalla = 0 +. 1;
|SI pantalla = 1; |Y #0#4 ! "X"; |HAZ cargadat; |FINSI;
|FPRC;

|PROCESO consulta; |TIPO 0;
modelito = (FEntrada / 100) ! 0;
|SI modelito = 4; |HAZ cargadat;  |FINSI;
|SI modelito = 1; |HAZ parametro; |FINSI;
|FPRC;

|PROCESO parametro;
|ABRE #1;
|LEE 040#1=;
|SI FSalida = 0;
   #0#2 = #1#6;
   #0#4 = #1#7;
   |PINTA #0#2;
   |PINTA #0#4;
|FINSI;
|CIERRA #1;
|FPRC;

|PROCESO cargadat;
alfanumer1 = #0#2;
alfanumer2 = #0#4;
alfanumer3 = #0#5;
numerica1  = #0#6;
numerica2  = #0#7;
numerica3  = #0#8;
numerica4  = #0#9;
numerica5  = #0#10;
numerica6  = #0#11;
numerica7  = #0#12;
alfanumer4 = #0#13;
numerica8  = #0#14;
numerica9  = #0#15;
numerica10 = #0#16;
numerica11 = #0#17;
alfanumer5 = #0#18;
numerica12 = #0#19;
numerica13 = #0#20;
numerica14 = #0#21;
numerica15 = #0#22;
numerica16 = #0#29;

|SUB_EJECUTA "repant16.run";
|SI errorlin = 0; |ERROR; |SINO; |HAZ carga; |FINSI;
|FPRC;

|PROCESO carga;
|SI modelito ! 4;
   #0#2   = alfanumer1;
   #0#3   = alfanumer6;
   #0#4   = alfanumer2;
   #0#5   = alfanumer3;
   #0#6   = numerica1;
   #0#7   = numerica2;
   #0#8   = numerica3;
   #0#9   = numerica4;
   #0#10  = numerica5;
   #0#11  = numerica6;
   #0#12  = numerica7;
   #0#13  = alfanumer4;
   #0#14  = numerica8;
   #0#15  = numerica9;
   #0#16  = numerica10;
   #0#17  = numerica11;
   #0#18  = alfanumer5;
   #0#19  = numerica12;
   #0#20  = numerica13;
   #0#21  = numerica14;
   #0#22  = numerica15;
   #0#29  = numerica16;

   |SI #0#14 < 1992;
       #0#23 = #0#16;
       #0#24 = #0#17;
   |SINO;
       #0#23 = #0#21;
       #0#24 = #0#22;
   |FINSI;

   npro  = 18; |HAZ sigrabo;

   |PINTA #0#2;
   |PINTA #0#3;
   |PINTA #0#4;
   |PINTA #0#13;
   |PINTA #0#23;
   |PINTA #0#24;
|FINSI;
|FPRC;
