|PROCESO profcoef3;
    #2#25 = #2#17 + #2#37;
    #2#25 = (#2#25-(#2#18+#2#19+#2#20+#2#21+#2#22+#2#23+#2#24+#2#38+#2#39))%10;
|SI #2#25 < 0;  #2#25 = 0;  |FINSI;
    #2#30 = 0;
|PARA nume1 = 18; |SI nume1 [ 25; |HACIENDO nume1 = nume1 + 1;
        #2#30 = #2#30 + #2nume1;
|SIGUE;
    #2#30 = #2#30 + #2#38 + #2#39 - #2#37;
    #2#40 = #2#17;
    #2#41 = #2#40 - #2#30;
    #2#31 = #2#26 - #2#27 - #2#28 - #2#29;
    #2#32 = #2#41 + #2#31;
|FPRC;

