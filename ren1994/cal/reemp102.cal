|| **************************************************************************
||                           PROCESOS DE LA IMPRESION
|| **************************************************************************

|PROCESO alataque;
empresa = #1#0;
|SI #1#110 = "S"; |O #1#110 = "s";
     |SI #1#131 = "N"; tipo = "A"; |HAZ ejecuciones; |FINSI;
|FINSI;
|SI #1#111 = "S"; |O #1#111 = "s";
     |SI #1#132 = "N"; tipo = "T"; |HAZ ejecuciones; |FINSI;
|FINSI;
|SI #1#112 = "S"; |O #1#112 = "s";
     |SI #1#133 = "N"; tipo = "C"; |HAZ ejecuciones; |FINSI;
|FINSI;
|GRABA 020#1a;
|LIBERA #1;
|FPRC;

|PROCESO ejecuciones;
|ABRE #8;
#8#0 = #1#0;
|LEE 040#8=;
|SI FSalida ! 0; |ACABA; |FINSI;
|CIERRA #8;

|HAZ hoja1;

|SI tipo = "A";
    |SI #5#139 > 0; #1#131 = "S"; |FINSI;
|FINSI;
|SI tipo = "T";
    |SI #5#139 > 0; #1#132 = "S"; |FINSI;
|FINSI;
|SI tipo = "C";
    |SI #5#139 > 0; #1#133 = "S"; |FINSI;
|FINSI;
|FPRC;

|PROCESO hoja1;
cuenta  = ""; fraccion = "";
nombre  = "";  cif  = "";  anyos  = "";
nombrec = "";  cifc = "";  anyosc = "";


|SI tipo = "A";
    nombre  = #1#1;  cif  = #1#2;  anyos  = #1#3;
    nombrec = #1#28; cifc = #1#29; anyosc = #1#30;
    cuenta  = #1#174;
|FINSI;
|SI tipo = "T";
    nombre  = #1#1;  cif  = #1#2;  anyos  = #1#3;
    |SI #8#3 = 2; |Y #8#5 = "T";
        nombrec = #1#28; cifc = #1#29; anyosc = #1#30;
    |FINSI;
    cuenta  = #1#175;
|FINSI;
|SI tipo = "C";
    nombre  = #1#28; cif  = #1#29; anyos  = #1#30;
    |SI #8#3 = 2; |Y #8#5 = "C";
        nombrec = #1#1;  cifc = #1#2;  anyosc = #1#3;
    |FINSI;
    cuenta  = #1#176; |QBF cuenta;
|FINSI;

|ABRE #2;
#2#0 = #1#40;
#2#1 = #1#41;
|LEE 040#2=;
|SI FSalida ! 0; |DDEFECTO #2; |FINSI;
|CIERRA #2;

|ABRE #4;
#4#0 = #1#12;
#4#1 = #1#13;
|LEE 040#4=;
|SI FSalida ! 0; |DDEFECTO #4; |FINSI;
|CIERRA #4;

|ABRE #5;
#5#0 = empresa;
#5#141 = tipo;
|LEE 040#5=;
|SI FSalida ! 0; |DDEFECTO #5; |FINSI;
|CIERRA #5;

informe = "rehoj102";
|INFOR informe;
|SI FSalida = 0;
    ingreso = #5#139;
    |SI #5#139 > 0;
        |PRINT;
        |PIEINF;
        |FININF;
    |FINSI;
|FINSI;
|FPRC;

|DEFBUCLE impresio;
#1#1;
142, 143;
#0#1, "S", "S";
#0#2, "S", "S";
be;
NULL, alataque;
|FIN;

|PROCESO impre; |TIPO 3;
|HAZ fecha;
|IMPRE 0;
|SI FSalida = 0;
    |SI #0#0 = "S";
        |BUCLE impresio;
    |SINO;
       |ABRE #1;
       |PARA pepe = 3; |SI pepe [ 32; |HACIENDO pepe = pepe + 1;
          |SI #0pepe ! 0;
              #1#0 = #0pepe;
              |LIBERA #1;
              |LEE 101#1=;
              |SI FSalida = 0; |Y #1#142 = "S"; |Y #1#143 = "N";
                  |HAZ alataque;
              |FINSI;
          |FINSI;
       |SIGUE;
       |CIERRA #1;
    |FINSI;
|FINSI;
|FPRC;

