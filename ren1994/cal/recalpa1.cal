  dias4 = fefin - feini + 1;

  |SI tippar = 2;   |Y funeral = 1;
      |SI muerto = "C";
          |SI tipod ! "C";  dias4 = dias3;  |FINSI;
      |SINO;
          |SI tipod ! "T";  dias4 = dias3;  |FINSI;
      |FINSI;
  |FINSI;

  |SI dias4 ] 0;
      ratio = dias4 / dias11;
  |SINO;
      ratio = 0;
  |FINSI;
  |SI periodo = 1;  ratio = 1; |FINSI;
  ratio = ratio ! 4;
  coefi1 = coefi11 * 100;

  |SI tippar = 2; |Y periodo = 2;
      ratio = 1;
  |FINSI;
|FPRC;

