|FICHEROS;
   repartid #0;
   redatcon #1;
|FIN;

|VARIABLES;
   modo = 0;
|FIN;

|INCLUYE i_fechat;
|INCLUYE i_cambio;

|PROCESO grabacon; |TIPO 2;
    modo = (FEntrada / 100) ! 0;
|ABRE #1;
    #1#0 = #0#0;
|LIBERA #1;
|LEE 110#1=;
|SI FSalida = 0;
    |SI modo = 1; |O modo = 2;
            #1#143 = "S";
    |SINO;
            #1#143 = "N";
    |FINSI;
    |GRABA 020#1a;
|FINSI;
|CIERRA #1;
|FPRC;

|PROCESO noentres; |TIPO 0;
|SI #0#3 = 1; |O #0#3 = 3;
    |CAMPO_MODIFICA #0#04, 1, "N";
    |CAMPO_MODIFICA #0#05, 1, "N";
    |CAMPO_MODIFICA #0#06, 1, "S";
|SINO;
    |CAMPO_MODIFICA #0#04, 1, "S";
    |CAMPO_MODIFICA #0#05, 1, "S";
    |CAMPO_MODIFICA #0#06, 1, "N";
|FINSI;
|FPRC;
