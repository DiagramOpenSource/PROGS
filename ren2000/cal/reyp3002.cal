|PROCESO recompa3;
     sw_fich = 1;
     swrrr = 0;
     total = 0;
     |BUCLE repatrie1;
     |SI swrrr = 1;
         varalf = " " * 110;
         varalf = varalf + "--------------"; |HAZ impre_03;
         importe = total;                     |HAZ importe;
