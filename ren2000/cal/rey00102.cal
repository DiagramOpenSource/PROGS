|FPRC;

|PROCESO Impresora;
  |SI #0#19 = "N";
      |INFOR "m210h001";   |PRINT;   |PIEINF;  |FININF;
      |INFOR "m210h002";   |PRINT;   |PIEINF;  |FININF;
