|FICHEROS;
     rew01003 #1003,MANTE;
|FIN;

|VARIABLES;
     nCampo       = 0;
     nCampo1      = 0;
     nCampo2      = 0;
     nCampo3      = 0;
     nCampo4      = 0;
     nCampo5      = 0;
     nCampo6      = 0;
     nCampo7      = 0;
     nCampo8      = 0;
|FIN;

|PROCESO Individual;  |TIPO 7;
     |C_M #1003Campo, 1, "S";
     |SI #1003Campo = "I";
         |C_M #1003Campo, 1, "N";
     |FINSI;
|FPRC;

|PROCESO Modifica1003;
    |PARA nCampo = 3;  |SI nCampo [ 66;  |HACIENDO nCampo = nCampo + 1;
          |C_M #1003nCampo, 1, "N";
    |SIGUE;

    |SI #1003#3 = "C";
         |C_M #1003#4,  1, "S";
         |C_M #1003#6,  1, "S";
         |C_M #1003#7,  1, "S";
         |C_M #1003#10, 1, "S";
         |C_M #1003#11, 1, "S";
         |C_M #1003#12, 1, "S";

         |PDEFECTO #1003, 13, 67;
     |SINO;
         |SI #1003#13 ! "   ";
             |C_M #1003#13, 1, "S";
             |C_M #1003#15, 1, "S";
             |C_M #1003#16, 1, "S";
             |C_M #1003#19, 1, "S";
             |C_M #1003#20, 1, "S";
             |C_M #1003#21, 1, "S";
         |FINSI;

         |SI #1003#22 ! "   ";
             |C_M #1003#22, 1, "S";
             |C_M #1003#24, 1, "S";
             |C_M #1003#25, 1, "S";
             |C_M #1003#28, 1, "S";
             |C_M #1003#29, 1, "S";
             |C_M #1003#30, 1, "S";
         |FINSI;

         |SI #1003#31 ! "   ";
             |C_M #1003#31, 1, "S";
             |C_M #1003#33, 1, "S";
             |C_M #1003#34, 1, "S";
             |C_M #1003#37, 1, "S";
             |C_M #1003#38, 1, "S";
             |C_M #1003#39, 1, "S";
         |FINSI;

         |SI #1003#40 ! "   ";
             |C_M #1003#40, 1, "S";
             |C_M #1003#42, 1, "S";
             |C_M #1003#43, 1, "S";
             |C_M #1003#46, 1, "S";
             |C_M #1003#47, 1, "S";
             |C_M #1003#48, 1, "S";
         |FINSI;

         |SI #1003#49 ! "   ";
             |C_M #1003#49, 1, "S";
             |C_M #1003#51, 1, "S";
             |C_M #1003#52, 1, "S";
             |C_M #1003#55, 1, "S";
             |C_M #1003#56, 1, "S";
             |C_M #1003#57, 1, "S";
         |FINSI;

         |SI #1003#58 ! "   ";
             |C_M #1003#58, 1, "S";
             |C_M #1003#60, 1, "S";
             |C_M #1003#61, 1, "S";
             |C_M #1003#64, 1, "S";
             |C_M #1003#65, 1, "S";
             |C_M #1003#66, 1, "S";
         |FINSI;

         |C_M #1003#4,  1, "N";
         |C_M #1003#6,  1, "N";
         |C_M #1003#7,  1, "N";
         |C_M #1003#10, 1, "N";
         |C_M #1003#11, 1, "N";
         |C_M #1003#12, 1, "N";
         |PDEFECTO #1003, 4, 13;
     |FINSI;
     |PINDA #0#1003;
|FPRC;

|PROCESO ControlaModelo;
     |SI #1003Campo = "   ";  |ACABA;  |FINSI;

     nCampo1 = Campo + 1;
     nCampo2 = Campo + 2;
     nCampo3 = Campo + 3;
     nCampo4 = Campo + 4;
     nCampo5 = Campo + 5;
     nCampo6 = Campo + 6;
     nCampo7 = Campo + 7;
     nCampo8 = Campo + 8;

     |C_M #1003nCampo2, 1, "S";
     |C_M #1003nCampo3, 1, "S";
     |C_M #1003nCampo6, 1, "S";
     |C_M #1003nCampo7, 1, "S";
     |C_M #1003nCampo8, 1, "S";
     |SI #1003nCampo1 [ 0;
         |C_M #1003nCampo2, 1, "N";  #1003nCampo2 = "N";
         |C_M #1003nCampo3, 1, "N";  #1003nCampo3 = "N";
                                     #1003nCampo4 = 0;
                                     #1003nCampo5 = 0;
         |C_M #1003nCampo8, 1, "N";  #1003nCampo8 = "N";
     |SINO;
         |C_M #1003nCampo6, 1, "N";  #1003nCampo6 = "N";
         |C_M #1003nCampo7, 1, "N";  #1003nCampo7 = "N";
     |FINSI;
|FPRC;

|PROCESO Calcula102;
     nCampo1 = Campo - 1;
     nCampo2 = Campo + 2;
     nCampo3 = Campo + 3;
     nCampo4 = Campo + 1;
     |SI #1003Campo = "S";
         #1003nCampo2 = #1003nCampo1 % 60;  |PINTA #1003nCampo2;
         #1003nCampo3 = #1003nCampo1 % 40;  |PINTA #1003nCampo3;
         |C_M #1003nCampo4, 1, "S";
     |SINO;
         #1003nCampo2 = 0;  |PINTA #1003nCampo2;
         #1003nCampo3 = 0;  |PINTA #1003nCampo3;
         |C_M #1003nCampo4, 1, "N"; #1003nCampo4 = "N";  |PINTA #1003nCampo4;
     |FINSI;
|FPRC;

|PROCESO Transferencia;  |TIPO 7;
     nCampo1 = Campo - 6;
     nCampo2 = Campo - 1;
     #1003Campo = "N";
     |SI #1003nCampo2 = "N";  |Y #1003nCampo1 < 0;
         #1003Campo = "S";
     |FINSI;
     |PINTA #1003Campo;
|FPRC;
