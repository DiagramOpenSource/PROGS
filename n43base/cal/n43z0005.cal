     nOrden1 = #n106der@#3;

     nOrden = 1;

     |SELECT #2#n106izq@;
     #n106izq@#0 = #n43m0102#0;
     #n106izq@#2 = "S";
     #n106izq@#3 = 9999;
     |LEE 000#n106izq@.];
     |SI FSalida = 0;
         |LEE 000#n106izq@.a;
     |SINO;
         |LEE 000#n106izq@.u;
     |FINSI;

     |SI FSalida = 0;  |Y #n106izq@#0 = #n43m0102#0;
                       |Y #n106izq@#2 = "S";
         nOrden = #n106izq@#3 + 1;
         |SI nOrden > 9999;  nOrden = 9999;  |FINSI;
     |FINSI;

