|PROCESO LeeFichero;
     |HORA aHora;
     fFecha  = ~;

     nOk     = -1;
     aFic    = aAbre - aPathPdt;
     aFicOri = aFic % 2599;  |QBF aFicOri;
     nCheq   = 1;
     nHay    = 0;
     |XABRE "", aAbre, nHandle;
     |PARA;  |SI;  |HACIENDO;
          |XLEE nHandle, 255, aLee;
          |SI FSalida [ 0;  |SAL;  |FINSI;

          |HAZ SacaDatos;
     |SIGUE;
     |XCIERRA nHandle;

     |SI nOk = -1;
         aInci = "El fichero no se ajusta al formato";
         |HAZ Graba9000;

         |HAZ CopiaFichero;

         |ACABA;
     |FINSI;

     |SI nHay = 0;
         |ACABA;
     |FINSI;

     nCheq = 0;
     |XABRE "", aAbre, nHandle;
     |PARA;  |SI;  |HACIENDO;
          |XLEE nHandle, 255, aLee;
          |SI FSalida [ 0;  |SAL;  |FINSI;

          |HAZ SacaDatos;
     |SIGUE;
     |XCIERRA nHandle;

     |HAZ CopiaFichero;
|FPRC;

|PROCESO BuscaFicheros;
     aDir = aPathPdt + "*.*";

     |_PDIR aDir;
     |PARA;  |SI FSalida = 0;  |HACIENDO;
