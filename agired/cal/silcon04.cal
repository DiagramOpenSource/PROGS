|FICHEROS;
     silcon04 #0;
|FIN;

|VARIABLES;
     modo   = 0;
     &clave = 0;
     pepe   = "";
     alfa1  = "";
|FIN;

|PROCESO mayus; |TIPO 0;
     #0Campo = #0Campo > #0Campo;
     |PINTA #0Campo;
|FPRC;

|PROCESO control; |TIPO 8;
     |CLS;
     |CABEZA "Campos Auxiliares";
     |PINPA #0#0;
     |PINDA #0#0;

     clave = 0;
     |PUSHV 1619 1853;
     |ATRI R;
     |CUADRO 1619 1853;
     |PINTA 1720,"                                 ";
     |PINTA 1720,"Clave Modificacion .:            ";
     |ATRI r;
     pepe = "<MYPASWD>";
     alfa1 = "";
     E_sup = 11;
     E_inf = "";
     alfa1 = 1742 ? -1;
     |QBF alfa1;
     |SI alfa1 ! pepe;
          |MENAV "    Clave incorrecta ...";
          clave = 1;
     |FINSI;
     |POPV;
|FPRC;

|PROCESO entra1; |TIPO 2;
     modo = ( FEntrada / 100 ) ! 0;
     |SI clave ! 0; |Y modo = 2; |O modo = 3;
          |SI modo = 2;
               |LEE 000#0=;
               |SI FSalida ! 0; |DDEFECTO #0; |FINSI;
               |PINDA #0#0;
          |FINSI;
          |MENAV "    Opcion no permitida ...";
          |ERROR;
          |PTEC 807;
     |FINSI;
|FPRC;

|PROCESO entra; |TIPO 7;
     modo = ( FEntrada / 100 ) ! 0;
     |SI clave ! 0; |Y modo = 1;
          |MENAV "    Opcion no permitida ...";
          |ERROR;
          |PTEC 807;
     |FINSI;
|FPRC;
