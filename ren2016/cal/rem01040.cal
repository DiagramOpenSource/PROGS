|FICHEROS;
     rem01040;
|FIN;

|VARIABLES;
      aEsc1            = "";
      aEsc2            = "";
      aRetu            = "";
      aTecla           = "";

     &enEmpresa        = 0;
     &enComplementaria = 0;
     &eaContri         = "";
|FIN;

|PROGRAMA;
     |ABRET #rem01040;

     |PINPA #0#rem01040;

     #rem01040#0 = enEmpresa;
     #rem01040#1 = enComplementaria;
     #rem01040#2 = eaContri;
     |LEE 000#rem01040.=;
     |SI FSalida ! 0;  |DDEFECTO #rem01040;  |FINSI;

     |PINDA #0#rem01040;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
          FSalida = "::{{001}}Salir";
          |LEETECLA aTecla;
          |SI aTecla = aEsc1; |SAL;  |FINSI;
          |SI aTecla = aEsc2; |SAL;  |FINSI;
          |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;

     |CIERRAT #rem01040;
|FPRO;
