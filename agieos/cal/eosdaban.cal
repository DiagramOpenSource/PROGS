|FICHEROS;
     eosdaban;
|FIN;

|VARIABLES;
     aAlfa       = "";
     aID         = "";

     nID         = 0;

     {10} aPopup = "";

     &eaCta      = "";
     &eaPais     = "";
     &eaIBAN     = "";
|FIN;

|PROCESO Tipo7IBAN_Fdaban;  |TIPO 7;
     aAlfa = #eosdaban#7;   |QBF aAlfa;

     |SI aAlfa ! "";  |ACABA;  |FINSI;

     eaCta  = #eosdaban#2 + #eosdaban#3 + #eosdaban#4 + #eosdaban#5;
     eaPais = #eosdaban#13;
     |HAZ CalcuIBAN;

     #eosdaban#7 = eaIBAN;
     |PINTA #eosdaban#7;
|FPRC;

|PROCESO Tipo0IBAN_Fdaban;  |TIPO 0;
     eaCta  = #eosdaban#2 + #eosdaban#3 + #eosdaban#4 + #eosdaban#5;
     eaPais = #eosdaban#13;
     |HAZ CheqIBAN;

     aAlfa = #eosdaban#7;    |QBF aAlfa;
     |SI aAlfa ! eaIBAN;
         |CONFI "0000SEl codigo IBAN puede ser erroneo, quiere calcularlo.";
         |SI FSalida = 0;
             #eosdaban#7 = eaIBAN;
             |PINTA #eosdaban#7;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo66C14;  |TIPO 66;
     aAlfa = "|C014WID";
     aID   = #eosdaban#aAlfa#;
     nID   = aID;

     aPopup1  = nID;  || Si fuera -1 en la posicion
     aPopup2  = 6;
     aPopup3  = "1.- Efectivo";
     aPopup4  = "2.- Adeudo en cuenta";
     aPopup5  = "3.- Domiciliacion";
     aPopup6  = "4.- Aplazamiento";
     aPopup7  = "5.- Aplazamiento + Ingreso";
     aPopup8  = "6.- Cuenta corriente tributaria (Ingreso y Devolucion)";

     IaPopup  = aPopup1  <-;
     |ESPECIFICA "TRACKPOPUP", aPopup;
     |SI FSalida ! 0;
         IaPopup  = aPopup3  <-;
         IaPopup  = IaPopup + (FSalida - 1);

         aAlfa = aPopup % 101;  #eosdaban#Campo# = aAlfa;
     |FINSI;
     |PINTA #eosdaban#Campo#;
|FPRC;
