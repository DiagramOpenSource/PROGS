|FICHEROS;
   dsmow116;
|FIN;

|VARIABLES;
   aAlfa  = "";
   aAlfa1 = "";
|FIN;

|PROCESO Tipo12w116;  |TIPO 12;
  |SI #dsmow116#3 = "S"; #dsmow116#4 = "S"; |FINSI;
  |SI #dsmow116#3 = "N"; #dsmow116#4 = " "; |FINSI;
|FPRC;

|PROCESO EsComplem;
  aAlfa = Contenido;

  |SI aAlfa ! #dsmow116#3;  ||solo si cambia
      |SI aAlfa = "N";
          aAlfa1 = "    SEsta Seguro de Crear una Complementaria";
      |SINO;
          aAlfa1 = "    SEsta Seguro de borrar los datos de Complementaria";
      |FINSI;
      |CONFI aAlfa1;
      |SI FSalida ! 0;
          #dsmow116#3 = aAlfa;
          |PINTA #dsmow116#3;
          |ERROR;
          |ACABA;
      |SINO;
          |MENAV "    Se ha producido un cambio en el Modelo. Entre datos para que Recalcule el Registro";
      |FINSI;
      |SI aAlfa = "N";
          |SI #dsmow116#5 = "X";
              aAlfa1 = "    SDejar el Modelo como No Impreso y No Archivado";
              |SI #dsmow116#6 ! "X";
                  aAlfa1 = "    SDejar el Modelo como No Impreso";
              |FINSI;
              |CONFI aAlfa1;
              |SI FSalida = 0;
                  #dsmow116#5 = " ";
                  #dsmow116#6 = " ";
              |FINSI;
          |FINSI;
      |FINSI;
  |FINSI;

  aAlfa = #dsmow116#3;
  |SI #dsmow116#3 = "N";
      #dsmow116#0 = ""; |PINTA #dsmow116#0;
      #dsmow116#1 = ""; |PINTA #dsmow116#1;
      #dsmow116#2 = 0;  |PINTA #dsmow116#2;
  |FINSI;
  |C_M #dsmow116#0,1,aAlfa;
  |C_M #dsmow116#1,1,aAlfa;
|FPRC;
