|FICHEROS;
     agrm0008 #8;         || Explotaciones
     agrm0010 #10;        || Ganados
|FIN;

|VARIABLES;
     &VEmpresa  = 0;
     &VAnyo     = 0;
     &VVGanados = 0;
     &Modo      = 0;
|FIN;

|RUTINA ClaveBaja10;
#10#0 = VEmpresa;
#10#1 = VAnyo;
#10#2 = 1;
|FRUT;

|RUTINA ClaveAlta10;
#10#0 = VEmpresa;
#10#1 = VAnyo;
#10#2 = 999;
|FRUT;

|PROGRAMA;
|SI Modo = 1; |ENTLINEAL #1#10, 3, 1, 22, 1, ClaveBaja10, ClaveAlta10; |FINSI;
|SI Modo = 2; |ENTLINEAL #1#10, 3, 2, 22, 1, ClaveBaja10, ClaveAlta10; |FINSI;
|SI Modo = 4; |ENTLINEAL #1#10, 3, 4, 22, 1, ClaveBaja10, ClaveAlta10; |FINSI;
|FPRO;

|PROCESO Explotacion; |TIPO 0;
|SI #10#7 = "            "; |ACABA; |FINSI;

|ABRE #8;
#8#0 = #10#0;
#8#1 = #10#1;
#8#2 = #10#7;
|LEE 040#8=;
|SI FSalida ! 0;
    |MENAV "       No existe C¢digo de Explotaci¢n ";
    |ERROR;
|FINSI;
|CIERRA #8;
|FPRC;

|PROCESO Tipo2; |TIPO 2;
VVGanados = VVGanados +. #10#11;
|FPRC;
