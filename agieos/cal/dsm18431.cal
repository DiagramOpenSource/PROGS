        |SINO;
            |C_M #dsm18431#7, 1,"N";
            |SI #dsm18431#1 < 2019;
                |C_M #dsm18431#16,1,"N";
                |C_M #dsm18431#17,1,"N";
                |C_M #dsm18431#18,1,"N";
                |C_M #dsm18431#19,1,"N";
                |C_M #dsm18431#20,1,"N";
                |C_M #dsm18431#21,1,"N";
                |C_M #dsm18431#22,1,"N";
            |FINSI;
        |FINSI;

        |SI #dsm18431#4 ! "ES";
            |C_M #dsm18431#10,1,"S";
        |FINSI;
     |FINSI;
     |SI enRegimen ! 1;
