    |SINO;
        |C_M #dsm18431#6,1,"S";
        |C_M #dsm18431#8,1,"N";

        |SI #dsm18431#1 < 2015;
            |C_M #dsm18431#7,1,"S";
            |C_M #dsm18431#11,1,"N";
            |C_M #dsm18431#12,1,"N";
            |C_M #dsm18431#13,1,"N";
            |C_M #dsm18431#14,1,"N";
