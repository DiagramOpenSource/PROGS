|FPRC;

|PROCESO recupera; |TIPO 0;
|POPV;
|FPRC;

|PROCESO mira_lib; |TIPO 0;
|POPV;
|SI #0#3  = 1;
    |SI #0#2 = "D";
        #0#5  = "libdescd";
        #0#6  = "libdespd";
        #0#88 = "libdessd";
    |SINO;
        #0#5  = "libdescl";
        #0#6  = "libdespr";
        #0#88 = "libdessu";
    |FINSI;
|FINSI;

|SI #0#3  = 2;
    |SI #0#2 = "D";
        #0#5  = "libdnscd";
        #0#6  = "libdnspd";
        #0#88 = "libdnssd";
    |SINO;
        #0#5  = "libdnscl";
        #0#6  = "libdnspr";
        #0#88 = "libdnssu";
    |FINSI;
|FINSI;

|C_M #0#5, 1, "N";
|C_M #0#6, 1, "N";
|C_M #0#88, 1, "N";

|SI #0#3  = 3;
    |C_M #0#5,  1, "S";
    |C_M #0#6,  1, "S";
    |C_M #0#88, 1, "S";
|FINSI;

|PINTA #0#5;
|PINTA #0#6;
|PINTA #0#88;
|FPRC;

|PROCESO mira_iva; |TIPO 0;
|SI #0#11 = 1;
    |SI #0#10 = "D";
        #0#12 = "eosivac5";
    |SINO;
        #0#12 = "eosivac3";
    |FINSI;
|FINSI;

|SI #0#11 = 2;
    |SI #0#10 = "D";
        #0#12 = "eosivac4";
    |SINO;
        #0#12 = "eosivac1";
    |FINSI;
|FINSI;

|SI #0#11 ! 3; |Y #0#95 = "S";
    alfa1 = #0#12 % -101;
    #0#12 = "eosivad" + alfa1;
|FINSI;

|C_M #0#12, 1, "N";
|SI #0#11 = 3;
    |C_M #0#12, 1, "S";
|FINSI;

|PINTA #0#12;
|FPRC;

|PROCESO filtro_255; |TIPO 0;
|SI #0Campo > 255; |Y #0Campo ! 999;
    |MENAV "     Introduzca Valor de 0-255, o bien 999 para anular";
    |ERROR;
|FINSI;
|FPRC;

