|PROCESO mira_exist; |TIPO 0;      || lee existencias (cabs.)
    inkey = FSalida;
    modo = (FEntrada / 100) ! 0;
|SI inkey ! 2;|Y modo ! 4;
    |ABRE #2;
        #2#0 = #0#0;    || empresa
        #2#1 = #0#1;    || actividad
        #2#2 = #0#2;    || a¤o
    |LEE 000#2=;
    |SI FSalida ! 0;
        |MENAV "    ATENCION: Control de existencias INEXISTENTE ...";
        |ERROR;
    |SINO;
        |PARA para1 = 7; |SI para1 [ 10; |HACIENDO para1 = para1 + 1;
            |SI #2para1 ! 0;
                    #0#3 = #2para1;
            |FINSI;
        |SIGUE;
        |PINTA #0#3;
    |FINSI;
    |CIERRA #2;
|FINSI;
|FPRC;

|PROCESO importe; |TIPO 0;         || calcula importe (lins.)
    nume1 = Contenido;
|SI nume1 ! #1Campo;
        #1#7 = #1#5 * #1#6;
    |PINTA #1#7;
|FINSI;
|FPRC;

|PROCESO tipo2; |TIPO 2;      || calcula acumulado (cabs.)
    #0#4 = #0#4 +. #1#7;      || total inventario
    #0#5 = #0#3 - #0#4;       || diferencia
|PINTA #0#4;
