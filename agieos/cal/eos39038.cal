|FICHEROS;
     eos39038 #0;
|FIN;

|VARIABLES;
     campo1 = 0;
     campo2 = 0;
     campo3 = 0;
|FIN;

|PROCESO antes; |TIPO 7;
  campo1 = Campo;
  campo2 = campo1 + 1;
  campo3 = campo1 + 2;

|SI #0campo2 = 0;
    |C_V #0campo1, 1, "N";  |C_M #0campo1, 1, "N";
    |C_V #0campo2, 1, "N";  |C_M #0campo2, 1, "N";
    |C_V #0campo3, 1, "N";  |C_M #0campo3, 1, "N";
|SINO;
    |C_V #0campo1, 1, "S";  |C_M #0campo1, 1, "S";
    |C_V #0campo2, 1, "S";
    |C_V #0campo3, 1, "S";  |C_M #0campo3, 1, "S";

    |SI #0campo2 = 99.99;
        |C_V #0campo2, 1, "N";
    |FINSI;
|FINSI;
|FPRC;

|PROCESO cuotas; |TIPO 0;
  campo1 = Campo;
  campo2 = campo1 + 1;
  campo3 = campo1 + 2;

  |SI #0Campo  = Contenido; |ACABA; |FINSI;
  |SI #0campo2 = 99.99;     |ACABA; |FINSI;

  #0campo3 = #0campo1 % #0campo2;
  |PINTA #0campo3;
|FPRC;
