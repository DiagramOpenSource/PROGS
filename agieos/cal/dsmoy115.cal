|FICHEROS;
     eosy0010;
     dsmoy000;
|FIN;

|VARIABLES;
|FIN;

|INCLUYE i_varemi;

|PROGRAMA;
     aFichero = "eosy0010" + Usuario;
     |NOME_DAT #eosy0010#aFichero#;

     |ABRE #eosy0010;
     |LEE 000#eosy0010.p;
     |CIERRA #eosy0010;

     |HAZ LeeUnidad;
     eaModelos  = "115";
     eaEjer     = #eosy0010#40;
     enPeriodo  = #eosy0010#39;
     |SI #eosy0010#47 = "T";
         |SI #eosy0010#39 = 1; eaPeriodo = "1T"; |FINSI;
         |SI #eosy0010#39 = 2; eaPeriodo = "2T"; |FINSI;
         |SI #eosy0010#39 = 3; eaPeriodo = "3T"; |FINSI;
         |SI #eosy0010#39 = 4; eaPeriodo = "4T"; |FINSI;
         #eosy0010#38 = #eosy0010#39;
         #eosy0010#39 = 0;
     |SINO;
         eaPeriodo  = ("00" + enPeriodo) % -102;
         #eosy0010#38 = 0;
     |FINSI;

     #dsmoy000#0 = eaModelos;
     #dsmoy000#6 = eaEjer;

     |HAZ eVersionMod;

     |SI #dsmoy000#6 [ 2016;  |HAZ moy11502;  |FINSI;
     |SI #dsmoy000#6 ] 2017;  |HAZ moy11503;  |FINSI;
|FPRO;
