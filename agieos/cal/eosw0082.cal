|FICHEROS;
     eosw0082 #82,MANTE;
     eoslinve #2;
     eoslinco #3;
     eosactiv #5;
|FIN;

|VARIABLES;
     &nSwDeDonde = 0;
|FIN;

|PROCESO Actividad;
     |SI #82Campo = 0;  |ACABA;  |FINSI;

     |ABRE #5;
     |SELECT #1#5;
     |SI nSwDeDonde = 1;
         #5#0 = #2#0;
         #5#2 = #2#14;
     |SINO;
         #5#0 = #3#0;
         #5#2 = #3#14;
     |FINSI;
     #5#1 = #82Campo;
     |LEE 040#5=;
     |SI FSalida = 0;
         |SI #5#15 = "N";
             |MENAV "     La Actividad no esta preparada para hacer libros";
             |ERROR;
         |FINSI;
         |SI #5#14 = "S";
             |MENAV "     La Actividad es un Comunero";
             |ERROR;
         |FINSI;
     |SINO;
         |MENAV "    Error no existe Actividad";
         |ERROR;
     |FINSI;
     |CIERRA #5;
|FPRC;
