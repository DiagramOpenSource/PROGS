|PROCESO Graba99;
  |SI Tipo = 2;  |Y #99#3 = 2;
      |ABRE #7;
      #7#0 = #99#0;
      #7#4 = 97;
      |LEE 040#7=;
      |SI FSalida ! 0;
          |MENAV "      No existe libros ventas para realizar el pase.";
          Tipo = 0;
      |FINSI;
      |CIERRA #7;
  |FINSI;

  |SI Tipo = 2;  |Y #99#3 = 4;
      |ABRE #6;
      #6#0 = #99#0;
      #6#4 = 97;
      |LEE 040#6=;
      |SI FSalida ! 0;
          |MENAV "      No existe libros compras para realizar el pase.";
          Tipo = 0;
      |FINSI;
      |CIERRA #6;
  |FINSI;

  |LIBERA #99;
  |LEE 140#99=;
  |SI FSalida = 0;  #99#7 = Tipo;  |GRABA 140#99a;  |FINSI;

  |SI Tipo = 0;  |ERROR;  |FINSI;
|FPRC;

|PROCESO PulsaTecla;  |TIPO 11;
  Tecla = FSalida;
  |SI #99#3 = 1;
      |SI Tecla = 9;  Tipo = 1;  |HAZ Graba99;  |FINSI;
  |FINSI;

  |SI #99#3 = 2;
