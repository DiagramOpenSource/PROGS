|FICHEROS;
     eos3905c #0;
     eosactiv #1;
     eosagga1 #2;
|FIN;

|VARIABLES;
     nCampo1 = 0;
     nCampo2 = 0;
     nCampo3 = 0;
     nCampo4 = 0;
     nCampo5 = 0;
|FIN;

|PROCESO Activi5c;  |TIPO 0;
     nCampo1 = Campo   - 1;
     nCampo2 = Campo;
     nCampo3 = nCampo1 + 2;
     nCampo4 = nCampo1 + 4;

     |SI #0nCampo1 = " "; |Y #0nCampo2 = "    "; |ACABA; |FINSI;

     |ABRE #1;
     |SELECT #2#1;
     #1#0 = #0#40;
     #1#4 = #0nCampo1;
     #1#5 = #0nCampo2;
     #1#2 = #0#41;
     |LEE 040#1=;
     |SI FSalida ! 0;
         |MENAV "      No existe esta actividad en la empresa";
     |FINSI;
     |CIERRA #1;

     |ABRE #2;
     #2#0 = #0nCampo1;
     #2#1 = #0nCampo2;
     |LEE 040#2=;
     |SI FSalida ! 0;
         |MENAV "      No existe el epigrafe el el fichero de Agricolas y Ganaderos";
     |FINSI;
     |CIERRA #2;

     |SI #0Campo = Contenido; |ACABA; |FINSI;

     #0nCampo3  = #2#2;
     #0nCampo4  = #2#4;
     |PINTA #0nCampo3;
     |PINTA #0nCampo4;
|FPRC;

|PROCESO TotalAgri;
     |PARA nCampo1 = 3;  |SI nCampo1 [ 35;  |HACIENDO nCampo1 = nCampo1 + 8;
           nCampo2 = nCampo1 + 1;
           nCampo3 = nCampo1 + 2;
           nCampo4 = nCampo1 + 3;
           nCampo5 = nCampo1 + 4;

           #0nCampo3 = #0nCampo1 * #0nCampo2;   |PINTA #0nCampo3;
           #0nCampo5 = #0nCampo3 - #0nCampo4;   |PINTA #0nCampo5;
     |SIGUE;
|FPRC;
