|FICHEROS;
   eoslisco #0;
   eoscomun #1;
   eoscomul #2;
   eosactiv #3;
|FIN;

|VARIABLES;
    sw      = 0;
    informe = "eoslisco";
    &swlin  = 0;
|FIN;

|INCLUYE i_consul;
|INCLUYE i_anyode;

|PROCESO pinta_anyo; |TIPO 7;
|HAZ i_anyodef;
#0#2 = i_anyodef;
|PINTA #0#2;
|FPRC;

|PROCESO impresi;
#3#0 = #2#3;
#3#1 = #2#5;
#3#2 = #2#7;
|LEE 040#3=;
|SI FSalida ! 0; |DDEFECTO #3; |FINSI;
swlin = 1; |PRINT;
|FPRC;

|PROCESO lineascom;
sw = 1;

#3#0 = #1#0;
#3#1 = #1#1;
#3#2 = #1#3;
|LEE 040#3=;
|SI FSalida ! 0; |DDEFECTO #3; |FINSI;
swlin = 0; |PRINT;

|BUCLELIN 0impresi#1;

swlin = 2; |PRINT;
|FPRC;

|DEFBUCLE eoslisco0;
#1#1;
;
#0#0, 1, #0#2;
#0#1, 6, #0#2;
e;
NULL,lineascom;
|FIN;

|PROCESO impre; |TIPO 3;
sw = 0;
|IMPRE 0;
|SI FSalida = 0;
    |INFOR informe;
    |SI FSalida = 0;
       |ABRE #3;
       |BUCLE eoslisco0;
       |SI sw = 1; |PIEINF; |FINSI;
       |FININF;
       |CIERRA #3;
    |SINO;
       |MENAV "     No existe Informe";
    |FINSI;
    |FINIMP;
|SINO;
   |MENAV "      Impresion Cancelada";
|FINSI;
|FPRC;
