|FICHEROS;
     eosda225 #0;
     eosirpf2 #1;
     eosclien #2;
     eospanta #3;
     eosirpf1 #4;
|FIN;

|VARIABLES;
  CampoFecha  = 0;
  CampoArchiv = 0;
  MiraSiHay   = 0;
|FIN;

|INCLUYE i_consul;

|PROCESO Mayus;
  #0Campo = #0Campo > #0Campo;
  |PINTA #0Campo;
|FPRC;

|PROCESO TipoModelo;
  |SI #0Campo = "0";
      |C_M #0#7, 1, "N";
      #0#7 = "T";
  |SINO;
      |C_M #0#7, 1, "S";
  |FINSI;
|FPRC;

|PROCESO Periodo;
  |SI #0#7 = "T";
      |SI #0#4 > 4;
          |MENAV "           Periodo Trimestral";
          |ERROR;
      |FINSI;
  |FINSI;
|FPRC;

|PROCESO MiraSiHay;
  MiraSiHay = 1;
  |ERROR10;
|FPRC;

|DEFBUCLE eosirpf1;
  #4#1;
  135;
  #2#0, 00000, #0#2, #0#6;
  #2#0, 99999, #0#2, #0#6;
  ;
  NULL, MiraSiHay;
|FIN;

|PROCESO Chequeo;
  MiraSiHay = 0;
  |BUCLE eosirpf1;
  |SI MiraSiHay = 0;  |ACABA;  |FINSI;

  #1#0  = #2#0;
  #1#38 = #0#2;
  #1#39 = #0#6;
  |LIBERA #1;
  |LEE 140#1=;
  |SI FSalida = 0;
      |SI #0#7 = "T";
          |SI #0#4 = 1;   CampoFecha = 26;  CampoArchiv = 40;  |FINSI;
          |SI #0#4 = 2;   CampoFecha = 29;  CampoArchiv = 43;  |FINSI;
          |SI #0#4 = 3;   CampoFecha = 32;  CampoArchiv = 46;  |FINSI;
          |SI #0#4 = 4;   CampoFecha = 35;  CampoArchiv = 49;  |FINSI;
      |SINO;
          CampoFecha  = 25 + #0#4;
          CampoArchiv = 39 + #0#4;
      |FINSI;

      |SI #1CampoFecha ! "          ";  |Y #1CampoArchiv = "S";
          #1CampoArchiv = "N";
          |GRABA 140#1a;
      |FINSI;
  |FINSI;
|FPRC;

|DEFBUCLE eosclien;
  #2#1;
  ;
  #0#0;
  #0#1;
  ;
  NULL, Chequeo;
|FIN;

|PROCESO impre;|TIPO 3;
  |ABRE #1;
  |BUCLE eosclien;
  |CIERRA #1;
|FPRC;
