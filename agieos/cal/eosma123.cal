|FICHEROS;
   eosma123 #0;
   eosdaban #1;
|FIN;

|PROCESO MiraSiImpreso; |TIPO 7;
   |SI #eosma123#27 = "S";
      |ATRI R;
      |CONFI "0400NModelo ya emitido. ¨ Modificar ?";
      |SI FSalida = 0;
         |ATRI N;
         #eosma123#27 = "N";
         #eosma123#28 = "";
      |SINO;
         |ATRI N;
         |PTEC 807;
      |FINSI;
   |FINSI;
|FPRC;

|PROCESO CalculaTotal;
   #eosma123#26 = #eosma123#17 + #eosma123#20 - #eosma123#25;
   |SI #eosma123#26 < 0; #eosma123#26 = 0; |FINSI;

   |PINTA #eosma123#26;
|FPRC;

|PROCESO MiraCuenta;
   |SI #eosma123#30 = "E";
      #eosma123#31 = ""; |PINTA #eosma123#31;
      #eosma123#32 = ""; |PINTA #eosma123#32;
      #eosma123#33 = ""; |PINTA #eosma123#33;
      #eosma123#34 = ""; |PINTA #eosma123#34;
      #eosma123#35 = ""; |PINTA #eosma123#35;
   |SINO;
      |SI #eosma123#31 = "    "; |O #eosma123#32 = "    ";
       |O #eosma123#33 = "  ";   |O #eosma123#34 = "          ";
          |ABRE #eosdaban;
          #eosdaban#0 = #eosma123#0;
          |LEE 000#eosdaban.=;
          |SI FSalida = 0;
             #eosma123#31 = #eosdaban#2; |PINTA #eosma123#31;
             #eosma123#32 = #eosdaban#3; |PINTA #eosma123#32;
             #eosma123#33 = #eosdaban#4; |PINTA #eosma123#33;
             #eosma123#34 = #eosdaban#5; |PINTA #eosma123#34;
             #eosma123#35 = #eosdaban#1; |PINTA #eosma123#35;
          |FINSI;
          |CIERRA #eosdaban;
      |FINSI;
   |FINSI;
|FPRC;
