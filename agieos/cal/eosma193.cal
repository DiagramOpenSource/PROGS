|FICHEROS;
   eosma193 #0;
   eosop193 #1,MANTE;
   eospc193 #2,MANTE;
|FIN;

|VARIABLES;
   &eaVarDni  = "";
   &enCalcCif = 0;
|FIN;

|RUTINA inf;
   #eosop193#0 = #eosma193#0;
   #eosop193#1 = #eosma193#2;
   #eosop193#2 = "A";
   #eosop193#3 = 1;
   #eosop193#4 = 1;
|FRUT;

|RUTINA sup;
   #eosop193#0 = #eosma193#0;
   #eosop193#1 = #eosma193#2;
   #eosop193#2 = "C";
   #eosop193#3 = 5;
   #eosop193#4 = 2;
|FRUT;

|PROCESO Operaciones; |TIPO 7;
   |PUSHV 11012380;
   |ENTLINEAL #1#1,3,1,16,1,inf,sup;
   |POPV;
   |PINTA #eosma193#4;
   |PINTA #eosma193#5;
   |PINTA #eosma193#7;
|FPRC;

|RUTINA inf1;
   #eospc193#0 = #eosop193#0;
   #eospc193#1 = #eosop193#1;
   #eospc193#2 = #eosop193#2;
   #eospc193#3 = #eosop193#3;
   #eospc193#4 = #eosop193#4;
   #eospc193#5 = "            ";
|FRUT;

|RUTINA sup1;
   #eospc193#0 = #eosop193#0;
   #eospc193#1 = #eosop193#1;
   #eospc193#2 = #eosop193#2;
   #eospc193#3 = #eosop193#3;
   #eospc193#4 = #eosop193#4;
   #eospc193#5 = "zzzzzzzzzzzz";
|FRUT;

|PROCESO Perceptores; |TIPO 7;
   |PUSHV 18012380;
   |ENTLINEAL #1#2,6,1,22,1,inf1,sup1;
   |POPV;
|FPRC;

|PROCESO NumPerceps; |TIPO 2;
   #eosop193#14 = #eosop193#14 +. 1;
   |PINTA #eosop193#14;
   #eosma193#3 = #eosma193#3 +. 1;
   #eosma193#4 = #eosma193#4 +. #eosop193#11;
   #eosma193#5 = #eosma193#5 +. #eosop193#13;
   |SI #eosop193#2 = "A"; |O #eosop193#2 = "B";
      |SI #eosop193#4 = 1;
         #eosma193#7 = #eosma193#7 +. #eosop193#13;
      |FINSI;
   |SINO;
      #eosma193#7 = #eosma193#7 +. #eosop193#13;
   |FINSI;
|FPRC;

|PROCESO Calcula;
   #eosop193#13 = #eosop193#11 % #eosop193#12;
   |PINTA #eosop193#13;
|FPRC;

|PROCESO LetraDni1;
   |SI FSalida ! 9; |ACABA; |FINSI;

   eaVarDni  = #1#Campo#;
   enCalcCif = 1;
   |HAZ CalculoDNI;
   #1#Campo# = eaVarDni;
   |PINTA #1#Campo#;
|FPRC;

|PROCESO LetraDni2; |TIPO 11;
   |SI FSalida ! 9; |ACABA; |FINSI;

   eaVarDni  = #2#Campo#;
   enCalcCif = 1;
   |HAZ CalculoDNI;
   #2#Campo# = eaVarDni;
   |PINTA #2#Campo#;
|FPRC;
