|FICHEROS;
     dsmow220;
     dsm30301;

     eosactiv;
|FIN;

|VARIABLES;
     nCampo1 = 0;
     nCampo2 = 0;
     nCampo3 = 0;

     aAlfa   = "";
|FIN;

|PROCESO Tipo12Fw220;  |TIPO 12;
|FPRC;

|PROCESO Baja14_220;
     #eosactiv#0 = #dsm30301#0;
     #eosactiv#1 = 1;
|FPRC;

|PROCESO Alta14_220;
     #eosactiv#0 = #dsm30301#0;
     #eosactiv#1 = 99;
|FPRC;

|PROCESO Tipo66Act_220;  |TIPO 66;
     |ABRE #eosactiv;
     |CONSULTA_F_CLAVE #1#eosactiv, Baja14_220, Alta14_220;
     |SI FSalida = 0;
         #dsmow220#Campo# = #eosactiv#1;  |PINTA #dsmow220#Campo#
     |FINSI;
     |CIERRA #eosactiv;
|FPRC;


|PROCESO Tipo0Act_220;  |TIPO 0;
     nCampo1 = Campo + 1;
     nCampo2 = Campo + 2;
     nCampo3 = Campo + 3;

     |C_M #dsmow220#nCampo3#, 1, "S";

     |SI #dsmow220#Campo# = 0;
         #dsmow220#nCampo1# = "";  |PINTA #dsmow220#nCampo1#;
         #dsmow220#nCampo2# = "";  |PINTA #dsmow220#nCampo2#;
         #dsmow220#nCampo3# = 0;   |PINTA #dsmow220#nCampo3#;

         |C_M #dsmow220#nCampo3#, 1, "N";

         |ACABA;
     |FINSI;

     |ABRE #eosactiv;
     #eosactiv#0 = #dsm30301#0;
     #eosactiv#1 = #dsmow220#Campo#;
     |LEE 000#eosactiv.=;
     |SI FSalida ! 0;
         |MENAV "     No existe Actividad";
         |ERROR;
     |FINSI;
     |CIERRA #eosactiv;

     #dsmow220#nCampo1# = #eosactiv#5;   |PINTA #dsmow220#nCampo1#;
     #dsmow220#nCampo2# = #eosactiv#6;   |PINTA #dsmow220#nCampo2#;
     #dsmow220#nCampo3# = #eosactiv#43;  |PINTA #dsmow220#nCampo3#;
|FPRC;
