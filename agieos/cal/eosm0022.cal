|VARIABLES;
     Tecla        = 0;
     SwEstar      = 0;
     XPara        = 0;
     Linea        = 0;
     nAnyo        = 0;

     &MAnyo       = 0;
     &LEpigra     = "";
     &NEpigra     = "";
     &TEpigra     = "";
|FIN;

|PROCESO LeeElepi;  |TIPO 0;
  Tecla = FSalida;

  |ABRE #2;
  #2#0  = LEpigra;
  #2#1  = NEpigra;
  #2#41 = TEpigra;
  |SI MAnyo ] 80;
      #2#2  = 1900 + MAnyo;    || Anyo
  |SINO;
      #2#2  = 2000 + MAnyo;    || Anyo
  |FINSI;
  |LEE 040#2=;
  |SI FSalida ! 0;  |DDEFECTO #2;  |FINSI;
  |CIERRA #2;

  |SI Tecla = 9;
      |PUSHV 0501 2380;
      |PINPA #0#2;
      |PINDA #0#2;
      |PAUSA;
      |POPV;
      |ERROR;
      |ACABA;
  |FINSI;

  |ABRE #1;

  SwEstar = 0;
  Linea   = 0;
  |PARA XPara = 4;  |SI XPara [ 36;  |HACIENDO XPara = XPara + 4;
        Linea = Linea + 1;
        |SI #0#4 = #2XPara;
            #1#0 = #2XPara;
            |LEE 040#1=;
            |SI FSalida = 0; |Y #1#3 = "N";
                SwEstar = 1;
                |SAL;
            |FINSI;
        |FINSI;
  |SIGUE;
  |CIERRA #1;

  |SI SwEstar = 0;
      |MENAV "      No existe el concepto en el elemento epigrafe o es de Empleo o Loterias";
      |ERROR;
      |ACABA;
  |FINSI;

  #0#13 = Linea;
|FPRC;

|PROCESO TUnidad;
  nAnyo = 365;
  |SI #0#1 = 4;  |O #0#1 = 8;  |O #0#1 = 12;
       |O #0#1 = 16; |O #0#1 = 20; |O #0#1 = 24;
         |O #0#1 = 28; |O #0#1 = 32;
      nAnyo = 366;
  |FINSI;

  #0#9  = (#0#07 * #0#08) / nAnyo;   |PINTA #0#9;
  #0#12 = (#0#10 * #0#11) / nAnyo;   |PINTA #0#12;
|FPRC;
