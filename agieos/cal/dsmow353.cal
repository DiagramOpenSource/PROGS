|FICHEROS;
   dsmow353 #0,MANTE;
|FIN;

|VARIABLES;
   nCont = 0;
|FIN;

|PROCESO Tipo12w353; |TIPO 12;
|FPRC;

|PROCESO SaltaResto;
   |SI #dsmow353#Campo# = 0;
      |PARA nCont = Campo; |SI nCont [ 22; |HACIENDO nCont = nCont + 1;
         |C_M #dsmow353#nCont#, 1, "N";
      |SIGUE;
   |FINSI;
|FPRC;

|PROCESO Seleccion;
   |SI #dsmow353#0 = "S";
      |C_M #dsmow353#1, 1, "N"; #dsmow353#1 = "00000"; |PINTA #dsmow353#1;
      |C_M #dsmow353#2, 1, "N"; #dsmow353#2 = "99999"; |PINTA #dsmow353#2;
      |PARA nCont = 3; |SI nCont [ 22; |HACIENDO nCont = nCont + 1;
         |C_M #dsmow353#nCont#, 1, "S";
      |SIGUE;
   |SINO;
      |C_M #dsmow353#1, 1, "S"; |C_M #dsmow353#2, 1, "S";
      |PARA nCont = 3; |SI nCont [ 22; |HACIENDO nCont = nCont + 1;
         |C_M #dsmow353#nCont#, 1, "N"; #dsmow353#nCont# = "00000"; |PINTA #dsmow353#nCont#;
      |SIGUE;
   |FINSI;
|FPRC;
