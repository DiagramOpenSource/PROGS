|PROCESO Tipo3;
   |IMPRE 0;
   |SI FSalida = 0;
      |SI #0#13 < 2015;
          |INFOR "dsz18402";
      |SINO;
          |INFOR "dsz58402";
      |FINSI;
      |SI FSalida = 0;
         |SI #dsz18402#0 = "N";
            nDEmpresa = #dsz18402#1; nHEmpresa = #dsz18402#2;
            |BUCLE ElGranInforme;
         |SINO;
            |PARA nCont = 3; |SI nCont [ 12; |HACIENDO nCont = nCont + 1;
               nDEmpresa = #dsz18402#nCont#; nHEmpresa = #dsz18402#nCont#;
               |BUCLE ElGranInforme;
            |SIGUE;
         |FINSI;
      |FINSI;
   |FINSI;
|FPRC;

|PROCESO Selec;
   |SI #dsz18402#0 = "N";
      |C_M #dsz18402#1, 1, "S";
      |C_M #dsz18402#2, 1, "S";
      |PARA nCont = 3; |SI nCont [ 12; |HACIENDO nCont = nCont + 1;
         #dsz18402#nCont# = 0; |C_M #dsz18402#nCont#, 1, "N"; |PINTA #dsz18402#nCont#;
      |SIGUE;
   |SINO;
      |C_M #dsz18402#1, 1, "N"; #dsz18402#1 = 0; |PINTA #dsz18402#1;
      |C_M #dsz18402#2, 1, "N"; #dsz18402#2 = 0; |PINTA #dsz18402#2;
      |PARA nCont = 3; |SI nCont [ 12; |HACIENDO nCont = nCont + 1;
         |C_M #dsz18402#nCont#, 1, "S";
      |SIGUE;
   |FINSI;
|FPRC;

