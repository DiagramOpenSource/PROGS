|PROCESO lee_inventario2;
    seleccio = 1;
|PRINT;
|FPRC;

|PROCESO lee_inventario1;
    #3#0 = #1#0;
|LEE 000#3=;             || lee eosclien
|SI FSalida = 0;
        #4#0 = #1#0;
        #4#1 = #1#1;
        #4#2 = #1#2;
    |LEE 000#4=;         || lee eosactiv
    |SI FSalida = 0;
            seleccio = 0;
        |INFOR "eosinvin";
        |BUCLELIN 2lee_inventario2#1;
        |SI seleccio ! 0;
            |PIEINF;
        |FINSI;
        |FININF;
    |SINO;
            alfa1 = "    Actividad [" + #1#0 + "-" + #1#1 + "-" + #1#2 + "] INEXISTENTE ...";
        |MENAV alfa1;
    |FINSI;
|SINO;
        alfa1 = "    Empresa [" + #1#0 + "] INEXISTENTE ...";
    |MENAV alfa1;
|FINSI;
|FPRC;

