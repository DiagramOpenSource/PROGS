|FICHEROS;
  eoscomun #0;
  eosactiv #2;
  eoscomul #3;

  eosclien #1;
|FIN;

|VARIABLES;
|FIN;

|PROCESO activida; |TIPO 6;
     |ABRE #2;
     #2#0 = #0#0;
     #2#1 = #0#1;
     #2#2 = #0#3;
     |LEE 040#2=;
     |SI FSalida ! 0;
         |MENAV "     No existe Actividad en el fichero de Actividades";
         |ERROR;
     |SINO;
         |SI #2#46 = "N";
             |MENAV "      La Actividad no es Comunidad de Bienes";
             |ERROR;
         |SINO;
              #0#2 = #2#3; |PINTA #0#2;
         |FINSI;
    |FINSI;
    |CIERRA #2;
|FPRC;

|PROCESO lineas; |TIPO 6;
    |ABRE #2;
    #2#0 = #3#3;
    #2#1 = #3#5;
    #2#2 = #0#3;
    |LEE 040#2=;
    |SI FSalida ! 0;
        |MENAV "     No existe Actividad en el fichero de Actividades";
        |ERROR;
    |SINO;
        |SI #2#14 = "N";
            |MENAV "      La Actividad no es un Comunero";
            |ERROR;
        |SINO;
             #3#4 = #2#3; |PINTA #3#4;
        |FINSI;
    |FINSI;
    |CIERRA #2;
|FPRC;

|PROCESO ElCliente; |TIPO 6;
  |ABRE #1;
  #1#0 = #3#3;
  |LEE 041#1=;
  |SI FSalida = 0;
      #3#4 = #1#2; |PINTA #3#4;
  |FINSI;
  |CIERRA #1;
|FPRC;
