|PROCESO postal; |TIPO 3;
    #0#40 = "";
    trabacod1 = #0#11;
    trabacod1 = ("00" + trabacod1) % -102;
    trabacod2 = #0#12;
    trabacod2 = ("000" + trabacod2) % -103;
    #0#40 = trabacod1 + trabacod2;
|FPRC;

