|FICHEROS;
   eossegu5 #0;
   eosm1300 #1;
   eosm1303 #11;
   dsmom004;
|FIN;

|VARIABLES;
   nDEmp  = 0;
   nHEmp  = 0;
   nAny   = 0;
   nDPer  = 0;
   nHPer  = 0;
   fFecha = @;
   nEjer  = 0;
|FIN;

|INCLUYE i_seguim;

|PROCESO el300;
    se_empre = #1#0;
    se_ejerc = #1#2;
    se_perio = #1#1;
    se_tipos = 300;
    se_activ = #1#45;
    se_impor = 0;
    |SI #1#33 > 0; se_impor =  #1#33; |FINSI;
    |SI #1#32 > 0; se_impor = -#1#32; |FINSI;
    |SI #1#31 > 0; se_impor = -#1#31; |FINSI;
    se_fecha = #1#3;
    se_factu = "";
    fFecha = se_fecha;
    |SI fFecha > "01.01.1900"; |HAZ seguimie; |FINSI;
|FPRC;

|DEFBUCLE eosm1300;
    #1#1;
    ;
    nDEmp, 0, nDPer, nAny;
    nHEmp, 9, nHPer, nAny;
    e;
    NULL, el300;
|FIN;

|PROCESO el303;
    se_empre = #11#0;
    se_ejerc = #11#2;
    se_perio = #11#1;
    se_tipos = 303;
    se_activ = 10;
    se_impor = 0;
    |SI #11#33 > 0; se_impor =  #11#33; |FINSI;
    |SI #11#32 > 0; se_impor = -#11#32; |FINSI;
    |SI #11#31 > 0; se_impor = -#11#31; |FINSI;
    se_fecha = #11#3;
    se_factu = "";
    fFecha = se_fecha;
    |SI fFecha > "01.01.1900"; |HAZ seguimie; |FINSI;
|FPRC;

|DEFBUCLE eosm1303;
    #11#1;
    ;
    nDEmp, 0, nDPer, nAny;
    nHEmp, 9, nHPer, nAny;
    e;
    NULL, el303;
|FIN;

|PROCESO eldsmom004;
    se_empre = #dsmom004#0;
    se_ejerc = #dsmom004#1 - 2000;
    se_perio = #dsmom004#2;
    se_tipos = 303;
    se_activ = 10;
    se_impor = #dsmom004#15;
    se_fecha = #dsmom004#10;
    se_factu = "";
    fFecha = se_fecha;
    |SI fFecha > "01.01.1900"; |HAZ seguimie; |FINSI;
|FPRC;

|DEFBUCLE dsmom004;
    #dsmom004#1;
    ;
    nDEmp, nEjer, nDPer, 303, 00;
    nHEmp, nEjer, nHPer, 303, 99;
    e;
    NULL, eldsmom004;
|FIN;

|PROCESO pase; |TIPO 3;
  |SI #0#5 = "N"; |ACABA; |FINSI;

  nAny  = #0#0;
  nEjer = 2000 + #0#0;
  nDPer = #0#1; nHPer = #0#2;
  nDEmp = #0#3; nHEmp = #0#4;

  |BUCLE dsmom004;
  |SI nEjer < 2014;
      |BUCLE eosm1303;
      |BUCLE eosm1300;
  |FINSI;
|FPRC;
