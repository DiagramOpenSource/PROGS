|FICHEROS;
     agrm0008 #8;        || Explotaciones
|FIN;

|VARIABLES;
     &VEmpresa = 0;
     &VAnyo    = 0;
     &Modo     = 0;
|FIN;

|RUTINA ClaveBaja8;
#8#0 = VEmpresa;
#8#1 = VAnyo;
#8#2 = "";
|FRUT;

|RUTINA ClaveAlta8;
#8#0 = VEmpresa;
#8#1 = VAnyo;
#8#2 = "zzzzzzzzzzzz";
|FRUT;

|PROGRAMA;
|SI Modo = 1; |ENTLINEAL #1#8, 3, 2, 22, 1, ClaveBaja8, ClaveAlta8; |FINSI;
|SI Modo = 2; |ENTLINEAL #1#8, 3, 2, 22, 1, ClaveBaja8, ClaveAlta8; |FINSI;
|SI Modo = 4; |ENTLINEAL #1#8, 3, 4, 22, 1, ClaveBaja8, ClaveAlta8; |FINSI;
|FPRO;
