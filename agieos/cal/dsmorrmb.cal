|FICHEROS;
    dsmorrmb #0;
    dsmorrem #1;
|FIN;

|VARIABLES;
    FSestado = 0;
|FIN;

|RUTINA Inferior;
#1#0 = #0#0;
#1#1 = #0#2;
#1#2 = 1;
#1#3 = " ";
#1#4 = 0;
|FRUT;

|RUTINA Superior;
#1#0 = #0#0;
#1#1 = #0#2;
#1#2 = 99999;
#1#3 = "z";
#1#4 = 9;
|FRUT;

|PROGRAMA;
|CLS;
|CABEZA "Remesas";

|PARA; |SI FSestado = 0; |HACIENDO;
       |PINPA #0#0;
       |PINDA #0#0;
       |ENDATOS #1#0;
       FSestado = FSalida;
       |SI FSalida = 0;
           |ENTLINEAL #1#1, 3, 2, 21, 0, Inferior, Superior;
       |FINSI;
|SIGUE;
|FPRO;
