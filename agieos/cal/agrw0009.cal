|FICHEROS;
     agrw0009 #15;         || Este def
|FIN;

|VARIABLES;
     &VEmpresa  = 0;
     &VAnyo     = 0;
     &VPEstima  = "";
     &Modo      = 0;
|FIN;

|RUTINA ClaveBaja15;
#15#0 = VEmpresa;
#15#1 = VAnyo;
#15#2 = VPEstima;
#15#3 = 1;
|FRUT;

|RUTINA ClaveAlta15;
#15#0 = VEmpresa;
#15#1 = VAnyo;
#15#2 = VPEstima;
#15#3 = 1;
|FRUT;

|PROGRAMA;
|PINPA #0#15;
|ATRI I;
|SI VPEstima = "A08"; |PINTA 1927, "Cajas y Bancos";   |FINSI;
|SI VPEstima = "A09"; |PINTA 1927, "Capital";          |FINSI;
|SI VPEstima = "A10"; |PINTA 1927, "Reservas";         |FINSI;

|SI Modo = 1; |ENTLINEAL #1#15, 4, 1, 22, 0, ClaveBaja15, ClaveAlta15; |FINSI;
|SI Modo = 2; |ENTLINEAL #1#15, 4, 2, 22, 0, ClaveBaja15, ClaveAlta15; |FINSI;
|SI Modo = 4; |ENTLINEAL #1#15, 4, 4, 22, 0, ClaveBaja15, ClaveAlta15; |FINSI;
|FPRO;
