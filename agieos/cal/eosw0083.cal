|FICHEROS;
     eosz0013 #0;
     eoslinco #1;
     eosactiv #2;
     eosconce #3;
     eosconc6 #4;
     eosw0083 #83,MANTE;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO NoBorrar;  |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
         |MENAV "     No se puede dar de Baja.";
         |ERROR;
         |ACABA;
     |FINSI;
|FPRC;

|PROCESO Concepto;  |TIPO 7;
     |LEE 040#83=;
     |SI #83#2 ! 0;
         |C_V #83#3, 1, "N";
         |C_M #83#3, 1, "N";
     |SINO;
         |C_V #83#2, 1, "N";
         |C_M #83#2, 1, "N";
     |FINSI;
|FPRC;

|PROCESO RecogeConcepto;  |TIPO 0;
     |SI #83#2 ! 0;
         |ABRE #3;
         #3#0 = #83#2;
         |LEE 040#3=;
         |SI FSalida ! 0;  |DDEFECTO #3;  |FINSI;
         |CIERRA #3;

         #83#4  = #3#1;   |PINTA #83#4;
         #83#5  = #3#14;  |PINTA #83#5;
         #83#10 = #3#12;  |PINTA #83#10;
         #83#11 = #3#13;  |PINTA #83#11;
     |SINO;
         |ABRE #4;
         #4#0 = #83#3;
         |LEE 040#4=;
         |SI FSalida ! 0;  |DDEFECTO #4;  |FINSI;
         |CIERRA #4;

         #83#4  = #4#1;   |PINTA #83#4;
         #83#5  = #4#21;  |PINTA #83#5;
         #83#10 = #4#19;  |PINTA #83#10;
         #83#11 = #4#20;  |PINTA #83#11;
     |FINSI;
|FPRC;
