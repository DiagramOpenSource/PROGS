|FICHEROS;
     agrw0004 #15;         || Este def
|FIN;

|VARIABLES;
     &VEmpresa  = 0;
     &VAnyo     = 0;
     &VPEstima  = "";
     &Modo      = 0;
|FIN;

|RUTINA ClaveBaja15;
#15#0 = VEmpresa;
#15#1 = VAnyo;
#15#2 = VPEstima;
#15#3 = 1;
|FRUT;

|RUTINA ClaveAlta15;
#15#0 = VEmpresa;
#15#1 = VAnyo;
#15#2 = VPEstima;
#15#3 = 99999;
|FRUT;

|PROGRAMA;
|PINPA #0#15;
|ATRI I;
|SI VPEstima = "A01"; |PINTA 1027, "Deudores";         |FINSI;
|SI VPEstima = "A02"; |PINTA 1027, "Proveedores";      |FINSI;
|SI VPEstima = "A03"; |PINTA 1027, "Otros Acreedores"; |FINSI;

|SI Modo = 1; |ENTLINEAL #1#15, 4, 1, 22, 0, ClaveBaja15, ClaveAlta15; |FINSI;
|SI Modo = 2; |ENTLINEAL #1#15, 4, 2, 22, 0, ClaveBaja15, ClaveAlta15; |FINSI;
|SI Modo = 4; |ENTLINEAL #1#15, 4, 4, 22, 0, ClaveBaja15, ClaveAlta15; |FINSI;
|FPRO;
