                     |XGRABA nHandle1, aAlfa;
                 |FINSI;

                 |XCIERRA nHandle1;
                 |RSYSTEM aAbre;

                 |RFBORRA aAbre;

                 aAlfa = aPathModelo + aFichXml;
                 |RFBORRA aAlfa;
             |FINSI;
         |SINO;
             |SI enAnyo = 2004;
                 aOrigen  = aPathModelo + aFichXml;
                 aDestino = eaInternet  + aFichXml;
                 |RCOPIA_FICHERO aOrigen, aDestino;

                 |RFBORRA aOrigen;
             |FINSI;
         |FINSI;
     |FINSI;

     |SI enAnyo ] 2005;  |Y aImpre = "C";
         nHay  = 1;
