|FICHEROS;
     dsm18407 #0;
|FIN;

|VARIABLES;
   &eaAlfa  = "";  &enError = 0;
   &eaTabla = "";  &enPosicion = 0; &eaDestino = ""; &enComp = 0;

   nLCod  = 0;
   nLDes  = 0;
   nCampo = 0;
   nCalc  = 0;

   aAlfa  = "";
   aAlfa1 = "";
   aAlfa2 = "";
|FIN;

|PROCESO Calculo07;
   #dsm18407#9 = #dsm18407#7 - #dsm18407#8; |PINTA #dsm18407#9;
|FPRC;

|PROCESO SacaPais07; |TIPO 7;
   eaTabla = "ztabla05.txt";  nLCod = 2; nLDes = 30; |HAZ CargaTabla;
   |MENSA "    <F2> Consulta paises ";
|FPRC;

|PROCESO CompPais07;

     |SI #dsm18407#10 = "  "; #dsm18407#10 = "ES"; |FINSI;

     |MENSA "     ";
     nCampo   = Campo + 1;

     |SI FSalida = 10;
         enPosicion = 20;
         |HAZ SacaTabla;
         nCalc = 100 + nLCod;
         aAlfa1   = eaDestino % nCalc;
         #dsm18407#Campo#  = aAlfa1; |PINTA #dsm18407#Campo#;
     |FINSI;

     enComp = nLCod;
     aAlfa = "|C" + (("000" + Campo) % -103) + "TIPO"; aAlfa1 = #dsm18407#aAlfa#;
     aAlfa = #dsm18407#Campo#; |QBF aAlfa;
     |SI aAlfa1 = "N"; nCalc = - (100 + nLCod); aAlfa = (("0" * enComp) + aAlfa) % nCalc; |FINSI;
     |SI aAlfa1 ! "N"; nCalc = 100 + nLCod;     aAlfa = (aAlfa + (" " * enComp)) % nCalc; |FINSI;
     eaAlfa = aAlfa;
     |HAZ CompruebaTabla;

     |SI enError = 1;
         |MENAV "0000 Contenido Incorrecto. Consulta la Tabla con F2";
         |ERROR;
         |PULSATECLA;
         |ACABA;
     |FINSI;

     nCalc = 100 + nLCod;                 aAlfa1   = eaDestino % nCalc;
     nCalc = ((nLCod + 2) * 100) + nLDes; aAlfa2   = eaDestino % nCalc;
     #dsm18407#Campo#  = aAlfa1;
     |C_P #dsm18407#Campo#,0,nCalc; nCalc = nCalc + nLCod + 1;
     |PINTA #dsm18407#Campo#;
     #dsm18407#nCampo# = aAlfa2; |PINTA #dsm18407#nCampo#;

     |HAZ FinTabla;
|FPRC;
