   nRango      = 0;
   nPos1       = 0;
   nPos2       = 0;
   &nTr210tr   = 0;
   &nEj210tr   = 0;
   &nTg210tr   = 0;
|FIN;

|INCLUYE i_pat210;

|PROCESO EvtoGrid1;
   |SI FSalida = 1; |O FSalida = 2;
       |LEE 000#dsm210dv.c;
   |FINSI;
|FPRC;

|PROGRAMA;

