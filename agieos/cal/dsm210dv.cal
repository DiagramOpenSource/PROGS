   |CLS;
   |CABEZA "Modelo 210: Divisas";

   |ABRE #dsm210dv;

   |PINPA #0#0; nPant = FSalida;
   |PINDA #0#0;

   nRango = 1 + 2 + 4 + 8 + 16 + 128;
   nPos1 = 0702;
   nPos2 = 2278;
   |LINEAL_SIMPLE #1#0, nRango, nPos1, nPos2, NULL, NULL, EvtoGrid1;

   |CIERRA #dsm210dv;
|FPRO;
