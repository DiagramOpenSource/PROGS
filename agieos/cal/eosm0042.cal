|FICHEROS;
     eosm0045 #29;
     eosm0042 #32;
|FIN;

|VARIABLES;
     nCampo1 = 0;
     nCampo2 = 0;
|FIN;

|PROCESO Periodo42;  |TIPO 0;
     |SI #32#9 > 12;  |Y #32#9 [ 1990;
         |MENAV "     Periodo incorrecto.";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Relacion;  |TIPO 7;
     |SI #32#5 ! 0;  |ACABA;  |FINSI;

     |ABRE #29;
     #29#0 = #32#0;
     #29#1 = #32#2;
     #29#4 = #32#11;
     |LEE 040#29=;
     |SI FSalida ! 0;  |DDEFECTO #29;  |FINSI;
     |CIERRA #29;

|FPRC;

|PROCESO Modificas;  |TIPO 7;
|FPRC;

|PROCESO Importes;
     |SI #32Campo ! Contenido;
         nCampo1    = Campo - 1;
         nCampo2    = Campo + 1;
         #32nCampo2 = #32nCampo1 % #32Campo;
         |PINTA #32nCampo2;
     |FINSI;
|FPRC;
