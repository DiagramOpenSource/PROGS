|VARIABLES;
   empresa_d   = 0;
   activid_d   = 0;
   anyo_d      = 0;
   frase_d     = "";
   informado_d = "";
   Linea1      = "";
   Linea2      = "";
   Linea3      = "";
|FIN;

|PROCESO Preg_Devol;
  |ABRE #eosactiv;
  #eosactiv#0 = empresa_d;
  #eosactiv#1 = activid_d;
  #eosactiv#2 = anyo_d;
  |LIBERA #eosactiv;
  |LEE 140#eosactiv.=;
  |SI FSalida = 0;
      |HAZ Cuest_Devol;
      #eosactiv#27 = informado_d;
      |GRABA 140#eosactiv.a;
  |FINSI;
  |CIERRA #eosactiv;
|FPRC;

|PROCESO Cuest_Devol;
  |SI #eosactiv#27 = "C"; frase_d = "A COMPENSAR"; |FINSI;
  |SI #eosactiv#27 = "D"; frase_d = "A DEVOLVER "; |FINSI;

  informado_d = #eosactiv#27;

  |PUSHV 0501 2380;
  |D_CUADRO 1309, 2269;
  |ATRI R;

  Linea1 = ((" Empresa ..: " + #eosactiv#0 + " " + #eosactiv#3) + (" " * 59)) % 159;
  Linea2 = ((" Actividad : " + #eosactiv#1 + " " + #eosactiv#6) + (" " * 59)) % 159;
  Linea3 = (("vaya a la casilla " + frase_d) + (" " * 59)) % 159;

  |PINTA 1410, Linea1;
  |PINTA 1510, Linea2;
  |PINTA 1610, "-----------------------------------------------------------";
  |PINTA 1710, " La Cantidad que nos da el ultimo trimestre es negativa y  ";
  |PINTA 1810, "tenemos informado en la actividad que el importe del I.V.A.";
  |PINTA 1910, Linea3;
  |PINTA 2010, "-----------------------------------------------------------";
  |PINTA 2110, " Desea cambiar lo informado <C>ompensar / <D>evolver :     ";
  |ATRI r;

  E_inf = "CD";
  E_sup = "1";

  informado_d = 2165 ? 1;
  |POPV;
|FPRC;
