|FICHEROS;
     rem00903;
|FIN;

|VARIABLES;
     aCodigo1   = "";
     aCodigo2   = "";
     aCodigo3   = "";
     aCodigo4   = "";
     aContenido = "";
     aCaracter  = "";
     aLineaMenu = "";
     aAlfa      = "";
     aAlfa1     = "";
     aAlfa2     = "";
     aAbre      = "";
     aPrograma  = "";
     aRetu      = "";
     aEsc1      = "";
     aEsc2      = "";
     aTecla     = "";
     aCampoA    = "";
     aCampoB    = "";
     aCampoC    = "";
     aCampoD    = "";
     aCampoE    = "";
     aCampoF    = "";
     aCampoG    = "";
     aLong      = "";
     aCadena    = "";
     aTab       = "";
     aCom       = "";
     aCarac1    = "";

     nCampo     = 0;
     nCodigos   = 0;
     nCodigo1   = 0;
     nCodigo2   = 0;
     nCodigo3   = 0;
     nPos       = 0;
     nPosi      = 0;
     nRango     = 0;
     nBoton1    = 0;
     nHandle    = 0;
     nLong      = 0;
     nCaracter  = 0;
     nLLaves    = 0;
     nGrid      = 0;
     nVent      = 0;
     nComillas  = 0;

     &eaAlfa    = "";
|FIN;

|PROGRAMA;
     |LEE 000#rem00903.p;
     |CONSULTA_CLAVE #1#rem00903;
|FPRO;

|PROCESO GrabaReg;
     |QBF aCampoA;
     |SI aCampoA = "";  |ACABA;  |FINSI;

     eaAlfa = aCampoE;
     |HAZ QuitaRarosCSV;
     aCampoE = eaAlfa;

     |DDEFECTO #rem00903;
     #rem00903#0 = aCampoA;
     #rem00903#1 = aCampoE;
     #rem00903#2 = aCampoB;
     #rem00903#3 = aCampoC;
     #rem00903#4 = aCampoD;
     #rem00903#5 = aCampoF;

     |GRABA 020#rem00903.n;
     |SI FSalida ! 0;
         |DEBUG;
     |FINSI;
     |LIBERA #rem00903;
|FPRC;

|PROCESO RecogeDatos;
     aAlfa = "/u/dsprofes/ren2018/cogetxt/rem00903.csv";
     |XABRE "", aAlfa, nHandle;
     |PARA;  |SI FSalida ] 0;  |HACIENDO;
          |XLEE nHandle, 250, aAlfa;
          |SI FSalida < 0;  |SAL;  |FINSI;

          aCampoA  = "";
          aCampoB  = "";
          aCampoC  = "";
          aCampoD  = "";
          aCampoE  = "";
          aCampoF  = "";

          aCadena   = aAlfa;  |QBF aCadena;
          aLong     = aCadena % 0;
          nLong     = aLong;
          nCampo    = 1;
          aTab      = ";";
          aCom      = &34;
          nComillas = 0;

          |PARA nCaracter = 1;  |SI nCaracter [ nLong;  |HACIENDO nCaracter = nCaracter + 1;
                nPos    = (100 * nCaracter) + 1;
                aCarac1 = aCadena % nPos;
                |SI aCarac1 = aCom;
                    |SI nComillas = 1;
                        nComillas = 0;
                    |SINO;
                        nComillas = 1;
                    |FINSI;

                    aCarac1 = "";
                |FINSI;

                |SI aCarac1 = aTab;  |Y nComillas = 0;
                    nCampo = nCampo + 1;
                |SINO;
                    |SI nCampo = 1;  aCampoA  = aCampoA  + aCarac1;  |FINSI;
                    |SI nCampo = 2;  aCampoB  = aCampoB  + aCarac1;  |FINSI;
                    |SI nCampo = 3;  aCampoC  = aCampoC  + aCarac1;  |FINSI;
                    |SI nCampo = 4;  aCampoD  = aCampoD  + aCarac1;  |FINSI;
                    |SI nCampo = 5;  aCampoE  = aCampoE  + aCarac1;  |FINSI;
                    |SI nCampo = 6;  aCampoF  = aCampoF  + aCarac1;  |FINSI;

                    |SI nCampo > 6;  |SAL;  |FINSI;
                |FINSI;
          |SIGUE;

          |HAZ GrabaReg;
     |SIGUE;
     |XCIERRA nHandle;
|FPRC;

|PROCESO T80;  |TIPO 80;
     |ABRET #rem00903;
     |LEE 000#rem00903.p;
     |SI FSalida ! 0;
         |HAZ RecogeDatos;
     |FINSI;

     FSalida = 2799;
|FPRC;

|PROCESO T90;  |TIPO 90;
     |CIERRAT #rem00903;
|FPRC;
