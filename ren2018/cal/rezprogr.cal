     |CARGA_DEF aDef, referen@;
     |SI FSalida ! 0;
         aAlfa = "     Error en cargar el Def " + eaPrograma + ".mas";
         |MENAV aAlfa;
         |ACABA;
     |FINSI;

     |SI nClave = 1;
         |ABRE #500;
         |DDEFECTO #500;
         #500#0 = #1#0;
         |LEE 101#500=;
