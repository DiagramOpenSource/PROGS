             eaPunto   = "";
             |HAZ ProgramaActivo;
         |FINSI;
         |LIBERA #500;
         |CIERRA #500;

         |DESCARGA_DEF #referen@;
     |FINSI;

     |SI nClave = 2;
         |ABRE #500;
         |DDEFECTO #500;
         #500#0 = #1#0;
         #500#1 = 1;
         |LEE 141#500];
         |PARA; |SI FSalida = 0;  |Y #500#0 = #1#0; |HACIENDO;
            eaPunto   = "";
            eaLetra   = "";

            |SI eaPrograma = "rem00058";
                eaPunto   = #500#13;
                eaLetra   = #500#13;
            |FINSI;

            |SI eaPrograma = "rem00065";
                eaPunto   = #500#11;
                eaLetra   = #500#11;
            |FINSI;
            |HAZ ProgramaActivo;

            |LEE 101#500s;
         |SIGUE;
         |CIERRA #500;

         |DESCARGA_DEF #referen@;
     |FINSI;

     |SI nClave = 33;
         |ABRE #500;
         |DDEFECTO #500;
         #500#0 = #1#0;
         #500#1 = "";
         #500#2 = 0;
         |LEE 141#500];
         |PARA; |SI FSalida = 0;  |Y #500#0 = #1#0; |HACIENDO;
            eaPunto   = #500#1;
            eaLetra   = #500#1;
            |HAZ ProgramaActivo;

            |LEE 101#500s;
         |SIGUE;
         |CIERRA #500;

         |DESCARGA_DEF #referen@;
     |FINSI;
|FPRC;

