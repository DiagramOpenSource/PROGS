|PROCESO TrataProgramaL;
     aDef = aPathDef + eaPrograma + ".mas";
     |CARGA_DEF aDef, referen@;
     |SI FSalida ! 0;
         aAlfa = "     Error en cargar el Def " + eaPrograma + ".mas";
         |MENAV aAlfa;
         |ACABA;
     |FINSI;

     |ABRE #500;
     |DDEFECTO #500;
     #500#0 = #1#0;
     #500#1 = 0;
     #500#2 = "          ";
     |LEE 141#500];
     |PARA; |SI FSalida = 0;  |Y #500#0 = #1#0; |HACIENDO;
          eaPunto   = "";
          |HAZ ProgramaActivo;

          |LEE 101#500s;
     |SIGUE;
