|FICHEROS;
     rem01053;
|FIN;

|VARIABLES;
      nCampo           = 0;
      nRango           = 0;
      nNumCampos       = 0;
      nGrid            = 0;

      aAlfa            = "";
      aQueQuiero       = "";
      aNumCampos       = "";
      aEsc1            = "";
      aEsc2            = "";
      aRetu            = "";
      aTecla           = "";

     &enEmpresa        = 0;
     &enComplementaria = 0;
     &eaContri         = "";
|FIN;

|PROCESO Baja1053;
     #rem01053#0 = enEmpresa;
     #rem01053#1 = enComplementaria;
     #rem01053#2 = eaContri;
     #rem01053#3 = "";
|FPRC;

|PROCESO Alta1053;
     #rem01053#0 = enEmpresa;
     #rem01053#1 = enComplementaria;
     #rem01053#2 = eaContri;
     #rem01053#3 = "z" * 10;
|FPRC;

|PROGRAMA;
     |ABRET #rem01053;

     |PINPA #0#rem01053;

     nRango      = 4 + 8 + 16 + 32 + 128;

     |LINEAL_SIMPLE #1#rem01053, nRango, 0602, 3498, Baja1053, Alta1053, NULL;
     nGrid = FSalida;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
          FSalida = "::{{001}}Salir," + nGrid;
          |LEETECLA aTecla;
          |SI aTecla = aEsc1; |SAL;  |FINSI;
          |SI aTecla = aEsc2; |SAL;  |FINSI;
          |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;

     |CIERRAT #rem01053;
|FPRO;
