|FICHEROS;
     rem01055;
     rem00903;

     rew00007,MANTE;
|FIN;

|VARIABLES;
      nCampo           = 0;
      nRango           = 0;
      nNumCampos       = 0;
      nGrid            = 0;
      nPanta           = 0;
      n1Vtn            = 0;
      nInmueble        = 0;

      aAlfa            = "";
      aQueQuiero       = "";
      aNumCampos       = "";
      aEsc1            = "";
      aEsc2            = "";
      aRetu            = "";
      aTecla           = "";
      aFic             = "";
      aTitular         = "";

     {-1}aVent         = "";
         aVent1        = 0;
         aVent2        = 0;
         aVent3        = 0;
         aVent4        = 0;
         aVent5        = "";

     &enEmpresa        = 0;
     &enComplementaria = 0;
     &eaContri         = "";
|FIN;

|PROCESO Baja1055;
     #rem01055#0 = enEmpresa;
     #rem01055#1 = enComplementaria;
     #rem01055#2 = eaContri;
     #rem01055#3 = nInmueble;
     #rem01055#4 = 1;
|FPRC;

|PROCESO Alta1055;
     #rem01055#0 = enEmpresa;
     #rem01055#1 = enComplementaria;
     #rem01055#2 = eaContri;
     #rem01055#3 = nInmueble;
     #rem01055#4 = 999;
|FPRC;

|PROCESO rem00903;
     #rem01055#0 = enEmpresa;
     #rem01055#1 = enComplementaria;
     #rem01055#2 = eaContri;
     #rem01055#3 = nInmueble;
     #rem01055#4 = #rem00903#0;
     |LEE 000#rem01055.=;
     |SI FSalida = 0;  |ACABA;  |FINSI;

     |DDEFECTO #rem01055;
     #rem01055#0  = enEmpresa;
     #rem01055#1  = enComplementaria;
     #rem01055#2  = eaContri;
     #rem01055#3  = nInmueble;
     #rem01055#4  = #rem00903#0;
     #rem01055#5  = aTitular;
     #rem01055#6  = #rem00903#1;
     #rem01055#7  = #rem00903#2;
     #rem01055#9  = #rem00903#3;
     #rem01055#11 = #rem00903#4;
     #rem01055#13 = #rem00903#5;
     |GRABA 020#rem01055.n;
     |LIBERA #rem01055;
|FPRC;

|DEFBUCLE rem00903;
     #rem00903#1;
     ;
      0;
     30;
     ;
     NULL, rem00903;
|FIN;

|PROCESO CargaResto;
     aAlfa = #rew00007#1 % 103;  nInmueble = aAlfa;
     aAlfa = #rew00007#1 % 510;  aTitular  = aAlfa;

     |BUCLE rem00903;
|FPRC;

|PROCESO rem01055;
     #rew00007#0 = (("000" + #rem01055#3) % -103) + "-" + #rem01055#5;
     |LEE 000#rew00007.=;
     |SI FSalida = 0;  |ACABA;  |FINSI;

     #rew00007#0 = (("000" + #rem01055#3) % -103) + "-" + #rem01055#5;
     #rew00007#1 = (("000" + #rem01055#3) % -103) + "-" + #rem01055#5;
     |GRABA 000#rew00007.n;
     |LIBERA #rew00007;
|FPRC;

|DEFBUCLE rem01055;
     #rem01055#1;
     ;
     enEmpresa, enComplementaria, eaContri,   1,   1;
     enEmpresa, enComplementaria, eaContri, 999, 999;
     ;
     NULL, rem01055;
|FIN;

|PROCESO Evnt7;
     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#rew00007.=;
     |FINSI;

     |SI FSalida = 4;
         |PTEC 806;
     |FINSI;
|FPRC;

|PROCESO BtnElige;
     |VENTANA 0537, 1557, -1, 1, "Elige Inmueble";
     n1Vtn = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = n1Vtn;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |PTEC 802;  |PAUSA;

     nRango = 4 + 8 + 16 + 128;

     |LINEAL_SIMPLE #1#rew00007, nRango, 0638, 1356, NULL, NULL, Evnt7;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = n1Vtn;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |FINVENTANA n1Vtn;

     |PINTA #rew00007#1;

     |HAZ CargaResto;

     |LEE 000#rem01055.p;
     |REFRESCACONTROL nGrid;
|FPRC;

|PROGRAMA;
     #rem01055#0 = enEmpresa;
     #rem01055#1 = enComplementaria;
     #rem01055#2 = eaContri;
     #rem01055#3 = "";
     #rem01055#4 = 1;
     |LEE 000#rem01055.];
     |SI FSalida ! 0;  |O #rem01055#0 ! enEmpresa;
                       |O #rem01055#1 ! enComplementaria;
                       |O #rem01055#2 ! eaContri;
         |MENAV "0000No hay deducciones o compensaciones para ejercicios siguientes";
         |ACABA;
     |FINSI;

     |PINPA #0#rew00007;
     nPanta = FSalida;

     |BUCLE rem01055;
     |LEE 000#rew00007.p;

     |CONTROL_SIMPLE nPanta, "BOTON,{{197}}", 0537, 0538, BtnElige;

     |PINDA #0#rew00007;
     |HAZ CargaResto;

     nRango      = 4 + 8 + 16 + 32 + 128;

     |LINEAL_SIMPLE #1#rem01055, nRango, 0702, 3498, Baja1055, Alta1055, NULL;
     nGrid = FSalida;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
          FSalida = "::{{001}}Salir," + nGrid;
          |LEETECLA aTecla;
          |SI aTecla = aEsc1; |SAL;  |FINSI;
          |SI aTecla = aEsc2; |SAL;  |FINSI;
          |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;
|FPRO;

|PROCESO T80;  |TIPO 80;
     aFic = "rew00007" + Usuario;
     |NOME_DAT #rew00007#aFic#;
     |ABRE #rew00007;  |CIERRA #rew00007;  |DELINDEX #rew00007;

     |ABRET #rew00007;
     |ABRET #rem01055;

     FSalida = 3499;
|FPRC;

|PROCESO rem01055B;
     |SI #rem01055#8 = 0;  |Y #rem01055#10 = 0;  |Y #rem01055#12 = 0;
         |BORRA 020#rem01055.a;
     |FINSI;

     |LIBERA #rem01055;
|FPRC;

|DEFBUCLE rem01055B;
     #rem01055#1;
     ;
     enEmpresa, enComplementaria, eaContri,   1,   1;
     enEmpresa, enComplementaria, eaContri, 999, 999;
     be;
     NULL, rem01055B;
|FIN;

|PROCESO T90;  |TIPO 90;
     |BUCLE rem01055B;

     |CIERRAT #rem01055;
     |CIERRAT #rew00007;   |DELINDEX #rew00007;
|FPRC;
