|PROCESO Graba1001;
     |DDEFECTO #1001;
     nConta   = 0;
     #1001#0  = enEmpresa;
     #1001#1  = enComplementaria;
     #1001#2  = eaContri;
     #1001#7  = #999#2;
     |GRABA 020#1001b;
|FPRC;

|PROCESO CreaLiquida;
     |SI eaContri ! "CONJUNTA  ";  |Y eaContri ! "CONJUNTAE ";
         |ABRE #999;
         |SELECT #2#999;
         #999#1 = eaContri;
         |LEE 040#999=;
         |SI FSalida  ! 0;  |DDEFECTO #999;  |FINSI;
         |CIERRA #999;
     |SINO;
         |ABRE #999;
         |SELECT #2#999;
         #999#1 = "TITULAR   ";
         |SI eaMuerto = "T";  |Y eaContri = "CONJUNTAE ";
             #999#1 = "CONYUGE   ";
         |FINSI;

         |LEE 040#999=;
         |SI FSalida  ! 0;  |DDEFECTO #999;  |FINSI;
         |CIERRA #999;
     |FINSI;

     |SI eaContri = "ZPATRITIT ";
         |ABRE #999;
         |SELECT #2#999;
         #999#1 = "TITULAR   ";
         |LEE 040#999=;
         |SI FSalida  ! 0;  |DDEFECTO #999;  |FINSI;
         |CIERRA #999;
     |FINSI;

     |SI eaContri = "ZPATRICON ";
         |ABRE #999;
         |SELECT #2#999;
         #999#1 = "CONYUGE   ";
         |LEE 040#999=;
         |SI FSalida  ! 0;  |DDEFECTO #999;  |FINSI;
         |CIERRA #999;
     |FINSI;

     #1001#0 = enEmpresa;
     #1001#1 = enComplementaria;
     #1001#2 = eaContri;
     |LEE 101#1001=;
     |SI FSalida ! 0;
         |HAZ Graba1001;
     |FINSI;
     |LIBERA #1001;

     #1002#0 = enEmpresa;
     #1002#1 = enComplementaria;
     #1002#2 = eaContri;
     #1002#3 = 0;
     #1002#4 = "   ";
     |LEE 101#1002];
     |SI FSalida ! 0; |O #1002#0 ! enEmpresa; |O #1002#1 ! enComplementaria; |O #1002#2 ! eaContri;
         |SI eaContri ! "ZPATRITIT "; |Y eaContri ! "ZPATRICON ";
             |BUCLE rem01000;
         |SINO;
             |BUCLE rem00999;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO GrabaLineal;
     |ABRE #1002;
     #1002#0 = #1001#0;
     #1002#1 = #1001#1;
     #1002#2 = #1001#2;
     #1002#3 = enBloque;
     #1002#4 = eaCasilla;
     |LEE 101#1002=;
     |SI FSalida = 0;
         |SI enSwAcum = 0;
             #1002#6 = enImporte;
         |SINO;
             #1002#6 = #1002#6 + enImporte;
         |FINSI;
         |GRABA 020#1002a;
     |FINSI;
     |LIBERA #1002;
     |CIERRA #1002;
|FPRC;

|PROCESO SacaImporte;
     enImporte = 0;

     |ABRE #1002;
     #1002#0 = #1001#0;
     #1002#1 = #1001#1;
     #1002#2 = #1001#2;
     #1002#3 = enBloque;
     #1002#4 = eaCasilla;
     |LEE 040#1002=;
     |SI FSalida = 0;
         enImporte = #1002#6;
     |FINSI;
     |CIERRA #1002;
|FPRC;

|PROCESO BorraFichero;
     aDef = eaPathDef + aFichero + ".mas";
     |CARGA_DEF aDef, referen@;
     |SI FSalida ! 0;
         aAlfa = "     Error en cargar el Def " + aFichero + ".mas";
         |MENAV aAlfa;
         |ACABA;
     |FINSI;

     |ABRE #500;
     |DDEFECTO #500;
     #500#0 = enEmpresa;
     #500#1 = enComplementaria;
     #500#2 = "          ";
     |LEE 141#500];
     |PARA; |SI FSalida = 0;  |Y #500#0 = enEmpresa;  |Y #500#1 = enComplementaria; |HACIENDO;
            |SI eaLiquida ! "";
                |SI #500#2 = eaLiquida;
                    |BORRA 020#500a;
                |FINSI;
            |SINO;
                |BORRA 020#500a;
            |FINSI;
            |LIBERA #500;

            |LEE 101#500s;
     |SIGUE;
     |LIBERA #500;
     |CIERRA #500;

     |DESCARGA_DEF #referen@;
|FPRC;

