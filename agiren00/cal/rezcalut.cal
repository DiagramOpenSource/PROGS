     #1002#7 = #998#3;
     |GRABA 020#1002n;
     |LIBERA #1002;
|FPRC;

|DEFBUCLE rem00999;
     #998#1;
     ;
     00, "   ";
     99, "zzz";
     ;
     NULL, GrabaLineasPat;
|FIN;

