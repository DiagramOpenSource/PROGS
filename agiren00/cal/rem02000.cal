|FICHEROS;
     rem02000 #0;
     rem02001 #1;
|FIN;

|VARIABLES;
     aAlfa   = "";
     aAlfa1  = "";
     aAlfa2  = "";
     nHandle = 0;
|FIN;

|PROCESO Recoge;
     |DFICE aAlfa;   |QBF aAlfa;
     aAlfa = aAlfa + "zdeduccion.txt";
     |XABRE "U", aAlfa, nHandle;
     |DDEFECTO #0;
     |PARA;  |SI  FSalida ] 0;  |HACIENDO;
             |XLEE nHandle, 101, aAlfa1;
             |SI FSalida < 0;  |SAL;  |FINSI;

             aAlfa2 = aAlfa1 % 102;    #0#0  = aAlfa2;
             aAlfa2 = aAlfa1 % 402;    #0#1  = aAlfa2;
             aAlfa2 = aAlfa1 % 740;    #0#2  = aAlfa2;
             aAlfa2 = aAlfa1 % 4830;   #0#3  = aAlfa2;
             aAlfa2 = aAlfa1 % 7903;   #0#4  = aAlfa2;
             aAlfa2 = aAlfa1 % 8303;   #0#5  = aAlfa2;
             aAlfa2 = aAlfa1 % 8706;   #0#6  = aAlfa2;
             aAlfa2 = aAlfa1 % 9402;   #0#7  = aAlfa2;
             aAlfa2 = aAlfa1 % 9702;   #0#8  = aAlfa2;
             |GRABA 020#0n;
             |LIBERA #0;

             #1#0 = #0#0;
             |LEE 101#1=;
             |SI FSalida ! 0;
                 #1#0 = #0#0;
                 #1#1 = #0#2;
                 |GRABA 020#1n;
             |FINSI;
             |LIBERA #1;
     |SIGUE;
     |XCIERRA nHandle;
|FPRC;

|PROCESO Tipo8;  |TIPO 8;
     |ABRE #0;
     |ABRE #1;
     |LEE 040#0p;
     |SI FSalida ! 0;
         |HAZ Recoge;
     |FINSI;
     |CIERRA #0;
     |CIERRA #1;
|FPRC;
