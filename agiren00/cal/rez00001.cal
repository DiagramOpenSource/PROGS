|FICHEROS;
     rez00001 #0;
     recofisc #1;
     reeleact #3;
     reepigra #4;
     rehacien #5;
     relement #6;
     rem00007 #7;
     rem00008 #8;
     rem00009 #9;
     rem00505 #10;
     rem00506 #11;
     rem00999 #12;
     rem01000 #13;
     rem02000 #14;
     rem02001 #15;
     retabla3 #16;
|FIN;

|VARIABLES;
     aPathRenta = "";
     aFichero   = "";
     aOrigen    = "";
     aDestino   = "";
     aBorrar    = "";
     aParametro = "";
|FIN;

|PROCESO CopiaFichero;
     aBorrar = aPathRenta + aFichero + ".dat";  |FBORRA aBorrar;
     aBorrar = aPathRenta + aFichero + ".ddx";  |FBORRA aBorrar;
     aBorrar = aPathRenta + aFichero + ".ixx";  |FBORRA aBorrar;

     aOrigen  = aPathRenta + aFichero + ".ddd";
     aDestino = aPathRenta + aFichero + ".dat";
     |COPIA_FICHERO aOrigen, aDestino;
|FPRC;

|PROCESO Tipo3;  |TIPO 3;
     |DFICE aPathRenta;

     |SI #0#0 = "S";
         aFichero = "recofisc";  |HAZ CopiaFichero;
         aFichero = "rem00007";  |HAZ CopiaFichero;
         aFichero = "rem00008";  |HAZ CopiaFichero;
         aFichero = "rem00009";  |HAZ CopiaFichero;
     |FINSI;

     |SI #0#1 = "S";
         aFichero = "rehacien";  |HAZ CopiaFichero;
     |FINSI;

     |SI #0#2 = "S";
         aFichero = "reeleact";  |HAZ CopiaFichero;
         aFichero = "relement";  |HAZ CopiaFichero;
     |FINSI;

     |SI #0#3 = "S";
         aFichero = "rem00505";  |HAZ CopiaFichero;
         aFichero = "rem00506";  |HAZ CopiaFichero;
     |FINSI;

     |SI #0#4 = "S";
         aFichero = "reepigra";  |HAZ CopiaFichero;
     |FINSI;

     |SI #0#5 = "S";
         aFichero = "rem01000";  |HAZ CopiaFichero;
     |FINSI;

     |SI #0#6 = "S";
         aFichero = "rem00999";  |HAZ CopiaFichero;
     |FINSI;

     |SI #0#7 = "S";
         aFichero = "rem02000";  |HAZ CopiaFichero;
         aFichero = "rem02001";  |HAZ CopiaFichero;
     |FINSI;

     |SI #0#8 = "S";
         aFichero = "retabla3";  |HAZ CopiaFichero;
     |FINSI;

     |ABRE #1;   |CIERRA #1;
     |ABRE #3;   |CIERRA #3;
     |ABRE #4;   |CIERRA #4;
     |ABRE #5;   |CIERRA #5;
     |ABRE #6;   |CIERRA #6;
     |ABRE #7;   |CIERRA #7;
     |ABRE #8;   |CIERRA #8;
     |ABRE #9;   |CIERRA #9;
     |ABRE #10;  |CIERRA #10;
     |ABRE #11;  |CIERRA #11;
     |ABRE #12;  |CIERRA #12;
     |ABRE #13;  |CIERRA #13;
     |ABRE #14;  |CIERRA #14;
     |ABRE #15;  |CIERRA #15;
     |ABRE #16;  |CIERRA #16;

     |SI aParametro ! "";
         |SI #0#9 = "S";  |SUB_EJECUTA_NP "rez00011;VER";  |FINSI;
     |SINO;
         |SI #0#9 = "S";  |SUB_EJECUTA_NP "rez00011";  |FINSI;
     |FINSI;
|FPRC;

|PROGRAMA;
     aParametro = PARAMETRO;
     |SI aParametro ! "";
         |DDEFECTO #0;
         |HAZ Tipo3;
     |SINO;
          |MANTE #0;
     |FINSI;
|FPRO;
