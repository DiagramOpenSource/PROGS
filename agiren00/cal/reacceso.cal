         |FINSI;

         |SI #99#7 = "RLI";
             |SI nSwRecoge = 0; |SUB_EJECUTA "recapreo";  |FINSI;
             nSwActiv = 1;
         |FINSI;

         |SI #99#7 = "13X";
             |SI nSwRecoge = 0; |SUB_EJECUTA "recapmod";  |FINSI;
             nSwActiv = 1;
         |FINSI;
     |FINSI;

