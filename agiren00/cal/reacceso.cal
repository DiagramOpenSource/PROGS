|PROCESO MiraQue;
     enEmprEos = #99#5;
     enActiEos = #99#6;
     enEmprRen = #0#0;
     enActiRen = #99#6;
     eaTitCony = #99#11;

     |SI #99#2 = "R1";
         nEsArrend = 0;
         |SI #99#7 = "PLA";
             |SI nSwRecoge = 0; |SUB_EJECUTA "recappla";  |FINSI;
             nSwActiv = 1;
         |FINSI;

         |SI #99#7 = "RLI";
             |SI nSwRecoge = 0; |SUB_EJECUTA "recapreo";  |FINSI;
             nSwActiv = 1;
         |FINSI;

         |SI #99#7 = "13X";
             |SI nSwRecoge = 0; |SUB_EJECUTA "recapmod";  |FINSI;
             nSwActiv = 1;
         |FINSI;
     |FINSI;

     |SI #99#2 = "R2";
         nEsArrend   = 1;
         enLineaArre = #99#17;
         |SI nSwRecoge = 0; |SUB_EJECUTA "recapreo";  |FINSI;
         nSwActiv = 1;
     |FINSI;
|FPRC;

|DEFBUCLE agiconpa;
     #99#1;
     2;
     #0#0, 00, aTipo;
     #0#0, 99, aTipo;
     ;
     NULL, MiraQue;
|FIN;

|PROCESO RecogeReo;
     |SALVA_FICHA #0;

     |DBASS aAgicli;

     |PATH_DAT #98 aAgicli;
     |ABRE #98;
     |LEE 040#98p;
     |SI FSalida ! 0; |DDEFECTO #98; |FINSI;
     |CIERRA #98;

     aAgicli = #98#3; |QBF aAgicli;
     |SI aAgicli = "*"; |ACABA; |FINSI;

     aAgicli = #98#8; |QBF aAgicli;
     |PATH_DAT #99 aAgicli;

     |CIERRAT #0;
     |BUCLE agiconpa;
     |ABRET #0;

     |REPON_FICHA #0;
|FPRC;

|PROCESO CogeActiv;
     aAlfa = (aGeneral8 % 1160) + " [*]";
     |SI aAlfa = mGeneral8;  |ACABA;  |FINSI;

     nSwActiv  = 0;
     aTipo     = "R1";
     nSwRecoge = 1;
     |HAZ RecogeReo;
     nSwRecoge = 0;
     |SI nSwActiv = 0;  |ACABA;  |FINSI;

     |PUSHV 0501 2380;
     |CUADRO 1221 1664;
     |ATRI R;
     |PINTA 1322, "    DETECTADO ENLACE RENTA CON R.E.O.     ";
     |PINTA 1422, "------------------------------------------";
     |PINTA 1522, " Desea hacer el pase en este momento :    ";

     E_sup = 1;
     E_inf = "SN";
     aSiNo = "S";
     aSiNo = 1561 ? 1;

     |ATRI 0;
     |SI aSiNo = "S"; |HAZ RecogeReo; |FINSI;
     |POPV;
|FPRC;

|PROCESO CogeArrend;
     nSwActiv   = 0;
     aTipo      = "R2";
     nSwRecoge = 1;  |HAZ RecogeReo;
     nSwRecoge = 0;
     |SI nSwActiv = 0; |ACABA; |FINSI;

     |PUSHV 0501 2380;
     |CUADRO 1221 1664;
     |ATRI R;
     |PINTA 1322, "DETECTADO ENLACE CON R.E.O.ARRENDAMIENTOS ";
     |PINTA 1422, "------------------------------------------";
     |PINTA 1522, " Desea hacer el pase en este momento :    ";

     E_sup = 1;
     E_inf = "SN";
     aSiNo  = "S";
     aSiNo  = 1561 ? 1;

     |ATRI 0;
     |SI aSiNo = "S"; |HAZ RecogeReo; |FINSI;
     |POPV;
|FPRC;

|PROCESO CogeReo;
     |HAZ CogeActiv;
     |HAZ CogeArrend;
|FPRC;

