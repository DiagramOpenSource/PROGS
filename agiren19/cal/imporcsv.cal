|VARIABLES;
     aAlfa1     = "";
     aAlfa2     = "";
     aCarac1    = "";
     aCharO     = "";
     aCharD     = "";
     aAlfaA     = "";

     nCalc1     = 0;

     &eaAlfa    = "";
|FIN;

|PROCESO CambiaChar;
     aAlfa2 = "";
     aAlfa1 = "";
     eaAlfa  = eaAlfa - aCharO;
     |PARA; |SI FSalida ! 0; |HACIENDO;
          aAlfaA = "";
          nCalc1 = (FSalida / 100) ! 0;
          |PARA; |SI nCalc1 > 99; |HACIENDO;
               aAlfa2 = eaAlfa % 0199;
               aAlfaA = aAlfaA + aAlfa2;
               eaAlfa  = eaAlfa - aAlfa2;
               nCalc1 = nCalc1 - 99;
          |SIGUE;

          nCalc1 = nCalc1 + 99;
          aAlfa1 = eaAlfa % nCalc1; eaAlfa = eaAlfa - aAlfa1;
          aAlfaA = aAlfaA + aAlfa1;
          eaAlfa  = aAlfaA + aCharD + eaAlfa;
          eaAlfa  = eaAlfa - aCharO;
     |SIGUE;
|FPRC;

|PROCESO QuitaRarosCSV;
     aCharO = "á"; aCharD = " "; |HAZ CambiaChar;
     aCharO = "é"; aCharD = "‚"; |HAZ CambiaChar;
     aCharO = "í"; aCharD = "¡"; |HAZ CambiaChar;
     aCharO = "ó"; aCharD = "¢"; |HAZ CambiaChar;
     aCharO = "ú"; aCharD = "£"; |HAZ CambiaChar;
     aCharO = "º"; aCharD = "§"; |HAZ CambiaChar;
     aCharO = "ª"; aCharD = "¦"; |HAZ CambiaChar;
     aCharO = "ñ"; aCharD = "¤"; |HAZ CambiaChar;
     aCharO = "Ñ"; aCharD = "¥"; |HAZ CambiaChar;
|FPRC;

|PROCESO QuitaRaros;
     aCharO = "€"; aCharD = "Ç"; |HAZ CambiaChar;
     aCharO = " "; aCharD = "á"; |HAZ CambiaChar;
     aCharO = "‚"; aCharD = "é"; |HAZ CambiaChar;
     aCharO = "¡"; aCharD = "í"; |HAZ CambiaChar;
     aCharO = "¢"; aCharD = "ó"; |HAZ CambiaChar;
     aCharO = "£"; aCharD = "ú"; |HAZ CambiaChar;
     aCharO = "¤"; aCharD = "ñ"; |HAZ CambiaChar;
     aCharO = "¥"; aCharD = "Ñ"; |HAZ CambiaChar;
     aCharO = "Á"; aCharD = "A"; |HAZ CambiaChar;
     aCharO = "É"; aCharD = "E"; |HAZ CambiaChar;
     aCharO = "Í"; aCharD = "I"; |HAZ CambiaChar;
     aCharO = "Ó"; aCharD = "O"; |HAZ CambiaChar;
     aCharO = "Ú"; aCharD = "U"; |HAZ CambiaChar;
     aCharO = "À"; aCharD = "A"; |HAZ CambiaChar;
     aCharO = "È"; aCharD = "E"; |HAZ CambiaChar;
     aCharO = "Ì"; aCharD = "I"; |HAZ CambiaChar;
     aCharO = "Ò"; aCharD = "O"; |HAZ CambiaChar;
     aCharO = "Ù"; aCharD = "U"; |HAZ CambiaChar;
     aCharO = "§"; aCharD = "º"; |HAZ CambiaChar;

     nCalc1 = &aCharO;
     |SI nCalc1 < 32; |O nCalc1 > 127; aCharD = " "; |FINSI;

     aCharO = &13 + &10;
     eaAlfa = eaAlfa - aCharO;
     |PARA; |SI FSalida ! 0; |HACIENDO;
          eaAlfa = eaAlfa - aCharO;
     |SIGUE;
|FPRC;
