|FICHEROS;
     recappla;
     reeospla;
     reeoscfg;
     retabla4;

     eosactiv@;
     eosclien@;
     eosm3130@;
     eosm0020@;
     eosplan1@;
     eoscomul@;
     agicontr@;

     rem00025;
     rem00026;
     rem00058;
     rem00031;
     rem00905;

     :/agitool/def/zmonedas;
|FIN;

|VARIABLES;
     nPorceGasto = 0;
     nPorce     = 0;
     Comodin    = "";
     Comodin1   = "";
     Path       = "";
     Concepto   = 0;
     Calc1      = 0;
     Calc2      = 0;
     Linea1     = 0;
     nImpCompra = 0;
     nImpVenta  = 0;
     nCampo     = 0;
     ExistIni   = 0;
     ExistFin   = 0;

     nActivid   = 0;
     nActi      = 0;
     Calc       = 0;
     nLinea     = 0;
     nAnyoRenta = 0;
     nPagos     = 0;

     Trim      = 0;
     SimpNorm  = 0;
     FEstado = 0;

     Pasada  = 0;
     Tipo    = "";
     Cont    = 0;
     TipoNum = 0;
     nPatrones = 0;
     Comunidad           = 0;

     nImporte            = 0;

     aTipo               = "";
     aCas                = "";

     &eaPrograma         = "";
     &eaPunto            = "";
     &enEmpresa          = 0;

     &enEmprEos          = 0;
     &enActiEos          = 0;
     &enEmprRen          = 0;
     &eaCaracter         = "";
     &eaTitCony          = "";

     &enOk               = 0;
     &enLin              = 0;
     &eaModal            = "";
     &ea32_1             = "";
     &ea32_2_1           = "";
     &en0180             = 0;
     &en0218             = 0;
     &en0219             = 0;
     &en0220             = 0;
     &en0221             = 0;
     &en0222             = 0;
     &en0223             = 0;
     &en0224             = 0;
     &en0225             = 0;
     &en0226             = 0;
|FIN;

|PROCESO Comunidad;
   #eosactiv@#0 = #recappla#0;
   #eosactiv@#1 = #recappla#1;
   #eosactiv@#2 = nAnyoRenta;
   |LEE 010#eosactiv@.=;
   |SI FSalida = 0;
      |SI #eosactiv@#46 = "S";
         Comunidad = 1;
         |C_M #recappla#5,1,"S";
         |C_M #recappla#6,1,"S";
      |SINO;
         Comunidad = 0;
         |C_M #recappla#5,1,"N";
         |C_M #recappla#6,1,"N";
      |FINSI;
   |SINO;
      Comunidad = 0;
      |C_M #recappla#5,1,"N";
      |C_M #recappla#6,1,"N";
   |FINSI;
|FPRC;

|PROCESO rem00031;
     |BORRA 020#rem00031.a;
     |LIBERA #rem00031;
|FPRC;

|DEFBUCLE rem00031;
     #rem00031#2;
     2;
     #recappla#2, 1, aTipo;
     #recappla#2, 9, aTipo;
     be;
     NULL, rem00031;
|FIN;

|PROCESO PagosACuenta;
   || Se elimina el registro de renta que apunta a la actividad

   |SI #recappla#3 = "T";
       aTipo = "TITULAR   ";
   |SINO;
       aTipo = "CONYUGE   ";
   |FINSI;

   |BUCLE rem00031;

   |SELECT #1#rem00031;
   Linea1 = 1;
   |PARA; |SI; |HACIENDO;
          #rem00031#0  = #recappla#2;
          #rem00031#1  = Linea1;
          |LEE 000#rem00031.=;
          |SI FSalida ! 0;  |SAL;  |FINSI;
          Linea1 = Linea1 + 1;
   |SIGUE;

   || Se crea de nuevo el registro en vacio

   |DDEFECTO #rem00031;
   #rem00031#0  = #recappla#2;
   #rem00031#1  = Linea1;
   |SI #eosactiv@#22 ! "A";
      |SI #eosactiv@#42 = "N";
         #rem00031#16 = 9;
      |SINO;
         |SI #eosactiv@#46 ! "S";
            #rem00031#16 = #eosactiv@#1;
         |SINO;
            #rem00031#16 = #eosactiv@#6;
         |FINSI;
      |FINSI;
   |SINO;
      #rem00031#16 = 0;
   |FINSI;
   |SI #recappla#3 = "T";
       #rem00031#2 = "TITULAR";
   |SINO;
       #rem00031#2 = "CONYUGE";
   |FINSI;

   || Se leen los 4 periodos del modelo

   |PARA Cont = 1; |SI Cont [ 4; |HACIENDO Cont = Cont + 1;
      |SI #eosactiv@#22 ! "A";
         || El modelo 130 en caso de que la actividad no sea de modulos
         |SI #eosactiv@#42 = "N";
            |SI #eosactiv@#46 ! "S";
               #eosm3130@#0 = #recappla#0;
            |SINO;
               #eosm3130@#0 = #recappla#5;
            |FINSI;
            #eosm3130@#1 = 9;
         |SINO;
            |SI #eosactiv@#46 ! "S";
               #eosm3130@#0 = #recappla#0;
               #eosm3130@#1 = #recappla#1;
            |SINO;
               #eosm3130@#0 = #recappla#5;
               #eosm3130@#1 = #recappla#6;
            |FINSI;
         |FINSI;
         #eosm3130@#2 = Cont;
         #eosm3130@#3 = nAnyoRenta;
         |LEE 010#eosm3130@.=;
         |SI FSalida = 0;
            |SI #eosm3130@#4 ! "          ";
               Calc = Cont + 10;

               |SI #eosm3130@#46 > 0;
                    #rem00031#Calc# = #rem00031#Calc# + #eosm3130@#46;
               |FINSI;

               #rem00031#15    = #rem00031#15    + (#eosm3130@#23 + #eosm3130@#36);
            |FINSI;
         |FINSI;
      |SINO;
         || El modelo 131 en caso de que la actividad sea de modulos
         |SI Comunidad = 0;
            #eosm0020@#0 = #recappla#0;
         |SINO;
            #eosm0020@#0 = #recappla#5;
         |FINSI;
         #eosm0020@#1 = nAnyoRenta;
         #eosm0020@#2 = Cont;
         |LEE 010#eosm0020@.=;
         |SI FSalida = 0; |Y #eosm0020@#4 ! "01.01.0000";
            Calc = Cont + 10;
            |SI #eosm0020@#47 > 0;
                #rem00031#Calc# = #rem00031#Calc# + #eosm0020@#47;
            |FINSI;
            #rem00031#15    = #rem00031#15 + #eosm0020@#54;
         |FINSI;
      |FINSI;
   |SIGUE;

   |SI #rem00031#11 ! 0; |O #rem00031#12 ! 0; |O #rem00031#13 ! 0;
    |O #rem00031#14 ! 0; |O #rem00031#15 ! 0;
       #rem00031#17 = #eosactiv@#14;

       |GRABA 020#rem00031.n;
       |LIBERA #rem00031;
       nPagos = 1;

       |LEE 010#rem00031.=;
   |FINSI;
   |LIBERA #rem00031;

   enEmpresa    = #recappla#2;
   eaPrograma   = "rem00031";
   eaPunto      = "  ";
   |HAZ ProgramaActivo;
|FPRC;

|PROCESO Graba26;
     |SI aCas = "";     |ACABA;  |FINSI;
     |SI nImporte = 0;  |ACABA;  |FINSI;

     |SELECT #2#rem00026;

     #rem00026#0 = #rem00025#0;
     #rem00026#1 = #rem00025#1;
     #rem00026#4 = aCas;
     |LEE 101#rem00026.=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     #rem00026#5 = #rem00026#5 + nImporte;

     |GRABA 020#rem00026.a;
     |LIBERA #rem00026;
|FPRC;

|PROCESO Clasifica;
     #rem00025#18 = Tipo;
     |SI #rem00025#18 = " ";
         |SI #rem00025#19 = "P"; #rem00025#18 = "5";  |FINSI;
         |SI #rem00025#19 = "A"; #rem00025#18 = "4";  |FINSI;
         |SI #rem00025#19 = "E"; #rem00025#18 = "1";  |FINSI;
     |FINSI;

     #reeoscfg#0 = "B";
     #reeoscfg#1 = TipoNum;
     |LEE 000#reeoscfg.=;
     |SI FSalida ! 0;
         |ACABA;
     |FINSI;

     |SI #reeoscfg#3 = "    ";
         |ACABA;
     |FINSI;

     nImporte = nImpCompra;
     aCas     =  #reeoscfg#3;  |QBF aCas;
     |HAZ Graba26;
|FPRC;

|PROCESO rem00905;
     #rem00026#0 = #rem00025#0;
     #rem00026#1 = #rem00025#1;
     #rem00026#2 = #rem00905#0;
     |LEE 000#rem00026.=;
     |SI FSalida = 0;  |ACABA;  |FINSI;

     |DDEFECTO #rem00026;
     #rem00026#0 = #rem00025#0;
     #rem00026#1 = #rem00025#1;
     #rem00026#2 = #rem00905#0;
     #rem00026#3 = #rem00905#1;
     #rem00026#4 = #rem00905#2;
     #rem00026#6 = #rem00905#6;

     |GRABA 020#rem00026.n;
     |LIBERA #rem00026;
|FPRC;

|DEFBUCLE rem00905;
     #rem00905#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, rem00905;
|FIN;

|PROCESO CierraDirecta;
     #rem00025#0 = #recappla#2;
     #rem00025#1 = Linea1;
     |LEE 101#rem00025.=;
     |SI FSalida ! 0;
         |ACABA;
     |FINSI;

     enLin    = #rem00025#1;
     eaModal  = #rem00025#11;
     ea32_1   = #rem00025#31;
     ea32_2_1 = #rem00025#32;
     |SUB_EJECUTA "rem00026;RECALCULA";

     #rem00025#25 = en0180;
     #rem00025#26 = en0218;
     #rem00025#27 = en0220 + en0223;
     #rem00025#28 = en0224;
     #rem00025#29 = en0225;
     #rem00025#30 = en0226;

     |GRABA 020#rem00025.a;
     |LIBERA #rem00025;
     |CIERRA #rem00025;

     eaPrograma   = "rem00025";
     eaPunto      = "  ";
     |HAZ ProgramaActivo;

     |SI #rem00025#13 = "S";
         |ABRE #rem00058;
         |SELECT #2#rem00058;
         #rem00058#0  = #rem00025#0;
         #rem00058#14 = "E1";
         #rem00058#15 = #rem00025#1;
         |LEE 040#rem00058.=;
         |SI FSalida ! 0;
             |SELECT #1#rem00058;
             #rem00058#0 = #rem00025#0;
             #rem00058#1 = 99;
             |LEE 040#rem00058.];
             |SI FSalida = 0;
                 |LEE 040#rem00058.a;
             |SINO;
                 |LEE 040#rem00058.u;
             |FINSI;
             nLinea = 1;

             |SI FSalida = 0;  |Y #rem00058#0 = #rem00025#0;
                 nLinea = #rem00058#1 + 1;
             |FINSI;
         |SINO;
             nLinea = #rem00058#1;
         |FINSI;

         |DDEFECTO #rem00058;
         |SELECT #1#rem00058;
         #rem00058#0  = #rem00025#0;
         #rem00058#1  = nLinea;
         |LEE 101#rem00058.=;
         |SI FSalida ! 0;
             |DDEFECTO #rem00058;
             #rem00058#0  = #rem00025#0;
             #rem00058#1  = nLinea;
             |GRABA 020#rem00058.b;
         |FINSI;

         #rem00058#2  = #rem00025#2;
         #rem00058#3  = "R";
         #rem00058#4  = #rem00025#4;
         #rem00058#5  = #rem00025#5;
         #rem00058#6  = #rem00025#6;
         #rem00058#7  = #rem00025#7;
         #rem00058#8  = #rem00025#8;
         #rem00058#9  = #rem00025#9;
         #rem00058#10 = #rem00025#10;
         #rem00058#11 = #rem00025#14;
         #rem00058#12 = #rem00025#15;
         #rem00058#14 = "E1";
         #rem00058#15 = #rem00025#1;

         |SI #rem00025#20 ! "8612";
             #rem00058#13 = 3;
             #rem00058#27 = #rem00025#28 % #rem00058#11;
             #rem00058#29 = #rem00025#29 % #rem00058#11;
             #rem00058#30 = #rem00058#27 - #rem00058#28 - #rem00058#29;
         |SINO;
             #rem00058#13 = 2;
             #rem00058#23 = #rem00025#28 % #rem00058#11;
             #rem00058#56 = #rem00025#29 % #rem00058#11;
             #rem00058#26 = #rem00058#23 - #rem00058#24 - #rem00058#25 - #rem00058#56;
         |FINSI;

         |GRABA 020#rem00058.a;
         |LIBERA #rem00058;
         |CIERRA #rem00058;

         eaPrograma   = "rem00058";
         eaPunto      = #rem00058#13;
         |HAZ ProgramaActivo;
     |FINSI;
|FPRC;

|PROCESO PasaPlanilla;
     #eosplan1@#0  = #recappla#0;
     #eosplan1@#83 = #recappla#1;
     #eosplan1@#1  = nAnyoRenta;
     #eosplan1@#2  = 1;
     |LEE 010#eosplan1@.];
     |SI FSalida = 0; |Y #eosplan1@#0 = #recappla#0; |Y #eosplan1@#83 = #recappla#1; |Y #eosplan1@#1 = nAnyoRenta;
         |PARA; |SI FSalida = 0; |Y #eosplan1@#0 = #recappla#0; |Y #eosplan1@#83 = #recappla#1; |Y #eosplan1@#1 = nAnyoRenta; |HACIENDO;
              Trim = #eosplan1@#2;
              |LEE 010#eosplan1@.s;
         |SIGUE;
     |SINO;
         |MENAV "    No hay planilla con datos";
         |ACABA;
     |FINSI;

     |SI #eosactiv@#22 = "B";
         SimpNorm = 0;
     |SINO;
         SimpNorm = 60;
     |FINSI;

     #eosplan1@#0  = #recappla#0;
     #eosplan1@#83 = #recappla#1;
     #eosplan1@#1  = nAnyoRenta;
     #eosplan1@#2  = Trim;
     |LEE 010#eosplan1@.=;

     Linea1 = 1;
     #rem00025#0 = #recappla#2;
     #rem00025#1 = 1;
     |LEE 010#rem00025.];
     |PARA; |SI FSalida = 0; |Y #rem00025#0 = #recappla#2; |HACIENDO;
             Linea1 = #rem00025#1 + 1;
             |LEE 010#rem00025.s;
     |SIGUE;

     |DDEFECTO #rem00025;
     #rem00025#0 = #recappla#2;
     #rem00025#1 = Linea1;
     |SI #recappla#3 = "T";
         #rem00025#2 = "TITULAR";
     |SINO;
         #rem00025#2 = "CONYUGE";
     |FINSI;

     nPorce = 100;
     |SI #eosactiv@#46 = "S";
         #eoscomul@#3 = #recappla#5;
         #eoscomul@#5 = #recappla#6;
         #eoscomul@#7 = nAnyoRenta;
         |LEE 010#eoscomul@.=;
         |SI FSalida = 0;
             #rem00025#14 = #eoscomul@#6;
         |FINSI;

         #rem00025#13 = "S";
         #rem00025#15 = #eosclien@#1;
         #rem00025#16 = #eosclien@#2;
         nPorce       = #eoscomul@#6;
     |FINSI;

     #rem00025#11 = "N";
     |SI #eosactiv@#22 = "S";
         #rem00025#11 = "N";
     |FINSI;

     #rem00025#19 = #eosactiv@#4;
     #rem00025#20 = #eosactiv@#5;
     #rem00025#21 = #eosactiv@#6;
     #rem00025#22 = #eosactiv@#11;
     #rem00025#23 = #eosactiv@#12;
     #rem00025#24 = #eosactiv@#9;
     #rem00025#35 = #eosactiv@#1;

     |BUCLE rem00905;

     nImpVenta  = #eosplan1@#92;

     nImporte = nImpVenta;
     aCas     = "0171";
     |HAZ Graba26;

     ExistIni   = #eosplan1@#48;
     ExistFin   = #eosplan1@#49;

     aCas     = "0182";
     |SI ExistFin ] ExistIni;
         nImporte = ExistFin - ExistIni;
     |SINO;
         nImporte = ExistIni - ExistFin;
     |FINSI;
     |HAZ Graba26;

     |PARA Cont = 1; |SI Cont [ 20; |HACIENDO Cont = Cont + 1;
           Concepto = Cont;
           |SI Cont [ 9;
               Calc1 = Cont + 68;
           |SINO;
               Calc1 = Cont + 109;
           |FINSI;

           nImpCompra = #eosplan1@#Calc1#;
           |SI #eosactiv@#23 = "P";
               Calc2 = Concepto + 20 + SimpNorm;
               Tipo  = "P";
           |SINO;
               Calc2 = Concepto + 40 + SimpNorm;
               |SI #recappla#4 = "M";
                   Tipo = "M";
               |SINO;
                   Tipo = "N";
               |FINSI;
           |FINSI;

           TipoNum = #reeospla#Calc2#;
           |HAZ Clasifica;
     |SIGUE;

     |GRABA 020#rem00025.n;
     |LIBERA #rem00025;

     |HAZ CierraDirecta;

     |SI nPagos = 1;  |ACABA;  |FINSI;

     |SELECT #1#rem00031;
     Linea1 = 1;
     |PARA; |SI; |HACIENDO;
            #rem00031#0  = #recappla#2;
            #rem00031#1  = Linea1;
            |LEE 000#rem00031.=;
            |SI FSalida ! 0;  |SAL;  |FINSI;
            Linea1 = Linea1 + 1;
     |SIGUE;

     || Se crea de nuevo el registro en vacio

     |DDEFECTO #rem00031;
     #rem00031#0  = #recappla#2;
     #rem00031#1  = Linea1;
     |SI #eosactiv@#22 ! "A";
         |SI #eosactiv@#42 = "N";
             #rem00031#16 = 9;
         |SINO;
             |SI #eosactiv@#46 ! "S";
                 #rem00031#16 = #eosactiv@#1;
             |SINO;
                 #rem00031#16 = #eosactiv@#6;
             |FINSI;
         |FINSI;
     |SINO;
         #rem00031#16 = 0;
     |FINSI;

     |SI #recappla#3 = "T";
         #rem00031#2 = "TITULAR";
     |SINO;
         #rem00031#2 = "CONYUGE";
     |FINSI;

     #rem00031#15 = #eosplan1@#79 % nPorce;

     |GRABA 020#rem00031.n;
     |LIBERA #rem00031;

     enEmpresa    = #recappla#2;
     eaPrograma   = "rem00031";
     eaPunto      = "  ";
     |HAZ ProgramaActivo;
|FPRC;

|PROCESO DesglosaBien;
     Comodin1 = Comodin + "eoscomul.mas";
     |CARGA_DEF Comodin1,eoscomul@;

     |PATH_DAT #eoscomul@#Path#;

     |ABRE #eoscomul@;
     |SELECT #2#eoscomul@;
     #eoscomul@#3 = #recappla#0;
     #eoscomul@#5 = #recappla#1;
     #eoscomul@#7 = nAnyoRenta;
     |LEE 010#eoscomul@.=;
     |SI FSalida = 0;
         #recappla#0 = #eoscomul@#0;
         #recappla#1 = #eoscomul@#1;
         #recappla#5 = #eoscomul@#3;
         #recappla#6 = #eoscomul@#5;
     |FINSI;
     |CIERRA #eoscomul@;
     |DESCARGA_DEF #eoscomul@;
|FPRC;

|PROGRAMA;
     |ABRE #retabla4;
     |LEE 000#retabla4.p;
     nPatrones = FSalida;
     |CIERRA #retabla4;

     |SI nPatrones ! 0;
         |SUB_EJECUTA_NP "rez00001;INSTALAR";
     |FINSI;

     nAnyoRenta = 19;              || 2019

     |DBASS Comodin; |QBF Comodin;
     Comodin1 = Comodin + "agicli/def/agicontr.mas";
     |CARGA_DEF Comodin1,agicontr@;
     |PATH_DAT #agicontr@#Comodin#;

     |ABRE #agicontr@;
     |LEE 010#agicontr@.p;
     |SI FSalida = 0;
         Path = #agicontr@#3;
         |QBF Path;
         |SI Path = "*"; |Y Path = "";
             |DBASS Comodin; |QBF Comodin;
             Path = Comodin + "agieos/fich/";
         |FINSI;
     |SINO;
         |DBASS Comodin; |QBF Comodin;
         Path = Comodin + "agieos/fich/";
     |FINSI;
     |CIERRA #agicontr@;
     |DESCARGA_DEF #agicontr@;

     |PATH_DAT #zmonedas#Path#;
     |ABRE #zmonedas;
     |LEE 040#zmonedas.p;
     |SI FSalida ! 0;  |O #zmonedas#1 ! "E";
         |CIERRA #zmonedas;
         |MENAV "    Para poder hacer el pase la REO debe estar en EUROS.";
         |ACABA;
     |FINSI;
     |CIERRA #zmonedas;

     Comodin = Comodin + "agieos/def/";

     |SI enEmpresa ! 0;
         #recappla#0 = enEmprEos;
         #recappla#1 = enActiEos;
         #recappla#2 = enEmprRen;
         #recappla#4 = eaCaracter;
         #recappla#3 = eaTitCony;
         |HAZ DesglosaBien;
         FSalida = 0;
     |SINO;
         |CLS;
         |CABEZA "Captura datos de planillas de REO a renta"
         |PINPA #0#recappla;
         |PINDA #0#recappla;

         Comodin1 = Comodin + "eosactiv.mas";
         |CARGA_DEF Comodin1,eosactiv@;
         |PATH_DAT #eosactiv@#Path#;
         |ABRE #eosactiv@;

         |ENDATOS #1#recappla;
         FEstado = FSalida;

         |CIERRA #eosactiv@;
         |DESCARGA_DEF #eosactiv@;
         FSalida = FEstado;
     |FINSI;

     |SI FSalida ! 0;
         |ACABA;
     |FINSI;

     Comodin1 = Comodin + "eosplan1.mas";  |CARGA_DEF Comodin1,eosplan1@;
     Comodin1 = Comodin + "eosclien.mas";  |CARGA_DEF Comodin1,eosclien@;
     Comodin1 = Comodin + "eosactiv.mas";  |CARGA_DEF Comodin1,eosactiv@;
     Comodin1 = Comodin + "eosm0020.mas";  |CARGA_DEF Comodin1,eosm0020@;
     Comodin1 = Comodin + "eosm3130.mas";  |CARGA_DEF Comodin1,eosm3130@;
     Comodin1 = Comodin + "eoscomul.mas";  |CARGA_DEF Comodin1,eoscomul@;

     |PATH_DAT #eosactiv@#Path#;
     |PATH_DAT #eosclien@#Path#;
