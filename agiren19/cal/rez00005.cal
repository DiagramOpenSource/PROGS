     |FINSI;
     |CIERRA #reacceso;
|FPRC;

|PROCESO GrabaFichero;
     aDef = aPathDef + aFichero + ".mas";
     |CARGA_DEF aDef, referen@;
     |SI FSalida ! 0;
         aAlfa = "     Error en cargar el Def " + aFichero + ".mas";
         |MENAV aAlfa;
         |ACABA;
     |FINSI;

     |ABRE #referen@;
