     |LEE 101#referen@.];
     |PARA; |SI FSalida = 0;  |Y #referen@#0 = #rez00005#0; |HACIENDO;
            |SALVA_DATOS #referen@;

            #referen@#0 = #rez00005#2;
            |GRABA 020#referen@.n;

            |REPON_DATOS #referen@;
            |LIBERA #referen@;

            |LEE 101#referen@.=;
            |SI #rez00005#3 = "S";
                |BORRA 020#referen@.a;
            |FINSI;
            |LIBERA #referen@;

            |LEE 101#referen@.s;
     |SIGUE;
     |LIBERA #referen@;
     |CIERRA #referen@;

     |DESCARGA_DEF #referen@;
|FPRC;

