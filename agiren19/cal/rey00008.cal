     |SI #rey00008#33 = "S";
          |CONFI "0000SSeleccionamos todas la rentas a emitir";
          nSwTodos = FSalida;
     |FINSI;

     |SI #rey00008#0 = "S";
          nDcodi = #rey00008#1;
          nHcodi = #rey00008#2;
          |BUCLE reacceso;
     |SINO;
          |PARA nCampo = 3; |SI i [ 32; |HACIENDO; i = i + 1;
               nDcodi = #rey00008#nCampo#;
               nHcodi = #rey00008#nCampo#;
               |BUCLE reacceso;
          |SIGUE;
     |FINSI;

     |SI nSwAlguno = 0;
          |ACABA;
     |FINSI;

     |SI #rey00008#33 = "S";         || Puntear
          |C_V #rew00001#4, 1, "N";

          |VENTANA 0501, 3399, -1, 16, "Selecci줻 de contribuyentes";
          nVentana = FSalida;

          |PTEC 802;  |PAUSA;

          nRango = 4 + 8 + 16 + 32;

          |ABRE #rew00001;
          |LEE 000#rew00001.p;

          |LINEAL_SIMPLE #1#rew00001, nRango, 0502, 3198, NULL, NULL, Evento;
          nGrid = FSalida;

          aEsc1  = &255 + "806";
          aEsc2  = &255 + "807";
          aRetu  = &255 + "802";
          |PARA; |SI;  |HACIENDO;
               FSalida = "::{{230}}Continuar," + nGrid;
