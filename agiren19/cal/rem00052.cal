     |SI aAlfa2 = "31.12";  nAnyoCompra = nAnyoCompra + 1;  |FINSI;

     #rew00652#21 = #rew00652#19 - #rew00652#20;

     |C_M #rew00652#22, 1, "S";
     |C_M #rew00652#24, 1, "S";
     |SI #rew00652#17 < "31.12.1996";
         |SI #rew00652#21 < 0;
             #rew00652#22 = -#rew00652#21;
             #rew00652#23 = 0;
             #rew00652#24 = 0;
             |C_M #rew00652#24, 1, "N";
         |SINO;
             #rew00652#23 = (1996 - nAnyoCompra) + 1;
             #rew00652#24 = #rew00652#21;
             #rew00652#22 = 0;
             |C_M #rew00652#22, 1, "N";
         |FINSI;
     |SINO;
         |SI #rew00652#21 < 0;
             #rew00652#22 = -#rew00652#21;
             #rew00652#23 = 0;
             #rew00652#24 = 0;
         |SINO;
             #rew00652#24 = #rew00652#21;
             #rew00652#22 = 0;
             #rew00652#23 = 0;
         |FINSI;
     |FINSI;

     |PINTA #rew00652#21;
     |PINTA #rew00652#22;
     |PINTA #rew00652#23;
     |PINTA #rew00652#24;
