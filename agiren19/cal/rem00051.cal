|PROCESO Totalm020;
     #rew00651#23 = #rew00651#21 - #rew00651#22;

     |SI #rew00651#25 ! -1;
         |C_M #rew00651#26, 1, "N";

         #rew00651#25 = 0;
         |SI #rew00651#19 = "V";
             |SI #rew00651#20 [ 39;                         #rew00651#25 = 40;  |FINSI;
             |SI #rew00651#20 ] 40;  |Y #rew00651#20 [ 49;  #rew00651#25 = 35;  |FINSI;
             |SI #rew00651#20 ] 50;  |Y #rew00651#20 [ 59;  #rew00651#25 = 28;  |FINSI;
             |SI #rew00651#20 ] 60;  |Y #rew00651#20 [ 65;  #rew00651#25 = 24;  |FINSI;
             |SI #rew00651#20 ] 66;  |Y #rew00651#20 [ 69;  #rew00651#25 = 20;  |FINSI;
             |SI #rew00651#20 ] 70;                    #rew00651#25 = 8;   |FINSI;
         |SINO;
             |SI #rew00651#20 [ 5;                     #rew00651#25 = 12;  |FINSI;
             |SI #rew00651#20 ] 6;   |Y #rew00651#20 [ 10;  #rew00651#25 = 16;  |FINSI;
             |SI #rew00651#20 ] 11;  |Y #rew00651#20 [ 15;  #rew00651#25 = 20;  |FINSI;
             |SI #rew00651#20 ] 16;                    #rew00651#25 = 25;  |FINSI;
         |FINSI;

         #rew00651#26 = #rew00651#24 % #rew00651#25;
     |SINO;
         |C_M #rew00651#26, 1, "S";
     |FINSI;

     |PINTA #rew00651#23;
     |PINTA #rew00651#25;
     |PINTA #rew00651#26;
