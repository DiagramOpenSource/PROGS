     |C_M #rew00651#20, 1, "S";
     |C_M #rew00651#21, 1, "S";
     |C_M #rew00651#22, 1, "S";
     |C_M #rew00651#24, 1, "S";
     |C_M #rew00651#25, 1, "S";
     |C_M #rew00651#29, 1, "S";

     |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
           #rew00651#nCampo# = #rem00051#nCampo#;

           |C_V #rew00651#nCampo#, 0, aAlfa;
           |SI aAlfa = "S";  |PINTA #rew00651#nCampo#;  |FINSI;
     |SIGUE;

     eaDeDonde  = "C3";
     eaLinDonde = #rem00051#1;
     eaLetra    = #rem00051#11;
     |HAZ BorraPatrimonio;

     |SI #rem00051#13 = "U";
         eaLetra   = "M1";
         |HAZ BorraPatrimonio;
     |FINSI;
|FPRC;

|PROCESO DescargaFantasma;
     |PARA nCampo = 0;  |SI nCampo [ nNumCampos;  |HACIENDO nCampo = nCampo + 1;
           |SI enSwLiquida = 1;
               |SI #rem00051#nCampo# = #rew00651#nCampo#;
                   enSwRetorno = 1;
               |FINSI;
           |FINSI;

           #rem00051#nCampo# = #rew00651#nCampo#;
     |SIGUE;

                        #rem00051#30 = "";
     |SI #rem00051#3  = "X";  #rem00051#30 = eaRojo;  |FINSI;

     |HAZ PintaDatosPantalla;
|FPRC;

|PROCESO Alta;
     |ESTADO_CONTROL nBotonAlta,   "DISABLE";
     |ESTADO_CONTROL nBotonModi,   "DISABLE";
     |ESTADO_CONTROL nBotonBaja,   "DISABLE";
     |ESTADO_CONTROL nBotonRdto,   "DISABLE";
     |ESTADO_CONTROL nBotonCalc1,  "ENABLE";

     |HAZ QuitaBtnSalir;

     |ESTADO_CONTROL nGrid, "DISABLE";

     |PULSATECLA;

     #rem00051#0 = enEmpresa;
     #rem00051#1 = 99;
     |LEE 000#rem00051.];
     |SI FSalida = 0;
         |LEE 000#rem00051.a;
     |SINO;
         |LEE 000#rem00051.u;
     |FINSI;

     |DDEFECTO #rew00651;
     #rew00651#0 = enEmpresa;
     #rew00651#1 = 1;
     |SI FSalida = 0;  |Y  #rem00051#0 = enEmpresa;
         #rew00651#1 = #rem00051#1 + 1;
     |FINSI;

     |DDEFECTO #rem00051;

     #rem00051#0 = #rew00651#0;
     #rem00051#1 = #rew00651#1;

     enModoReg = 1;
     |HAZ CargaFantasma;

     |C_M #rew00651#1, 1, "S";
     |ENDATOS #1#rew00651;
     |SI FSalida = 0;
         |HAZ DescargaFantasma;

         |GRABA 020#rem00051.n;
         |LIBERA #rem00051;

         |HAZ Patrimonio;
     |FINSI;

     |SI enModo ! 4;
         |ESTADO_CONTROL nBotonAlta, "ENABLE";
     |FINSI;

     |ESTADO_CONTROL nBotonRdto,  "ENABLE";
     |ESTADO_CONTROL nBotonCalc1, "DISABLE";

     |HAZ PonBtnSalir;

     enModoReg = 4;
     |ESTADO_CONTROL nGrid, "ENABLE";
     |REFRESCACONTROL nGrid;

     |FOCOTECLADO nGrid;
|FPRC;

|PROCESO Modificar;
     |SI nEntrar = 0;  |ACABA;  |FINSI;

     |LEE 101#rem00051.=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |ESTADO_CONTROL nBotonAlta,  "DISABLE";
     |ESTADO_CONTROL nBotonModi,  "DISABLE";
     |ESTADO_CONTROL nBotonBaja,  "DISABLE";
     |ESTADO_CONTROL nBotonRdto,  "DISABLE";
     |ESTADO_CONTROL nBotonCalc1, "ENABLE";

