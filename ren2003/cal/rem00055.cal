|PROCESO Totalm020;
     #60#7 = #60#5 - #60#6;
     |SI #60#7 < 0;
         #60#8 = -#60#7;
         #60#9 = 0;
     |SINO;
         aAlfa1      = #60#3 % -104;
         aAlfa3      = #60#3 % 0105;
         aAlfa2      = #60#3 % -104;
         nAnyoCompra = aAlfa1;
         nAnyoVenta  = aAlfa2;
         nDiferencia = nAnyoVenta - nAnyoCompra;

         |SI aAlfa3 = "31.12";  nAnyoCompra = nAnyoCompra + 1;  |FINSI;

         nAnyos = (1996 - nAnyoCompra) + 1;
         nCoefi = 0;
         |SI nAnyos ] 3;
             nCoefi = (nAnyos - 2) * 14.28;
         |FINSI;
         |SI nCoefi > 99; nCoefi = 100; |FINSI;

         #60#8 = 0;
         #60#9 = #60#7 % (100 - nCoefi);
     |FINSI;

     |PINTA #60#7;
     |PINTA #60#8;
     |PINTA #60#9;

     |C_M #60#10, 1, "S";
     |C_M #60#11, 1, "S";
     |SI #60#7 = 0;  |O #60#7 < 0;
         |C_M #60#10, 1, "N";  #60#10 = 0;  |PINTA #60#10;
         |C_M #60#11, 1, "N";  #60#11 = 0;  |PINTA #60#11;
