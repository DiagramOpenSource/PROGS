|VARIABLES;
   aDirecPLB  = "";
   aFichePLB  = "";
|FIN;

|PROCESO EsComersan;
   |DDEF aDirecPLB; |QBF aDirecPLB;
   aFichePLB = aDirecPLB + "comz0001.mas";
   |XABRE "E", aFichePLB, 0;
   |SI FSalida = 0;
      aFichePLB = aDirecPLB + "salud001.mas";
      |XABRE "E", aFichePLB, 0;
      |SI FSalida = 0;
         FSalida = 1;
      |SINO;
         FSalida = 0;
      |FINSI;
   |FINSI;

   ||FSalida = 0;
   || ... Solo para pruebas  FSalida = 0; || Cuidado, quitar antes de sacar
|FPRC;

|PROCESO EsForest;
   |DDEF aDirecPLB; |QBF aDirecPLB;
   aFichePLB = aDirecPLB + "cacontar.mas";
   |XABRE "E", aFichePLB, 0;
   |SI FSalida = 0;
/*
      aFichePLB = aDirecPLB + "salud001.mas";
      |XABRE "E", aFichePLB, 0;
      |SI FSalida = 0;
         FSalida = 1;
      |SINO;
         FSalida = 0;
      |FINSI;
*/
   |FINSI;
|FPRC;

|PROCESO EsLuxe;
     |DDEF aDirecPLB; |QBF aDirecPLB;
     aFichePLB = aDirecPLB + "luxeg001.mas";
     |XABRE "E", aFichePLB, 0;
     |SI FSalida = 0;
          aFichePLB = aDirecPLB + "salud001.mas";
          |XABRE "E", aFichePLB, 0;
          |SI FSalida = 0;
               FSalida = 1;
          |SINO;
               FSalida = 0;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO EsPceGroup;
     |DDEF aDirecPLB; |QBF aDirecPLB;
     aFichePLB = aDirecPLB + "cacepm03.mas";
     |XABRE "E", aFichePLB, 0;
     |SI FSalida = 0;
          aFichePLB = aDirecPLB + "salud001.mas";
          |XABRE "E", aFichePLB, 0;
          |SI FSalida = 0;
               FSalida = 1;
          |SINO;
               FSalida = 0;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO EsIber21;
     aFichePLB = aDirecPLB + "dscomer9/bin/iber0001.tab";
     |XABRE "E", aFichePLB, 0;
|FPRC;

|PROCESO EsTagloma; || Como en gestion!!!
     |DBASS aDirecPLB;
     aFichePLB = aDirecPLB + "dscomer9/def/tagm0001.mas";
     |XABRE "E", aFichePLB, 0;
     |SI FSalida ] 0;
          |HAZ EsIber21;
          |SI FSalida ] 0;
               FSalida = 1;
          |SINO;
               FSalida = 0;
          |FINSI;
     |FINSI;
|FPRC;
