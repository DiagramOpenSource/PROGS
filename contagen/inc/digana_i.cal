|VARIABLES;
    dn_desde = 0;
    dn_hasta = 0;
    dn_nume1 = 0;
    dn_error = 0;
    dn_trozo = "";
    dn_masca = "";
    dn_alfa1 = "";
    dn_cuent = "";
|FIN;

|CALCULO dig_ana1; |TIPO 0;             || carga variables
    dn_nume1 = Campo - 2;    dn_desde = #0dn_nume1;  || a partir de ...
    dn_nume1 = Campo - 1;    dn_hasta = #0dn_nume1;  || nro. digitos
    dn_masca = #0Campo;      |QBF dn_masca;         || mascara
    dn_nume1 = (dn_desde * 100) + dn_hasta;
    dn_trozo = dn_nume1;
|FCAL;

|CALCULO dig_ana2;
    dn_error = 0;                   || switch de error
|| ...  dn_cuent = #1#0;                || cuenta
    dn_alfa1 = dn_cuent % dn_trozo;   || mascara
|SI dn_alfa1 ! dn_masca;|Y dn_desde ! 0;|Y dn_hasta ! 0;
        dn_error = 1;               || no es valida la cuenta
|FINSI;
|FCAL;
