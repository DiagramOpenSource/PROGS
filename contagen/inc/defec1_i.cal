|VARIABLES;
 nMensa9  = 0;
 nMensa   = 0;
 uno      = "";
 dos      = "";
 tres     = "";
 mascara  = "";
 nuno     = 0;
 ndos     = 0;
 nivel    = 0;
 nNoBal   = 0;
 nNoBal8  = 0;
 nNoBalX  = 0;
 mensa0   = "";
 unoU     = "";
 unoI     = "";
 unoF     = "";
 nVale    = 0;
 nunohay  = 0;
 nUno2    = 0;
 aUno3    = "";
 nUno5    = 0;
|FIN;

|PROCESO defect1;

  |PDEFECTO #cacuenta,4,9;
  nNoBal  = 0;
  nNoBal8 = 0;
  nNoBalX = 0;
  nUno2   = 0;
  aUno3   = "A";
  nUno5   = 1;

  uno     = #cacuenta#0; |QBF uno;
  |SI uno = ""; |VETE findefe; |FINSI;
  uno  = uno % 0;
  nuno = uno;

  |DDEFECTO #ca8cumay;
  nNoBal  = 1;
  nNoBal8 = 1;
  nNoBalX = 1;

  |ABRE #cacuebal;
  uno  = #cacuenta#0; |QBF uno;
  tres = uno;
  dos  = uno + "000000000000";

  |SI nuno ] 5;
      ndos = 5;
      uno = dos % A105;
      |HAZ leodefect1;
      |SI nNoBal = 0; |Y nNoBal8 = 0; |Y nNoBalX = 0; |VETE findefe; |FINSI;
  |FINSI;

  |SI nuno ] 4;
      ndos = 4;
      uno = dos % A104;
      |HAZ leodefect1;
      |SI nNoBal = 0; |Y nNoBal8 = 0; |Y nNoBalX = 0; |VETE findefe; |FINSI;
  |FINSI;

  |SI nuno ] 3;
      ndos = 3;
      uno = dos % A103;
      |HAZ leodefect1;
      |SI nNoBal = 0; |Y nNoBal8 = 0; |Y nNoBalX = 0; |VETE findefe; |FINSI;
  |FINSI;

  |SI nuno ] 2;
      ndos = 2;
      uno = dos % A102;
      |HAZ leodefect1;
      |SI nNoBal = 0; |Y nNoBal8 = 0; |Y nNoBalX = 0; |VETE findefe; |FINSI;
  |FINSI;

  ndos = 1;
  uno = dos % A101;
  |HAZ leodefect1;
  |SI nNoBal = 0; |Y nNoBal8 = 0; |Y nNoBalX = 0;
      |VETE findefe;
  |SINO;
      |SI nNoBal = 1;
          mensa0 = "     No esta definida esta cuenta: " + tres + " en Cuentas de Balance 1990";
          |SI nMensa9 = 0; |MENAV mensa0; |FINSI;
          |HAZ sacaX1;
      |FINSI;
||      |SI nNoBalX = 1;  || Tique 2775_639
||          mensa0 = "    La Cuenta: " + tres + " no es correcta en el Plan Contable " + eNum2008;
||          |SI nMensa = 0; |MENAV mensa0; |FINSI;
||      |SINO;
          |SI nNoBal8 = 1;
              mensa0 = "     No esta definida esta cuenta: " + tres + " en Cuentas de Balance 2008";
             |SI nMensa = 0; |MENAV mensa0; |FINSI;
          |FINSI;
||      |FINSI;
  |FINSI;

 |ET findefe;
  nivel = #cacuenta#1;
  || la 3a clave esta formada por la cta + zzzz y el campo fantasma del nivel;
  mascara = #cacuenta#0;
  |QBF mascara;
  mascara = mascara +"zzzzzzzzzzzz";
  mascara = mascara % 112;
  #cacuenta#54    = mascara;
  #cacuenta#55    = nivel;
  #cacuenta#3     = "N";
  |SI #cacuenta#1 = dig3; #cacuenta#3 = "S"; |FINSI;
  |CIERRA #cacuebal;
|FPRC;

|PROCESO saca1;
   #cacuenta#4 = #cacuebal#1;
   #cacuenta#5 = #cacuebal#2;
   #cacuenta#6 = #cacuebal#3;
   #cacuenta#7 = #cacuebal#4;
   #cacuenta#8 = #cacuebal#5;
   nNoBal = 0;
|FPRC;

|PROCESO sacaX1;
   #cacuenta#4 = nUno2;
   #cacuenta#5 = aUno3;
   #cacuenta#6 = nUno5;
|FPRC;

|PROCESO saca_2008_1;
   nNoBal8 = 0;
|FPRC;

|DEFBUCLE saca_2008_1;
   #ca8m0002#2;
   ;
   enEjerBal, 00, 0, uno;
   enEjerBal, 99, 2, unoU;
   ;
   NULL, saca_2008_1;
|FIN;

|PROCESO leodefectos1;
     nVale = 0;
     |ABRE #ca8cumay;
     #ca8cumay#0  = uno;
     #ca8cumay#10 = eNum2008;
     #ca8cumay#12 = enEjerBal;
     |LEE 001#ca8cumay.=;
     |SI FSalida = 0;
         nUno2 = #ca8cumay#2;
         aUno3 = #ca8cumay#3;
         nUno5 = #ca8cumay#5;
         nVale = 1;
     |FINSI;
     |CIERRA #ca8cumay;
|FPRC;

|PROCESO LeoPorNivel1;
     nunohay = 1;
|FPRC;

|DEFBUCLE LeoPorNivel1;
     #ca8cumay#1;
     ;
     enEjerBal, eNum2008, unoI;
     enEjerBal, eNum2008, unoF;
     ;
     NULL, LeoPorNivel1;
|FIN;

|PROCESO leodefect1;
     |SI nNoBal = 1;
         #cacuebal#0 = uno;
         |LEE 040#cacuebal.=;
         |SI FSalida = 0;
             |HAZ saca1;
         |FINSI;
     |FINSI;

     |SI nNoBalX = 1;
         |SI ndos = 5;
             |HAZ leodefectos1;
             |SI nVale = 1; nNoBalX = 0; |FINSI;
         |FINSI;
         |SI ndos = 4; |O ndos = 3;
             nunohay = 0;
             |SI nuno > ndos;
                 unoI = uno + "0";
                 unoF = uno + "9";
                |BUCLE LeoPorNivel1;
             |FINSI;
             |SI nunohay = 0;
                |HAZ leodefectos1;
                |SI nVale = 1; nNoBalX = 0; |FINSI;
             |FINSI;
         |FINSI;
         |SI ndos = 2;
             |HAZ leodefectos1;
             |SI nuno [ 2; |Y nVale = 1; nNoBalX = 0; |FINSI;
         |FINSI;
         |SI ndos = 1; |Y nuno = 1;
             |HAZ leodefectos1;
             |SI nVale = 1; nNoBalX = 0; |FINSI;
         |FINSI;
     |FINSI;

     |SI nNoBal8 = 1;
         |SI nuno > 3;
             uno   = (uno + "     " ) % 105;
             unoU  = uno;
         |SINO;
             unoU  = (uno + "99999" ) % 105;
             uno   = (uno + "     " ) % 105;
         |FINSI;
         |BUCLE saca_2008_1;
     |FINSI;
|FPRC;
