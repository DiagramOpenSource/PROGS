|FINSI;
|SI sw_print = 1;
    |PINTA #4#5;
    |PINTA #4#6;
    |PINTA #4#7;
|FINSI;
    sw_print = 0;
|SI #1#8 = "D";
        #4#5 = #4#5 + #1#9;
        #4#13 = #4#13 + #1#9;
        #4#14 = #4#14 + #1#9;
|SINO;
        #4#6 = #4#6 + #1#9;
        #4#13 = #4#13 - #1#9;
        #4#15 = #4#15 + #1#9;
|FINSI;
    #4#7 = #4#5 - #4#6;
    #4#16 = #4#14 - #4#15;
    #4#18 = #1#18;
|SI sw_pinta = 0;
    |PINTA #4#1;
    |PINTA #4#2;
    |PINTA #4#3;
    |PINTA #4#4;
        sw_pinta = 1;
|FINSI;
    Pos = lin;
|PINDA #0#4;
    Pos = -1;
|PINTA #4#14;
|PINTA #4#15;
|PINTA #4#16;

    lin = lin + 100;
|SI lin > 700;
    |HAZ cambiapa;
        sw_pinta = 0;
    |SI sw_error = 1;
        |ACABA;
    |FINSI;
    |PINTA #4#5;
    |PINTA #4#6;
    |PINTA #4#7;
|FINSI;
|FPRC;

