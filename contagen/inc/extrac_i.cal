|PROCESO buclepan;
|SI sw = 0;
        sw = 1;
    |DDEFECTO #4;
    |HAZ leecab;
|FINSI;
    #4#8 = #1#1;
    #4#9 = #1#2;
    #4#10 = #1#7;
    #4#11 = #1#8;
|SI #4#11 = "D";
