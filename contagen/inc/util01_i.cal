|VARIABLES;
     aCadena   = "";
     aLong     = "";
     nLong     = 0;
     nCampo    = 0;
     nPos      = 0;
     aCaracter = "";
     &eaAlfa   = "";
     aCarac1   = "";
     aCarac2   = "";
     aCarac3   = "";
     aCarac4   = "";
     aCarac5   = "";
     aCarac6   = "";
     aCarac7   = "";
     aCarac8   = "";
     aCarac9   = "";
     aCarac10  = "";
     aCarac11  = "";
     aCarac12  = "";
     aCarac13  = "";
     aCarac14  = "";
     aCarac15  = "";
|FIN;

|PROCESO QuitaRaros_D;
     aCadena = "";
     aLong   = eaAlfa % 0;
     nLong   = aLong;
     |PARA nCampo = 1;  |SI nCampo [ aLong;  |HACIENDO nCampo = nCampo + 1;
           nPos = (100 * nCampo) + 1;
           aCaracter = eaAlfa % nPos;
           |SI aCaracter = " ";  aCaracter = "_";  |FINSI;
           |SI aCaracter = ",";  aCaracter = "_";  |FINSI;
           |SI aCaracter = ".";  aCaracter = "";   |FINSI;
           |SI aCaracter = "/";  aCaracter = "_";  |FINSI;
           |SI aCaracter = "\";  aCaracter = "_";  |FINSI;
           |SI aCaracter = "•";  aCaracter = &209; |FINSI;
           |SI aCaracter = "§";  aCaracter = &209; |FINSI;
           |SI aCaracter = "ß";  aCaracter = &176; |FINSI;
           |SI aCaracter = "¶";  aCaracter = &170; |FINSI;

           aCadena = aCadena + aCaracter;
     |SIGUE;
     eaAlfa = aCadena;
     eaAlfa = eaAlfa > eaAlfa;
|FPRC;

|PROCESO QuitaRaros_Acentos;
     aCarac1   = "µ";
     aCarac2   = "ê";
     aCarac3   = "÷";
     aCarac4   = "‡";
     aCarac5   = "È";
     aCarac6   = "¿";
     aCarac7   = "…";
     aCarac8   = "Õ";
     aCarac9   = "”";
     aCarac10  = "⁄";
     aCarac11  = "¡";
     aCarac12  = "√";
     aCarac13  = "©";
     aCarac14  = "≥";

     aCadena = "";
     aLong   = eaAlfa % 0;
     nLong   = aLong;
     |PARA nCampo = 1;  |SI nCampo [ aLong;  |HACIENDO nCampo = nCampo + 1;
           nPos = (100 * nCampo) + 1;
           aCaracter = eaAlfa % nPos;

           |SI aCaracter = "•";  aCaracter = &209; |FINSI;
           |SI aCaracter = "§";  aCaracter = &209; |FINSI;
           |SI aCaracter = "ß";  aCaracter = &176; |FINSI;
           |SI aCaracter = "¶";  aCaracter = &170; |FINSI;

           |SI aCaracter = "†";  aCaracter = "a";  |FINSI;
           |SI aCaracter = "Ç";  aCaracter = "e";  |FINSI;
           |SI aCaracter = "°";  aCaracter = "i";  |FINSI;
           |SI aCaracter = "¢";  aCaracter = "o";  |FINSI;
           |SI aCaracter = "£";  aCaracter = "u";  |FINSI;

           |SI aCaracter = aCarac1;  aCaracter = "A";   |FINSI;
           |SI aCaracter = aCarac2;  aCaracter = "E";   |FINSI;
           |SI aCaracter = aCarac3;  aCaracter = "I";   |FINSI;
           |SI aCaracter = aCarac4;  aCaracter = "O";   |FINSI;
           |SI aCaracter = aCarac5;  aCaracter = "U";   |FINSI;

           |SI aCaracter = aCarac6;  aCaracter = "A";   |FINSI;
           |SI aCaracter = aCarac7;  aCaracter = "E";   |FINSI;
           |SI aCaracter = aCarac8;  aCaracter = "I";   |FINSI;
           |SI aCaracter = aCarac9;  aCaracter = "O";   |FINSI;
           |SI aCaracter = aCarac10; aCaracter = "U";   |FINSI;
           |SI aCaracter = aCarac11; aCaracter = "A";   |FINSI;

           |SI aCaracter = aCarac12; aCaracter = "";  |FINSI;
           |SI aCaracter = aCarac13; aCaracter = "e"; |FINSI;
           |SI aCaracter = aCarac14; aCaracter = "o"; |FINSI;

           aCadena = aCadena + aCaracter;
     |SIGUE;
     eaAlfa = aCadena;
|FPRC;
