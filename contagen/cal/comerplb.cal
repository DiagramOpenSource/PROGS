|VARIABLES;
   &eaSalida  = "";
   &eaEntrada = "";

   aAlfa = "";

   nNumChar = 0;
   nCont    = 0;
   nCalc    = 0;
|FIN;

|PROCESO Encriptado;
   eaSalida = "";
   aAlfa = eaEntrada; |QBF aAlfa;
   aAlfa = aAlfa % 0; nNumChar = aAlfa;
   |PARA nCont = 1; |SI nCont [ nNumChar; |HACIENDO nCont = nCont + 1;
      nCalc = (nCont * 100) + 1; aAlfa = eaEntrada % nCalc;
      nCalc = &aAlfa;
      |SI nCalc > 79;
         nCalc = 79 - (nCalc - 79);
      |SINO;
         nCalc = 79 + (79 - nCalc);
      |FINSI;
      eaSalida = eaSalida + &nCalc;
   |SIGUE;
|FPRC;
