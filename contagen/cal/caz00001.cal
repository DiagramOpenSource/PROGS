|FICHEROS;
  caz00001 #999;
  catesfin #0,MANTE;
  n43saldo #43;

  &catesfi@ #900;
  &n43sald@ #943;
|FIN;

|VARIABLES;
   &Fin     = 1;
   &eaPathO = "";
   &eaPathI = "";
   aMas     = "";
   aDef     = "";
   &aParam  = "";
|FIN;

|INCLUYE dscont_i;

|PROCESO Acumula;
   |SI #0#3 > 0;  #999#0 = #999#0 + #0#3;     |FINSI;
   |SI #0#3 < 0;  #999#1 = #999#1 + (-#0#3);  |FINSI;
   #999#2 = #999#0 - #999#1;
|FPRC;

|DEFBUCLE catesfin;
     #0#1;
     ;
     "            ";
     "zzzzzzzzzzzz";
     ;
     NULL, Acumula;
|FIN;

|RUTINA ClaveBaja0;
    #0#0 = "            ";
    #0#1 = 0;
|FRUT;

|RUTINA ClaveAlta0;
    #0#0 = "zzzzzzzzzzzz";
    #0#1 = 9;
|FRUT;


|PROGRAMA;
   aParam = PARAMETRO; |QBF aParam;
   |SI aParam = "";
       |HAZ inicio;
       |DFICE eaPathO;
       eaPathI = eaPathO;
   |FINSI;

   |DBASE aMas;
   aDef = aMas + "def/catesfin";
   |CARGA_DEF aDef, catesfi@;
   |SI FSalida ! 0;
       |MENAV "    Error cargado def " + aDef;
       |ACABA;
   |FINSI;

   aDef = aMas + "def/n43saldo";
   |CARGA_DEF aDef, n43sald@;
   |SI FSalida ! 0;
       |MENAV "    Error cargado def " + aDef;
       |DESCARGA_DEF #catesfi@;
       |ACABA;
   |FINSI;

   |SI aParam = "";
       |CLS;
       |CABEZA "Sit.Tesoreria y Financiacion";
       |PINPA #0#999;
       |DDEFECTO #999;
       |ABRE #0;
       |BUCLE catesfin;
       |CIERRA #0;
       |PINDA #0#999;

       |PARA; |SI Fin = 0; |HACIENDO;
              Fin = 1;
              |ENTLINEAL #1#0, -1, 2, 22, 0, ClaveBaja0, ClaveAlta0;
       |SIGUE;
   |SINO;
       |HAZ Actua_Con43;
   |FINSI;

   |DESCARGA_DEF #943;
   |DESCARGA_DEF #900;
|FPRO;
