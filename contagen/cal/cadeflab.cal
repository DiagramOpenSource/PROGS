   |SI eswCta ! 0;  |ACABA; |FINSI;  ||Cuenta erronea

   |HAZ LeeCuenta;
   |SI eswCta ! 0;  |ACABA; |FINSI;  ||Cuenta erronea

   nNume1 = Campo + 1;
   #Fichero@#nNume1# = #cacuenta#2; |PINTA #Fichero@#nNume1#;
|FCAL;


|CALCULO Centro11; |TIPO 11;  || para deflabc y deflaby
  nInkey = FSalida;
  eOpCen = 0;
  |SI nInkey  = 10;
      eOpCen = 1;
      enCentro = #Fichero@#0;
      |HAZ CentroTrabajo;
  |FINSI;
|FCAL;

|CALCULO PintaCentro; |TIPO 0;
  nInkey = FSalida;
  eOpCen = 0;
  enCentro = #Fichero@#0;
  |HAZ CentroTrabajo;
  |SI eswLect = 1;
      |ERROR;
  |SINO;
      #Fichero@#1 = eaNomCentro; |PINTA #Fichero@#1;
  |FINSI;
|FCAL;

|CALCULO c_analiticaC; |TIPO 7;
  |SI enSwAna = 0; |ACABA; |FINSI;
  eaCuenta = #2#2;
  |HAZ c_analitica;
|FCAL;

|CALCULO c_analiticaZ; |TIPO 7;
  |SI enSwAna = 0; |ACABA; |FINSI;
  eaCuenta = #13#6;
  |HAZ c_analitica;
|FCAL;

|CALCULO c_analiticaX; |TIPO 7;
  |SI enSwAna = 0; |ACABA; |FINSI;
  eaCuenta = #3#3;
  |HAZ c_analitica;
|FCAL;

|CALCULO c_analiticaY; |TIPO 7;
  |SI enSwAna = 0; |ACABA; |FINSI;
  eaCuenta = #4#5;
  |HAZ c_analitica;
|FCAL;

|CALCULO c_analitica;
  ModoEntrLin = (FEntrada / 100) ! 0;
  |HAZ LeeTablaAna;
  |SI swRelac = 0;
      #Fichero@#Campo# = eaCtaAna;
      |C_M #Fichero@#Campo#,1,eaAnaMod;
  |SINO;
      |SI ModoEntrLin = 1; #Fichero@#Campo# = ""; |FINSI;
      |SI eaPAna = "S";
          |C_M #Fichero@#Campo#,1,"N";
      |SINO;
          |C_M #Fichero@#Campo#,1,"S";
      |FINSI;
  |FINSI;
  |PINTA #Fichero@#Campo#;
|FCAL;


|CALCULO anacta; |TIPO 6;

  nInkey = FSalida;
  |SI enSwAna = 0; |ACABA; |FINSI;

  |C_M #Fichero@#Campo#,0,aAlfa1;
  |SI aAlfa1 = "N"; |ACABA; |FINSI;

  salidas = FSalida;
  |HAZ FPuntosAna;
  eOpAna = 0; || 1 = Consulta pantalla
  |SI nInkey = 10; eOpAna = 1;  |FINSI;
  eaCtaAna = #Fichero@#Campo#;
  |HAZ LeeCtaAna;
  |SI eswLect = 1;
      |MENAV "    Error.! No Existe Cuenta Analitica";
      |ERROR;
  |SINO;
      #Fichero@#Campo# = eaCtaAna;
      |PINTA #Fichero@#Campo#;
  |FINSI;
|FCAL;


|CALCULO Seccion11; |TIPO 11;  || para cadeflax
  nInkey = FSalida;
  eOpCen = 0;
  |SI nInkey  = 10;
          eOpCen = 1;
      |SI Campo = 0;
          eOpCen = 2;
      |FINSI;
      eaArea    = #Fichero@#0;
      eaSeccion = #Fichero@#1;
      |HAZ SecTrabajo;
      #Fichero@#0 = eaArea;    |PINTA #Fichero@#0;
      #Fichero@#1 = eaSeccion; |PINTA #Fichero@#1;
      FSalida = 0;
  |FINSI;
|FCAL;

|CALCULO PintaSeccion; |TIPO 0;
  nInkey = FSalida;
  eOpCen = 0;
  eaArea    = #Fichero@#0;
  eaSeccion = #Fichero@#1;
  |HAZ SecTrabajo;
  |SI eswLect = 1;
      |ERROR;
  |SINO;
      #Fichero@#0 = eaArea;    |PINTA #Fichero@#0;
      #Fichero@#1 = eaSeccion; |PINTA #Fichero@#1;
      #Fichero@#2 = eaNomSecc; |PINTA #Fichero@#2;
  |FINSI;
|FCAL;

|| *****************************************************************
|PROGRAMA;

   |HAZ inicio;
   |HAZ DirAplicSt;

   |ABRE #210;
   |DDEFECTO #210;
   |LEE 001#210p;
   |CIERRA #210;
   snFiltrA= #210#185;
   snFiltr = #210#95;
   snFilBa = #210#119;
   aDepAnt = #210#22;
   aSubAnt = #210#57;

   |C_M #0#22,1,eaDepar;
   |C_M #0#52,1,eaDepar;
   |C_M #0#57,1,eaSubde;
   |C_M #0#59,1,eaSubde;

   |HAZ Analitica;
   |SI enSwAna = 1;
       |C_A #2#3,1,24;
       |C_V #2#4,1,"S";
       |C_M #2#4,1,"S";
       |C_A #3#4,1,22;
       |C_V #3#5,1,"S";
       |C_M #3#5,1,"S";
       |C_V #4#4,1,"S";
       |C_M #4#4,1,"S";
   |FINSI;

   |CLS;
   |CABEZA "Defectos Entrada Laboral";

   |ABRE #dsmom030;

   |ABRE #0;
   |LEE 101#0p;
   |SI FSalida ! 0;
      |DDEFECTO #0;
      #0#22 = eaDfDepC;
      #0#23 = eaDfDepN;
      #0#52 = #210#20;
      #0#57 = eaDfSubC;
      #0#58 = eaDfSubN;
      #0#59 = #210#68;
      |GRABA 020#0n;
      |LEE 101#0=;
   |FINSI;

   aDepAnt = #0#22;
   aSubAnt = #0#57;
   |PINPA #0#0;
   |PINDA #0#0;
   |HAZ Boton;
   nHePulsado = 0;

   |ENDATOS #1#0;
   |SI FSalida = 0;
       |GRABA 020#0a;
       |LIBERA #0;
       |CIERRA #0;
       |HAZ cambiodep;
   |SINO;
       |LIBERA #0;
       |CIERRA #0;
   |FINSI;
   |CIERRA #dsmom030;

   |HAZ FinBoton;

   |SI nHePulsado = 1;
       |SUB_EJECUTA_NP "cadefxab;Pulsa";
   |FINSI;
|FPRO;

|PROCESO cambiodep;
  |SI aDepAnt ! #0#22; |O aSubAnt ! #0#57;
           || enEmpresa = #canempre#2;
           || enPeriodo = #canempre#3;
           enCambio = 0; |SI aDepAnt ! #0#22; enCambio = 1; |FINSI;
           eaDfDepC  = #0#22;
           eaDfSubC  = #0#57;
           eaDfDepN  = #0#23;
           eaDfSubN  = #0#58;
           |SUB_EJECUTA_NP "caut0003;2";
  |FINSI;
|FPRC;

|PROCESO Boton;
   |CONTROL_SIMPLE 0, "LBOTON,F5- Defectos por Centro", 0542, 0560, Boton5;
   nBoton5 = FSalida;

   |SI #0#60 = 0; |Y #0#61 = "N";
       |HAZ NoPulso;
   |SINO;
       |HAZ SiPulso;
   |FINSI;
|FPRC;

|PROCESO NoPulso;
   |ESTADO_CONTROL nBoton5,  "DISABLE";
|FPRC;

|PROCESO SiPulso;
     |ESTADO_CONTROL nBoton5,  "ENABLE";
|FPRC;

|PROCESO Boton5;
     |PTEC 827;
|FPRC;

|PROCESO Boton7; |TIPO 0;
     nTecla = FSalida;
     |SI #0#60 = 0; |Y #0#61 = "N"; |ACABA; |FINSI; || No hay Centros

     |SI nTecla = 13; |Y nHePulsado = 0;
         nHePulsado = 1;
         |PTEC 806;
     |FINSI;
|FPRC;

|PROCESO FinBoton;
     |FIN_CONTROL_WINDOWS nBoton5;
|FPRC;
