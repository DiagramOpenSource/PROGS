|PROCESO RepasoContra0;

 |SI #2#8 = "D"; eaCuenta = #1#10;   |FINSI;
 |SI #2#8 = "H"; eaCuenta = #1#8;    |FINSI;

 |DDEFECTO #31;
 #31#0 = #2#0;
 #31#1 = #2#1;
 #31#2 = #2#2;
 #31#3 = #2#3;
 |LEE 001#31=;
 |SI FSalida = 0;
     |SI eaCuenta = #31#4; |O #31#4 = "            ";
         |BORRA 020#31a;
     |FINSI;
 |FINSI;
|FPRC;

|DEFBUCLE RepasoContra;
 #2#1;
 ;
 #1#0,#1#1,#1#2,0001;
 #1#0,#1#1,#1#2,9999;
 e;
 NULL, RepasoContra0;
|FIN;

|PROCESO LeeDescarga;
  |ABRE #22;
  #22#0 = aElDep;
  |LEE 041#22=;
  aCtaPer = #22#2;
  nCtaPer = #22#3;
  |CIERRA #22;
|FPRC;

|PROCESO AperCierre;
 |SI nSwCab = 0;
     aElDep = #2#21;
     |HAZ LeeDescarga;
     #1#8  = aCtaPer; #1#9  = nCtaPer;
     #1#10 = aCtaPer; #1#11 = nCtaPer;
     nSwCab = 1;
 |FINSI;
 |SI aElDep ! #2#21;
     aElDep = #2#21;
     |HAZ LeeDescarga;
 |FINSI;

 |SI #902#8 = "D"; |Y #1#10 ! aCtaPer;
     aConDebe  = aCtaPer;
     |PARA nPara1 = 0; |SI nPara1 [ 24; |HACIENDO nPara1 = nPara1 + 1;
           #902nPara1 = #2nPara1;
     |SIGUE;
     |HAZ AltaContra;
 |FINSI;
 |SI #902#8 = "H"; |Y #1#8  ! aCtaPer;
     aConHaber = aCtaPer;
     |PARA nPara1 = 0; |SI nPara1 [ 24; |HACIENDO nPara1 = nPara1 + 1;
           #902nPara1 = #2nPara1;
     |SIGUE;
     |HAZ AltaContra;
 |FINSI;
|FPRC;

|PROCESO AltaContra;
 |DDEFECTO #31;
 #31#0 = #902#0;
 #31#1 = #902#1;
 #31#2 = #902#2;
 #31#3 = #902#3;
 |LEE 001#31=;
 nEstado = FSalida;
 |SI nEstado = 0;
     eaCuenta = #31#4; |QBF eaCuenta;
     |SI eaCuenta ! ""; |ACABA; |FINSI;
 |FINSI;

 |SI #902#8 = "H";
     #31#4 = aConDebe;
 |SINO;
     #31#4 = aConHaber;
 |FINSI;
 eaCuenta = #31#4; |HAZ SacaNivel;
 #31#5 = enNivel;
 #31#6 = #902#24;
 |SI nEstado ! 0; |GRABA 020#31n; |FINSI;
 |SI nEstado = 0; |GRABA 020#31a; |FINSI;
|FPRC;

|DEFBUCLE ElEspejo;
 #902#1004;
 8;
 #2#0, #2#1, #2#2, 0001,   aSigno;
 #2#0, #2#1, #2#2, nLinea, aSigno;
 ;
 NULL, AltaContra;
|FIN;


|PROCESO camaasil0;

   |SI #1#0 = 0; |O #1#0 = 99;
       |HAZ AperCierre;        || especial Apertura y Cierre
       |ACABA;
   |FINSI;

   aAlfa1 = #2#19; aAlfa1 = aAlfa1 > " ";  || Parametro de Factura
   |SI #2#8 = "D";
       |SI aConDebe = "";
           |SI nTipAsto = 0;
               aConDebe = #2#4;
           |SINO;
               |SI aAlfa1 = "F"; |O aAlfa1 = "B"; |O aAlfa1 = "X";
                   aConDebe = #2#4;
               |FINSI;
           |FINSI;
       |SINO;
           |SI aConDebe ! #2#4;
               |SI nTipAsto = 0;
                   aConDebe = #2#4;
               |SINO;
                   |SI aAlfa1 = "F"; |O aAlfa1 = "B"; |O aAlfa1 = "X";
                       aConDebe = #2#4;
                   |FINSI;
               |FINSI;
          |FINSI;
       |FINSI;
       |SI CtrpDebe = 0; |Y aConDebe ! "";
           #1#8     = aConDebe;
           CtrpDebe = 1;
       |FINSI;
       nDebe = (nDebe + #2#9) ! EURODB;
   |FINSI;

   |SI #2#8 = "H";
       |SI aConHaber = "";
           |SI nTipAsto = 0;
               aConHaber = #2#4;
           |SINO;
               |SI aAlfa1 = "F"; |O aAlfa1 = "B"; |O aAlfa1 = "X";
                   aConHaber = #2#4;
               |FINSI;
           |FINSI;
       |SINO;
           |SI aConHaber ! #2#4;
               |SI nTipAsto = 0;
                   aConHaber = #2#4;
               |SINO;
                   |SI aAlfa1 = "F"; |O aAlfa1 = "B"; |O aAlfa1 = "X";
                       aConHaber = #2#4;
                   |FINSI;
               |FINSI;
           |FINSI;
       |FINSI;
       |SI CtrpHaber = 0; |Y aConHaber ! "";
           #1#10     = aConHaber;
           CtrpHaber = 1;
       |FINSI;
       nHaber = (nHaber + #2#9) ! EURODB;
   |FINSI;

   |PARA nPara1 = 0; |SI nPara1 [ 24; |HACIENDO nPara1 = nPara1 + 1;
         #902nPara1 = #2nPara1;
   |SIGUE;

   |HAZ AltaContra;

   nLinea = #2#3 - 1;
   |SI nLinea > 0;
       aSigno = "D"; |SI #2#8 = "D"; aSigno = "H"; |FINSI;
       |BUCLE ElEspejo;
   |FINSI;

   |SI nDebe = nHaber;
       aConDebe  = "";
       aConHaber = "";
   |FINSI;
|FPRC;

|DEFBUCLE camaasil0;
 #2#1;
 ;
 #1#0,#1#1,#1#2,0001;
 #1#0,#1#1,#1#2,9999;
 e;
 NULL, camaasil0;
|FIN;
