     |COLOR 14, 0;
     |ATRI 0;
|FPRC;

|PROCESO ColorF;        || activa el color graficos en Avisos
     |COLOR 4, 0;
     |ATRI 0;
|FPRC;


|PROCESO Tipo3; |TIPO 3;          || desde campo 'linea'

     aAlfa1 = #0#10;   |QBF aAlfa1;
     aAlfa2 = #0#8;    |QBF aAlfa2;
     |SI aAlfa1 = "";  |Y aAlfa2 = "";    ||al menos una cuenta cargada;
         aAvisos = "ATENCION!!! Apunte sin cuentas ...";
         |HAZ Aviso5;
         |PAUSA;
         |HAZ Aviso9;
         |HAZ DejoBien;
         |ERROR;
         |ACABA;
     |FINSI;

     |SI #0#7 = 0;                       || importe distinto de cero
         aAvisos = "ATENCION!!! Importe cero ...";
         |HAZ Aviso5;
         |PAUSA;
         |HAZ Aviso9;
         |ERROR;
         |ACABA;
     |FINSI;

     |SI aAlfa1 ! "";
         aCtDeAnt = #0#10;
         nDiDeAnt = #0#11;
     |FINSI;

     |SI aAlfa2 ! "";
         aCtHaAnt = #0#8;
         nDiHaAnt = #0#9;
     |FINSI;

     nCoAnt  = #0#4;
     aDeAnt  = #0#5;
     aDhAnt  = #0#6;
     nImpAnt = #0#7;
|FPRC;

|PROCESO Salta; |TIPO 0;
     nModoEntrada = (FEntrada / 100) ! 0;
     salidas = FSalida;
     |SI nModoEntrada = 1; |Y #0#1 ! 1;
         |SI nCoAnt ! 0;
             #0#4 = nCoAnt;
             #0#5 = aDeAnt;
             #0#6 = aDhAnt;
         |FINSI;
         |PINTA #0#4;
         |PINTA #0#5;
     |FINSI;
|FPRC;


|PROCESO Tipo2; |TIPO 2;

     |HAZ Signo;

     |SI #0#6 = "D"; #11#12 = #11#12 +. #0#7; |FINSI;
     |SI #0#6 = "H"; #11#13 = #11#13 +. #0#7; |FINSI;

     #11#14 = #11#12 - #11#13;

     |PINTA #11#12;  |PINTA #11#13;  |PINTA #11#14;
|FPRC;

|PROCESO DejoBien;
     |SI nModoEntrada ! 1; |ACABA; |FINSI;

     |SI #0#6 = "D"; #11#12 = #11#12 - #0#7; |FINSI;
     |SI #0#6 = "H"; #11#13 = #11#13 - #0#7; |FINSI;
     #11#14 = #11#12 - #11#13;
     |PINTA #11#12;  |PINTA #11#13;  |PINTA #11#14;
|FPRC;

|PROCESO ConGruEnl;
     #11#11 = "N";
     |SI #11#14 ! 0;
         |CONFI "    NASIENTO DESCUADRADO. Desea Continuar?";
         |SI FSalida ! 0;
             |ERROR;
         |SINO;
             #11#11 = "S";
         |FINSI;
         |ACABA;
     |FINSI;

     nNume = #11#12;
     |SI #11#10 [ 0; nNume = -nNume; |FINSI;
     |SI #11#10 ! nNume;
         ||MENAV "    EL ASIENTO NO COINCIDE CON EL IMPUESTO A DEVENGAR";
     |FINSI;
     #11#11 = "S";
|FPRC;

|PROCESO Tipo11;  |TIPO 11;
      salidas = FSalida;
      |SI salidas = 1;
          |HAZ ConGruEnl;
          salidas = 0;
      |FINSI;
|FPRC;

