|PROCESO ComoSiempre;
     nImport    = #11#8 + #11#9 - #11#40;

     |SI nImport < 0;
         nLineal = nLineal + 1;
         |DDEFECTO #0;
         #0#0  = #11#29;
         #0#1  = nLineal;
         #0#2  = #100#31;
         #0#3  = #100#32;
         #0#4  = #100#48;
         #0#5  = #100#49;
         #0#6  = "D";
         #0#7  = -#11#14;
         #0#10 = #100#31;
         #0#11 = #100#32;
         |SI #0#7 < 0;
             #0#6 = "H";
             #0#7 = -#0#7;
             #0#8 = #100#31;
             #0#9 = #100#32;
             #0#10 = "";
             #0#11 = 0;
         |FINSI;
         |GRABA 020#0n;

