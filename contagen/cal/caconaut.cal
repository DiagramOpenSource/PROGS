     |PINTA #0Campo;
|FCAL;

|CALCULO QueTipo; |TIPO 0;
     aAlfa1 = "S";
     |SI #0#27 ! "I";
         aAlfa1 = "N";
         #0#35 = 0; |PINTA #0#35;
         #0#36 = 1; |PINTA #0#36;
     |FINSI;
     |C_M #0#35, 1, aAlfa1;
     |C_M #0#36, 1, aAlfa1;
|FCAL;

|CALCULO fechas; |TIPO 0;
  |SI #0#34 = "N";
      |SI #0Campo > fec2;|O #0Campo < fec1;
          |MENAV "    Fecha fuera de Periodo ...";
