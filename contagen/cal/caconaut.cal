|PROCESO LeeApuntes0;
   |SI Haybasica = 0
       |SI #camaasil#21 < #0#32; |O #camaasil#21 > #0#33;
           |ACABA;
       |FINSI;
   |SINO;
       nDepar = #camaasil#21; |SI nDepar = 0;  nDepar = 1;   |FINSI;
       |SI nDepar < #0#32; |O nDepar > #0#33;  ||desde/hasta actividad
           |ACABA;
       |FINSI;
   |FINSI;

     nContador = nContador + 1;
     #5#0 = nContador;
     #5#1 = #camaasil#0;
     #5#2 = #camaasil#1;
     #5#3 = #camaasil#2;
     #5#4 = #camaasil#3;
     #5#5 = #camaasil#7;
     |SI #camaasil#8 = "D";
         #5#6 = #camaasil#9;
         #5#7 = 0;
     |SINO;
         #5#7 = #camaasil#9;
         #5#6 = 0;
     |FINSI;
     TotDebe = TotDebe + #5#6;
     TotHabe = TotHabe + #5#7;
     #5#9 = TotDebe - TotHabe;
     |SI #5#9 = 0;  HaySaldo = HaySaldo + 1;  |FINSI;
     #5#8  = #camaasil#18;

