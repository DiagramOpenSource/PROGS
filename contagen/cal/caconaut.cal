|RUTINA GrabaImp;
     #6#0 = BuscaImp;
     #6#1 = DebeHab;
     #6#2 = 0;
     #6#3 = " ";
     |LEE 101#6];
     |SI FSalida = 0;
         |SI #6#0 ! BuscaImp;  |LIBERA #6;  |ACABA;  |FINSI;
         |SI #6#1 ! DebeHab;   |LIBERA #6;  |ACABA;  |FINSI;
         |SI #6#3 = " ";
             EncoImp = 1;
             #6#4 = #6#4 + 1;
             |SI #6#4 = #0#36
                 #6#3 = "C";
             |FINSI;
             nNume1 = 4 + #6#4;
             #6nNume1 = #5#0;
             |GRABA 020#6a;
             |LIBERA #6;
             |ACABA;
        |FINSI;
     |FINSI;
    |ET LeoOtra;
    |LIBERA #6;
    |LEE 101#6s;
    |SI FSalida = 0;
        |SI #6#0 ! BuscaImp;  |LIBERA #6;  |ACABA;  |FINSI;
        |SI #6#1 ! DebeHab;   |LIBERA #6;  |ACABA;  |FINSI;
        |SI #6#3 = " ";
