|FICHEROS;
   :/basica/def/zmonedas;
   :/contagen/def/camoneda;
|FIN;

|INCLUYE dscont_i;

*****************************************************************************
|PROCESO LeeConversor;
     |ABRE #zmonedas;
     |LEE 040#zmonedas.p;
     |SI FSalida ! 0;  |DDEFECTO #zmonedas;  |FINSI;
     |CIERRA #zmonedas;

     |ABRE #camoneda;
     #camoneda#0 = enEmpresa;
     #camoneda#1 = enPeriodo;
     |LEE 101#camoneda.=;
     |SI FSalida ! 0;
         |DDEFECTO #camoneda;
         #camoneda#0 = enEmpresa;
         #camoneda#1 = enPeriodo;
         |GRABA 020#camoneda.n;
     |FINSI;
     |LIBERA #camoneda;
     |CIERRA #camoneda;

     enConverModelo     = 0;
     enRedonModelo      = 2;
     enRedonPaYaModelo  = 2;
     |SI #zmonedas#9 = "P";
         enRedonModelo      = 0;
         enRedonPaYaModelo  = 0;
     |FINSI;

     |SI #zmonedas#9 = "E";
         |SI #camoneda#2 = "P";
             enConverModelo    = 166.386;
             enRedonModelo     = 0;
             enRedonPaYaModelo = 2;
         |FINSI;
     |SINO;
         |SI #camoneda#2 = "E";
             enConverModelo    = 0.00601012;
             enRedonModelo     = 2;
             enRedonPaYaModelo = 0;
         |FINSI;
     |FINSI;

     enConverContasoc    = 0;
     enRedonContasoc     = 2;
     enRedonPaYaContasoc = 2;
     |SI #zmonedas#7 = "P";
         enRedonContasoc     = 0;
         enRedonPaYaContasoc = 0;
     |FINSI;

     |SI #zmonedas#7 = "E";
         |SI #camoneda#2 = "P";
             enConverContasoc = 166.386;
             enRedonContasoc     = 0;
             enRedonPaYaContasoc = 2;
         |FINSI;
     |SINO;
         |SI #camoneda#2 = "E";
             enConverContasoc = 0.00601012;
             enRedonContasoc     = 2;
             enRedonPaYaContasoc = 0;
         |FINSI;
     |FINSI;


     |SI enEjercicio < 2002;
         enConverRenta00    = 0;
         enRedonRenta00     = 0;
         enRedonPaYaRenta00 = 0;
         |SI #camoneda#2 = "E";
             enConverRenta00    = 0.00601012;
             enRedonRenta00     = 2;
             enRedonPaYaRenta00 = 0;
         |FINSI;
     |SINO;
         enConverRenta00    = 0;
         enRedonRenta00     = 2;
         enRedonPaYaRenta00 = 2;
         |SI #camoneda#2 = "P";
             enConverRenta00    = 166.386;
             enRedonRenta00     = 0;
             enRedonPaYaRenta00 = 2;
        |FINSI;
     |FINSI;
|FPRC;
