            |Y #2#4 ] 0;
                             |HACIENDO;
            #2#47 = conta;
            |GRABA 021#2a;
            |LIBERA #2;
            |LEE 101#2s;
    |SIGUE;
    #2#0 = dia;
    #2#1 = fech;
    #2#2 = docu;
