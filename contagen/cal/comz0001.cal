        |PINTA 1112, "ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ";
        |BLANCO 1212 1966;
        |C_M #0#3, 1, "N"; |C_V #0#3, 1, "N";
        |C_M #0#6, 1, "N"; |C_V #0#6, 1, "N";
        |C_M #0#7, 1, "N"; |C_V #0#7, 1, "N";
        |C_M #0#8, 1, "N"; |C_V #0#8, 1, "N";
     |FINSI;

     |ENDATOS #1#0;
     |SI FSalida = 0; |Y #0#0 = "SI";
        |GRABA 020#comz0001.a;
        dir_fich1 = #1#2;
