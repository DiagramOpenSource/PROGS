           aAlfa = dir_fich1; |QBF aAlfa;
           aAlfa = aAlfa + "cliconta.txt"; |FBORRA aAlfa;
        |FINSI;
     |FINSI;
  |SINO;
     |ABRE #comm0002;
     |LEE 000#comm0002.p;
     |SI FSalida = 0;
        #0#0 = "SI";
        #0#1 = #comm0002#1;
        #0#2 = #comm0002#2;
        #0#3 = #comm0002#3;
        |ABRE #comm0001;
        #comm0001#0 = #0#1;
        |LEE 000#comm0001.=;
        |SI FSalida = 0;
           dir_fich1 = #comm0001#2; |QBF dir_fich1;

           |DFICB dir_empr; |QBF dir_empr;
           dir_empr = dir_empr + (("00000" + #comm0002#15) % -105) + #comm0002#16 + "/";

           nSwCorreo = 1; |HAZ b_apuntes;

           |CIERRA #2; |DELINDEX #2;
        |FINSI;
        |CIERRA #comm0001;
     |FINSI;
     |CIERRA #comm0002;
  |FINSI;

  |CIERRA #comz0001;

  |ERROR; |ACABA;
|FPRO;

||PROCESO inicio1;  |TIPO 10;
