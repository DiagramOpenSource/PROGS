|VARIABLES;
   aClau340 = "";
   nNumFras = "";
   nSwRecti = 0;
|FIN;

|PROCESO ClauDefecto;
   |SI aClau340 = " "; |Y aTipoIVA ! "N"; || No tiene indicada la operacion
       |SI #dsmom030#26 =   2; aClau340 = "Q"; |FINSI;
       |SI #dsmom030#26 =   3; aClau340 = "N"; |FINSI;
       |SI #dsmom030#72 = "S"; |Y aTipoIVA = "S";  aClau340 = "P"; |FINSI;
       |SI #dsmom030#26 =   6; aClau340 = "I"; |FINSI;
       |SI #dsmom030#30 =   3; aClau340 = "I"; |FINSI;
       |SI #dsmom030#26 =   7; |O #dsmom030#26 = 18; aClau340 = "D"; |FINSI;
       |SI #dsmom030#28 =   8; aClau340 = "D"; |FINSI;
       |SI #dsmom030#34 =   4; aClau340 = "I"; |FINSI;

       |SI enEjercicio > 2011;
           |SI aTipoIVA = "R";
               |SI #dsmom030#49 = "X"; aClau340 = "R"; |FINSI;
               |SI #dsmom030#0 ] 800; |Y #dsmom030#0 [ 899; aClau340 = "S"; |FINSI;
               |SI #dsmom030#46 = "X"; aClau340 = "T"; |FINSI;
               |SI #dsmom030#50 = "X"; aClau340 = "U"; |FINSI;
               |SI #dsmom030#77 = "X"; aClau340 = "V"; |FINSI;
           |FINSI;
           |SI aTipoIVA = "S";
               |SI #dsmom030#48 = "X"; aClau340 = "S"; |FINSI;
               |SI #dsmom030#28 =   7; aClau340 = "X"; |FINSI;
           |FINSI;
       |FINSI;

       |SI aClau340 = " ";
           |SI nNumFras > 1; aClau340 = "A"; |FINSI;
       |FINSI;
   |FINSI;
|FPRC;

|PROCESO VeoRecti;

   nSwRecti = 0;

   |SI snFiltr ! "X";

      |ABRE #dsmom030;
      |DDEFECTO #dsmom030;
      #dsmom030#0 = enConcepto;
      |LEE 040#dsmom030.=;
      |CIERRA #dsmom030;

      |SI aTipoIVA = "R";
          |SI #dsmom030#26 ! 7; |Y #dsmom030#26 ! 18;
            |Y #dsmom030#26 ! 8; |Y #dsmom030#26 ! 17;
             |Y #dsmom030#26 ! 19;
              nSwRecti = 1;
          |FINSI;
      |FINSI;
      |SI aTipoIVA = "S";
          |SI #dsmom030#28 ! 8; |Y #dsmom030#28 ! 16;
            |Y #dsmom030#28 ! 17;
              nSwRecti = 1;
          |FINSI;
      |FINSI;

      |SI nSwRecti = 1;
          |MENAV "0000Fra Rectificativa, Concepto no configurado para la Casilla correcta del Modelo 303.";
          |SI snFiltr = "N";
              nSwRecti = 0;
          |FINSI;
      |FINSI;
  |FINSI;
|FPRC;
