       salidas  = FSalida;
       |HAZ FiltraPuntos;
       #camaasil#Campo# = eaCuenta;
       |PINTA #camaasil#Campo#;

       cta_t = Campo;
       dig_t = cta_t + 1;
       |HAZ blanco;

       emCta = 1;
       eaCuenta = #camaasil#cta_t#;
       enDiarioNivel = #camaasil#0;
       |HAZ SacaNivel;
       #camaasil#dig_t# = enNivel;
   |FINSI;
|FPRC;

|PROCESO blanco;              || desde calculo 'cuenta'
   alfa1 = #camaasil#cta_t#;   |QBF alfa1;
   |SI alfa1 = "";
      |ABRET #4;
      #cacuenta#0 = #camaasil#cta_t#;
      #cacuenta#1 = #camaasil#dig_t#;
      |LEE 001#4=;
      |SI FSalida ! 0;
         |DDEFECTO #4;
         #cacuenta#3 = "S";       || pase directo si
         |GRABA 040#4n;
      |FINSI;
      #cacuenta#3 = "S";       || pase directo si
      |GRABA 040#4a;
      |CIERRAT #4;
   |FINSI;                  || graba automaticamente las cuentas en blanco
|FPRC;

|PROCESO concepto; |TIPO 0;             || desde campo 'concepto'
   inkey = FSalida;
   |SI inkey = 2; |ACABA; |FINSI;
   |SI inkey = 17; |HAZ lacontra; inkey = 0; |ACABA; |FINSI;

   |SI inkey = 10;
       enConcepto = #camaasil#6;
       |HAZ VerConcepto30;
       #camaasil#6 = enConcepto; |PINTA #camaasil#6;
   |FINSI;

   enFilConc = 0;
   xx = (FEntrada / 100) ! 0;
   |SI #camaasil#12 = " "; enFilConc = 1; |FINSI;

   enFilNO = 0;
   |SI predefine ! 0;
       |SI #camaasil#12 ! " ";
           aAlfa1 =#casieali#56; aAlfa1 = aAlfa1 > " ";
           |SI aAlfa1 = "I"; |O aAlfa1 = "R";
               enFilNO = 1;
           |FINSI;
       |FINSI;
   |FINSI;

   |SI #camaasil#8 = "D"; |O #camaasil#8 = "X"; |O #camaasil#8 = "x";
       eaCuenta   = #camaasil#16;
       enConcepto = #camaasil#6;
       |HAZ FiltroConcepto;  |SI eswLect ! 0; |ACABA; |FINSI;
   |FINSI;
   |SI #camaasil#8 = "H"; |O #camaasil#8 = "X"; |O #camaasil#8 = "x";
       eaCuenta   = #camaasil#10;
       enConcepto = #camaasil#6;
       |HAZ FiltroConcepto;  |SI eswLect ! 0; |ACABA; |FINSI;
   |FINSI;

   |SI xx = 1;
      #camaasil#7 = #dsmom030#1;
   |SINO;
      |SI #camaasil#6 = coant;
          #camaasil#7 = deant;
      |SINO;
          #camaasil#7 = #dsmom030#1;
      |FINSI;
   |FINSI;
   |PINTA #camaasil#7;

   |SI ModoEntrada = 2; |Y enSwEmb = 1; |Y HayAcceso = 1;
       |Y #camaasil#8 = "D"; |Y eaTEmbargo ! "N"; |Y eaTEmbargo ! "F";
           |Y enCEmbargo ! 0; |Y enCEmbargo ! #camaasil#6;

         |CONFI "    NLinea Embargo. Si cambia al Concepto se borraran las anotaciones. Continuar";
         |SI FSalida ! 0;
             |ERROR;
         |FINSI;
   |FINSI;
|FPRC;

|PROCESO descrip; |TIPO 0;        || desde campo 'descripcion'
   inkey = FSalida;
   |SI inkey = 17; |HAZ lacontra; inkey = 0; |ACABA; |FINSI;
   |SI FSalida = 15;
      #camaasil#7 = deant; |PINTA #camaasil#7;
   |FINSI;
|FPRC;

|PROCESO nocero; |TIPO 0;          || desde campo 'importe'
   inkey = FSalida;
   f1auto = 0;
   swCambiar = 0;
   |SI inkey = 2; |ACABA; |FINSI;

   |SI inkey = 17; |HAZ lacontra; inkey = 0; |ACABA; |FINSI;
   |HAZ aviso9;
   |SI #camaasil#Campo# = 0;|Y inkey ! 10; |Y inkey ! 11;
      |ERROR;
      |ACABA;
   |SINO;
      |SI inkey = 10; |O inkey = 11;   || <F2>
         |SI #camaasil#8 = "D";  #camaasil#9 = #camaasic#6 * -1;  |FINSI;
         |SI #camaasil#8 = "H";  #camaasil#9 = #camaasic#6;       |FINSI;
         |PINTA #camaasil#9;
         |SI inkey = 11;
            |ERROR;
            |PTEC 802;
         |FINSI;
      |FINSI;
      |SI inkey ! 2;                 || cuando no es flecha arriba
          |HAZ c_predefi;            || opera en predef. al salir del apunte
      |FINSI;
      |SI inkey = 9; |O inkey = 10;
        nDebe = #1#4; nHaber = #1#5;
        |SI #0#8 = "D"; nDebe  = nDebe  + #0#9; |FINSI;
        |SI #0#8 = "H"; nHaber = nHaber + #0#9; |FINSI;
        |SI nDebe ! nHaber; |AVISO; |ERROR; |ACABA; |FINSI;
         f1auto = 1;
         |PTEC 806;
      |FINSI;
   |FINSI;

   |SI inkey < 9; |O inkey > 11;
       |SI HayAcceso = 1; |Y #camaasil#8 ! "H";
           |Y eaTEmbargo ! "N"; |Y eaTEmbargo ! "F";
              |Y enCEmbargo ! 0; |Y enCEmbargo = #camaasil#6;

            |SI enEmbImp ! 0; |O enSwEmb = 1;
                nume1 = enEmbImp;
                nume2 = Contenido;
                |SI enSwEmb = 1; nume1 = nume1 + nume2; |FINSI;
                |SI #camaasil#9 > nume1;
                     |MENAV "    Error! El Importe es superior al importe del Embargo";
                     #camaasil#9 = nume1; |PINTA #camaasil#9;
                     |ERROR;
                |FINSI;
            |FINSI;
       |FINSI;
   |FINSI;
|FPRC;

|PROCESO signo;                    || desde calculo 'c_importe' y 'cuefin'
   |SI #camaasil#16 ! doce;
      #camaasil#8 = "D";
      #camaasil#4 = #camaasil#16;
      #camaasil#5 = #camaasil#17;
   |FINSI;
   |SI #camaasil#10 ! doce;
      #camaasil#8 = "H";
      #camaasil#4 = #camaasil#10;
      #camaasil#5 = #camaasil#11;
   |FINSI;
   |SI #camaasil#16 ! doce;|Y #camaasil#10 ! doce;
      #camaasil#8 = "x";
      #camaasil#4 = "";
      #camaasil#5 = 0;
   |FINSI;
|FPRC;

