         #camaasil#11 = dihaant;
      |FINSI;
      |SI Campo = 16;
         #camaasil#16 = ctdeant;
         #camaasil#17 = dideant;
      |FINSI;
      |PINTA #camaasil#Campo#;
      inkey = 0;
   |FINSI;

   |SI inkey ! 11;
