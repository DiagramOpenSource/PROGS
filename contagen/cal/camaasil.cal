|PROCESO c_cuenta; |TIPO 7;
  |SI swCambiar = 1; |HAZ ElCambio; |ACABA; |FINSI;

   xx = (FEntrada / 100) ! 0;
   |CAMPO_MODIFICA #camaasil#10, 1, "S";
   |CAMPO_MODIFICA #camaasil#16, 1, "S";
   |SI xx ! 1;              || no modificable cuenta en blanco en no alta
      |SI #camaasil#10 ! doce; |O #camaasil#16 ! doce;
         |SI Campo=10; |Y #camaasil#16!doce; |CAMPO_MODIFICA #camaasil#10,1,"N"; |FINSI;
         |SI Campo=16; |Y #camaasil#10!doce; |CAMPO_MODIFICA #camaasil#16,1,"N"; |FINSI;
      |FINSI;
   |FINSI;
   |SI predefine = 0;  |ACABA;  |FINSI;

   |SI #casieali#2="D"; |Y Campo=10; |CAMPO_MODIFICA #camaasil#10,1,"N"; |ACABA;  |FINSI;
   |SI #casieali#2="H"; |Y Campo=16; |CAMPO_MODIFICA #camaasil#16,1,"N"; |ACABA;  |FINSI;

   |HAZ c_contra;
   p_cuenta = #casieali#3;
   |QBF p_cuenta;
   |SI p_cuenta = "=";
      |SI #casieali#2 = "D";
         #camaasil#16 = ctpre;              || cuenta DEBE (incompleta)
         #camaasil#17 = dipre;              || digito DEBE
         #camaasil#10 = "";                 || cuenta HABER
         #camaasil#11 = 0;                  || digito HABER
      |SINO;
         #camaasil#16 = "";                 || cuenta DEBE
         #camaasil#17 = 0;                  || digito DEBE
         #camaasil#10 = ctpre;              || cuenta HABER (incompleta)
         #camaasil#11 = dipre;              || digito HABER
      |FINSI;
      |PINTA #camaasil#16;
      |PINTA #camaasil#10;
      |PTEC 802;           || pulsa un 'Intro'
      |ACABA;
   |FINSI;
   alfa3 = p_cuenta % 0;
   nume1 = alfa3;
   nume1 = nume1 + 99;
   alfa2 = p_cuenta % -101;       || coge el asterisco

   |SI alfa2 = "*"; |O i_dc3 = 1;
      |SI alfa2 ! "*";
          nume1 = alfa3;
          nume1 = nume1 + 100;
      |FINSI;
      |SI nModPred = 1; p_cuenta = #camaasil#4; nume1 = 112; |FINSI;

      |SI #casieali#2 = "D";
         #camaasil#16 = p_cuenta % nume1;   || cuenta DEBE (incompleta)
         #camaasil#17 = #casieali#4;        || digito DEBE
         #camaasil#10 = "";                 || cuenta HABER
         #camaasil#11 = 0;                  || digito HABER
      |SINO;
         #camaasil#16 = "";                 || cuenta DEBE
         #camaasil#17 = 0;                  || digito DEBE
         #camaasil#10 = p_cuenta % nume1;   || cuenta HABER (incompleta)
         #camaasil#11 = #casieali#4;              || digito HABER
      |FINSI;
      |PINTA #camaasil#16;
      |PINTA #camaasil#10;
      |SI alfa2 = "*";
          |PTEC 816;            || pulsa un 'Fin'
      |FINSI;
   |SINO;
      |SI #casieali#2 = "D";
          #camaasil#16 = #casieali#3;              || cuenta DEBE (completa)
          #camaasil#17 = #casieali#4;              || digito DEBE
          #camaasil#10 = "";                 || cuenta HABER
          #camaasil#11 = 0;                  || digito HABER
      |SINO;
          #camaasil#16 = "";                 || cuenta DEBE
          #camaasil#17 = 0;                  || digito DEBE
          #camaasil#10 = #casieali#3;              || cuenta HABER (completa)
          #camaasil#11 = #casieali#4;              || digito HABER
      |FINSI;
      |PINTA #camaasil#16;
      |PINTA #camaasil#10;
      |PTEC 802;           || pulsa un 'Intro'
   |FINSI;
