|PROCESO RevisaIva;
   |ABRE #casieaca;
   #casieaca#0 = #camaasic#7;
   |LEE 000#casieaca.=;
   |CIERRA #casieaca;

   |ABRE #casieali;
   #casieali#0 = #camaasic#7;
   #casieali#1 = 1;
   |LEE 000#casieali.];
   |SI FSalida = 0; |Y #casieali#0 = #camaasic#7;
      |PARA; |SI FSalida = 0; |Y #casieali#0 = #camaasic#7; |HACIENDO;
         |SI #casieali#56 = #camaasil#19; |Y #casieali#57 = #camaasil#20;
            |SAL;
         |SINO;
            |LEE 000#casieali.s;
         |FINSI;
      |SIGUE;
   |FINSI;
   |CIERRA #casieali;

   |ABRE #camaniva; |ABRE #caivalin;
   |SELECT #2#camaniva;
   #camaniva#7 = #camaasic#0;
   #camaniva#8 = #camaasic#1;
   #camaniva#9 = #camaasic#2;
   |LEE 101#camaniva.=;
   |SI FSalida = 0;
      #caivalin#0 = #camaniva#0;
      #caivalin#1 = #camaniva#1;
      #caivalin#2 = #camaasil#20;
      |LEE 101#caivalin.=;
      |SI FSalida = 0;
         aAcumula = #camaasil#19; nImporte = #camaasil#9;
         |SI aAcumula = "b"; nImporte = -nImporte; aAcumula = "B"; |FINSI;
         |SI aAcumula = "i"; nImporte = -nImporte; aAcumula = "I"; |FINSI;
         |SI aAcumula = "r"; nImporte = -nImporte; aAcumula = "R"; |FINSI;
         |SI aAcumula = "d"; nImporte = -nImporte; aAcumula = "D"; |FINSI;
         |SI aAcumula = "p"; nImporte = -nImporte; aAcumula = "P"; |FINSI;

         |SI #camaasil#19 = "B";
            |SI Flag > 0;
               #caivalin#6 = nImporte; #caivalin#12 = #camaasil#4;
               |SI #caivalin#6 ! 0; |Y #caivalin#8 ! 0;
                  #caivalin#7 = (#caivalin#8 / #caivalin#6) * 100;
               |FINSI;
               |SI #caivalin#6 ! 0; |Y #caivalin#10 ! 0;
                  #caivalin#9 = (#caivalin#10 / #caivalin#6) * 100;
               |FINSI;
            |FINSI;
         |FINSI;
         |SI #camaasil#19 = "I";
            |SI Flag > 0;
               #caivalin#8 = nImporte; #caivalin#13 = #camaasil#4;
               |SI #caivalin#6 ! 0; |Y #caivalin#8 ! 0;
                  #caivalin#7 = (#caivalin#8 / #caivalin#6) * 100;
               |FINSI;
            |FINSI;
         |FINSI;
         |SI #camaasil#19 = "P";
            |SI Flag > 0;
               #caivalin#22 = nImporte; #caivalin#23 = #camaasil#4;
               |SI #camaasic#7 ! 0;
                  #caivalin#21 = #casieali#60;
                  #caivalin#20 = #camaniva#21;
               |SINO;
                  #caivalin#20 = #caivalin#6;
                  #caivalin#21 = (#caivalin#22 / #caivalin#20) * 100;
               |FINSI;
            |FINSI;
         |FINSI;
         |SI #camaasil#19 = "R";
            #camaniva#23 = #camaniva#23 +. nImporte;
            |SI Flag > 0;
               #caivalin#10 = nImporte; #caivalin#14 = #camaasil#4;
               |SI #caivalin#6 ! 0; |Y #caivalin#10 ! 0;
                  #caivalin#9 = (#caivalin#10 / #caivalin#6) * 100;
               |FINSI;
            |FINSI;
         |FINSI;
         |SI #camaasil#19 = "D";
            |SI Flag > 0;
               #caivalin#10 = nImporte; #caivalin#15 = #camaasil#4;
            |FINSI;
         |FINSI;
         |GRABA 020#caivalin.a; |LIBERA #caivalin;
         #camaniva#19 = 0; #camaniva#20 = 0; #camaniva#21 = 0;
         #camaniva#23 = 0; #camaniva#26 = 0;
         |BUCLE RecalculaIva;
         |GRABA 020#camaniva.a;
      |SINO;
         |SI #camaasil#19 = "F"; |O #camaasil#19 = "f";
            #camaniva#19 = 0; #camaniva#20 = 0; #camaniva#21 = 0;
            #camaniva#23 = 0; #camaniva#26 = 0;
            |BUCLE RecalculaIva;
            |GRABA 020#camaniva.a;
            |SI Flag > 0;
               |SI #camaniva#36 = "R";|Y #casieaca#6 = "S";  |HAZ cobros;  |FINSI;
               |SI #camaniva#36 = "S";|Y #casieaca#6 = "S";  |HAZ pagos;   |FINSI;
               |HAZ TrataRecti;
               |HAZ TrataSiiPred;
            |SINO;
               nCont = 1;
               |HAZ VerPuente;
               |SI swVPuente = 1;
                   |SI #camaniva#36 = "R"; |BUCLE BorraCob; |FINSI;
                   |SI #camaniva#36 = "S"; |BUCLE BorraPag; |FINSI;
                   |HAZ CierraPuente;
              |FINSI;
            |FINSI;
         |FINSI;
      |FINSI;
      |LIBERA #camaniva;
   |FINSI;
   |SELECT #1#camaniva;
   |CIERRA #caivalin; |CIERRA #camaniva;
|FPRC;

|PROCESO refresca; |TIPO 2;        || calculos de (des)actualizacion
   xx = (FEntrada / 100) ! 0;
   |SI p_cen = 0;
       |HAZ a_contrap;     || contrapartidas asiento
       |HAZ a_asiento;     || acumulados asiento

       MasMenos     = 0 +. #camaasil#9;
       DebeHaber    = #camaasil#8;
       aCUENTA      = #camaasil#4;
       nREGISTRO    = #camaasil#24;
       nDIARIO      = #camaasil#0;
       aFECHA       = #camaasil#1;
       nDOCUMENTO   = #camaasil#2;
       nNIVELCUENTA = #camaasil#5;
       |HAZ ActDiario;          || diario
       |HAZ ActCuenta;          || cuenta
       |HAZ ActTesteo;          || cuenta

       |HAZ pinta_cta;     || pinta los datos de la cuenta

       #cafantas#0 = #camandia#2;           || diario debe
       #cafantas#1 = #camandia#3;           || diario haber
       #cafantas#2 = #cafantas#0 - #cafantas#1;     || diario saldo
       |ATRI I;
       |PINTA 0639, #cafantas#0;
       |PINTA 0653, #cafantas#1;
       |PINTA 0667, #cafantas#2;
       |ATRI 0;

       || ... Ya no se actualiza el IVA por Lineas de Apuntes

   |FINSI;
   Flag = 0 +. 1;
   |SI xx = 2; |Y nModPred  = 0; |Y enSwAgr ! 2;
      |SI #camaasil#12 = "S"; |O #camaasil#12 = "R";
         |HAZ RevisaIva;
      |FINSI;
   |FINSI;

   entrada = 1;
   aRefAnter = #camaasil#26;
|FPRC;

|PROCESO a_ParaCaja;
         enSwCaja = 0;
         |ABRE #502;
         |SELECT #2#502;
         #502#3  = #camaasil#0;
         #502#4  = #camaasil#1;
         #502#5  = #camaasil#2;
         #502#6  = #camaasil#3;
         #502#17 = 0;
         #502#0  = 1;
         #502#1  = 1;
         |LEE 041#502];
         |SI FSalida = 0; |Y #502#3 = #camaasil#0; |Y #502#4 = #camaasil#1;
           |Y #502#5 = #camaasil#2; |Y #502#6 = #camaasil#3;
              |SI #502#18 = -1;
                  enSwCaja = 1;
                  |ABRE #501;
                  |DDEFECTO #501;
                  #501#17 = #502#17;
                  #501#0 = #502#0;
                  #501#1 = #502#1;
                  |LEE 041#501=;
                  aTipoIVA = #501#13;
                  |CIERRA #501;
              |SINO;
                  enSwCaja = 2;
              |FINSI;
         |FINSI;
         |SELECT #1#502;
         |CIERRA #502;
|FPRC;

|PROCESO saalta; |TIPO 0;               || desde campo 'linea'

   ModoEntrada = (FEntrada / 100) ! 0;
   inkey = FSalida;

   |SI ModoEntrada = 1; |Y #camaasil#3 ! 1;
      |SI coant ! 0;
         #camaasil#6 = coant;
         #camaasil#8 = dhant;
      |FINSI;
      |PINTA #camaasil#6;
   |FINSI;

   |SI ModoEntrada = 1;
      #camaasil#21 = dpant; |PINTA #camaasil#21;
      #camaasil#28 = sdant; |PINTA #camaasil#28;
      |SI eaLnRef = "S";
         |QBF aRefAnter;
         |SI aRefAnter = ""; aRefAnter = #camaasic#17; |FINSI;
         #camaasil#26 = aRefAnter; |PINTA #camaasil#26;
      |SINO;
         |QBF refant;
         |SI refant = ""; refant = #camaasic#17; |FINSI;
         |SI #camaasil#3 = 1;
             #camaasil#26 = refant; |PINTA #camaasil#26;
         |SINO;
             #camaasil#26 = refant; |PINTA #camaasil#26;
         |FINSI;
      |FINSI;
   |FINSI;

   enSwCaja = 0;
   |SI ModoEntrada ! 1;
       |HAZ a_ParaCaja;
       |SI enSwCaja = 2;
           |MENAV "0000Movimiento de Caja ya exportado al Ejercicio Siguiente";
           |MENAV "0000No modificara el Registro ";
           enSwCaja = 1;
       |FINSI;
   |FINSI;

   |SI predefine ! 0;             || carga los campos relacionados con IVAS
       |HAZ CarCampoPred;
   |SINO;
       #camaasil#12 = "";
       #camaasil#13 = 0;
       #camaasil#14 = 0;
       #camaasil#15 = "";
       #camaasil#19 = "";
       #camaasil#20 = 0;
   |FINSI;
|FPRC;

|PROCESO CarCampoPred;
    #camaasil#12 = #casieaca#3;    || tipo (soportado/repercutido)
    #camaasil#13 = #casieaca#4;    || libro
    #camaasil#14 = i_factu;  || factura/orden
    #camaasil#15 = i_serie;  || serie
    #camaasil#19 = #casieali#56;   || tipo acumulador IVA
    |SI #casieali#1 = #casieaca#5; #camaasil#19 = "F"; |FINSI;
    |SI #casieaca#9 = 1;
        |SI #casieali#56 = "B"; #camaasil#19 = "b"; |FINSI;
        |SI #casieali#56 = "I"; #camaasil#19 = "i"; |FINSI;
        |SI #casieali#56 = "R"; #camaasil#19 = "r"; |FINSI;
        |SI #casieali#56 = "P"; #camaasil#19 = "p"; |FINSI;
        |SI #casieali#56 = "D"; #camaasil#19 = "d"; |FINSI;
        |SI #casieali#56 = "b"; #camaasil#19 = "B"; |FINSI;
        |SI #casieali#56 = "i"; #camaasil#19 = "I"; |FINSI;
        |SI #casieali#56 = "r"; #camaasil#19 = "R"; |FINSI;
        |SI #casieali#56 = "p"; #camaasil#19 = "P"; |FINSI;
        |SI #casieali#56 = "d"; #camaasil#19 = "D"; |FINSI;
