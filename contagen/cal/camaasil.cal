|PROCESO a_contrap;
   swcambio=0;
   |SI j_d = 0; sw_debe = 0; |FINSI;
   |SI j_h = 0; sw_habe = 0; |FINSI;
   xx = (FEntrada / 100) ! 0
   |SI xx = 3;
      |SI j_d = #camaasil#3; sw_debe = 0; j_d = 0; |FINSI;
      |SI j_h = #camaasil#3; sw_habe = 0; j_h = 0; |FINSI;
      |ACABA;
   |FINSI;

   |SI sw_debe = 0;
      |SI #camaasil#8 ! "H";
         #camaasic#08 = #camaasil#16;             || contrap. HABER
         #camaasic#09 = #camaasil#17;             || digito HABER
         sw_debe = 1;
         j_d = #camaasil#3;
      |FINSI;
   |SINO;
      |SI j_d = #camaasil#3;
         |SI #camaasil#8 ! "H";
             #camaasic#08 = #camaasil#16;             || contrap. HABER
             #camaasic#09 = #camaasil#17;             || digito HABER
             j_d = #camaasil#3;
         |SINO;
             j_d     = 0;
             sw_debe = 0;
             #camaasic#8 = "";
             #camaasic#9 = 0;
             swcambio   =  1;
         |FINSI;
      |FINSI;
   |FINSI;
   || -------------------------------------------------------------------
   |SI sw_habe = 0;
      |SI #camaasil#8 ! "D";
         #camaasic#10 = #camaasil#10;             || contrap. DEBE
         #camaasic#11 = #camaasil#11;             || digito DEBE
         sw_habe = 1;
         j_h = #camaasil#3;
      |FINSI;
   |SINO;
      |SI j_h = #camaasil#3;
         |SI #camaasil#8 ! "D";
            #camaasic#10 = #camaasil#10;             || contrap. DEBE
            #camaasic#11 = #camaasil#11;             || digito DEBE
            j_h = #camaasil#3;
         |SINO;
            j_h = 0;
            sw_habe = 0;
            #camaasic#10 = "";
            #camaasic#11 = 0;
            swcambio   =  1;
         |FINSI;
      |FINSI;
   |FINSI;

   |SI swcambio ! 0;
       |SI #camaasil#8 ! "D";
           #camaasic#10 = #camaasil#10;             || contrap. DEBE
           #camaasic#11 = #camaasil#11;             || digito DEBE
           j_h = #camaasil#3;
           sw_habe = 1;
       |SINO;
           #camaasic#8 = #camaasil#16;             || contrap. DEBE
           #camaasic#9 = #camaasil#17;             || digito DEBE
           j_d = #camaasil#3;
           sw_debe = 1;
       |FINSI;
   |FINSI;
|FPRC;

|PROCESO a_asiento;                || calculo 'refresca'
   |SI #camaasil#8 = "D";|O #camaasil#8 = "x";  #camaasic#4 = #camaasic#4 +. #0#9;  |FINSI;
   |SI #camaasil#8 = "H";|O #camaasil#8 = "x";  #camaasic#5 = #camaasic#5 +. #0#9;  |FINSI;
   #camaasic#6 = #camaasic#4 - #camaasic#5;
   |SI #camaasil#12 = "R"; #camaasic#13 = 1; |FINSI;
   |SI #camaasil#12 = "S"; #camaasic#13 = 2; |FINSI;
   |SI #camaasil#12 = "N"; #camaasic#13 = 3; |FINSI;
   |PINTA #camaasic#4;
   |PINTA #camaasic#5;        || acumulados asiento
   |PINTA #camaasic#6;
