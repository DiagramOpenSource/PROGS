   |SI xx = 1;
       |MENSA "    F12-Extracto;F2-Diario;F3-Anotaciones;F4 Busqueda;F6-Nuevo Asto;F9-Contrap.";
   |SINO;
       |MENSA "    F12-Extracto;F2-Diario;F3-Anotaciones;F4 Busqueda;F7-Cambio D/H;F9-Contrap.";
   |FINSI;

   |HAZ diez;
   |SI predefine = 0;  |ACABA;  |FINSI;

   |SI #camaasil#3 = 1;            || se supone que la primera linea del apunte es la 1
       |HAZ c_empieza;
   |FINSI;

   |ET arriba;
   nSwSal = 0;
   |CAMPO_MODIFICA #camaasil#16, 1, "S";
   |CAMPO_MODIFICA #camaasil#10, 1, "S";
   |HAZ lee_predef;
   |SI p_eof ! 0;           || cuando se acaba el predefinido
      |SI #casieaca#2 = "S";          || si hay iva
          |HAZ VerSiHay; ||por si pulsa CTRL-Q varias veces
          |SI nSwSal = 1;
              sw_ptec = 0;
              inkey = 0;
              |PTEC 806;
              |ACABA;
          |FINSI;

          |HAZ TrataRecti;
          |HAZ TrataSiiPred;
          NumDocum = 0;
          |SI #casieaca#3 = "R";|Y #casieaca#6 = "S";  |HAZ cobros;  |FINSI;
