   alfa6     = "";
   ivacomun  = "";
   i_nom     = "";
   i_cuent   = "";
   iva_am    = 0;
   tiva_am   = 0;
   aCuenta12   = "";
   aCuenta13   = "";
   nREBU       = 0;
   nIva        = 0;
