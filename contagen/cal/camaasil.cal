|FPRC;

|PROCESO LosUltimos;                  || desde rutina 'altatra'
  alfa1 = Usuario;     |QBF alfa1;
  alfa1 = "T" + (alfa1 % 107);
  |NOME_DAT #24 alfa1;
  |HAZ Ultim;

  alfa1 = "camemori";
  |NOME_DAT #24 alfa1;
  |HAZ Ultim;
|FPRC;

|PROCESO Ultim;
  |DDEFECTO #24;
  |ABRE #24;
  #24#00 = enEmpresa;
  #24#25 = enPeriodo;
  |LEE 101#24=;
  FEstado = FSalida;
  #24#01 = #24#02;  #24#02 = #24#03;  #24#03 = #0#0;      || diario
  #24#04 = #24#05;  #24#05 = #24#06;  #24#06 = #0#1;      || fecha
  #24#07 = #24#08;  #24#08 = #24#09;  #24#09 = #0#2;      || documento
  #24#10 = #24#11;  #24#11 = #24#12;  #24#12 = #0#4;      || cuenta
  #24#13 = #24#14;  #24#14 = #24#15;  #24#15 = #0#5;      || digito
  #24#16 = #24#17;  #24#17 = #24#18;  #24#18 = #0#8;      || signo
  #24#19 = #24#20;  #24#20 = #24#21;  #24#21 = #0#9;      || importe
  #24#22 = #24#23;  #24#23 = #24#24;  #24#24 = #0#3;      || apunte
  |SI FEstado = 0;  |GRABA 020#24a;  |FINSI;
  |SI FEstado ! 0;  |GRABA 020#24n;  |FINSI;
  |LIBERA #24;
  |CIERRA #24;
|FPRC;

|PROCESO Tipo15asil; |TIPO 15;
     #camaasil#27 = Usuario % 810;
|FPRC;

|PROCESO Tipo16asil; |TIPO 16;
     #camaasil#27 = Usuario % 810;
|FPRC;

|PROCESO Apunte7; |TIPO 7;
     |HAZ EsTagloma;
     |SI FSalida = 0;
          |SI nBtnItemDoc = -1;
               |CONTROL_SIMPLE 0, "BOTON, ItemDoc", 1002, 1010, BtnItemDoc;
               nBtnItemDoc = FSalida;
          |FINSI;
          aAlfa1 =  "DISABLE";
          |SI #00#12 = "S"; |O #00#12 = "R";
               |SI #00#14 ! 0; aAlfa1 =  "ENABLE"; |FINSI;
          |FINSI;
          |ESTADO_CONTROL nBtnItemDoc, aAlfa1;
     |FINSI;
|FPRC;

|PROCESO BtnItemDoc;
     aAlfa1 = (("00"+#00#0)%-102)+ #00#1 + (("000000"+#00#2)%-106)+(("0000"+#00#3)%-104);
     aAlfa1 = "tagz0001;ASI" + aAlfa1;
     |SUB_EJECUTA_NP aAlfa1;
|FPRC;
