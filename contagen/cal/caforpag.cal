|FICHEROS;
     caivaasi;
     caforpag;
|FIN;

|VARIABLES;
     nModo               = 0;
|FIN;

|PROCESO T1;  |TIPO 1;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
         |MENAV "0000Activada integracion con gestion comercial. Baja no permitida";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO T7;  |TIPO 7;
     |ABRE #caivaasi;
     |LEE 000#caivaasi.p;
     |SI FSalida ! 0; |DDEFECTO #caivaasi; |FINSI;
     |CIERRA #caivaasi;

     |SI #caivaasi#191 = "N";
         |ACABA;
     |FINSI;

     nModo = (FEntrada / 100) ! 0;

     |SI nModo = 1;
         |MENAV "0000Activada integracion con gestion comercial. Alta no permitida";
         |ERROR;
         |PTEC 807;
     |FINSI;

     |SI nModo = 2;
         |C_M #caforpag#1, 1, "N";
         |C_M #caforpag#2, 1, "N";
         |C_M #caforpag#3, 1, "N";
         |C_M #caforpag#4, 1, "N";
         |C_M #caforpag#5, 1, "N";
     |FINSI;
|FPRC;
