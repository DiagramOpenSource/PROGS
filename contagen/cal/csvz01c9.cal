|VARIABLES;
     nLong               = 0;
     nNume               = 0;
     nNume1              = 0;
     nNume2              = 0;

     aAlfa               = "";
     aLong               = "";
     aLetra              = "";
     aCadena             = "";

     &eaColum            = "";
     &enColum            = 0;

     {30}&eaPopup       = "";
|FIN;

|PROCESO SacaNumero;
     enColum = 0;
     |QBF eaColum;

     |SI eaColum = ""; |ACABA;  |FINSI;

     eaColum = eaColum > "";
     aLong   = eaColum % 0;
     nLong   = aLong;

     aCadena = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
     |SI nLong = 1;
         aAlfa   = aCadena - eaColum;
         aAlfa   = FSalida - "01";
         enColum = aAlfa;
     |FINSI;

     |SI nLong = 2;
         aAlfa  = eaColum % 101;
         aAlfa  = aCadena - aAlfa;
         aAlfa  = FSalida - "01";
         nNume1 = aAlfa;

         aAlfa  = eaColum % 201;
         aAlfa  = aCadena - aAlfa;
         aAlfa  = FSalida - "01";
         nNume2 = aAlfa;

         enColum = (nNume1 * 26) + nNume2;
     |FINSI;
|FPRC;

|PROCESO CogeLetra;
     |SI nNume = 1;   aLetra = "A";  |FINSI;
     |SI nNume = 2;   aLetra = "B";  |FINSI;
     |SI nNume = 3;   aLetra = "C";  |FINSI;
     |SI nNume = 4;   aLetra = "D";  |FINSI;
     |SI nNume = 5;   aLetra = "E";  |FINSI;
     |SI nNume = 6;   aLetra = "F";  |FINSI;
     |SI nNume = 7;   aLetra = "G";  |FINSI;
     |SI nNume = 8;   aLetra = "H";  |FINSI;
     |SI nNume = 9;   aLetra = "I";  |FINSI;
     |SI nNume = 10;  aLetra = "J";  |FINSI;
     |SI nNume = 11;  aLetra = "K";  |FINSI;
     |SI nNume = 12;  aLetra = "L";  |FINSI;
     |SI nNume = 13;  aLetra = "M";  |FINSI;
     |SI nNume = 14;  aLetra = "N";  |FINSI;
     |SI nNume = 15;  aLetra = "O";  |FINSI;
     |SI nNume = 16;  aLetra = "P";  |FINSI;
     |SI nNume = 17;  aLetra = "Q";  |FINSI;
     |SI nNume = 18;  aLetra = "R";  |FINSI;
     |SI nNume = 19;  aLetra = "S";  |FINSI;
     |SI nNume = 20;  aLetra = "T";  |FINSI;
     |SI nNume = 21;  aLetra = "U";  |FINSI;
     |SI nNume = 22;  aLetra = "V";  |FINSI;
     |SI nNume = 23;  aLetra = "W";  |FINSI;
     |SI nNume = 24;  aLetra = "X";  |FINSI;
     |SI nNume = 25;  aLetra = "Y";  |FINSI;
     |SI nNume = 26;  aLetra = "Z";  |FINSI;
|FPRC;

|PROCESO SacaLetra;
     eaColum = "";

     |SI enColum = 0; |ACABA;  |FINSI;

     |SI enColum [ 26;
         nNume = enColum;
         |HAZ CogeLetra;
         eaColum = aLetra;
     |SINO;
         nNume1 = ((enColum / 26) - 0.49) ! 0;
         nNume2 = 0;
         |SI nNume1 > 26;
             nNume2 = (nNume1 - 26) * 26;
             nNume1 = 26;
         |SINO;
             nNume2 = nNume2 + (enColum $ 26);
         |FINSI;

         nNume  = nNume1;  |HAZ CogeLetra;  eaColum = aLetra;
         nNume  = nNume2;  |HAZ CogeLetra;  eaColum = eaColum + aLetra;
     |FINSI;

     eaColum = eaColum > "";
|FPRC;

|PROCESO CargaCamaasil;
