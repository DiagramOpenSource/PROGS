|PROCESO RestaCompensa;

   |SI #901#6 = #11#26; |ACABA; |FINSI;

   |SI nOpe38 = 0;
       #11nCampo3 = #11nCampo3 + #901#7;
   |FINSI;

   |SI nOpe38 = 1;
       nNume1 = #11nCampo3 - #901#7;
       |SI nNume1 ] nLimite;
           #11nCampo3 = nNume1;
           #901#7 = 0;
           #901#4 = 0;
       |FINSI;
       |SI nNume1 < nLimite;
           #901#7 = #901#7 + (nLimite - #11nCampo3);
           nCompensa = #901#7;
           |HAZ CalCuotaCom;
           #901#4 = nCuota;
           #11nCampo3 = nLimite;
       |FINSI;
       |GRABA 020#901a;
       |LIBERA #901;
    |FINSI;

    |SI nOpe38 = 2;
        |SI #901#3 > 0;
            nNume1 = #11nCampo3 + #901#3;
            |SI nNume1 [ nLimite;
                #901#7     = #901#3;
                #11nCampo3 = nNume1;
                nCompensa = #901#7;
                |HAZ CalCuotaCom;
                #901#4 = nCuota;
            |FINSI;
            |SI nNume1 > nLimite;
                #901#7 = (nLimite - #11nCampo3);
                nCompensa = #901#7;
                |HAZ CalCuotaCom;
                #901#4 = nCuota;
                #11nCampo3 = nLimite;
            |FINSI;
            |GRABA 020#901a;
        |FINSI;
        |LIBERA #901;
    |FINSI;
|FPRC;

