|CALCULO impre; |TIPO 3;

   aFichero = "x" + Usuario; |NOME_DAT #999 aFichero; |DELINDEX #999;
   aFichero = "y" + Usuario; |NOME_DAT #991 aFichero; |DELINDEX #991;

