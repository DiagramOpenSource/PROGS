|PROCESO BuscaEmpresas;
   nError = 0;
   |ABRE #30;
   |SELECT #4#30;
   |SI #0#16 = -1;  aAlfa1 = #0#17;  |FINSI;
   |SI #0#18 = -1;  aAlfa1 = #0#19;  |FINSI;
