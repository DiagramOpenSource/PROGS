|PROGRAMA;
   aParam = PARAMETRO; |QBF aParam;

   |SI aParam = ""; aParam = "I"; |FINSI;

   |HAZ Primero;
   |SI enEjercicio < 2014;
       |MENAV "    Fras en Regimen Especial de Cr. de Caja a partir del Ejercicio 2014";
       |ACABA;
   |FINSI;

   aParam1 = aParam % 204;
   aParam  = aParam % 101;
   #0#6    = aParam;

   |SI aParam1 = "";
       #0#14 = enEmpresa;
       #0#15 = eaNombreEmp;
       |SI aParam = "I";
           #0#16 = -1;
           #0#17 = enEjercicio - 1;
           #0#18 = enPeriodo;
           #0#19 = enEjercicio;
           aAlfa2 = "Origen";
       |SINO;
           #0#16 = enPeriodo;
           #0#17 = enEjercicio;
           #0#18 = -1;
           #0#19 = enEjercicio + 1;
           aAlfa2 = "Destino";
       |FINSI;
       |HAZ BuscaEmpresas;

       |SI nError = 1;
           aAlfa2 = "0000Error no Existe Empresa " + aAlfa2;
           |MENAV aAlfa2;
           |ACABA;
       |FINSI;
       |SI #0#17 < 2014;
           aAlfa2 = "0000Error Empresa Origen con Ejercicio inferior a 2014";
           |MENAV aAlfa2;
           |ACABA;
       |FINSI;

       |SI #0#6 = "I";
           aAlfa2 = #0#17;
           aAlfa1 = fec1 % 106; aAlfa1 = aAlfa1 + aAlfa2; dfec = aAlfa1;
           aAlfa1 = fec2 % 106;
           |SI dig1 ! 1; aAlfa2 = #0#19; |FINSI;
           aAlfa1 = aAlfa1 + aAlfa2; hfec = aAlfa1;
       |SINO;
           dfec = fec1;
           hfec = fec2;
       |FINSI;

       |CLS;
       |PINPA #0#0;
       |SI aParam = "D";
           |PINTA 0606, "                                                                  ";
           |ATRI R; |PINTA 0606, "Anular Traspaso de Facturas RECC pendientes"; |ATRI 0;
           |PINTA 1254, "Anuladas t.: ";
           |PINTA 2207, "Modificaremos Cobros/Pagos de las Fras en Ejercicio Origen";
       |FINSI;
       |PINDA #0#0;

       |ENDATOS #1#0;
