|FICHEROS;
     caw00008 #999,MANTE;
|FIN;

|VARIABLES;
     aFichero   = "";
     handlec    = "";
     aAlfa      = "";
     aplicacion = "";
     dirbase = "";
     dirempresa = "";
     dirfichero = "";
     empresa = "";
     total = 0;
     ri = 0;
     temporal1 = "";
     temporal2 = "";
     temporal3 = "";
     temporal4 = "";
     temporal5 = "";
     handle = "";
     i = 0;
     total_lineas = 0;
     total_campos = 0;
     linea = 0;
     ant_linea = 0;
     posicion = 0;
     pintar = 0;

     dolar1 = "";
     dolar2 = "";
     alfa1 = "";
     para1 = 0;
     elsw1 = 0;
     topep = 0;
     punto_coma = "";
     primero = "";
     sw_paso = 0;
     traba = "";
     bytes = 0;
     quebusco = "";
     hebuscado = "";
     desbus = 0;
     estoesloquehay = "";
     encontrado = 0;
     esfichero = 0;
     unavez = 0;
     rafa1 = "";
     rafa2 = "";

     {1000}fichero = "";
     pos_fichero = 0;

     &eaOrigen   = "";
     &eaDestino  = "";
     aCopiaOri   = "";
     aCopiaDesti = "";
     aAlfa1      = "";

     &eswNoLab   = 0;
     &eswNoIva   = 0;
     &fich_alta  = "";
|FIN;



|RUTINA SP;
     pos_fichero = Pos;
     Pos = -1;
|FRUT;

|RUTINA RP;
     Pos = pos_fichero;
|FRUT;

|RUTINA carga_campos;
     |DDEFECTO #caw00008;
     #caw00008#0 = (A\temporal2 % 108);
     #caw00008#1 = (A\temporal2 % 925);
     |GRABA 020#999n;
|FRUT;

|PROCESO quitabarra;
     rafa1 = temporal1 % -108;
     |ET arriba;
         rafa2 = rafa1 - "/";
         |SI rafa1 = rafa2;  |ACABA;  |FINSI;
         rafa1 = rafa1 % 207;
         |VETE arriba;
|FPRC;

|RUTINA ClaveBaja998;
     #999#0 = "        ";
|FRUT;

|RUTINA ClaveAlta998;
     #999#0 = "zzzzzzzz";
|FRUT;

|PROCESO CopiaFichero;
     aAlfa1 = "     Copiando Fichero [" + #999#0 +"]";
     |MENSA aAlfa1;

     aCopiaOri   = eaOrigen  + #999#0 + ".dat";  |QBT aCopiaOri;
     aCopiaDesti = eaDestino + #999#0 + ".dat";  |QBT aCopiaDesti;
     |COPIA_FICHERO aCopiaOri, aCopiaDesti;
     |SI FSalida < 0;
         aAlfa1 = "       Ha habido un error en copiar fichero [" + #999#0 + ".dat]";
         |MENAV aAlfa1;
     |FINSI;

     aCopiaOri   = eaOrigen  + #999#0 + ".ddx";  |QBT aCopiaOri;
     aCopiaDesti = eaDestino + #999#0 + ".ddx";  |QBT aCopiaDesti;
     |COPIA_FICHERO aCopiaOri, aCopiaDesti;
     |SI FSalida < 0;
         aAlfa1 = "       Ha habido un error en copiar fichero [" + #999#0 + ".ddx]";
         |MENAV aAlfa1;
     |FINSI;

     aCopiaOri   = eaOrigen  + #999#0 + ".ixx";  |QBT aCopiaOri;
     aCopiaDesti = eaDestino + #999#0 + ".ixx";  |QBT aCopiaDesti;
     |COPIA_FICHERO aCopiaOri, aCopiaDesti;
     |SI FSalida < 0;
         aAlfa1 = "       Ha habido un error en copiar fichero [" + #999#0 + ".ixx]";
         |MENAV aAlfa1;
     |FINSI;

     |SI #999#0 = "cadeflab";
         fich_alta = eaDestino;
         eswNoLab = 0;
         eswNoIva = 1;
         |HAZ ActDefecto;
     |FINSI;
     |SI #999#0 = "caivaasi";
         fich_alta = eaDestino;
         eswNoLab = 1;
         eswNoIva = 0;
         |HAZ ActDefecto;
     |FINSI;
|FPRC;

|DEFBUCLE caw00008;
     #999#1;
     2;
     "        ", "S";
     "zzzzzzzz", "S";
     ;
     NULL, CopiaFichero;
|FIN;

|PROCESO CargaTempo;
     aFichero = (("08" + Usuario + "zzzzzzzz")) % -108;
     |NOME_DAT #999 aFichero;
     |ABRE #999;  |CIERRA #999;  |DELINDEX #999;

     |ABRE #999;

     |DBASS aAlfa;  |QBF aAlfa;
     temporal1 = "rt  " + aAlfa + "contagen/fich/contagen.dir";
     total     = 0;
     |FABRE temporal1;
     handlec = FSalida;
     handle  = FSalida;
     |SI handle ] 0;
         |HAZ SP;
         Ifichero = fichero1<-;
         |PARA i = 0;|SI total < 1000;|HACIENDO i = i + 1;
               temporal1 = handle+" 200";
               |HAZ RP;
               |FLEE temporal1;
               rafa1 = temporal1 % 101;
               |SI rafa1 = ":";
                   |HAZ quitabarra;
                   temporal1 = rafa1;
               |FINSI;

               |HAZ SP;

               |SI FSalida < 0;   |SAL;  |FINSI;

               ri = i $ 5;
               |SI ri = 0;  || 1
                   fichero = temporal1;
               |SINO;
                   |SI ri = 1;  || 2
                       temporal2 =  fichero + "        ";
                       temporal2 = temporal2 % 108;
                       temporal2 = (temporal2+temporal1+"                             ") % 133;
                       fichero = temporal2;
                   |SINO;
                       |SI ri = 3;  || 3
                           temporal1 = temporal1 % 101;
                           |SI temporal1 = 1;|O temporal1 = 2; || ficheros con indice
                           |SI total = 0;  |Y dolar2 ! "";  |Y sw_paso = 0;
                               sw_paso =  1;
                               primero = fichero % 108;
                               |QBF primero;
                           |SINO;
                               Ifichero = Ifichero + 1;
                                total    = total + 1;
                           |FINSI;
                       |FINSI;
                   |FINSI;      || 3
               |FINSI;      || 2
            |FINSI;      || 1
         |SIGUE;
         |FCIERRA handle;
     |FINSI;

     |SI dolar2 ! "";
         total = total - 1;
         Ifichero = Ifichero + 1;
     |FINSI;

     Ifichero = fichero1<-;
     |PARA i = 0;|SI i < total;|HACIENDO i = i + 1;
           temporal1 = fichero % 108;
           |QBF temporal1;
           fichero   = fichero + temporal1;
           temporal2 = fichero;
           |HAZ carga_campos;
           Ifichero  = Ifichero + 1;
     |SIGUE;

     |ENTLINEAL #1#999, -1, 4, 22, 1, ClaveBaja998, ClaveAlta998;
     |CIERRA #999;
|FPRC;

|PROCESO Auxiliar;
     |BUCLE caw00008;
|FPRC;

|PROCESO Tipo11w00008;  |TIPO 11;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     |LEE 101#999=;
     |SI #999#2 = "N";
         #999#2 = "S";
     |SINO;
         #999#2 = "N";
     |FINSI;
     |GRABA 020#999a;
     |LIBERA #999;

     |PINTA #999#2;

     |PTEC 809;
|FPRC;

|PROCESO BorraTempo;
     |ABRE #999;  |CIERRA #999;  |DELINDEX #999;
|FPRC;
