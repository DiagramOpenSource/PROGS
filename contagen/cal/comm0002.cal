|FICHEROS;
   comm0002 #0;

   comm0001 #10;
   Referen@ #100;
|FIN;

|VARIABLES;
   &eaEntrada = "";
   &eaSalida  = "";
   aClave   = "";

   aAlfa = "";

   nCalc = 0;
   nTecla = 0;
|FIN;

|PROCESO PintaEmpCont; |TIPO 14;
   |DDEF aAlfa; |QBF aAlfa; aAlfa = aAlfa + "canempre.mas";
   |CARGA_DEF aAlfa, Referen@;
   |SI FSalida = 0;
      |DFICB aAlfa; |QBF aAlfa;
      |PATH_DAT #Referen@#aAlfa#; |ABRE #Referen@;
      #Referen@#2 = #comm0002#15;
      #Referen@#3 = #comm0002#16;
      |LEE 000#Referen@.=;
      |SI FSalida = 0;
         aAlfa = #Referen@#1; |QBF aAlfa;
         aAlfa = (aAlfa + (" " * 23)) % 0123;
         |PINTA 1143, aAlfa;
      |FINSI;
      |CIERRA #Referen@;
   |FINSI;
|FPRC;

|PROCESO MiraEmpCont;
   nTecla = FSalida;

   |DDEF aAlfa; |QBF aAlfa; aAlfa = aAlfa + "canempre.mas";
   |CARGA_DEF aAlfa, Referen@;
   |SI FSalida = 0;
      |DFICB aAlfa; |QBF aAlfa;
      |PATH_DAT #Referen@#aAlfa#; |ABRE #Referen@;
      |SI nTecla = 10;
         #Referen@#2 = #comm0002#15;
         #Referen@#3 = #comm0002#16;
         |LEE 000#Referen@.];
         |CONSULTA_CLAVE #1#Referen@;
         |SI FSalida = 0;
            #comm0002#15 = #Referen@#2; |PINTA #comm0002#15;
            #comm0002#16 = #Referen@#3; |PINTA #comm0002#16;
            aAlfa = #Referen@#1; |QBF aAlfa;
            aAlfa = (aAlfa + (" " * 23)) % 0123;
            |PINTA 1143, aAlfa;
         |FINSI;
         |ERROR;
      |SINO;
         #Referen@#2 = #comm0002#15;
         #Referen@#3 = #comm0002#16;
         |LEE 000#Referen@.=;
         |SI FSalida = 0;
            aAlfa = #Referen@#1; |QBF aAlfa;
            aAlfa = (aAlfa + (" " * 23)) % 0123;
            |PINTA 1143, aAlfa;
         |SINO;
            |MENAV "    Empresa inexistente";
            |ERROR;
         |FINSI;
         |CIERRA #Referen@;
      |FINSI;
   |FINSI;
|FPRC;

|PROCESO PideClave;
   eaEntrada = #comm0002#11; |HAZ Encriptado; aClave = eaSalida;

   E_sup = 16;
   E_inf = "";
   aClave = 1752 ? -1;

   aAlfa = aClave; |QBF aAlfa;
   aAlfa = aAlfa % 0; nCalc = aAlfa;
   aAlfa = "*" * nCalc;
   #comm0002#14 = aAlfa; |PINTA #comm0002#14;
   eaEntrada = aClave; |HAZ Encriptado; #comm0002#11 = eaSalida;
|FPRC;

|PROCESO MiraIP;
   aAlfa = #comm0002#5 + "." + #comm0002#6 + "." + #comm0002#7 + "." + #comm0002#8;
   |SI aAlfa = "0.0.0.0";
      |C_M #comm0002#17, 1, "S";
   |SINO;
      #comm0002#17 = ""; |PINTA #comm0002#17;
      |C_M #comm0002#17, 1, "N";
   |FINSI;
|FPRC;

|PROGRAMA;
   |ABRE #comm0002;
   |LEE 101#comm0002.p;
   |SI FSalida ! 0;
      |DDEFECTO #comm0002;
      |GRABA 020#comm0002.b;
   |FINSI;

   |CLS;
   |CABEZA "PARAMETROS IMPORTACION AUTOMATICA DE CLIENTES/PROVEEDORES";
   |PINPA #0#0; |PINDA #0#0;
   |ENDATOS #1#0;
   |SI FSalida = 0;
      |GRABA 020#comm0002.a; |LIBERA #comm0002;
   |FINSI;

   |CIERRA #comm0002;
|FPRO;
