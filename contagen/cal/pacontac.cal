|PROCESO pasa;  |TIPO 3;

    nAlguno    = 0;
    eaNomFich  = ("0p" + Usuario) % 108;
    |NOME_DAT #caivatm4#eaNomFich#;
    |DELINDEX #caivatm4;

    aFichero = "p" + Usuario;
    |NOME_DAT #17 aFichero;
    |DELINDEX #17;
    nNuevo = #0#10;
    |SI #0#0 = "S";
        |SI #0#3 = "S";
            |SI #0#5 = 8;
                |SI #0#10 ! 9; |HAZ subcta_08;  |FINSI;
                |SI #0#10 = 9; |HAZ subcta_Ult; |FINSI;
            |FINSI;

            |SI #0#5 = 9;         || Nuevo Txt
                 |SI #0#10 = 0; #0#10 = 4; |FINSI;
                 |SI #0#10 = 1; |HAZ subcta_t09;  |FINSI;
                 |SI #0#10 = 4; |HAZ subcta_t09;  |FINSI;
                 |SI #0#10 = 5; |HAZ subcta_t09;  |FINSI;
                 |SI #0#10 = 6; |HAZ subcta_t09;  |FINSI;
                 |SI #0#10 = 2; |HAZ subcta_t092; |FINSI;
                 |SI #0#10 = 3; |HAZ subcta_t092; |FINSI;
                 |SI #0#10 = 7; |HAZ subcta_t092; |FINSI;
                 |SI #0#10 = 8; |HAZ subcta_t093; |FINSI;
                 |SI #0#10 = 9; |HAZ subcta_t093; |FINSI;
            |FINSI;
            #0#10 = nNuevo;
        |FINSI;

        |SI #0#17 ! "N";
            |HAZ CtaAnalitica;
        |FINSI;

        |ABRE #caivatm4;
        |PINTA 2221, "                                                  ";
        |SI #0#4 = "S";

            |HAZ contador;
            docum = nDOCUMENTO;
            nRegi = nREGISTRO;
            sw1   = 0;
            |ABRE #2;
            |ABRE #3;
            nSwErrores = 0;
            |SI #0#5 = 8;
                |SI #0#10 ! 9; |HAZ diario_08;  |FINSI;
                |SI #0#10 = 9; |HAZ diario_Ult; |FINSI;
            |FINSI;
            |SI #0#5 = 9;
                |HAZ diario_texto;
            |FINSI;
            #0#10 = nNuevo;
            |CIERRA #3;
            |CIERRA #2;
            nDOCUMENTO = docum;
            nREGISTRO  = nRegi;
            |HAZ ActContador;
            |SI nSwErrores = 1;
                |MENAV "0000Hay Asientos con Cuenta = ERROR que no se han pasado";
                |MENAV "0000Revisar Diario";
            |FINSI;
        |FINSI;
        |SI sumar ! 0;
            |HAZ GrabaIva;
        |FINSI;
        |DELINDEX #17;
        |CIERRA #caivatm4;

        |SI nAlguno = 1; |SUB_EJECUTA_NP "caut0018;PASE"; |FINSI;
    |FINSI;
|FPRC;

|PROCESO nuSacaNivel;

 |QBF eaCuenta;
 alfa1  = eaCuenta % 0;
 nNume1 = alfa1;

 |SI nNume1 < MaximoDigitos;
     nNume2 = MaximoDigitos - nNume1;
     alfa4 = "0" * nNume2;

     nSwNivel = 0;
     aAlfa = eaCuenta % 103;
     |SI aAlfa = "472"; |O aAlfa = "477";
        nSwNivel = 1;
     |SINO;
        |SI nNume1 [ 6;
            |SI aAlfa = "400"; |O aAlfa = "410"; |O aAlfa = "430";
                nSwNivel = 1;
            |FINSI;
        |FINSI;
     |FINSI;

     |SI nSwNivel = 0;
         alfa1 = eaCuenta % 104;
         alfa2 = eaCuenta % 508;
     |SINO;
         alfa1 = eaCuenta % 103;
         alfa2 = eaCuenta % 409;
     |FINSI;
     |QBF alfa2;

     alfa3 = alfa1 + alfa4 + alfa2;
     eaCuenta = alfa3;

 |FINSI;

 |HAZ SacaNivel;
|FPRC;


