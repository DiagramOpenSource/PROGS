        |FINSI;
        |SI #0#5 = 9;
            #4#2 = varalfa % 1340;    || Titulo
        |FINSI;
        #4#3 = "S";               || pase directo

        |ATRI I;
        |PINTA 2221, #4#0;
        |PINTA 2234, #4#2;
        |ATRI 0;

        mascara = #4#0;
        |QBF mascara;
        mascara = mascara +"zzzzzzzzzzzz";
        mascara = mascara % 112;

        #4#54 = mascara;
        #4#55 = #4#1;

        |HAZ defect1;

        |SI FEstado = 0;
            |SI #0#15 = "S"; |GRABA 020#4a; |FINSI;
        |FINSI;
        |SI FEstado ! 0; |GRABA 020#4b; |FINSI;
        |HAZ Cli_Prov;
