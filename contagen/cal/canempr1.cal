|VARIABLES;
   &v_any    = 0;
   &v_codigo = 0;
   &v_period = 0;
   &v_nombre = "";
   &v_nomemp = "";
   &enganche = 0;
   &r_emp = 0;
   &r_per = 0;
   &r_ope = 0;
   copia_seg = "";
   &enEmpres = 0;
   &enPeriod = 0;
   &enEjerci = 0;
   &enActivi = 0;
   &eaCodSub = "";
   &efPn10 = @;
   &efPn11 = @;
   nNume1  = 0;
   aAlfa   = "";
   aBase   = "";
   aAbre   = "";

   &ext1      = "";
   &enDesde   = 0;
   &eaPlan0   = "";
   &eaPlan1   = "";
   &eaBloqueo = "";
   &enEmpresa = 0;
|FIN;

|PROCESO lee_version; |TIPO 9;
   |SI FSalida ! 1; |Y FSalida ! 7;
      enganche = 1;
      v_codigo = #48#2;
      v_period = #48#3;
      v_any    = #48#26;
      v_nombre = #48#1;
      v_nomemp = #48#1;
      |SI v_codigo ! 0;
         |SUB_EJECUTA_NP "cacerrad;NOBORRARESTO";
      |FINSI;
   |FINSI;
|FPRC;

|PROCESO consulta; |TIPO 0;
   r_emp = #48#2;
   |SI FSalida = 9;
      r_ope = 0;
      |CIERRAT #48;
      |SUB_EJECUTA_NP "caconemp";
      |ABRET #48;
      #48#2 = r_emp;
      #48#3 = r_per;
      |PTEC 802;
   |SINO;
      r_ope = 1;
      r_emp = #48#2;
      |SUB_EJECUTA "caconemp";
      #48#2 = r_emp;
      |SI r_per ! -1;
          #48#3 = r_per;
      |SINO;
          |MENAV "    Empresa Inexistente en el Fichero ";
          |ERROR;
      |FINSI;
   |FINSI;
|FPRC;

|PROCESO consultaP; |TIPO 0;
   r_emp = #48#2;
   r_per = #48#3;
   r_ope = 2;
   |SUB_EJECUTA "caconemp";
   #48#2 = r_emp;
   |SI r_per = -1;
      |MENAV "    Periodo Inexistente en el Fichero ";
      |ERROR;
   |SINO;
       #48#3 = r_per;
   |FINSI;
|FPRC;

|PROCESO losDatos; |TIPO 13;

 enEmpres = #48#2;
 enPeriod = #48#3;
 nNume1   = #48#26;
 |SI nNume1 > 80;
     enEjerci = nNume1 + 1900;
 |SINO;
     enEjerci = nNume1 + 2000;
 |FINSI;
 enActivi = #48#30;
 eaCodSub = #48#31;

 |CIERRAT #48;
 |SUB_EJECUTA_NP "caz00002;NOLOQUITESQUETECORTOLASMANOS";
 |ABRET #48;
 #48#2 = enEmpres;
 #48#3 = enPeriod;
 |POPV;
 |ATRI I;
 |PINTA 1716, efPn10;
 |SI efPn11 > "01.01.0000";
     |PINTA 1757, efPn11;
 |FINSI;
 |ATRI 0;
 |SI #48#34 > "01.01.0000";
     |PINTA 1554, "Fecha Bloqueo: ";
     |C_V #48#34,1,"S";
 |SINO;
     |PINTA 1554, "                         ";
     |C_V #48#34,1,"N";
 |FINSI;
 |PINTA 1062, "                  ";
 |SI enDesde = 1;
     |ATRI P; |PINTA 1062, "PCG: "; |ATRI I;
     |PINTA 1067, eaPlan0;
     |PINTA 1070, eaPlan1;
     |ATRI 0;
 |FINSI;
|FPRC;
