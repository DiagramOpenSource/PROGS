|FICHEROS;
   anm00017 #0;
   caagrupr #10;
   anm0017@ #117;
|FIN;

|VARIABLES;
 aAlfa1   = "";
 aMas     = "";
 aDef     = "";
 nSwError = 0;
 nSalgo   = 0;
 sw       = 0;
 nInkey   = 0;
|FIN;

|INCLUYE dscont_i;
|INCLUYE dscoan_i;
||*************************************************
|PROCESO desglose; |TIPO 0;
   aAlfa1 = #0#2;
   |C_M #0#3, 1, aAlfa1;
   |SI #0#2 = "N";
       #0#3 = 0; |PINTA #0#3;
   |FINSI;
|FPRC;

|PROCESO Pnivel7; |TIPO 7;
  |SI #0#2 = "S"; |Y #0#3 = 0;
      #0#3 = dig3; |PINTA #0#3;
  |FINSI;
|FPRC;

|PROCESO Pnivel0; |TIPO 0;
   |C_M #0#3, 0, aAlfa1;
   |SI aAlfa1 = "N"; |ACABA; |FINSI;

   |SI #0#3 = 0;
       |ERROR;
   |SINO;
       |SI #0#3 > dig3;
           |MENAV "    No existe Nivel de Cuentas";
           |ERROR;
       |FINSI;
   |FINSI;
|FPRC;

|PROCESO LeeEspejo;
 #117#0 = #10#0;
 |LEE 001#117=;
 |SI FSalida = 0;
     #117#1 = #10#1;
     |GRABA 020#117a;
 |SINO;
     #117#0 = #10#0;
     #117#1 = #10#1;
     |GRABA 020#117n;
 |FINSI;
|FPRC;

|DEFBUCLE LeeEspejo;
 #10#1;
 ;
 00;
 99;
 ;
 NULL, LeeEspejo;
|FIN;

|PROCESO ParaCopiar;
 nSwError = 0;
 |DBASE aMas;
 aDef = aMas + "def/anm00017";
 |CARGA_DEF aDef, anm0017@;
 |SI FSalida ! 0;
     |ACABA;
 |FINSI;

 |ABRE #117;
 |BUCLE LeeEspejo;
 |CIERRA #117;
 |DESCARGA_DEF #117;
|FPRC;

|PROCESO Tip11_a; |TIPO 11;
 nInkey = FSalida;
 |SI sw = 0;
      |SI nInkey = 1; |O nInkey = 7;
          nSalgo = 1;
      |FINSI;
 |FINSI;
 |SI nInkey = 10;
     |HAZ ParaCopiar;
     |PTEC 806; sw = 1;
 |FINSI;
|FPRC;

|PROCESO Tip0_a; |TIPO 0;
 sw = 0;
|FPRC;

|RUTINA min;
   #0#0 = 1;
|FRUT;

|RUTINA max;
   #0#0 = 99;
|FRUT;

|PROGRAMA;
   |CLS;
   |CABEZA "Agrupaciones Pr./Comp. Analitica";

   |HAZ inicio;
   |HAZ Analitica;
   |SI  enSwAna = 0;
         aAlfa1 = "    CONTABILIDAD NO ANALITICA ";
         |SI eaAnNiv = "E";
             aAlfa1 = "    EMPRESA NO ANALITICA ";
         |FINSI;
         |MENAV aAlfa1;
         |ACABA;
   |FINSI;

   |PINPA #0#0;
   nSalgo  = 0;
   |PARA; |SI nSalgo = 0; |HACIENDO;
          sw = 0;
          |ENTLINEAL #1#0, -1, 1, 20, 0, min, max;
          |SI nSalgo = 1; |SAL; |FINSI;
   |SIGUE;
|FPRO;
