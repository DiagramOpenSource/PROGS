|PROCESO CambioTitulo;

  nSw = 0;
  aTitul2 = aTitul1;

  |SI aCampo0M = "=";
      aTitul2 = #11#2;
      nSw = 1;
      #0#3 = #11#0 + #11#2; |PINTA #0#3;
      |ACABA;
  |FINSI;

  aTitul1M = aTitul1;
  |SI #0#2 = "S"; aTitul1M = aTitul1M > " "; |FINSI;

  aAlfa1 = aTitul1M - aCampo0M;
  nLaPos = FSalida;
  |SI nLaPos ! 0;
      nSw = 1;
      aAlfa2 = nLaPos;
      aAlfa3 = aTitul1 % aAlfa2;

      aAlfa4 = aAlfa3 > " ";  || Esta en mayuscula
      |SI aAlfa4 = aAlfa3;
          aCampoCam = aCampo1M;
      |SINO;
          aAlfa4 = aAlfa3 < " ";
          |SI aAlfa4 = aAlfa3;
              aCampoCam = aCampo1m;  ||Esta en minusculas
          |SINO;
              aCampoCam = aCampo1x;
          |FINSI;
      |FINSI;

      aAlfa4 = "";
      aAlfa5 = "";
      aAlfa3 = aAlfa2 - (aAlfa2 % -102);
      nNume3 = aAlfa3; nNume3 = nNume3 - 1;
      |SI nNume3 ! 0;
          aAlfa3 = nNume3; aAlfa3 = "1" + (("00" + aAlfa3) % -102);
          aAlfa4 = aTitul1 % aAlfa3;
      |FINSI;
      aAlfa3 = aAlfa2 % -102; nNume2 = aAlfa3;
      nNume4 = nNume3 + nNume2 + 1;
      aAlfa3 = nNume4; aAlfa3 = aAlfa3 + "36";
      aAlfa5 = aTitul1 % aAlfa3;
      aTitul2 = aAlfa4 + aCampoCam + aAlfa5;

      aAlfa1 = aAlfa6; |QBF aAlfa1;
      aAlfa1 = aAlfa1 + " " + aTitul2;
      #0#3 = aAlfa1; |PINTA #0#3;

  |FINSI;
|FPRC;

