         aCampo1M = aCampo1 > " ";
         aCampo1m = aCampo1 < " ";
         aCampo1x = (aCampo1M % 101) + (aCampo1m % 235);

         aAlfa6 = "Cuentas:";
         |BUCLE CaCuenta;

         |SI nSwConiso = 0;  |HAZ CaConiso;  |FINSI;
    |FINSI;
    |SI aCampo0 = "=";
        |SI aDCuenta = "            ";
            nPaseS = 1;
            enContar = enContar + 1;
            |SI nSwConiso = 0;  |HAZ CaConiso;  |FINSI;
        |FINSI;
    |FINSI;
|FPRC;

