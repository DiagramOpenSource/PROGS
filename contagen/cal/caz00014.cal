         |ABRE #10;
         |SELECT #3#10;
         #10#4 = #11#0;
         #10#5 = #11#1;
         #10#0 = 0;
         #10#1 = "01.01.0000";
         #10#2 = 0;
         |LEE 000#10];
         |SI FSalida = 0; |Y #10#4 = #11#0;
             nSwAcum = 1;
         |FINSI;
         |SELECT #1#10;
         |CIERRA #10;
     |FINSI;

     eaMov = "SI"; |SI nSwAcum = 0; eaMov = "NO"; |FINSI;
     |SI nSwAcum = 1; |Y aMov = "S"; nSwAcum = 0; |FINSI;

     |SI nSwAcum = 0;
         #11#2 = aTitul2;
