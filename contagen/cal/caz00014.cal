       #13#1 = #11#2;
       |GRABA 020#13a;
   |SINO;
       #13#23 = "P";
       #13#10 = #11#0;
       #13#11 = #11#1;
       |LEE 101#13=;
       |SI FSalida = 0;
           #13#1 = #11#2;
           |GRABA 020#13a;
       |FINSI;
   |FINSI;
   |LIBERA #13;
