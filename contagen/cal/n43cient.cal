|FRUT;

|PROCESO hazlasuma;
   sumal = sumal + #4#8;
   swalgun = 1;

   |SI #3#12 = "N"; #4#6 = #1#4; |FINSI;
   |SI #3#13 = "N"; |Y #4#6 = #1#4;
       #4#7 = #1#5;
   |FINSI;
   |SI #3#12 = "N"; |O #3#13 = "N";
       |GRABA 020#4a;
   |FINSI;
   |LIBERA #4;
|FPRC;

|DEFBUCLE hazlasuma;
   #4#1;
   ;
   #1#0, #1#2, 0;
   #1#0, #1#2, 99999;
   be;
   NULL,hazlasuma;
|FIN;

|PROCESO contrapartidas;
   |PUSHV 0701 2380;
   swalgun = 0;
   total = #1#7;
   sumal = 0;
   |BUCLE hazlasuma;
   difer = total - sumal;
   |SI swalgun = 0;
      |ABRE #4;
      #4#0 = #1#0;
      #4#1 = #1#1;
      #4#2 = #1#2;
      #4#3 = 1;
      #4#4 = #3#5;
      #4#5 = #3#6;
      #4#6 = #1#10;
      #4#7 = #1#11;
      #4#8 = #1#7;

      |SI #1#6 = "D";  signo = "H";  |SINO;  signo = "D";  |FINSI;

      |GRABA 101#4n;
      |CIERRA #4;

      sumal = #1#7;
      difer = 0;
   |FINSI;

   aBlancs = " " * 78;
   |PINTA 2102, aBlancs;
   |PINTA 2202, aBlancs;
   |PINTA 2302, aBlancs;
   |ENTLINEAL #1#4, -3, 2, 19, 1, menorl, mayorl;
   |POPV;
|FPRC;
