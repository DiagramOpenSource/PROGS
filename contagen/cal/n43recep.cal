        |SI nSwCartera = 0;
            #5#0 = 999;
            |LEE 001#5];
            |SI FSalida = 0;
                |LEE 001#5a;
            |SINO;
                |LEE 001#5u;
            |FINSI;
            |SI FSalida = 0;
                nBanco = #5#0 + 1;
                |DDEFECTO #5;
                #5#0 = nBanco;
                #5#2 = banco;
                #5#3 = oficina;
                #5#5 = cuenta;
                #5#11 = saldo;
                #5#12 = dhcta;
                |GRABA 020#5c;
            |FINSI;
        |SINO;

            |BUCLE dscam014;
            |SI nBanco ! 0;
                #5#0 = nBanco;
                |LEE 001#5=;
                nEstado = FSalida;
                #5#1 = #614#1;
                #5#2 = #614#34;
                #5#3 = #614#35;
                #5#4 = #614#36;
                #5#5 = #614#37;
                #5#6 = #614#10;
                #5#11 = saldo;
                #5#12 = dhcta;
                |SI nEstado = 0; |GRABA 020#5a; |FINSI;
                |SI nEstado ! 0; |GRABA 020#5c; |FINSI;
            |FINSI;

        |FINSI;

        |SI nBanco ! 0;
            |XCIERRA handle;

            |CIERRA #5;
            |SUB_EJECUTA_NP "n43saldo";
            |ABRE #5;

            |XABRE "UB", fiche, handle;
            |SI FSalida < 0;
                |XABRE "CUB", fiche, handle;
            |FINSI;
            |SI FSalida ] 0;
                |XPOSICION handle, nLaPosicion, 0;
            |FINSI;
        |FINSI;

    |FINSI;
