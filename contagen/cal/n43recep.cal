     |XABRE "UB", fiche, handle;
     |SI FSalida < 0;
         |XABRE "CUB", fiche, handle;
         |SI FSalida = 0; nCopio = 1; |FINSI;
     |FINSI;
     |XCIERRA handle;

     |SI nCopio = 1;
         |DFICE aBase;
         eaOrigen  = fiche;
         eaDestino = aBase + traalf1;
         |HAZ eReciboFich;
         |SI enErrPas = 0; fiche = eaDestino; |FINSI;
     |FINSI;
