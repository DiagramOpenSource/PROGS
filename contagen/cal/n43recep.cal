|PROCESO proce;

     unida = #2#0; |QBF unida;

     aFichero = "4z" + Usuario;
     |NOME_DAT #44 aFichero;
     |DELINDEX #44;

     |DBASE aMas;
     aDef = aMas + "def/n43saldo";
     |CARGA_DEF aDef, n43temp@;
     |SI FSalida ! 0;
         |ACABA;
     |FINSI;

     aFichero = "nt" + Usuario;
     |NOME_DAT #900 aFichero;
     |DELINDEX #900;

     aEl13 = &13;
     aEl10 = &10;

     |ABRE #44;
     |ABRE #11;
     nNume1      = 0;
     nLaPosicion = 0;
     |PARA vic = 1; |SI vic [ 20; |HACIENDO vic = vic + 1;
           jn = vic + 20;
           |SI #2jn = "S";
               traalf1 = #2vic; |QBF traalf1;
               |SI traalf1 ! "";
                   |HAZ nombre;
                   |XABRE "UB", fiche, handle;
                   |SI FSalida < 0;
                       |XABRE "CUB", fiche, handle;
                   |FINSI;
                   |SI FSalida ] 0;
                       SwPrim = 0;
                       sw_eof = 0;
                       alfa   = "";
                       |PARA; |SI sw_eof = 0; |HACIENDO;

                              |XLEE handle, 1, aCampo;
                              |SI FSalida > 0;
                                  nLaPosicion = nLaPosicion + 1;
                                  |SI aCampo = aEl13; |O aCampo = aEl10;
                                     |SI alfa ! "";
                                         |HAZ PasoRegistro;
                                         alfa = "";
                                     |FINSI;
                                 |SINO;
                                     |SI SwPrim = 0;
                                         |SI aCampo < "0"; |O aCampo > "9";
                                             aCampo = "";
                                         |SINO;
                                             SwPrim = 1;
                                         |FINSI;
                                     |FINSI;
                                     alfa = alfa + aCampo;
                                 |FINSI;
                              |SINO;
                                  sw_eof = 1;
                              |FINSI;
                       |SIGUE;
                       |SI alfa ! ""; |HAZ PasoRegistro; |FINSI;
                       |XCIERRA handle;
                       Pos = menos;
                    |FINSI;
                |FINSI;
           |FINSI;
     |SIGUE;

     |CIERRA #11;
     |CIERRA #44;
|FPRC;

