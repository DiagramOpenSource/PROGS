       aDef14   = aPCartera + "def/dscam014";
       |CARGA_DEF aDef14, dscam014@;
       |SI FSalida ! 0;
           aDef14 = ""; |HAZ CierraBancoCartera;
           |ACABA;
      |FINSI;

      |PATH_DAT #614 aPathFicE;

   |FINSI;
|FPRC;

|PROCESO CierraBancoCartera;
     |SI aDef14 ! ""; |DESCARGA_DEF #dscam014@; |FINSI;
|FPRC;

