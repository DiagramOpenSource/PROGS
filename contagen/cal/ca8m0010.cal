|FICHEROS;
   ca8m0010#0,MANTE;    || cuentas

   ca8comay #1;
   ca8cumay #2;
   ca8z0010 #10;
|FIN;

|VARIABLES;
   &entrada = 1;
   nModo    = 0;
   cinco    = "     ";
   cta      = "     ";
   ctanum   = 0;
   ctaalf   = "";
   longalf  = "";
   longnum  = "";
   uno      = "";
   dos      = "";
   nada     = "";
   mascara  = "";
   aAlfa1   = "";
   nNume1   = 0;
   nInkey   = 0;
   aCuenta  = "";
|FIN;

|INCLUYE dscont_i;

||  ***************************************

|PROCESO inf0;
 #2#10 = #0#0;
 #2#0  = "    ";
|FPRC;

|PROCESO sup0;
 #2#10 = #0#0;
 #2#0 = "zzzzz";
|FPRC;


|PROCESO nivel_11; |TIPO 11;

 nInkey = FSalida;
 |SI nInkey = 10;
     |ABRE #2;
     #2#10 = #0#0;
     #2#0  = #0#1;
     |CONSULTA_F_CLAVE #1#2, inf0, sup0;
     #0#1 = #2#0; |PINTA #0#1;
     |CIERRA #2;
 |FINSI;
|FPRC;

|PROCESO nivel_a; |TIPO 7;
|FPRC;


|PROCESO nivel_m; |TIPO 0;

   nInkey = FSalida;
   |SI nInkey = 10; |Y Campo = 3;
       |ABRE #2;
       #2#10 = #0#0;
       #2#0  = #0#3;
       |CONSULTA_F_CLAVE #1#2, inf0, sup0;
       #0#3 = #2#0; |PINTA #0#3;
       |CIERRA #2;
   |FINSI;

   nModo = (FEntrada / 100) ! 0;
   |SI #0Campo = cinco;
       |SI nModo = 4; |ACABA; |FINSI;
   |FINSI;

   |SI Campo = 3;
       cta = #0Campo; |QBF cta;
       |SI cta = ""; |ACABA; |FINSI;
   |FINSI;


   |SI Campo = 3; |O Campo = 1;
       aCuenta = #0Campo; |QBF aCuenta;
       |SI Campo = 3;
           aAlfa1 = aCuenta % -101;
           |SI aAlfa1 = "*"; aCuenta = aCuenta - aAlfa1; |FINSI;
       |FINSI;
       |ABRE #2;
       #2#10 = #0#0;
       #2#0  = aCuenta;
       |LEE 001#2=;
       |SI FSalida ! 0;
           |MENAV "    No existe Cuenta en las Cuentas de Mayor";
           |ERROR;
       |SINO;
           |SI Campo = 1; #0#2 = #2#1; |PINTA #0#2; |FINSI;
           |SI Campo = 3; #0#4 = #2#1; |PINTA #0#4; |FINSI;
       |FINSI;
       |CIERRA #2;
   |FINSI;
|FPRC;
