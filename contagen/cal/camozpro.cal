|FICHEROS;
  camozpro #0;
|FIN;

|VARIABLES;
   aParam = "";
   aAlfa1 = "";
   &enEmpresa  = 0;
   &enEjer     = 0;
   &enPerConta = 0;
   aPrograma  = "";
   &Haymodelo = 0;
   nMes11 = 0;
|FIN;

|PROGRAMA;
  |HAZ DirAplicSt;
  |SI Haymodelo = 0;
      |MENAV "    Opcion innaccesible ";
      |ACABA;
  |FINSI;

  aParam = PARAMETRO; |QBF aParam;
  |SI aParam = ""; aParam = "dsmoy001"; |FINSI;

  aPrograma = aParam % 108;
  aAlfa1    = aParam % 902; |QBF aAlfa1;
  |SI aAlfa1 ! "";
      aPrograma = aPrograma + ";" + aAlfa1;
  |FINSI;
  |SI aPrograma = "dsmoy012";
      aPrograma = aPrograma + ";conta";
  |FINSI;

  enEmpresa  = #48#2;
  enEjer     = #48#26;
  enPerConta = #48#3;
  nMes11     = #48#11;

  |SI enEjer < 80;
      enEjer = 2000 + enEjer;
  |SINO;
      enEjer = 1900 + enEjer;
  |FINSI;

  |SI aPrograma = "dsmoy001"; |Y nMes11 ! 1;
      |MENAV "0000Empresa con Ejercicio Partido";
      |MENAV "0000Debe lanzar el informe desde el punto de Modelos";
      |ACABA;
  |FINSI;

  aPrograma = ":modelos/" + aPrograma;
  |SUB_EJECUTA_NP aPrograma;

  |PULSATECLA;
|FPRO;
