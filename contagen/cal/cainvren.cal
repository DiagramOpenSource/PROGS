|INCLUYE dscont_i;
|FICHEROS;
   cainvren #0;
   cauxinva #1;
|FIN;

|VARIABLES;
   nume1 = 0;
|FIN;

|PROCESO tipo3; |TIPO 3;
   |SI #cainvren#2 = 0;  |ACABA;  |FINSI;
   |ABRE #1;
   #cauxinva#0 = #cainvren#1;
   |LEE 001#1=;
   |SI FSalida ! 0;  |LEE 001#1a;  |FINSI;
   |PARA ; |SI FSalida = 0; |Y #cauxinva#0 ] #cainvren#0; |HACIENDO;
      |BORRA 001#1a;
      #cainvren#3 = #cauxinva#0; |PINTA #cainvren#3;
      #cauxinva#0 = #cauxinva#0 + #cainvren#2;
      #cainvren#4 = #cauxinva#0; |PINTA #cainvren#4;
      |GRABA 021#1n;
      |LIBERA #1;
      |LEE 001#1a;
   |SIGUE;
   |CIERRA #1;
|FPRC;

|PROCESO leelin;
   |ABRE #1;
   |LEE 001#1u;
   |SI #cauxinva#0 ! #cainvren#0;
      #cainvren#1 = #cauxinva#0; |PINTA #cainvren#1;
   |SINO;
      |MENAV "0000Este es el Ultimo Registro";
      |CIERRA #1;
      |ERROR;
      |ACABA;
   |FINSI;
   |CIERRA #1;
|FPRC;

|PROCESO tengosit;
   |SI #cainvren#1 < #cainvren#0;
      |MENAV "      Hasta Inferior a Desde";
      |ERROR;
      |ACABA;
   |FINSI;
   nume1 = #cainvren#1 + #cainvren#2;
   |SI nume1 > 999999;
      |MENAV "     EXCEDIDO EL LIMITE DE LINEAS";
      |ERROR;
      |ACABA;
   |FINSI;
   |ABRE #1;
   #cauxinva#0 = #cainvren#1;
   |LEE 001#1];
   |SI FSalida ! 0;
      |CIERRA #1;
      |ACABA;
   |FINSI;
   |SI #cauxinva#0 = #cainvren#1;
      |LEE 001#1s;
      |SI FSalida ! 0;
         |CIERRA #1;
         |ACABA;
      |FINSI;
   |FINSI;
   |SI nume1 ] #cauxinva#0;
      |MENAV "     No se Puede Intercalar MACHACARIA LINEAS EXISTENTES";
      |ERROR;
   |FINSI;
   |CIERRA #1;
|FPRC;
