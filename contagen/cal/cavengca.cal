|CALCULO cheq_libro; |TIPO 0;
   ModoEntrada  = (FEntrada / 100) ! 0;
   |ABRE #calibros;
   #calibros#0 = #0#1;
   |LEE 010#calibros.=;
   |SI FSalida ! 0;
       |MENAV "      No existe libro";
       |CIERRA #calibros;
       |C_M #0#1,1,"S";
       |ERROR;
       |ACABA;
   |FINSI;
   |CIERRA #calibros;

   |SI #calibros#2 ! aTipoIVA;
       |MENAV "     Libro de IVA No Correcto";
       |C_M #cavengca#1,1,"S";
       |ERROR;
       |ACABA;
   |FINSI;

   #0#2 = #calibros#1; |PINTA #0#2;
   |SI ModoEntrada = 1;
       |HAZ ult_fra;
       #0#3 = NumFactura;
       |PINTA #0#3;
   |FINSI;

