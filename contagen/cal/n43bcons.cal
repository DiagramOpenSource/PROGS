|FICHEROS;
    n43bcons #0;
    n43consl #1;
    n43datos #2;
|FIN;

|VARIABLES;
    error1 = "     No existe impresora";
    error2 = "     No existe informe";
|FIN;

|INCLUYE dscont_i;

|CALCULO cuenta; |TIPO 6;
     eaCuenta = #0Campo;
     salidas  = FSalida;
     |HAZ FiltraPuntos;
     #0Campo = eaCuenta;
     |PINTA #0Campo;

     |HAZ SacaNivel;
|FCAL;

|CALCULO imprimir;
    #2#0 = #1#1;
    |LEE 101#2=;
    |SI FSalida = 0;
         |SI #2#11 ! "S";
             #2#9 = "N";
             |GRABA 020#2a;
         |FINSI;
    |FINSI;
    |LIBERA #2;

    |BORRA 040#1a;
    |LIBERA #1;
|FCAL;

|DEFBUCLE cpconsol;
   #1#1;
   17, 12, 10;
   #0#0, #0#2, #0#4, #0#6;
   #0#1, #0#3, #0#5, #0#7;
   ;
   NULL, imprimir;
|FIN;

|CALCULO borra; |TIPO 3;
  |SI #0#8 = "S";
      |ABRE #2;
      |BUCLE cpconsol;
      |CIERRA #2;
  |FINSI;
|FCAL;
