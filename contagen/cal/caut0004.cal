|PROCESO LaCopia;

  |PATH_DAT #1 eaOrigen;
  |PATH_DAT #2 eaOrigen;
  |PATH_DAT #3 eaOrigen;
  |PATH_DAT #4 eaOrigen;

  aQueQuiero = "|NC";

  aNumCampos = #1#aQueQuiero#; nNumCampo1 = aNumCampos; nNumCampo1 = nNumCampo1 - 1;
  aNumCampos = #2#aQueQuiero#; nNumCampo2 = aNumCampos; nNumCampo2 = nNumCampo2 - 1;
  aNumCampos = #3#aQueQuiero#; nNumCampo3 = aNumCampos; nNumCampo3 = nNumCampo3 - 1;
  aNumCampos = #4#aQueQuiero#; nNumCampo4 = aNumCampos; nNumCampo4 = nNumCampo4 - 1;

  |DBASE aBass;  |QBT aBass;

  aDef = aBass + "def/camautoc.mas";
  |CARGA_DEF aDef, imautoc@;
  |SI FSalida ! 0;
      |MENAV "    Error en def camautoc. Proceso interrumpido";
      |ACABA;
  |FINSI;

  aDef = aBass + "def/camautol.mas";
  |CARGA_DEF aDef, imautol@;
  |SI FSalida ! 0;
      |MENAV "    Error en def camautol. Proceso interrumpido";
