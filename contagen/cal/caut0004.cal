|PROCESO Rellena;

   |SI aEstEmp ! "T";
       |SI #30#21 ! aEstEmp;  |ACABA; |FINSI;
   |FINSI;

   nLaEmpresa = #30#2;
   |HAZ ElTipoEmpresa;
   |SI nSwTipo = 1; |ACABA; |FINSI;

   |SI aParam = "E2010";
       |HAZ SelEjercicio;
       |SI nSwEjer = 1; |ACABA; |FINSI;
   |FINSI;

   |DDEFECTO #998;
   #998#0 = Linea;
   #998#1 = #30#2;
   #998#2 = #30#3;
   |HAZ CalDirectorio;
   |SI nSwNoDir = 0;
       |GRABA 020#998n;
       Linea = Linea + 1;
   |FINSI;
|FPRC;

