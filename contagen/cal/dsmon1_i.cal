|INCLUYE dscont_i;
*****************************************************************************
|PROCESO ElCambioMoneda;
     |SI aMonedaC = aMonedaA; |ACABA; |FINSI;

     |SI aMonedaC = "P";                  || pesetas
         mulMoneda = 1 / 166.386;
     |FINSI;
     |SI aMonedaC = "E";                  || euros
         mulMoneda = 166.386;
     |FINSI;
     impMoneda = (impMoneda * mulMoneda) ! EURODB;
|FPRC;

|PROCESO ElCambio100;
     impMoneda = (impMoneda % enTP100) ! EURODB;
|FPRC;
