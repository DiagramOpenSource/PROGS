|CALCULO Tipo11_15; |TIPO 11;
   nInkey = FSalida;
   |SI nInkey = 10;
       |PUSHV 05012380;
       |SI enSwAna = 1;
           |C_V #114#26,1,"S";
           |C_V #114#29,1,"S";
       |FINSI;
       |BLIN 6; |BLIN 7;
