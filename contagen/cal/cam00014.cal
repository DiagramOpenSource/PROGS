     |ATRI O;
     |ENTLINEAL #1#15, -5, 4, 22, 0, ClaveBajaAx15, ClaveAltaAx15;
     |POPV;
     |ERROR;
 |FINSI;
 |SI nInkey = 11; |Y #14#19 = "X";
     docum = -1;
     |IMPRE 0;
     |SI FSalida ! 0; |ACABA; |FINSI;
     |INFOR informe;
     |SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;
     |SI #14#11 = 1; eaTit1 = "Libros        "; |FINSI;
     |SI #14#11 = 2; eaTit1 = "Amortizaciones"; |FINSI;
