|FICHEROS;
     cacuenta;
     camandia;

     :modelos/dsmom030;
|FIN

|VARIABLES;
     aOpcion             = "";
     aMas                = "";

     &eaAlfa             = "";
|FIN;

|INCLUYE dscont_i;

|PROCESO BajaCacuenta;
     #cacuenta#3 = "S";
     #cacuenta#0 = "";
|FPRC;

|PROCESO AltaCacuenta;
     #cacuenta#3 = "S";
     #cacuenta#0 = "z" * 12;
|FPRC;

|PROCESO T66;
     |SI aMas = "cacuenta";
         |ABRE #cacuenta;

         #cacuenta#0 = eaAlfa;
         |LEE 000#cacuenta.=;

         |CONSULTA_F_CLAVE #6#cacuenta, BajaCacuenta, AltaCacuenta;
         |SI FSalida = 0;
             eaAlfa = #cacuenta#0;
         |FINSI;

         |CIERRA #cacuenta;
     |FINSI;

     |SI aMas = "dsmom030";
         |ABRE #dsmom030;

         #dsmom030#0 = eaAlfa;
         |LEE 000#dsmom030.=;

         |CONSULTA_CLAVE #1#dsmom030;
         |SI FSalida = 0;
             eaAlfa = ("000" + #dsmom030#0) % -103;
         |FINSI;

         |CIERRA #dsmom030;
     |FINSI;

     |SI aMas = "camandia";
         |ABRE #camandia;

         #camandia#0 = eaAlfa;
         |LEE 000#camandia.=;

         |CONSULTA_CLAVE #1#camandia;
         |SI FSalida = 0;
             eaAlfa = ("00" + #camandia#0) % -102;
         |FINSI;

         |CIERRA #camandia;
     |FINSI;
|FPRC;

|PROCESO T00;
     |SI aMas = "cacuenta";
         eaCuenta = eaAlfa;
         salidas  = FSalida;
         |HAZ FiltraPuntosX;
         eaAlfa = eaCuenta;

         |ABRE #cacuenta;
         #cacuenta#0 = eaAlfa;
         |LEE 000#cacuenta.=;
         |SI FSalida ! 0;
             |MENAV "0000La cuenta contable no existe.";
             eaAlfa = "";
         |FINSI;
         |CIERRA #cacuenta;
     |FINSI;

     |SI aMas = "dsmom030";
         |ABRE #dsmom030;

         #dsmom030#0 = eaAlfa;
         |LEE 000#dsmom030.=;
         |SI FSalida ! 0;
             |MENAV "0000La concepto no existe.";
             eaAlfa = "";
         |FINSI;
         |CIERRA #dsmom030;
     |FINSI;

     |SI aMas = "camandia";
         |ABRE #camandia;
         #camandia#0 = eaAlfa;
         |LEE 000#camandia.=;
         |SI FSalida ! 0;
             |MENAV "0000El diario no existe.";
             eaAlfa = "";
         |FINSI;
         |CIERRA #camandia;
     |FINSI;
|FPRC;

|PROGRAMA;
     aOpcion = PARAMETRO % 103;
     aMas    = PARAMETRO % 408;

     |SI aOpcion = "T66";
         |HAZ T66;
     |FINSI;

     |SI aOpcion = "T00";
         |HAZ T00;
     |FINSI;
|FPRO;

|PROCESO T80;  |TIPO 80;
     |HAZ inicio;

     |PULSATECLA;
     |PULSATECLA;
     |PULSATECLA;
|FPRC;

|PROCESO T90;  |TIPO 90;
|FPRC;
