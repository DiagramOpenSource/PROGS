|FICHEROS;
 :/modelos/def/dsmom100 #100;
|FIN;

|VARIABLES;
 &enSw100  = 0;
 &enEmp100 = 0;
 &enPer100 = 0;
|FIN;


|PROCESO eBuscoMaestra;
     enSw100 = 0;
     |ABRE #100;
     |LEE 040#100p;
     |SI FSalida = 0;
         |SI  #100#1  = enEmp100; |Y #100#2  = enPer100; enSw100 = 1; |FINSI;
         |SI  #100#4  = enEmp100; |Y #100#5  = enPer100; enSw100 = 1; |FINSI;
         |SI  #100#7  = enEmp100; |Y #100#8  = enPer100; enSw100 = 1; |FINSI;
         |SI  #100#10 = enEmp100; |Y #100#11 = enPer100; enSw100 = 1; |FINSI;
         |SI  #100#13 = enEmp100; |Y #100#14 = enPer100; enSw100 = 1; |FINSI;
         |SI  #100#16 = enEmp100; |Y #100#17 = enPer100; enSw100 = 1; |FINSI;
         |SI  #100#19 = enEmp100; |Y #100#20 = enPer100; enSw100 = 1; |FINSI;
     |FINSI;
     |CIERRA #100;
|FPRC;
