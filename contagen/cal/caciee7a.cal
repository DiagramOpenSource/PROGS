|PROCESO PonSaldo;

     aAlfa3 = #0#2; |QBF aAlfa3;
     |SI aAlfa3 = ""; |Y nSwError = 0;
         |MENAV "    ATENCION!!!. APUNTES CON CUENTAS EN BLANCO. REPASE EL ASIENTO";
         nSwError = 1;
     |FINSI;

     |SI #0#6 = "D";
         #11#19 = #11#19 + #0#7;
     |FINSI;
     |SI #0#6 = "H";
         #11#20 = #11#20 + #0#7;
     |FINSI;
     #11#21 = #11#19 - #11#20;

     |PINTA #11#19;
     |PINTA #11#20;
     |PINTA #11#21;

     |SI #0#2 = #100#7; |Y nDesglose = 0;
         |SI #0#6 = "D";  nBeneficio = nBeneficio - #0#7;  |FINSI;
         |SI #0#6 = "H";  nBeneficio = nBeneficio + #0#7;  |FINSI;
     |FINSI;
|FPRC;

|| ***********************************************************
|PROCESO CuentaDirectoNueva;
     |SI #901#6 = #11#26;
         |SI #901#10 ! doce;
             #0#2 = #901#10;
         |SINO;
             eaCuenta = #0#2 - "NUEVA "
             #0#2 = eaCuenta;
         |FINSI;
     |FINSI;
     |HAZ CuentaDirecto;
     |SI #901#6 = #11#26;
         #901#10 = #0#2;
         |GRABA 020#901a;

         |SI #901#9 = 35; #11#53 = #0#2; |FINSI;
         |SI #901#9 = 44; #11#54 = #0#2; |FINSI;
         |SI #901#9 = 48; #11#55 = #0#2; |FINSI;
     |FINSI;
|FPRC;

|PROCESO CuentaDirecto;
     eaCuenta = #0#2; |QBF eaCuenta;
     nBuena = 0;
     aAlfa1 = eaCuenta % -101;
     |SI aAlfa1 = "*";
         eaCuenta = eaCuenta - "*";
         nBuena = 1;
     |SINO;
         |SI aAlfa1 = "a"; |O aAlfa1 = "u";
             nBuena = 2;
         |FINSI;
     |FINSI;
     |SI nBuena = 0;
         aAlfa1 = eaCuenta - ".";
         |SI aAlfa1 ! eaCuenta; nBuena = 2; |FINSI;
     |FINSI;

     |SI nBuena = 0;
         |ABRE #1;
         |DDEFECTO #1;
         #1#0 = eaCuenta;
         |LEE 001#1=;
         |SI FSalida ! 0; |O #1#3 = "N";
             nBuena = 1;
         |FINSI;
         |CIERRA #1;
     |FINSI;
     |SI nBuena = 1;
         nNume1 = 100 + MaximoDigitos;
         aAlfa1 = (eaCuenta + "000000000000") % nNume1;
         eaCuenta = aAlfa1;
         nBuena = 2;
     |FINSI;
     |SI nBuena = 2;
         salidas  = FSalida;
         Campo    = 2;
         mierda   = 1;
         |HAZ FiltraPuntos;
         #0#2     = eaCuenta;
     |FINSI;
|FPRC;

