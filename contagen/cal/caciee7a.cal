         #0#5 = aDeAnt;
         |PINTA #0#5;
     |FINSI;
|FPRC;

|PROCESO Concepto; |TIPO 0;             || desde campo 'concepto'
     salidas = FSalida;
     |ABRE #4;
     #4#0 = #0#4;
     |LEE 040#4=;
   |SI #dsmom030#8 ! "S";
       |MENAV "    El concepto no corresponde al Tipo de Apunte";
       |ERROR;
   |FINSI;
     |SI FEntrada = 101;
         |SI #0#4 = nCoAnt; |Y #11#19 ! #11#20;
             #0#5 = aDeAnt;
         |SINO;
             #0#5 = #4#1;
         |FINSI;
     |SINO;
         |SI #0#4 = nCoAnt;
             #0#5 = aDeAnt;
         |SINO;
             #0#5 = #4#1;
         |FINSI;
     |FINSI;
     |PINTA #0#5;
     |CIERRA #4;
|FPRC;

