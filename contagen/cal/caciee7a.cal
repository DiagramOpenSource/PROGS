|PROCESO CuenFin; |TIPO 0;
     |HAZ Signo;
     |ABRE #1;
     nCampo  = Campo;
     nCampo1 = Campo + 1;
     #1#0 = #0nCampo;
     #1#1 = #0nCampo1;
     |LEE 001#1=;
     |SI FSalida ! 0;  |ERROR;  |ACABA;  |FINSI;
     |SI #1#3 = "N";
         |MENAV "     La Cuenta NO tiene PASE DIRECTO";
         |ERROR;
     |FINSI;
     |CIERRA #1;
|FPRC;

|PROCESO CuentaTl;
     aAlfa1 = #0nCampo;    |QBF aAlfa1;
     aAlfa2 = aAlfa1 % 0;
     aLong  = aAlfa2;
     nCont  = 0;
     nSw    = 0;

     |SI aLong = dig4;  nSw = 1;  #0nCampo1 = 1;  |FINSI;
     |SI aLong = dig5;  nSw = 1;  #0nCampo1 = 2;  |FINSI;
     |SI aLong = dig6;  nSw = 1;  #0nCampo1 = 3;  |FINSI;
     |SI aLong = dig7;  nSw = 1;  #0nCampo1 = 4;  |FINSI;
     |SI aLong = dig8;  nSw = 1;  #0nCampo1 = 5;  |FINSI;
     |SI aLong = dig9;  nSw = 1;  #0nCampo1 = 6;  |FINSI;
     |SI aLong = 0;     nSw = 1;  #0nCampo1 = 0;  |FINSI;

     |SI nSw = 0;
         #0nCampo1 = 0;
         |MENAV "      Longitud de Cuenta Fuera de Nivel ";
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Cuenta; |TIPO 6;                    || desde campo 'cuenta'
     salidas = FSalida;
     |SI salidas = 15;
         |SI Campo = 8;
             #0#8 = aCtHaAnt;
             #0#9 = nDiHaAnt;
         |FINSI;
         |SI Campo = 10;
             #0#10 = aCtDeAnt;
             #0#11 = nDiDeAnt;
         |FINSI;
         |PINTA #0Campo;
         salidas = 0;
     |FINSI;

     mierda  = 0;

     eaCuenta = #0Campo;
     salidas  = FSalida;
     |HAZ FiltraPuntos;
     #0Campo = eaCuenta;
     |PINTA #0Campo;

     nCampo  = Campo;
     nCampo1 = Campo + 1;
     |HAZ CuentaTl;
|FPRC;

|PROCESO CCuenta; |TIPO 7;
     nNume = (FEntrada / 100) ! 0;
     |C_M #0#8,  1, "S";
     |C_M #0#10, 1, "S";

     |SI #0#8 ! "            "; |O #0#10 ! "            ";
         |SI Campo = 8;  |Y #0#10 ! "            "; |C_M #0#8, 1,"N"; |FINSI;
         |SI Campo = 10; |Y #0#8  ! "            "; |C_M #0#10,1,"N"; |FINSI;
     |FINSI;
|FPRC;

|PROCESO NoCero; |TIPO 0;
     salidas = FSalida;
     |HAZ Aviso9;
     |SI #0Campo = 0; |Y salidas ! 11;
         |ERROR;
         |ACABA;
     |SINO;
         |SI salidas = 11;   || <F3>
             |SI #0#6 = "D";  #0#7 = #11#21 * -1;  |FINSI;
             |SI #0#6 = "H";  #0#7 = #11#21;       |FINSI;
             |PINTA #0#7;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO Signo;
     |SI #0#10 ! "            ";
         #0#6 = "D";
         #0#2 = #0#10;
         #0#3 = #0#11;
         #0#8 = "";
         #0#9 = 0;
     |FINSI;

     |SI #0#8  ! "            ";
         #0#6 = "H";
         #0#2 = #0#8;
         #0#3 = #0#9;
         #0#10 = "";
         #0#11 = 0;
     |FINSI;
|FPRC;

|PROCESO CImporte; |TIPO 7;
     |HAZ Signo;
     nNume = (FEntrada / 100) ! 0;
     |SI nNume = 1;
         #0#7 = nImpAnt;
         |PINTA #0#7;
     |FINSI;
|FPRC;

|PROCESO CDescrip; |TIPO 7;
       |SI eaSobreEs ! "S";
           |PTEC 816;
       |FINSI;
|FPRC;

|PROCESO Descrip; |TIPO 0;
