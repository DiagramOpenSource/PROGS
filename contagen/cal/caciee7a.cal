|PROCESO PonAsiento35_44;  || Provision

     |SI #901#6 ! #11#26;
         nImporteL = #901#4;
         aSigno    = "H";
     |SINO;
         nImporteL = #901#2 - #901#4;
         aSigno    = "D";
     |FINSI;
     |SI nImporteL = 0; |ACABA; |FINSI;

