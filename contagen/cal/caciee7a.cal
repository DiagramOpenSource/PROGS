|PROCESO Aviso5;         || desde calculo 'c_importe' (camaasil)
     |PUSHV 09101171;

     |HAZ ColorF;
     |CUADRO 09 10 11 71;
     |HAZ ColorC;
     |QBF aAvisos;

     aAlfa1 = aAvisos % 0;  nNume1 = aAlfa1;
     nNume2 = ((60 - nNume1) / 2) ! 0;
     nNume1 = 60 - nNume1 - nNume2;
     aAvisos = (" " * nNume1) + aAvisos + (" " * nNume2);        || lo centra

     |PINTA 1011, aAvisos;
     |ATRI 0;
     |AVISO;
|FPRC;

