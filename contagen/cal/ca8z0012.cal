|FICHEROS;
  ca8m0000 #0;
  ca8w0999 #1,MANTE;
  ca8z0012 #10;
|FIN;

|VARIABLES;
 aParam    = "";
 &nBalance = 0;
 nInkey    = 0;
 aAlfa1    = "";
 nNumero   = 0;
|FIN;

|INCLUYE dscont_i;

|| ******************************************** Calculos Pantalla

|CALCULO descri;  |TIPO 0;
  nInkey = FSalida;
  |SI nInkey = 1; nInkey =  -1; |FINSI;
|FCAL;

|CALCULO wTipo11; |TIPO 11;
 |SI FSalida = 0; |O FSalida = 10;
     |LEE 101#1=;
     |ENCAMPO #13#1;
     |GRABA 020#1a;
     |LIBERA #1;
 |FINSI;
|FCAL;

|CALCULO wTipo12; |TIPO 12;
|FCAL;

|| /////////////////////// PROCESOS DE TRABAJO \\\\\\\\\\\\\\\\\\\\
|RUTINA min;
  #1#14 = #10#4;
  #1#0  = #10#2;
  #1#1  = #10#0;
  #1#11 = 000001;
|FRUT;

|RUTINA max;
  #1#14 = #10#4;
  #1#0  = #10#2;
  #1#1  = #10#0;
  #1#11 = 999999;
|FRUT;

|PROCESO Tipo3;  |TIPO 3;
  |ENTLINEAL #2#1, -4, 4, 22, 0, min, max;
|FPRC;

|| **************************************************************************
|PROGRAMA;
   aParam = PARAMETRO; |QBF aParam;
   aAlfa1 = aParam % 205;
   |SI aAlfa1 = "";
       |HAZ inicio;
       |SI enEjercicio < 2008;
           |MENAV "    Plan Contable de 2008. No modificables en este Ejercicio";
           |ACABA;
       |FINSI;
   |SINO;
      enEjerBal = aAlfa1; |SI enEjerBal < 2008; enEjerBal = 2008; |FINSI;
   |FINSI;

   aParam = aParam % 101;
   |SI aParam = ""; aParam = "0"; |FINSI;
   nBalance   = aParam;

   |DDEFECTO #10;
   #10#4 = enEjerBal;
   #10#0 = nBalance;
   |SI #10#0 = 0;
       #10#1 = "BALANCE DE SITUACION AL CIERRE DEL EJERCICIO";
   |FINSI;
   |SI #10#0 = 1;
       #10#1 = "CUENTA DE PERDIDAS Y GANACIAS CORRESPONDIENTE AL EJERCICIO ";
   |FINSI;
   |SI #10#0 = 2;
       #10#1 = "ESTADO DE INGRESOS Y GASTOS RECONOCIDOS";
   |FINSI;
   |SI #10#0 = 4;
       #10#1 = "ESTADO DE FLUJO DE EFECTIVO";
   |FINSI;

   |CLS;
   |CABEZA "Defincion de Balances Notas Memoria";
   |PINPA #0#10;
   |PINDA #0#10;

   FSalida = 1;
   |PARA; |SI 0 < FSalida; |HACIENDO;
          |ENDATOS #1#10;
          |SI 0 [ FSalida;
              |SI nInkey = -1; |SAL; |FINSI;
              |HAZ Tipo3;
              FSalida = 1;
          |FINSI;
   |SIGUE;
|FPRO;
