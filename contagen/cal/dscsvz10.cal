         #dscsvw09#8  = aAlfa;
     |FINSI;

     |SI nCampo = 8;  #dscsvw09#9  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 9;  #dscsvw09#10 = #dscsvw10#1;  |FINSI;
     |SI nCampo = 10; #dscsvw09#11 = #dscsvw10#1;  |FINSI;
