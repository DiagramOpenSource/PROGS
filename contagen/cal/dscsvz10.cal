     |SI nCampo = 3;  #dscsvw08#3  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 4;  #dscsvw08#26 = #dscsvw10#1;  |FINSI;
     |SI nCampo = 5;  #dscsvw08#7  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 7;  #dscsvw08#9  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 8;  #dscsvw08#52 = #dscsvw10#1;  |FINSI;
     |SI nCampo = 11; #dscsvw08#34 = #dscsvw10#1;  |FINSI;
     |SI nCampo = 13; #dscsvw08#10 = #dscsvw10#1;  |FINSI;
|FPRC;

|PROCESO GrabaProveedor;
     #dscsvw08#0  = nReg;
     #dscsvw08#24 = "P";

     |SI nCampo = 0;  #dscsvw08#11 = #dscsvw10#1;  |FINSI;
     |SI nCampo = 2;  #dscsvw08#2  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 3;  #dscsvw08#3  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 4;  #dscsvw08#26 = #dscsvw10#1;  |FINSI;
     |SI nCampo = 5;  #dscsvw08#7  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 6;  #dscsvw08#6  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 8;  #dscsvw08#34 = #dscsvw10#1;  |FINSI;
     |SI nCampo = 13; #dscsvw08#10 = #dscsvw10#1;  |FINSI;
     |SI nCampo = 16; #dscsvw08#52 = #dscsvw10#1;  |FINSI;
|FPRC;

|PROCESO GrabaAsiento;
     #dscsvw06#0  = nReg;

     |SI nCampo = 2;
         aFecha      = #dscsvw10#1;
         |HAZ CalculaFecha;
         #dscsvw06#2 = aFecha;
     |FINSI;

     |SI nCampo = 3;  #dscsvw06#5  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 5;  #dscsvw06#8  = #dscsvw10#1;  |FINSI;

     |SI nCampo = 6;
         aAlfa = #dscsvw10#1;
         |HAZ QuitaPuntos;
         #dscsvw06#10  = aAlfa;
         #dscsvw06#9   = "D";

         |SI #dscsvw06#10 < 0;
             #dscsvw06#10  = -#dscsvw06#10;
             #dscsvw06#9   = "H";
         |FINSI;
     |FINSI;

     |SI nCampo = 11;
         aAlfa = #dscsvw10#1;  |QBF aAlfa;
         |SI aAlfa ! "";
             aAlfa1 = aAlfa % 105;
             |SI aAlfa1 ! "43000";
                 #dscsvw10#1 = "43000" + aAlfa;
             |FINSI;

             #dscsvw06#5  = #dscsvw10#1;
             #dscsvw06#35 = aAlfa;
         |FINSI;
     |FINSI;

     |SI nCampo = 12;
         aAlfa = #dscsvw10#1;  |QBF aAlfa;
         |SI aAlfa ! "";
             aAlfa1 = aAlfa % 105;
             |SI aAlfa1 ! "40000";
              |Y aAlfa1 ! "41000";
              |Y aAlfa1 ! "44000";
                 #dscsvw10#1 = "40000" + aAlfa;
             |FINSI;

             #dscsvw06#5  = #dscsvw10#1;
             #dscsvw06#35 = aAlfa;
         |FINSI;
     |FINSI;

     |SI nCampo = 1;   #dscsvw06#33  = #dscsvw10#1;  |FINSI;  ||Tipo de asiento
     |SI nCampo = 4;   #dscsvw06#34  = #dscsvw10#1;  |FINSI;  ||Referencia
|FPRC;

|PROCESO GrabaIva;
     #dscsvw09#0  = nReg;

     |SI nCampo = 0;
         aFecha      = #dscsvw10#1;  |QBF aFecha;
         |HAZ CalculaFecha;
         #dscsvw09#1 = aFecha;
     |FINSI;

     |SI nCampo = 1;  #dscsvw09#2  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 2;  #dscsvw09#3  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 3;  #dscsvw09#4  = #dscsvw10#1;  |FINSI;
     |SI nCampo = 4;  #dscsvw09#5  = #dscsvw10#1;  |FINSI;

     |SI nCampo = 5;
         aAlfa = #dscsvw10#1;
         |HAZ QuitaPuntos;
         #dscsvw09#6  = aAlfa;
     |FINSI;

     |SI nCampo = 6;
         aAlfa = #dscsvw10#1;
         |HAZ QuitaPuntos;
         #dscsvw09#7  = aAlfa;
     |FINSI;

     |SI nCampo = 7;
