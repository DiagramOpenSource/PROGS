|FICHEROS;
   otp00012 #10;
|FIN;

|VARIABLES;
  &enSPA = 0;

  nSpaReg = 0;
  nSwEnter = 0;
  aAlfa  = "";
  nPrimero = 0;
|FIN;

|RUTINA inf;
   #otp00012#0 = 0000;
|FRUT;

|RUTINA sup;
   #otp00012#0 = 9999;
|FRUT;

|PROCESO Tipo07012; |TIPO 7;
   |SI nSwEnter = 0;
      nSpaReg = #otp00012#0;
   |FINSI;
|FPRC;

|PROCESO Tipo13012; |TIPO 13;
   nSwEnter = 0;
   |SI FSalida = 0;
      |SI nPrimero = 1;
         nPrimero = 0;
      |SINO;
         |PTEC 807; nSwEnter = 1;
      |FINSI;
   |FINSI;
|FPRC;

|PROGRAMA;
   aAlfa = PARAMETRO; |QBF aAlfa;
   |PATH_DAT #otp00012#aAlfa#;
   aAlfa = "ctrlspas";     |NOME_DAT #otp00012#aAlfa#;

   nPrimero = 1;
   |ABRE #otp00012;
   |ENTLINEAL #1#otp00012, -1, 4, 21, 1, inf, sup;
   |CIERRA #otp00012;

   enSPA = nSpaReg;
|FPRO;
