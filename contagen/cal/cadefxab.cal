|CALCULO PCentro11; |TIPO 11;
  nInkey = FSalida;
  eOpCen = 0;
  |SI nInkey  = 10;
      eOpCen   = 1;
      enCentro = #20#0;
      |HAZ CentroTrabajo;
      |SI eswLect = 0;
          #20#0 = enCentro;     |PINTA #20#0;
          #20#62 = eaNomCentro; |PINTA #20#62;
      |FINSI;
      nInkey = 0;
 |FINSI;
|FCAL;

|CALCULO PCentro; |TIPO 0;
  eOpCen = 0;
  enCentro = #20#0;
  |HAZ CentroTrabajo;
  |SI eswLect = 1;
      |ERROR;
  |SINO;
      #20#0 = enCentro; |PINTA #20#0;
      #20#62 = eaNomCentro; |PINTA #20#62;
  |FINSI;
  #20#60 = #20#0;
|FCAL;

|CALCULO ParaEntrar; |TIPO 0;
  ModoEntrada = (FEntrada / 100) ! 0;

  Comodin = "|NC";
  aNumCmps = #cadefxab#Comodin#;
  nNumCmps = aNumCmps;
  nNumCmps = nNumCmps - 1;

  |DDEFECTO #0;
  |PARA Cont = 1; |SI Cont [ nNumCmps; |HACIENDO Cont = Cont + 1;
         |SI ModoEntrada = 1;
             #0Cont = #50Cont;
         |SINO;
             #0Cont = #20Cont;
         |FINSI;
  |SIGUE;
  #0#0  = #20#0; #0#62 = #20#62;
  |SI ModoEntrada = 1;
      |SI #0#42 = "C"; #0#42 = "N"; |HAZ Busca42; |FINSI;
      |SI #0#42 = "S"; |HAZ Carga1; |FINSI;
      |SI #0#42 = "A"; |HAZ Carga3; |FINSI;
      |SI #0#106 = "C"; |O #0#107 = "C"; |HAZ Busca100; |FINSI;
      |SI #0#106 = "C"; #0#106 = "N"; |FINSI;
      |SI #0#107 = "C"; #0#107 = "N"; |FINSI;
      |SI #0#106 = "A"; |O #0#107 = "A"; |HAZ Carga13; |FINSI;
  |FINSI;
  #0#78  = #50#78;
  #0#79  = #50#79;
  #0#108 = #50#108;
  #0#109 = #50#109;
  #0#110 = #50#110;

  |PINPA #0#0;
  |PINDA #0#0;
  |SI ModoEntrada = 1; |ENDATOS #1#0; |FINSI;
  |SI ModoEntrada = 2; |ENDATOS #2#0; |FINSI;
  |SI FSalida = 0;
      |PARA Cont = 0; |SI Cont [ nNumCmps; |HACIENDO Cont = Cont + 1;
            #20Cont = #0Cont;
      |SIGUE;
      |PINDA #0#20;
  |SINO;
      |ERROR;
  |FINSI;
|FCAL;

