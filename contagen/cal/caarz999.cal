|PROCESO LaDescripcion;
  aSepSer   = #caivaasi#97;
  aNumIzq   = #caivaasi#98;

  |SI #3#19 ! "X"; aDescrIVA = #3#7; |FINSI;

  |SI #3#19 ! "P";
      aAlfa5 = "";
      aAlfa1 = #3#7;  |QBF aAlfa1;  || Descripcion

      aAlfa2 = #caivaasi#45;
      |SI #3#19 = "I"; |O #3#19 = "R"; |O #3#19 = "B";
          |O #3#19 = "i"; |O #3#19 = "r"; |O #3#19 = "b";
            |SI #caivaasi#130 ! "=  ";
                aAlfa2 = #caivaasi#130;
            |FINSI;
      |FINSI;
      |SI #3#19 = "X"; aAlfa2 = eaAnyadir; |FINSI;

      |QBF aAlfa2;  || lo que a¤ade

      aAlfa3 = aAlfa2 % 0;
      nDCon0 = aAlfa3;
      |PARA nDCon1 = 1; |SI nDCon1 [ nDCon0; |HACIENDO nDCon1 = nDCon1 + 1;
            aAlfa3 = nDCon1;
            aAlfa3 = aAlfa3 + "01";
            aDCon2 = aAlfa2 % aAlfa3;
            |HAZ adapta2_c;
            aAlfa5 = aAlfa5 + aDCon1;
      |SIGUE;
      #3#7 = aAlfa1 + aAlfa5;
   |SINO;
        aAlfa5 = "";
        aAlfa1 = #3#7;  |QBF aAlfa1;  || Descripcion
        aAlfa2 = #caivaasi#96; |QBF aAlfa2;  || lo que a¤ade
        aAlfa3 = aAlfa2 % 0;

        nDCon0 = aAlfa3;
        |PARA nDCon1 = 1; |SI nDCon1 [ nDCon0; |HACIENDO nDCon1 = nDCon1 + 1;
              aAlfa3 = nDCon1;
              aAlfa3 = aAlfa3 + "01";
              aDCon2 = aAlfa2 % aAlfa3;
              |HAZ adapta2_c;
              aAlfa5 = aAlfa5 + aDCon1;
        |SIGUE;
        #3#7 = aAlfa1 +  aAlfa5;
   |FINSI;
   aDescrip = #3#7;
   |SI #caivaasi#131 = "S"; |Y #3#19 ! "X"; aDescrIVA = aDescrip; |FINSI;
|FPRC;

|PROCESO adapta2_c;  ||El concepto
     aDCon1 = "";
     |SI aDCon2 = "S";
         aDCon1 = #camaniva#2; |QBF aDCon1;
         |SI aDCon1 ! ""; aDCon1 = aDCon1 + aSepSer; |QBF aDCon1; |FINSI;
     |FINSI;                                         || Serie
     |SI aDCon2 = "F";
         aDCon1 = #camaniva#3; |QBF aDCon1;
         |SI aNumIzq = "S";
             aDCon1 = ("000000" + aDCon1) % -106;
         |FINSI;
     |FINSI;                                         || Factura
     |SI aDCon2 = "C";
         aDCon1 = #camaniva#12; |QBF aDCon1;
     |FINSI;                                         || Cuenta Cliente
     |SI aDCon2 = "N";
         aDCon1 = #camaniva#13; |QBF aDCon1;
     |FINSI;                                         || Nombre Cliente
     |SI aDCon2 = "R";
         |SI eaReferen ! "";
             aDCon1 = eaReferen; |QBF aDCon1;
         |FINSI;
     |FINSI;
                                     || Referencia
     |SI aDCon2 = "P";
         aDCon1 = eaReferen;
         |QBF aDCon1;
     |FINSI;                                         || Referencia Comersan

     |SI aDCon1 ! "";
         |SI aDCon2 = "F";
             aAlfa7 = aAlfa2 - "S";
             |SI aAlfa7 ! aAlfa2; |Y #camaniva#2 > "     ";
                 |ACABA;
             |FINSI;
         |FINSI;
         aDCon1 = " " + aDCon1;
     |FINSI;
|FPRC;
