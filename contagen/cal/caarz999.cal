       nHaber = nHaber + #3#9;
       #camaasic#05 = #camaasic#05 + #3#09;
       |SI #camaasic#11 = 0; #camaasic#10 = #3#4; #camaasic#11 = #3#5; |FINSI;  || contrp.DEBE
