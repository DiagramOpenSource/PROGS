      |DDEFECTO #206;
      #206#0 = nLibro;
      #206#2 = aTipoIVA;
      |GRABA 020#206n;
  |FINSI;
  |CIERRA #206;

  aAlfa1 = #3#1;
  aAlfa1 = aAlfa1 % 402;
  nPerio = aAlfa1; nNume2 = nPerio;
  |SI enEjercicio [ 2009;
      aAlfa = #caivaasi#46; |SI aTipoIVA = "N"; aAlfa = #caivaasi#149; |FINSI;
      |SI aAlfa = "A"; nPerio = 0; |FINSI;
      |SI aAlfa = "T";
          nPerio = 1;
          |SI nNume2 ] 4; nPerio = 2; |FINSI;
          |SI nNume2 ] 7; nPerio = 3; |FINSI;
          |SI nNume2 ] 10;nPerio = 4; |FINSI;
     |FINSI;
  |FINSI;

  |SI elDocumen = 0;
