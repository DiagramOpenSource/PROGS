  #camaniva#5  = nPerio;    || Periodo
  #camaniva#7  = #3#0;      || Diario
  #camaniva#8  = #3#1;      || Fecha contable
  #camaniva#9  = #3#2;      || Documento
  #camaniva#36 = aTipoIVA;  || Tipo de IVA/IRPF

  |GRABA 020#camaniva.n;
  swIVA = 1;
