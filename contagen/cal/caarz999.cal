  |FINSI;
  |SI #caivaasi#189 = "S"; |Y aTipoIVA = "R"; |Y enEjercicio ] 2017;
       aAlfa1 = #camaniva#3;
       |SI #caivaasi#98 = "S"; aAlfa1 = ("000000" + aAlfa1) % -106; |FINSI;
       aAlfa2 = #camaniva#2; |QBF aAlfa2;
       |SI aAlfa2 ! "";  aAlfa1 = aAlfa2 + #caivaasi#97 + aAlfa1; |FINSI;
