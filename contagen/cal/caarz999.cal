   #caivalin#31 = #camaniva#11;

   efFechaIVA = #camaniva#4;  enLineaIVA = 0;
   enPorcIVA  = #caivalin#7;  enPorcREC = #caivalin#9;
   eaCtaIVA   = #caivalin#13; eaCtaREC  = #caivalin#14;
   |HAZ BuscaTipoIVA;
