      #camaniva#0 = nLibro;
      #camaniva#1 = 9999999999;
      |LEE 010#camaniva.];
      |SI FSalida = 0;
          |LEE 040#camaniva.a;
      |SINO;
          |LEE 040#camaniva.u;
      |FINSI;
      |SI FSalida = 0; |Y #camaniva#0 = nLibro;
          NumDoc = #camaniva#1 + 1;
      |FINSI;
  |FINSI;

  |DDEFECTO #camaniva;
