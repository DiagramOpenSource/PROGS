      #caivalin#20 = nBase;                 || Base imponible
      #caivalin#21 = nTipIRPF;              || Tipo IRPF
      #caivalin#22 = #3#9;                  || Cuota IRPF
      #caivalin#23 = #3#4;                  || Cuenta
  |FINSI;
