|INCLUYE dscont_i;
|FICHEROS;
   camaycab #1;
|FIN;

|VARIABLES;
   &entrada = 1;
   anter = 0;
   men1  = "     NO EXISTE EMPRESA !";
   fecalf = "";
   mensa1 = "    ERROR ESTA FECHA NO CORRESPONDE AL EJERCICIO";
   traba1 = "";
|FIN;



|PROCESO entrada;
   |PINTA 0739, "                                        ";
   |SI FEntrada ! 101;  |ACABA;  |FINSI;
   anter = #camaycab#0;
   #camaycab#0 = 98;
   #camaycab#1 = fec2;
   #camaycab#2 = 999999;
   |LEE 040#1>;
   |SI FSalida = 0;
      |LEE 040#1a;
   |SINO;
      |LEE 040#1u;
   |FINSI;
   |SI FSalida ! 0;  |DDEFECTO #1;  #camaycab#2 = 0;  |FINSI;
   #camaycab#0 = anter;
   #camaycab#2 = #camaycab#2 + 1;
   #camaycab#3 = #camaycab#2;
   #camaycab#4 = 0;
   #camaycab#5 = 0;
   |PINTA #camaycab#0;
   |PINTA #camaycab#1;
   |PINTA #camaycab#2;
   |PINTA #camaycab#3;
|FPRC;

|PROCESO fecha;
   |SI #camaycab#1 < fec1; |O #camaycab#1 > fec2;
      |MENAV mensa1;
      |ERROR;
      |ACABA;
   |FINSI;
|FPRC;

|PROCESO document;
   |SI FEntrada = 101;
      #camaycab#3 = #camaycab#2;
      |PINTA #camaycab#3;
   |FINSI;
|FPRC;
