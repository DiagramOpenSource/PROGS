|PROCESO Actualizar;
   swSelec = 0;
   |SI desde = "C";
       |SI #0#2 ! doce;
           |SI #1#10 ] #0#1; |Y #1#10 [ #0#2; swSelec = 1; |FINSI;
       |FINSI;
       |SI #0#4 ! doce;
           |SI #1#10 ] #0#3; |Y #1#10 [ #0#4; swSelec = 1; |FINSI;
       |FINSI;
       |SI #0#6 ! doce;
           |SI #1#10 ] #0#5; |Y #1#10 [ #0#6; swSelec = 1; |FINSI;
       |FINSI;
   |FINSI;
   |SI desde = "P";
       |SI #0#8 ! doce;
           |SI #1#10 ] #0#7; |Y #1#10 [ #0#8; swSelec = 1; |FINSI;
       |FINSI;
       |SI #0#10 ! doce;
           |SI #1#10 ] #0#9; |Y #1#10 [ #0#10; swSelec = 1; |FINSI;
       |FINSI;
       |SI #0#12 ! doce;
           |SI #1#10 ] #0#11; |Y #1#10 [ #0#12; swSelec = 1; |FINSI;
       |FINSI;
       |SI #0#14 ! doce;
           |SI #1#10 ] #0#13; |Y #1#10 [ #0#14; swSelec = 1; |FINSI;
       |FINSI;
   |FINSI;
   |SI swSelec = 0; |ACABA; |FINSI;

   nCambio = 0;
   |SI #1#23 = "C";
       |SI #0#16 ! 0; |Y #0#16 = #1#42;  nCambio = 1; nNume1 = #0#24;  |FINSI;
       |SI #0#17 ! 0; |Y #0#17 = #1#42;  nCambio = 1; nNume1 = #0#25;  |FINSI;
       |SI #0#18 ! 0; |Y #0#18 = #1#42;  nCambio = 1; nNume1 = #0#26;  |FINSI;
       |SI #0#19 ! 0; |Y #0#19 = #1#42;  nCambio = 1; nNume1 = #0#27;  |FINSI;
   |SINO;
       |SI #0#20 ! 0; |Y #0#20 = #1#42;  nCambio = 1; nNume1 = #0#28;  |FINSI;
       |SI #0#21 ! 0; |Y #0#21 = #1#42;  nCambio = 1; nNume1 = #0#29;  |FINSI;
       |SI #0#22 ! 0; |Y #0#22 = #1#42;  nCambio = 1; nNume1 = #0#30;  |FINSI;
       |SI #0#23 ! 0; |Y #0#23 = #1#42;  nCambio = 1; nNume1 = #0#31;  |FINSI;
   |FINSI;
   |SI nCambio ! 0;
        #1#42 = nNume1;
        |GRABA 020#1a;
   |FINSI;
   |LIBERA #1;

   |PINTA 2202, "Cliente / Proveedor: ";
   |PINTA 2223, #1#10;
   |PINTA 2236, #1#1;
|FPRC;

