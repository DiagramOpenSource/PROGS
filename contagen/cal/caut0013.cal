|CALCULO LaSeleccion; |TIPO 0;
 |C_M #0#1, 1,"S"; |C_M #0#2, 1,"S";
 |C_M #0#3, 1,"S"; |C_M #0#4, 1,"S";
 |C_M #0#5, 1,"S"; |C_M #0#6, 1,"S";
 |C_M #0#7, 1,"S"; |C_M #0#8, 1,"S";
 |C_M #0#9, 1,"S"; |C_M #0#10,1,"S";
 |C_M #0#11,1,"S"; |C_M #0#12,1,"S";
 |C_M #0#13,1,"S"; |C_M #0#14,1,"S";
 |SI #0#0 = "C";
      |C_M #0#7, 1,"N"; |C_M #0#8, 1,"N";
      |C_M #0#9, 1,"N"; |C_M #0#10,1,"N";
      |C_M #0#11,1,"N"; |C_M #0#12,1,"N";
      |C_M #0#13,1,"N"; |C_M #0#14,1,"N";
 |FINSI;
 |SI #0#0 = "P";
      |C_M #0#1, 1,"N"; |C_M #0#2, 1,"N";
      |C_M #0#3, 1,"N"; |C_M #0#4, 1,"N";
      |C_M #0#5, 1,"N"; |C_M #0#6, 1,"N";
 |FINSI;
|FCAL;

|CALCULO ElPtec; |TIPO 7;
 |C_M #0Campo,0,aAlfa1;
 |SI aAlfa1 = "S";
     |PTEC 816;
 |FINSI;
|FCAL;

|CALCULO Hasta; |TIPO 0;
 |C_M #0Campo,0,aAlfa1;
 |SI aAlfa1 = "S";
     nNume1 = Campo - 1;
     |SI #0Campo < #0nNume1;
         |MENAV "    Error!. Cuenta Hasta Menor que la Cuenta Desde";
         |ERROR;
     |FINSI;
 |FINSI;
|FCAL;

|CALCULO cuenta;|TIPO 6;
     |C_M #0Campo, 0, aAlfa1;
     |SI aAlfa1 = "N"; |ACABA; |FINSI;

     eaCuenta = #0Campo;
     salidas  = FSalida;
     |HAZ FiltraPuntos;
     #0Campo = eaCuenta; |PINTA #0Campo;
|FCAL;

|CALCULO cambio; |TIPO 0;
  aAlfa1 = "S";
  |SI #0Campo = 0; aAlfa1 = "N"; |FINSI;
  nNume1 = Campo + 8;
  nNume2 = Campo + 16;
  nNume3 = Campo + 24;
  |C_M #0nNume1,1,aAlfa1;
  |SI aAlfa1 = "N";
      #0nNume1 = 0;  |PINTA #0nNume1;
      #0nNume2 = ""; |PINTA #0nNume2;
      #0nNume3 = ""; |PINTA #0nNume3;
  |FINSI;
|FCAL;

|CALCULO TrIVA; |TIPO 0;
   nInkey = FSalida;
   |SI nInkey = 2; |ACABA; |FINSI;

   |C_M #0Campo,0,aAlfa1;
   |SI aAlfa1 = "N"; |ACABA; |FINSI;

   enEjercicio = #0#15;
   nNume1      = Campo + 16;

   aTipoIVA = "R";
   |SI Campo ] 20; |Y Campo [ 23; aTipoIVA = "S"; |FINSI;
   |SI Campo ] 28; |Y Campo [ 31; aTipoIVA = "S"; |FINSI;

   |SI nInkey = 10;
       efFechaIVA = ~; enLineaIVA = 0; enIvaTot = 1; |HAZ SelecTipoIVA; enIvaTot = 0;
       #0Campo = enLineaIVA; |PINTA #0Campo;
   |FINSI;

   |SI #0Campo = 0;
       #0nNume1 = ""; |PINTA #0nNume1;
   |SINO;
       efFechaIVA = "01.01.2000";
       |SI Campo ] 24; |Y Campo [ 31;
           efFechaIVA = "31.12.2999";
           |SI #0#15 = 2010; efFechaIVA = "01.07.2010"; |FINSI;
           |SI #0#15 = 2012; efFechaIVA = "01.09.2012"; |FINSI;
       |FINSI;
       enLineaIVA = #0Campo; |HAZ SacaIVA;
       |SI enLineaIVA < 0;
           |MENAV "    Control de cuentas no definido";
           |ERROR; |ACABA;
       |FINSI;
       nNume2 = enPorcIVA * 100; aAlfa1 = nNume2; aAlfa1 = aAlfa1 % -102;
       nNume2 = enPorcIVA ! 0;   aAlfa2 = nNume2;
       aAlfa3 = ("     " + aAlfa2 + "," + aAlfa1) % -105;
       #0nNume1 = aAlfa3;
       |PINTA #0nNume1;
  |FINSI;
|FCAL;

