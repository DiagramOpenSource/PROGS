  |HAZ SacaNivel;
  act_nivel = enNivel;

  acor_c = act_cuent; acor_d = act_nivel;
  |HAZ acortacuen;
  act_cuent = acor_c; act_nivel = acor_d;

  |SI act_cuent = #1#0;
      sw_exp = 1;
      |SI descar ] 0;
          nDebe = nDebe + descar;
      |SINO;
          nHaber = nHaber - descar;
      |FINSI;
       nSaldo = nDebe - nHaber;
  |FINSI;

  nSaldo12 = nSaldo;
  eaCuenta = #1#0;
|FPRC;

