|FICHEROS;
   caratios #0;
   caimprat #5;
|FIN;

|VARIABLES;
   informe = "caratios";
   seleccio = 0;
|FIN;

|INCLUYE dscont_i;

|PROCESO tipo3; |TIPO 3;
   |IMPRE 0;
   |SI FSalida = 0;
      |INFOR informe;
      |ABRE #0;
      #caratios#0 = #caimprat#0;
      |LEE 000#0];
      |PARA; |SI FSalida = 0; |Y #caratios#0 [ #caimprat#1; |HACIENDO;
         |PRINT;
         |PIEINF;
         |LEE 000#0s;
         seleccio = 1;
      |SIGUE;
      |CIERRA #0;
      |FININF;
      |SI seleccio = 0;
         |MENAV "    Seleccion vacia ...";
      |FINSI;
   |SINO;
      |MENAV "    Proceso abortado ...";
   |FINSI;
   |FINIMP;
|FPRC;
