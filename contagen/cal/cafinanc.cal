|PROCESO VerElemento;
      |SI #0#44 [ "        "; |ACABA; |FINSI;
      enEmpresa = #0#0;
      eaDElem   = #0#44;
      enDNEle   = #0#45;
      enDeAqui  = 1;
      |SUB_EJECUTA_NP ":modelos/dsmoz036";
