   #41#0 = enEmpresa;
   #41#2 = "        ";
   #41#3 = 00;
|FRUT;

|RUTINA supA2;
   #41#0 = enEmpresa;
   #41#2 = "zzzzzzzz";
   #41#3 = 99;
|FRUT;

|CALCULO ElElem0; |TIPO 0;
  |C_M #0Campo,0,alfa1;
  |SI alfa1 = "N"; |ACABA; |FINSI;

  |SI nInkey  = 10;
      |ABRE #41;
      #41#2 = #0#44;
      #41#3 = #0#45;
      |CONSULTA_F_CLAVE #1#41,infA2,supA2;
      #0#44 = #41#2; |PINTA #0#44;
      #0#45 = #41#3; |PINTA #0#45;
      |CIERRA #41;
  |FINSI;

  |SI nInkey  = 11;
      |HAZ VerElemento;
      |ERROR;
      |ACABA;
  |FINSI;

  |SI #0#44 ! aElem; |O #0#45 ! nElem;
      |SI aElem ! "        ";
          |CONFI "    NDesea Cambiar el Elemento de Inmovilizado ";
          |SI FSalida ! 0;
              #0#44 = aElem; |PINTA #0#44;
              #0#45 = nElem; |PINTA #0#45;
              |ERROR;
              |ACABA;
          |FINSI;
      |FINSI;
  |FINSI;


  alfa1 = #0#44; |QBF alfa1;
  |SI alfa1 = "";
      #0#45 = 0;
      |PINTA #0#45;
      |PTEC 802;
      |ACABA;
  |FINSI;

  |SI #0#44 ! aElem;
      |ABRE #41;
      #41#0 = enEmpresa;
      #41#2 = #0#44;
      #41#3 = 00;
      |LEE 011#41];
      |SI FSalida ! 0; |O #41#0 ! enEmpresa; |O #41#2 ! #0#44;
          #0#45 = 0;
          |PINTA #0#45;
          |PTEC 802;
      |FINSI;
      |CIERRA #41;
   |FINSI;
|FCAL;

|CALCULO ElNumElem0; |TIPO 0;

  |SI FSalida = 2; |ACABA; |FINSI;
  |C_M #0Campo,0,alfa1;
  |SI alfa1 = "N"; |ACABA; |FINSI;

  alfa1 = #0#44; |QBF alfa1; |SI alfa1 = ""; |ACABA; |FINSI;

  |SI #0#45 ! nElem;
      |SI #0#44 = aElem;
          |CONFI "    NDesea Cambiar el Elemento de Inmovilizado ";
          |SI FSalida ! 0;
              #0#45 = nElem; |PINTA #0#45;
              |ERROR;
              |ACABA;
          |FINSI;
      |FINSI;
  |FINSI;

  |ABRE #41;
  #41#0 = enEmpresa;
  #41#2 = #0#44;
  #41#3 = #0#45;
  |LEE 041#41=;
  nEstado = FSalida;
  |CIERRA #41;
  |SI nEstado ! 0;
      |CONFI "    SEl Elemento de Inmovilizado no Existe, Desea darlo de Alta S/N";
      |SI FSalida = 0;
          |HAZ AltaElemento;
      |SINO;
          |ERROR;
          |ACABA;
      |FINSI;
  |FINSI;
  nSwError = 0;
  |HAZ ModiElemento;
  |SI nSwError = 1;
      |ERROR;
  |FINSI;
|FCAL;

