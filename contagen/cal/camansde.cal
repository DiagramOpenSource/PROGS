|PROGRAMA;

   |SI eaCodSubdep = "";
       |HAZ inicio;
       |SI enSubde = 0;
           |MENAV "   La Empresa no tiene definidos los Subdepartamentos como <O> ";
           |ACABA;
       |FINSI;
       |CLS;
   |FINSI;

   |SI enSubde ! 1; |ACABA; |FINSI;

   |CABEZA "Suddepartamentos";
   |PINPA #0#0;

   |ENTLINEAL #1#0, -3, 1, 20, 0, min, max;
|FPRO;

|RUTINA min;
   #0#0 = enEmpresa;
   #0#1 = eaCodDep;
   #0#2 = "";
   |SI FSalida = "POSICION";
       |SI eaCodSubdep > " ";
           #0#2 = eaCodSubdep;
       |FINSI;
   |FINSI;
|FRUT;

|RUTINA max;
   #0#0 = enEmpresa;
   #0#1 = eaCodDep;
   #0#2 = "zzz";
|FRUT;

|CALCULO ElsTip11; |TIPO 11;
 |SI FSalida = 1;
     eaCodSubdep = #0#2;
 |FINSI;
|FCAL;

