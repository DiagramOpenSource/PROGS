|FICHEROS;
   caconcic #0;
   caconcil #1;
   cacuenta #2;
|FIN;

|VARIABLES;
   &entrada  = 1;
   cta   = 0;
   cta_t = 0;
   dig_t = 0;
   long  = "  ";
   sw    = 0;
|FIN;

|INCLUYE dscont_i;

|PROCESO mayor;
   #caconcic#Campo# = #caconcic#Campo# > " ";
   |PINTA #caconcic#Campo#;
|FPRC;

|PROCESO cuenta; |TIPO 6;
     eaCuenta = #caconcic#Campo#;
     salidas  = FSalida;
     |HAZ FiltraPuntos;
     #caconcic#Campo# = eaCuenta;
     |PINTA #caconcic#Campo#;

   cta_t = Campo;
   dig_t = Campo + 1;
   |HAZ cuenta_t;
|FPRC;

|PROCESO cuenta_t;
   cta  = #caconcic#cta_t#;
   long = cta % 0;
   sw   = 0;
   |SI long = dig4; sw = 1; #caconcic#dig_t# = 1; |FINSI;
   |SI long = dig5; sw = 1; #caconcic#dig_t# = 2; |FINSI;
   |SI long = dig6; sw = 1; #caconcic#dig_t# = 3; |FINSI;
   |SI long = dig7; sw = 1; #caconcic#dig_t# = 4; |FINSI;
   |SI long = dig8; sw = 1; #caconcic#dig_t# = 5; |FINSI;
   |SI long = dig9; sw = 1; #caconcic#dig_t# = 6; |FINSI;

   |SI sw = 0;
      #caconcic#dig_t# = 0;
      |MENAV "    Longitud de cuenta fuera de Nivel !";
      |ERROR;
      |ACABA;
   |FINSI;
|FPRC;

|PROCESO cuefin; |TIPO 0;
   |ABRE #2;
   #cacuenta#0 = #caconcic#cta_t#;
   #cacuenta#1 = #caconcic#dig_t#;
   |LEE 040#2=;
   |SI FSalida ! 0; |CIERRA #2; |ERROR; |ACABA; |FINSI;
   |SI #cacuenta#3 = "N";
      |CIERRA #2;
      |MENAV "      La cuenta no tiene pase DIRECTO !";
      |ERROR;
      |ACABA;
   |FINSI;
   #caconcic#2 = #cacuenta#2;
   |PINTA #caconcic#2;
   |CIERRA #2;
|FPRC;
