|FICHEROS;
   caliniv0 #900,MANTE;
   caliniva #901,MANTE;
   calinivr #902,MANTE;

   cafantas #903;
|FIN;

|INCLUYE dscont_i;
|INCLUYE dscoan_i;
|INCLUYE dscoiv_i;

|VARIABLES;
  aAlfa1  = "";
  alfa1  = "";
  inkey = 0;
  &nTPorCien = 0;
|FIN;

|PROCESO Tip11mr; |TIPO 11;
 inkey = FSalida;
 eOpDep = 0;
 |SI inkey = 10;
     eOpDep = 1;
     |HAZ Actividad;
     eOpDep = 0;
 |FINSI;
|FPRC;

|PROCESO Tip0mr; |TIPO 0;
 eOpDep = 0;
 enActividad = #902#0;
 |HAZ Actividad;
 |SI eswLect = 1;
     |MENAV "      Actividad Inexistente";
     |ERROR;
 |SINO;
     alfa1 = enActividad;
     alfa1 = ("00" + alfa1) % A02;
     enFilRegIVA = 1;
     enConcepto  = #901#4;
     |HAZ FilRegimenIVA;
     |SI eswLect ! 0; |ACABA; |FINSI;

     #902#0 = enActividad; |PINTA #902#0;
     #902#1 = eaNombreAct; |PINTA #902#1;
     |SI efActFin > "01.01.1900"; |Y snFilBa ! "N";
         aAlfa1 = "    NAtencion!!!. Actividad con Fecha de BAJA " + efActFin + ", Desea introducir Facturas ? ";
         |CONFI aAlfa1;
          |SI FSalida ! 0;
              |ERROR;
              |ACABA;
          |FINSI;
      |FINSI;
      |SI eaEsCom = "S";
          |CONFI "    NAtencion!!!. Actividad Comunera, ¨ Desea introducir Elementos ? ";
          |SI FSalida ! 0;
              |ERROR;
              |ACABA;
          |FINSI;
      |FINSI;
  |FINSI;
|FPRC;

|PROCESO Tip2mr; |TIPO 2;
 nTPorCien = nTPorCien +. #902#2;
 #903#18   = nTPorCien;
 |ATRI P; |PINTA 1472, #903#18; |ATRI 0;
|FPRC;
