|FICHEROS;
    camocab5 #0;
   :/modelos/def/dsmom042 #1;
   :/modelos/def/dsmom041 #2;
|FIN;

|VARIABLES;
   alfa1    = "";
   &nLib_am = 0;
   &aSer_am = "";
   &nFra_am = 0;

   &enSwEncuentro = 0;
   &aElemento     = "";
   &nNumero       = 0;
   &nLinea        = 0;
   &enNumLin      = 0;
|FIN;

|INCLUYE dscont_i;
|INCLUYE dscoan_i;

|| ****************************************************************************

|PROCESO Encuentro;
 |SI #1#9 ] fec1; |Y #1#9 [ fec2;
     enSwEncuentro = 1;
     aElemento    = #1#2;
     nNumero      = #1#3;
     nLinea       = #1#4;
     enNumLin     = enNumLin + 1;
 |FINSI;
|FPRC;

|DEFBUCLE BuscoFact;
 #1#2;
 14,15,16;
 enEmpresa, "        ", 00, "C", fec1, 001, nLib_am, aSer_am, nFra_am;
 enEmpresa, "zzzzzzzz", 99, "C", fec2, 999, nLib_am, aSer_am, nFra_am;
 e;
 NULL, Encuentro;
|FIN;

|PROGRAMA;
  |HAZ DirAplicSt;
  |SI Haymodelo = 0;
      |ACABA;
  |SINO;
      alfa1 = aPModelo + "fich/";
      |PATH_DAT #1 alfa1;
      |PATH_DAT #2 alfa1;
  |FINSI;

  enNumLin      = 0;
  enSwEncuentro = 0;
  |BUCLE BuscoFact;
|FPRO;
