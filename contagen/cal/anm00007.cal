|FICHEROS;
 anm00007 #0;
 cacuenta #1;
 anm00001 #2;
 anm00010 #3;
|FIN;

|VARIABLES;
 &entrada = 1;
 &sw_rel  = 1;
 &rel_sw  = 1;
 aAlfa1   = "";
 nBorro   = 0;
 nModo    = 0;
|FIN;

|INCLUYE dscont_i;
|INCLUYE dscoan_i;

|| **********************************************************************

|CALCULO Tipo11; |TIPO 11;
     |SI FSalida ! 20;  |ACABA;  |FINSI;

     salidas  = FSalida;
     |HAZ FiltraPuntos;
|FCAL;

|CALCULO cuenta; |TIPO 6;
     eaCuenta = #0Campo
     salidas  = FSalida;
     |HAZ FiltraPuntos;
     #0Campo = eaCuenta;
     |PINTA #0Campo;
|FCAL;

|CALCULO cuefin; |TIPO 0;
   emCta    = 1;
   eaCuenta = #0#0;
   |HAZ SacaNivel;
   |SI eswCta ! 0;  |ACABA; |FINSI;  ||Cuenta erronea
   #0#1 = enNivel;

   |HAZ LeeCuenta;
   |SI eswCta ! 0;  |ACABA; |FINSI;  ||Cuenta erronea

   |HAZ Tipo7;
|FCAL;

|CALCULO leecta; |TIPO 6;
  salidas = FSalida;
  |HAZ FPuntosAna;
|FCAL;

|CALCULO hastacta; |TIPO 0;
   |SI FSalida = 2; |ACABA; |FINSI;
   |SI #3#2 > #3#3;
       |MENAV "    Error. La Cta Analitica Hasta es inferior a la Cta Analitica Desde";
       |ERROR;
   |FINSI;
|FCAL;

|PROCESO BorroLineas;
 |BORRA 020#3a;
 |LIBERA #3;
|FPRC;

|DEFBUCLE BorroLineas;
 #3#1;
 ;
 #0#0, 001;
 #0#0, 999;
 be;
 NULL, BorroLineas;
|FIN;

|PROCESO Tipo1; |TIPO 1;
 nModo = (FEntrada / 100) ! 0;
 |SI nModo = 3;
     |BUCLE BorroLineas;
 |FINSI;
|FPRC;

|RUTINA min10;
 #3#0 = #0#0;
 #3#1 = 001;
|FRUT;

|RUTINA max10;
 #3#0 = #0#0;
 #3#1 = 999;
|FRUT;

|PROCESO Tipo7; |TIPO 7;
 |ENTLINEAL #1#3, -2, 7, 21, 0, min10, max10;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
 nModo = (FEntrada / 100) ! 0;
 |SI FSalida = 0;
     |SI #0#4 = "S";
         |SI nModo = 1; |HAZ AltaUno; |FINSI;
         |ENTLINEAL #1#3, -2, 2, 21, 0, min10, max10;
     |SINO;
         nBorro = 0;
         |ABRE #3;
         #3#0 = #0#0;
         #3#1 = 1;
         |LEE 041#3=;
         |SI FSalida = 0; |Y #3#0 = #0#0;
             |CONFI "    SBorro Relacion de Ctas Analiticas posibles ";
             |SI FSalida = 0;
                 nBorro = 1;
             |FINSI;
         |FINSI;
         |CIERRA #3;
         |SI nBorro = 1;
             |BUCLE BorroLineas;
         |FINSI;
     |FINSI;
 |FINSI;
|FPRC;

|PROCESO AltaUno;
 |ABRE #3;
 #3#0 = #0#0;
 #3#1 = 001;
 |LEE 001#3=;
 |SI FSalida ! 0;
     #3#0 = #0#0;
     #3#1 = 001;
     #3#2 = "       ";
     #3#3 = "zzzzzzz";
     |GRABA 020#3n;
 |FINSI;

 |CIERRA #3;
|FPRC;
|| ************************************************************************

|RUTINA min1;
 #0#0 = "            ";
|FRUT;

|RUTINA max1;
 #0#0 = "zzzzzzzzzzzz";
|FRUT;

|PROGRAMA;
 |CLS;
 |CABEZA "Relacion Cuentas";

 |HAZ inicio;
 |HAZ Analitica;
 |SI  enSwAna = 0;
       aAlfa1 = "    CONTABILIDAD NO ANALITICA ";
       |SI eaAnNiv = "E";
           aAlfa1 = "    EMPRESA NO ANALITICA ";
       |FINSI;
       |MENAV aAlfa1;
       |ACABA;
 |FINSI;

 |PINPA #0#0;
 |ENTLINEAL #1#0, -1, 1, 22, 0, min1, max1;
|FPRO;
