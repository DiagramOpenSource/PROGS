|PROCESO nombre;  || nombre del fichero
    Pos   = -1;
    aFichero = aFich;
    aFich    = aDire + aFich;

    nCopio = 0;
    |XABRE "UB", aFich, handle;
    |SI FSalida < 0;
        |XABRE "CUB", aFich, handle;
        |SI FSalida = 0; nCopio = 1; |FINSI;
    |FINSI;
    |XCIERRA handle;

