|FICHEROS;
   camanefe #347;
|FINSI;

|VARIABLES;
   &cdiari = 0;
   &cdocum = 0;
   &cfecha = @;
   &clinea = 0;
   &cRegistro = 0;
   estado = 0;

   alfa1 = "";
   alfa2 = "";
|FIN;

|INCLUYE dscont_i;

|PROCESO ElEjer; |TIPO 0;
  |SI #347#4 > enEjercicio;
      #347#4 = enEjercicio;
      |ERROR;
  |FINSI;
|FPRC;

|PROCESO Unosolo; |TIPO 0;
  |SI Campo = 7;
      |SI #347#7 = "S"; #347#8 = "N"; |PINTA #347#8; |FINSI;
  |FINSI;
  |SI Campo = 8;
      |SI #347#8 = "S"; #347#7 = "N"; |PINTA #347#7; |FINSI;

      |SI #347#8 = "S"; |Y eaCodSubdep = "   ";
          |CONFI "    NNo informado el Subdepartamento en esta Op. de Arrendamiento. Continuar";
          |SI FSalida ! 0;
               #347#8 = "N"; |PINTA #347#8;
               |ERROR;
          |FINSI;
      |FINSI;
  |FINSI;

|FPRC;


|PROGRAMA;

   |PINTA 1155, "                        ";
   |PINTA 1255, "                        ";
   |PINTA 1355, "                        ";
   |PINTA 1455, "  Operacion Seguro:     ";
   |PINTA 1555, "  Arrendamiento L.:     ";
   |PINTA 1655, "                        ";
   |D_CUADRO 1155,1678;
   |ATRI R; |PINTA 1256, " Ejercicio Operacion "; |ATRI 0;

   |ABRE #347;
   #347#0 = cdiari;
   #347#1 = cfecha;
   #347#2 = cdocum;
   #347#3 = clinea;
   |LEE 101#347=;
   estado = FSalida;
   |SI estado ! 0;
      #347#0 = cdiari;
      #347#1 = cfecha;
      #347#2 = cdocum;
      #347#3 = clinea;
      #347#4 = enEjercicio;
      #347#5 = 99;
      #347#6 = cRegistro;
   |FINSI;

   |PINDA #0#347;

   |ENDATOS #2#347;
   |SI FSalida = 0;
      |SI estado = 0; |GRABA 020#347a; |FINSI;
      |SI estado ! 0; |GRABA 020#347n; |FINSI;
   |FINSI;

   |LIBERA #347;
   |CIERRA #347;
|FPRO;
