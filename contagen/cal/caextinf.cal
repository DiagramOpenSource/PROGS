|PROCESO SelTipo;
   |DDEFECTO #2;
   #2#0 = #11#1;
   |LEE 001#2=;
   |SI FSalida ! 0;
       nSw = 1;
   |SINO;
       |SI #0#16 ! "T"; |Y #2#3 ! #0#16;
           nSw = 1;
       |FINSI;
   |FINSI;
|FPRC;

|PROCESO imprime;
   nSw = 0;
   |HAZ SelTipo;
   |SI nSw = 1; |ACABA; |FINSI;

   |SI #11#2 = 2;
       |SI #11#6 < #0#9;  nSw = 1; |FINSI;
       |SI #11#6 > #0#10; nSw = 1; |FINSI;
   |FINSI;
   |SI #11#2 = 3;
       |SI #11#3 < #0#0; |O #11#3 > #0#1;  nSw = 1; |FINSI;
       |SI #11#4 < #0#2; |O #11#4 > #0#3;  nSw = 1; |FINSI;
       |SI #11#5 < #0#4; |O #11#5 > #0#5;  nSw = 1; |FINSI;
   |FINSI;
   |SI nSw = 1; |ACABA; |FINSI;

   |SI #11#2 ! 1;
       |SI nSwP = 0;
           aCuenta = #11#6;
           nDiario = #11#3;
           fFecha  = #11#4;
           nDocum  = #11#5;
       |FINSI;
       nSwP = 1;
   |FINSI;
   |SI #11#2 = 2;
       |SI aCuenta ! #11#6; nSwSalto = 1; |FINSI;
   |FINSI;
   |SI #11#2 = 3;
       |SI nDiario ! #11#3; |O fFecha ! #11#4; |O nDocum ! #11#5;
           nSwSalto = 1;
        |FINSI;
   |FINSI;

   swprim = 0;
   |BUCLE LasLineas;
   seleccio = 1;
   aCuenta  = #11#6;
   nDiario  = #11#3;
   fFecha   = #11#4;
   nDocum   = #11#5;
