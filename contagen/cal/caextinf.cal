|PROCESO HazTodo;
|| ... Observaciones DsArchi
       aAlfa1 = #0#2; aAlfa1 = aAlfa1 % 0402; enM1 = aAlfa1;
       aAlfa1 = #0#3; aAlfa1 = aAlfa1 % 0402; enM2 = aAlfa1;
       eaObservaciones = "";
       |SI #0#0 = #0#1;
           |SI #0#0 = 00; eaObservaciones = "DE APERTURA"; |FINSI;
           |SI #0#0 = 99; eaObservaciones = "DE CIERRE";   |FINSI;
       |FINSI;
       |SI eaObservaciones = "";
           SwApertura = 0;
           SwCierre   = 0;
           NumMes = enM1; |HAZ NombreMes; NombreMes = " " + NombreMes; |QBF NombreMes;
           |SI #0#2 = fec1; |Y #0#0 = 00; NombreMes = " LA APERTURA"; SwApertura = 1; |FINSI;
           eaObservaciones = "DE" + NombreMes + " ";

           NumMes = enM2; |HAZ NombreMes; NombreMes = " " + NombreMes;
           |SI #0#3 = fec2; |Y #0#1 = 99; NombreMes = "L CIERRE"; SwCierre = 1; |FINSI;
           |SI enM1 ! enM2; |O SwApertura = 1; |O SwCierre = 1;
               eaObservaciones = eaObservaciones + "A" + NombreMes;
           |FINSI;
       |FINSI;
       eaObservaciones = eaObservaciones > " ";

   |SI nOpcion = 2;
       eaInforme = informe;
       |HAZ ElIMPREArchi;
       |SI swErrorDSArchi = 1;
           nSalir = 1;
           |ACABA;
        |FINSI;
   |FINSI;

   seleccio = 0;

   |ABRE #2;

   |SI #0#15 = "T";|O #0#15 = "G";
       nDTipo = 1; nHTipo = 1;
       |BUCLE lectura1;
   |FINSI;

   nSwSalto = 0;
   nSwP     = 0;
   |SI #0#15 = "T";|O #0#15 = "C";
       nDTipo = 2; nHTipo = 2;
       |BUCLE lectura2;
    |FINSI;

   nSwSalto = 0;
   nSwP     = 0;
   |SI #0#15 = "T";|O #0#15 = "A";
       nDTipo = 3; nHTipo = 3;
       |BUCLE lectura3;
   |FINSI;

   |CIERRA #2;

   |SI seleccio = 0;
       |SI #48#27 = "N"; |Y enSwMvo ! 2; |Y nOpcion = 1;
           |MENAV "    ATENCION!!! Seleccion Vacia ...";
       |FINSI;
   |SINO;
       |PIEINF;
   |FINSI;

   |SI nOpcion = 2;
       |FININF;
       |FINIMP;
       |SI swErrorDSArchi = 0; |Y seleccio ! 0;
           |HAZ Archivando;
       |FINSI;
       Impresora = "CrystalReport";
   |FINSI;

|FPRC;

|PROCESO InformePDF;
   nSwError = 0;

   aIP = ""; |IP_REMOTO aIP;
   aContiene = "";
   |ESPECIFICA "RBASS", aContiene;
   aAlfa = aContiene1;
   aAlfa = aAlfa - "\";
   |SI FSalida ! 0;
      nCalc = ((FSalida / 100) ! 0) + 99;
      aAlfa = aAlfa % nCalc;
   |SINO;
      aAlfa = aAlfa - "/";
      |SI FSalida ! 0;
         nCalc = ((FSalida / 100) ! 0) + 99;
         aAlfa = aAlfa % nCalc;
      |FINSI;
   |FINSI;

   aAlfa = eaPathTemporal;
   aNombrePDF = (("00" + enOrMvo) % - 102) + aNombrePDF;

   |SI aIP ! "";
       |RMKDIR aAlfa;
   |SINO;
       |MKDIR aAlfa;
   |FINSI;

   Impresora = "CrystalReport;" + aAlfa + aNombrePDF;
   |IMPRE -1;
   |SI FSalida = 0;
       |INFOR informe;
       |SI FSalida ! 0;
          || |SI nSwMen = 0; |MENAV "     Error en informe."; |FINSI;
          nSwError = 1;
       |FINSI;
   |FINSI;
   enHayInforme = 1;
|FPRC;

|PROCESO impresion; |TIPO 3;

    |SI nArchi = 1;
        |MENU aMenu;
        nOpcion = FSalida;
        |SI nOpcion = 0;
            |ACABA;
        |FINSI;
    |SINO;
         nOpcion = 1;
    |FINSI;
    nOpSegur = nOpcion

    |SI nOpSegur = 3;  |SALVA_DATOS #caextinf;  |FINSI;

    |SI nOpSegur = 1; |O nOpSegur = 3;
        nOpcion = 1;
        |HAZ impre_Uno;
        |SI nOpSegur = 3;  |REPON_DATOS #caextinf;  |FINSI;
        |SI nSalir = 1; |ACABA; |FINSI;
    |FINSI;

    |SI nOpSegur = 2; |O nOpSegur = 3;
        nOpcion = 2;
        |HAZ impre_Uno;
    |FINSI;
|FPRC;

|PROCESO impre_Uno;

   nDTipo = 1; nHTipo = 3;
   |SI #0#15 = "G"; nDTipo = 1; nHTipo = 1; |FINSI;
   |SI #0#15 = "C"; nDTipo = 2; nHTipo = 2; |FINSI;
   |SI #0#15 = "A"; nDTipo = 3; nHTipo = 3; |FINSI;

   aDEstado = " "; aHEstado = "z";
   |SI #0#17 ! "T";
       aDEstado = #0#17;
       aHEstado = #0#17;
   |FINSI;

   nSalir = 0;
   |SI nOpcion = 2;
        Impresora = "CrystalReport";
   |FINSI;
   |SI nOpcion = 1;
       |SI enSwMvo ! 2;
           Impresora = "CrystalReports";
           |IMPRE -1;
           |SI FSalida ! 0;
               |MENAV "0000Error en Impresora. ";
               nSalir = 1;
               |ACABA;
           |FINSI;

           |INFOR informe;
           |SI FSalida ! 0;
               |MENAV "     Error en informe.";
               |FINIMP;
               nSalir = 1;
               |ACABA;
           |FINSI;
       |SINO;
            aNombrePDF = "RevCon.pdf";
            |HAZ InformePDF;
            |SI nSwError = 1; nSalir = 1; |ACABA; |FINSI;
       |FINSI;
   |FINSI;

   |SI #48#27 = "S"; |Y enSwMvo ! 2;
       |HAZ MultiEmpresa;
   |SINO;
       |DFICE dirfich0; |QBF dirfich0;
       |HAZ HazTodo;
   |FINSI;

   |SI nOpcion = 1;
       |FININF;
       |FINIMP;
   |FINSI;
|FPRC;
