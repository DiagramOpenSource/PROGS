|PROCESO lee_apuntes1;
   #dsmom030#0 = #camaasil#6;
   |LEE 000#2=;
   |SI FSalida = 0;
      alfa1 = #camaasil#7;    |QBF alfa1;    || descripcion apunte
      alfa2 = #dsmom030#1;    |QBF alfa2;    || descripcion concepto
      alfa3 = alfa1 - alfa2;
      |SI alfa3 = alfa1;
         #camaasil#7 = alfa2 + alfa1;       || si no la contiene la a¤ade
         |GRABA 020#1a;
         |SI FSalida = 0; nroapu = nroapu + 1;     |FINSI;
      |FINSI;
   |SINO;
      alfa1 = "    Concepto: [" + #dsmom030#0 + "] inexistente.";
      |MENAV alfa1;
   |FINSI;
   |LIBERA #1;
|FPRC;

|DEFBUCLE lee_apuntes;
   #1#1;
   6;
   #0#1, #0#3, #0#5,    1, #0#7;
   #0#2, #0#4, #0#6, 9999, #0#8;
   be;
   NULL, lee_apuntes1;
|FIN;

|PROCESO tipo3; |TIPO 3;
   |SI #cainscto#0 = "SI";
      |ABRE #2;
      |BUCLE lee_apuntes;
      |CIERRA #2;
      alfa1 = "    Apuntes revisados: [" + nroapu + "]";
      |MENAV alfa1;
   |FINSI;
|FPRC;
