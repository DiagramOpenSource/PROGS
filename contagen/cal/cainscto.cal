|PROCESO mayus; |TIPO 0;
   #cainscto#Campo# = #cainscto#Campo# > #cainscto#Campo#;
   |PINTA #cainscto#Campo#;
|FPRC;

|PROCESO fechas; |TIPO 7;
   #cainscto#3 = fec1;
   #cainscto#4 = fec2;
   |PINTA #cainscto#3;
   |PINTA #cainscto#4;
|FPRC;
