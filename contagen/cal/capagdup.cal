|FICHEROS;
   caconimp #8;
|FIN;

|VARIABLES;
   &nPagDup = 0;
   &nSwPagDup = 0;
|FIN;

|INCLUYE dscont_i;

|PROCESO &PDuplex;
   |ABRE #8;
   |DDEFECTO #8;
   #8#0 = 1;
   |LEE 000#8=;
   |CIERRA #8;

   |SI #caconimp#1 = "L";|Y #caconimp#5 = "S";
      |SI nSwPagDup = 0;
         nPagDup = nPagDup + 1;
         nSwPagDup = 1;
      |SINO;
         nSwPagDup = 0;
      |FINSI;
   |SINO;
      nPagDup = nPagDup + 1;
      nSwPagDup = 1;
   |FINSI;
|FPRC;
