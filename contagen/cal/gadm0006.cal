|FICHEROS;
     gadm0006;
     caforpag;
|FIN;

|VARIABLES;
     nRango              = 0;
     nGrid               = 0;
     nVent               = 0;

     aEsc1               = "";
     aEsc2               = "";
     aRetu               = "";
     aTecla              = "";
     aAlfa               = "";

 {-1}aVent               = "";
         aVent1          = 0;
         aVent2          = 0;
         aVent3          = 0;
         aVent4          = 0;
         aVent5          = "";
|FIN;

|PROCESO T66C2;  |TIPO 66;
     #caforpag#0 = #gadm0006#2;
     |LEE 000#caforpag.=;

     |CONSULTA_CLAVE #1#caforpag;
     |SI FSalida = 0;
         #gadm0006#2 = #caforpag#0;
         #gadm0006#3 = #caforpag#1;

         |PINTA #gadm0006#2;
         |PINTA #gadm0006#3;
     |FINSI;
|FPRC;

|PROCESO T7C2;  |TIPO 7;
     |SI #gadm0006#2 = 0;
         #gadm0006#2 = 99;
     |FINSI;
|FPRC;

|PROCESO T0C2;  |TIPO 0;
     #caforpag#0 = #gadm0006#2;
     |LEE 000#caforpag.=;
     |SI FSalida ! 0;
         |MENAV "0000La forma de pago en la contabilidad no existe.";
         |ERROR;
         |ACABA;
     |FINSI;

     #gadm0006#3 = #caforpag#1;
     |PINTA #gadm0006#3;
|FPRC;

|PROGRAMA;
     |VENTANA 0501, 2399, -1, 16, "Parametrizacion Libros / Series";
     nVent = FSalida;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVent;
     aVent5 = "MODAL";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     nRango = 1 + 4 + 8 + 16 + 32;

     |LINEAL_SIMPLE #1#gadm0006, nRango, 0502, 2198, NULL, NULL, NULL;
     nGrid = FSalida;

     aEsc1  = &255 + "806";
     aEsc2  = &255 + "807";
     aRetu  = &255 + "802";
     |PARA; |SI;  |HACIENDO;
         FSalida = "::{{001}}Salir," + nGrid;
         |LEETECLA aTecla;
         |SI aTecla = aEsc1; |SAL;  |FINSI;
         |SI aTecla = aEsc2; |SAL;  |FINSI;
         |SI aTecla = aRetu; |SAL;  |FINSI;
     |SIGUE;

     aVent1 = -1;
     aVent2 = -1;
     aVent3 = 0;
     aVent4 = nVent;
     aVent5 = "MODELESS";
     |ESPECIFICA "ESTADO_VENTANA", aVent;

     |FINVENTANA nVent;
|FPRO;

|PROCESO T80;  |TIPO 80;
     |ABRET #gadm0006;
     |ABRET #caforpag;
|FPRC;

|PROCESO T90;  |TIPO 90;
     |CIERRAT #gadm0006;
     |CIERRAT #caforpag;
|FPRC;
