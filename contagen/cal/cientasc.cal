   #cientcon#1;
   ;
   #1#0, #1#2, 0;
   #1#0, #1#2, 99999;
   be;
   NULL,hazlasuma;
|FIN;

|PROCESO contrapartidas;
   |PUSHV 0701 2380;
   swalgun = 0;
   total = #1#7;
   sumal = 0;
   |BUCLE hazlasuma;
   difer = total - sumal;
   |SI swalgun = 0;
      |ABRE #cientcon;
      #cientcon#0 = #1#0;
      #cientcon#1 = #1#1;
      #cientcon#2 = #1#2;
      #cientcon#3 = 1;
      #cientcon#4 = #cisalant#5;
      #cientcon#5 = #cisalant#6;
      #cientcon#6 = #1#10;
      #cientcon#7 = #1#11;
      #cientcon#8 = #1#7;

      |SI #1#6 = "D";  signo = "H";  |SINO;  signo = "D";  |FINSI;
      MasMenos     = #cientcon#8;
      DebeHaber    = signo;
      aCUENTA      = #cientcon#4;
      nNIVELCUENTA = #cientcon#5;
      nREGISTRO    = -1;
      nDIARIO      = 01;     ||No tiene diario asignado
      aFECHA       = #1#3;
      nDOCUMENTO   = -1;
      |HAZ ActCuentM;          || cuenta de pase Directo S

      |GRABA 101#cientcon.n;
      |CIERRA #cientcon;

      sumal = #1#7;
      difer = 0;
   |FINSI;

   aBlancs = " " * 78;
   |PINTA 2102, aBlancs;
   |PINTA 2202, aBlancs;
   |PINTA 2302, aBlancs;
   |ENTLINEAL #1#cientcon, -3, 2, 20, 1, menorl, mayorl;
   |POPV;
|FPRC;

