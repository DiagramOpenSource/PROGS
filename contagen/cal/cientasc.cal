|PROCESO hazlasuma;
   sumal = sumal + #cientcon#8;
   swalgun = 1;

   |SI #cisalant#12 = "N"; #cientcon#6 = #cientasl#4; |FINSI;
   |SI #cisalant#13 = "N"; |Y #cientcon#6 = #cientasl#4;
       #cientcon#7 = #cientasl#5;
   |FINSI;
   |SI #cisalant#12 = "N"; |O #cisalant#13 = "N";
       |GRABA 020#cientcon.a;
   |FINSI;
   |LIBERA #cientcon;
|FPRC;

