|PROCESO eltipo;
 alfa1 = "S"; alfa2 = "N";
 |SI #0#1 = "E"; alfa1 = "N"; alfa2 = "S"; |FINSI;
 |C_M #0#9, 1,alfa1; |C_M #0#11,1,alfa1;
 |C_M #0#10,1,alfa2; |C_M #0#12,1,alfa2;
