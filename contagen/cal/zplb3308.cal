|FICHEROS;
   capcem3@ #3;
|FIN;

|VARIABLES;
   aMas       = "";
   &enErChile = 0;
   &eaC1Chile = "";
   &eaC2Chile = "";
   aCuenta    = "";
|FIN;

|INCLUYE  dscont_i;

|PROCESO eParamChile;

  enErChile = 0;
  eaC1Chile = eaCuenta;
  eaC2Chile = "";

  |DBASE aBase; |QBF aBase;
  aMas = aBase + "def/capcem03";
  |CARGA_DEF aMas, capcem3@;
  |SI FSalida ! 0;
      |ACABA;
  |FINSI;

  aCuenta = eaCuenta % 104;

  |ABRE #3;
  |DDEFECTO #3;
  #3#0 = aCuenta;
  |LEE 041#3=;
  |SI FSalida ! 0;
      enErChile = 1;
  |SINO;
      eaC1Chile = #3#2;
      eaC2Chile = #3#3;
  |FINSI;
  |CIERRA #3;

  |DESCARGA_DEF #capcem3@;
|FPRC;
