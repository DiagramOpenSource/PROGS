|VARIABLES;
 &eaOrigen  = "";
 &eaDestino = "";
 &enErrPas  = 0;
  aIPRemoto = "";
|FIN;

|PROCESO eReciboFich;
  enErrPas = 0;
  aIPRemoto = "";
  |IP_REMOTO aIPRemoto;  |QBT aIPRemoto;
  |SI aIPRemoto ! "";
      |RECIBE_FICHERO eaOrigen, eaDestino;
  |SINO;
      |COPIA_FICHERO eaOrigen, eaDestino;
  |FINSI;
  |SI FSalida ! 0;  enErrPas  = 1; |FINSI;
|FPRC;

|PROCESO eEnvioFich;
  enErrPas = 0;
  aIPRemoto = "";
  |IP_REMOTO aIPRemoto;  |QBT aIPRemoto;
  |SI aIPRemoto ! "";
      |ENVIA_FICHERO eaOrigen, eaDestino;
  |SINO;
      |COPIA_FICHERO eaOrigen, eaDestino;
  |FINSI;
  |SI FSalida ! 0;  enErrPas  = 1; |FINSI;
|FPRC;
