|FICHEROS;
    imagen@  #59;
|FIN;

|VARIABLES;
    a_Alfa   = "";
    a_Mas    = "";
    a_Fich   = "";
    x_entra = 0;
    x_lugar = 0;
    x_cuan1 = 0;
    x_cuan2 = 0;
    x_cuan3 = 0;
    x_cuan4 = 0;
    x_cuan5 = 0;
    x_alfa1 = "";
    x_alfa2 = "";
    x_alfa3 = "";
    x_alfa4 = "";
    x_alfa5 = "";
    x_alfa6 = "";

    x_nume1 = 0;
    x_nume2 = 0;
    x_ceros = "";

    x_cuen111 = "";
    x_cuen211 = "";
    x_cuen311 = "";
|FIN;

|PROCESO x_punto;
  x_lugar = 0;
  x_cuan1 = 0;
  x_cuan2 = 0;
  x_cuan3 = 0;
  x_cuan4 = 0;
  x_cuan5 = 8;
  x_alfa2 = "";
  x_alfa3 = "";
  x_alfa4 = "";
  x_alfa5 = "";
  x_alfa6 = "";
  |PARA x_nume1 = 1; |SI x_nume1 [ x_cuan5;  |HACIENDO x_nume1 = x_nume1 + 1;
        x_nume2 = (x_nume1 * 100) + 1;
        x_alfa2 = x_alfa1 % x_nume2;
        |SI x_alfa2 ! " ";
            |SI x_alfa2 = ".";
                x_lugar = x_nume1;
            |SINO;
                |SI x_lugar=0; x_alfa3=x_alfa3+x_alfa2; x_cuan3=x_cuan3+1; |FINSI;
                |SI x_lugar!0; x_alfa4=x_alfa4+x_alfa2; x_cuan4=x_cuan4+1; |FINSI;
            |FINSI;
        |FINSI;
  |SIGUE;

  |SI x_lugar ! 0;
      x_nume1 = x_cuan5 - (x_cuan3 + x_cuan4);
      x_ceros = "0" * x_nume1;
      x_alfa5 = x_alfa3 + x_ceros + x_alfa4;

      x_cuen111 = x_alfa4 % 101;
      x_cuen111 = x_cuen111 > " ";
      |SI x_cuen111 = "U";  |O x_cuen111 = "A";
          |HAZ ult_amortiza;
      |FINSI;
  |SINO;
      x_alfa5 = x_alfa1;
  |FINSI;
  x_alfa1 = x_alfa5;
|FPRC;

|PROCESO ult_amortiza;
  |SI Haymodelo = 0; |ACABA; |FINSI;

  a_Fich  = aPModelo + "fich/";
  a_Mas   = aPModelo + "def/dsmom041.mas";
  |CARGA_DEF a_Mas, imagen@;

  |ABRE #59;
  x_cuan2   =  x_cuan3 + 100; || longitud de la 1¦ parte en Alfa.
  x_cuen211 =  x_cuan2; || longitud de la 1¦ parte en Alfa.
  x_cuan1 = (x_cuan5 - x_cuan3) + 100;
  x_cuan1 = - x_cuan1;
  x_cuen311 = x_cuan1; || longitud de la 2¦ parte en Alfa.

  x_alfa2 = x_alfa3 + "99999999";
  x_alfa2 = x_alfa2 % 108;


  #59#0 = enEmpresa;
  #59#2 = x_alfa2;
  #59#3 = 0;

  |LEE 000#59];
  |SI FSalida ! 0;
      |LEE 011#59u;
  |SINO;
      |LEE 011#59a;
      |SI FSalida ! 0;
          |LEE 011#59p;
      |FINSI;
  |FINSI;

  x_alfa2 = #59#2 % x_cuen211;
  x_alfa6 = #59#2 % x_cuen311;

  |SI x_cuen111 = "U";
      x_alfa5 = #59#2;
  |SINO;
      |SI x_alfa2 ! x_alfa3;
          x_nume1 = (x_cuan5 - x_cuan3 - 1);
          x_ceros = ("0" * x_nume1) + "1";
          x_alfa5 = x_alfa3 + x_ceros;
      |SINO;
          x_nume2 = x_alfa6;
          x_nume2 = x_nume2 + 1;
          x_alfa6 = x_nume2;
          x_alfa6 = "00000000" + x_alfa6;
          x_alfa6 = x_alfa6 % x_cuen311;
          x_alfa5 = x_alfa2 + x_alfa6;
      |FINSI;
  |FINSI;
  |DDEFECTO #59;
  |CIERRA #59;

  |DESCARGA_DEF #imagen@;
|FPRC;
