         |SI nNume1 = 4; aAlfa1 = "Cuenta Recargo                      "; |FINSI;
         |SI nNume1 = 5; aAlfa1 = "Cuenta I.R.P.F.                     "; |FINSI;
         |SI nNume1 = 6; aAlfa1 = "Cuenta Descuentos                   "; |FINSI;
     |FINSI;
 |FINSI;

 |ATRI I;
 |PINTA 2221, aAlfa1;
