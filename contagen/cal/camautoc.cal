      |PINTA 1601, "  5= Total Factura             15= Cuota Recargo         ";
      |PINTA 1701, "  6= Total Cuotas IVA          16= Descuento             ";
      |PINTA 1801, "  7= Total Cuotas Recargo                                ";
      |ATRI 0;
  |FINSI;
|FINSI;

