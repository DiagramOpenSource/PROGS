|PROCESO ordenar;
  |NOPARA;

  aFichero = ("1S" + Usuario)  % 108;
  |NOME_DAT #11 aFichero;
  |DELINDEX #11;

  aFichero = ("1R" + Usuario)  % 108;
  |NOME_DAT #934 aFichero;
  |DELINDEX #934;

  |HAZ CalculaUlt;

  |ABRE #115;
  |SELECT #3#115;

  |ABRE #2;
  |ABRE #11;
  |CUADRO 2041 2276;
  |ATRI P;  |PINTA 2142, "      <PREPARANDO TEMPORALES>     ";  |ATRI 0;

  sw     = 0;
  |BUCLE gra_tempo;
  |CIERRA #11;
  |CIERRA #2;
  |SELECT #1#115;
  |CIERRA #115;

  |SI sw_error ! 0;
      aAlfa1 = ("00000" + enEmpresa ) % -105;
      aAlfa1 = "    NAtencion!. En la Empresa: " + aAlfa1 + " el Cambio provoca facturas Duplicadas.Ya existe Libro Destino+Serie+Fra";
      |MENAV aAlfa1;
      |SIPARA;
      |ACABA;
  |FINSI;

  |PINTA 2142, "                                  ";

  |SI sw = 0;
      |MENAV "    ATENCION!!!.  No existen Facturas para Cambiar de Libro. ";
  |SINO;
      |ABRE #10;
      |DDEFECTO #10;
      #10#0 = #0#4;
      |LEE 141#10=;
      |SI FSalida ! 0;
          #10#0 = #0#4;
          #10#1 = #0#5;
          |GRABA 020#10b;
      |FINSI;

      nOpera = 1;
      numero = 0;
      sw     = 0;
      nSwImp = 0;
      |ABRE #15;
      |BUCLE lee_tempo;
      |CIERRA #15;

      |SI nSwImp ! 0; |PIEINF; |FININF; |FINIMP; |FINSI;

      #10#4 = numero;
      |GRABA 020#10a;
      |LIBERA #10;
      |CIERRA #10;
  |FINSI;

  |DELINDEX #11;
  |DELINDEX #934;
  |SIPARA;
|FPRC;

