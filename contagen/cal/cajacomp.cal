|INCLUYE dscont_i;

|PROCESO nodepar; |TIPO 7;
   |CAMPO_MODIFICA #cajacomp#6, 1, eaDepar;
   |CAMPO_MODIFICA #cajacomp#7, 1, eaDepar;
|FPRC;

|PROCESO nosubde; |TIPO 7;
   |CAMPO_MODIFICA #cajacomp#8, 1, eaSubde;
   |CAMPO_MODIFICA #cajacomp#9, 1, eaSubde;
|FPRC;

|| ...... Calculo Campos
|PROCESO comienza; |TIPO 8;
   |HAZ inicio;
   |ABRE #90;
   |LEE 101#90p;
   |SI FSalida ! 0;
      |MENAV "    Error, no Existe fichero <Saldo Dia Anterior> ";
      |LIBERA #90;
      |CIERRA #90;
      |PTEC 807;
      |ACABA;
   |FINSI;
   cpartida = #cajasald#2;
   dpartida = #cajasald#3;
   afecha = #cajasald#7; |SI afecha < fec1; afecha = fec1; |FINSI;
   antd = 0; anth = 0;
   |SI #cajasald#1 > 0;
      anth = #cajasald#1;
   |SINO;
      antd = -#cajasald#1;
   |FINSI;
   saldo = #cajasald#1;
   t_saldo = saldo;
   |LIBERA #90;
   |CIERRA #90;
|FPRC;

|PROCESO defectos; |TIPO 7;
   |SI paso = 0;
      #cajacomp#1 = afecha; |PINTA #cajacomp#1;
      #cajacomp#2 = fec2;   |PINTA #cajacomp#2;
      paso = 1;
   |FINSI;
|FPRC;

|PROCESO fechas; |TIPO 0;
   |SI #cajacomp#Campo# < fec1; |O #cajacomp#Campo# > fec2;
      |MENAV "    ERROR ESTA FECHA NO CORRESPONDE AL EJEFCICIO ";
      |ERROR;
      |ACABA;
   |FINSI;
   || ..................................................
   |SI Campo = 2;
      |SI #cajacomp#2 < #cajacomp#1;
         |MENAV "    ERROR EN FECHA ";
         |ERROR;
         |ACABA;
      |FINSI;
   |FINSI;
|FPRC;

|PROCESO contar;
   |SI sw = 0;
      diaant = #cajainli#0;
      fecant = #cajainli#1;
      docant = #cajainli#2;
      sw = 1;
   |FINSI;
   |SI diaant ! #cajainli#0; |O fecant ! #cajainli#1; |O docant ! #cajainli#2;
      diaant = #cajainli#0;
      fecant = #cajainli#1;
      docant = #cajainli#2;
      s_debe = 0;
      s_haber = 0;
   |FINSI;

   |DDEFECTO #4;
   #cacuenta#0 = #cajainli#4;
   #cacuenta#1 = #cajainli#5;
   |LEE 000#4=;

   |SI #cajainli#8 = "P";
      a_haber = 0;
      a_debe = #cajainli#9;
   |SINO;
      a_haber = #cajainli#9;
      a_debe = 0;
   |FINSI;

   saldo = saldo + (a_haber - a_debe);

   eaDescrip = #cajainli#7;
   |SI #200#164 = "S"; |Y #cajainli#19 ] 2000;
       eaDescrip = eaDescrip + #cajainli#19;
   |FINSI;

   |PRINT;
   s_debe  = s_debe + a_debe;
   s_haber = s_haber + a_haber;
   t_haber = t_haber + a_haber;
   t_debe = t_debe + a_debe;
   t_saldo = saldo;
   swprim  = 1;
|FPRC;

|DEFBUCLE cacomdia0;
   #1#2;
   12,18;
   #0#3, #0#1,      0, 0000, #0#6,#0#8;
   #0#4, #0#2, 999999, 9999, #0#7,#0#9;
   ;
   NULL, contar;
|FIN;

|PROCESO impre;  |TIPO 3;

   |ABRE #200;
   |DDEFECTO #200;
   |LEE 001#200p;
   |CIERRA #200;

   sw = 0;

   |MENU form;
   op_form = FSalida;
   |SI op_form [ 0; |ACABA; |FINSI;

   |SI op_form = 1; informe = inform1; |FINSI;
   |SI op_form = 2; informe = inform2; |FINSI;

   |IMPRE 0;
   |SI FSalida ! 0; |ACABA; |FINSI;

   |INFOR informe;
   |SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;

   |ABRE #4;

   |BUCLE cacomdia0;

   || |SI sw ! 0; |PIEINF; |FINSI;

   |SI sw ! 0;
      nume1 = t_haber - t_debe + #cajasald#1;
      |SI nume1 > 0;
         hoyh = 0;
         hoyd = nume1;
      |SINO;
         hoyh = -nume1;
         hoyd = 0;
      |FINSI;
      |PIEINF;
   |FINSI;

   |FININF;
   |FINIMP;
   |CIERRA #4;
|FPRC;
