   &entrada = 1;
   fecalf = "";
   mensa1 = "    ERROR ESTA FECHA NO CORRESPONDE AL EJERCICIO";
   varalf = "";
   sw = 0;
   diaant = 0;
   docant = 0;
   fecant = @;
   informe = "cajacomp";
   inform1 = "cajacomp";
   inform2 = "cajacom2";

   &a_haber = 0;
   &a_debe = 0;
   &saldo = 0;
   &s_haber = 0;
   &s_debe = 0;

   &t_haber = 0;
   &t_debe = 0;
   &t_saldo = 0;

   &anth = 0;
   &antd = 0;
   &hoyh = 0;
   &hoyd = 0;
   &swprim = 0;
   &eaDescrip = "";
   nume1 = 0;
   afecha = @;
   paso = 0;
   cpartida = "";
   dpartida = 0;
   {-1}form  = "";           || Variables menu formato <1> o <2>
   form1 = "2400";
   form2 = "1";
   form3 = "Elija formato: ";
   form4 = "12";
   form5 = " < 1 > ";
   form6 = " < 2 > ";
