|PROGRAMA;
   aParam = PARAMETRO; |QBF aParam;
   |SI aParam = "";
       |HAZ inicio;
   |SINO;
       enEmpresa   = 1;
       enEjercicio = 2008;
   |FINSI;
   |SI enEjercicio < 2008;
       |DFICE aPathPlan;  ||estoy en Empresa del 2007 o inferior
   |SINO;

       |ABRE #30;
       |SELECT #4#30;
       #30#0  = enEmpresa;
       #30#26 = 07;
       |LEE 001#30=;
       nEstado = FSalida;
       |SELECT #1#30;
       |CIERRA #30;

       |SI nEstado = 0;
           nNume1 = #30#2;
           nNume2 = #30#3;
       |SINO;
           |HAZ BuscoPatron;
       |FINSI;

       aAlfa1 = nNume1; aAlfa1 = ("00000" + aAlfa1) % -105;
       aAlfa2 = nNume2; aAlfa2 = ("0"     + aAlfa2) % -101;
       |DFICB aPathPlan;
       aPathPlan = aPathPlan + aAlfa1 + aAlfa2 + "/";
       |CIERRA #30;
   |FINSI;

   |CLS;
   |CABEZA "Tabla Equivalencia Plan 1990 y Plan 2008";
   |PINPA #0#10;
   |PINDA #0#10;

   FSalida = 1;
   |PARA; |SI 0 < FSalida; |HACIENDO;
          |ENDATOS #1#10;
          |SI 0 [ FSalida;
              |SI nInkey = -1; |SAL; |FINSI;
              |HAZ Tipo3;
              FSalida = 1;
          |FINSI;
   |SIGUE;
|FPRO;

