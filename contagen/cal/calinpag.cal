|PROCESO comprueba;
    |SI emision ! #calinpag#6; |Y enAux = 0;
       aAlfaFec = "01." + (("00" + #48#11) % -102) + "."; |QBF aAlfaFec;
       nCalcFec = #48#26;
       |SI nCalcFec > 80;
          nCalcFec = nCalcFec + 1900;
       |SINO;
          nCalcFec = nCalcFec + 2000;
       |FINSI;
       aAlfaFec = aAlfaFec + nCalcFec;
       fDFecha = aAlfaFec;

       nCalcFec = #48#12; nCalcFec = nCalcFec + 1;
       |SI nCalcFec > 12;
          nCalcFec = nCalcFec - 12;
          aAlfaFec = nCalcFec;
          nCalcFec = #48#26 + 1;
       |SINO;
          nCalcFec = #48#26;
       |FINSI;
       |SI #48#26 > 80;
          nCalcFec = nCalcFec + 1900;
       |SINO;
          nCalcFec = nCalcFec + 2000;
       |FINSI;
       aAlfaFec = "01." + (("00" + aAlfaFec) % -102) + "." + nCalcFec;
       fHFecha = aAlfaFec; nCalcFec = fHFecha; nCalcFec = nCalcFec - 1;
       fHFecha = nCalcFec;

       |SI #calinpag#6 < fDFecha; |O #calinpag#6 > fHFecha; |BORRA 020#calinpag.a; |FINSI;
       |ACABA;
    |FINSI;

   sw_alguno = 1;
