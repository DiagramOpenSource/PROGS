                     #catmpvto#2 = orden;
                     #catmpvto#3 = 0;
                     |LEE 000#catmpvto.];
                     |SI FSalida = 0; |Y #catmpvto#0 = libro; |Y #catmpvto#1 = serie; |Y #catmpvto#2 = orden;
