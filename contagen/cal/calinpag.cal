|PROCESO ayudita; |TIPO 0;
   xx = (FEntrada / 100) ! 0;
   |SI xx = 1;
      #0#6 = emision;            || emision
      nIncre1 = #3#4;
      |HAZ CalIncre;  || cuenta un vencimiento mas
      |HAZ DiasFijos;
      #0#7 = fectra1;            || vto.
      #0#26 = nBan;
      |PINTA #0#6;
      |PINTA #0#7;
      |PINTA #0#26;
   |FINSI;
|FPRC;

|PROCESO manuales; |TIPO 2;
   xx = (FEntrada / 100) ! 0;
   |SI xx = 1;
      |HAZ carga_pro;
      nSwNuevos = 1;
   |FINSI;
    |SI xx = 2;
       nFlag = 0 +. 1;
       |SI nFlag < 0;
          fFv = #0#7;
          fFe = #0#6;
          nIm = #0#8;
          nTR = #0#11;
          nBan = #0#26;
       |SINO;
          |SI nSwNuevos = 0;
             |SI fFv ! #0#7; |O nIm ! #0#8; |O nTR ! #0#11; |O fFe ! #0#6; |O nBan ! #0#26;
                nSwNuevos = 1;
             |SINO;
                nSwNuevos = 0;
             |FINSI;
          |FINSI;
       |FINSI;
    |FINSI;
    |SI xx = 3;
       nNume = enModoEsc; enModoEsc = 3; |HAZ VuelcaRecs; enModoEsc = nNume;
       |HAZ CierraPuente; |HAZ CreaPuente;
    |FINSI;
|FPRC;

