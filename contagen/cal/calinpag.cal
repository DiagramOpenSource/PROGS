|PROCESO carga;
   nSwModo = 0;

   |ABRE #0;
   recibo = 0;
   |HAZ importe;
   |SI #3#2 ! 0;            || numero de vencimientos
      fectra1 = emision;

      nIncre1   = #3#3;
      |HAZ CalIncre;
      recibo = recibo + 1;
      |HAZ graba;

      recibo = recibo + 1;
      |PARA nPara1 = recibo; |SI nPara1 [ #3#2; |HACIENDO nPara1 = nPara1 + 1;
            nIncre1 = #3#4;
            |HAZ CalIncre;
            recibo = nPara1;
            |HAZ graba;                     || incremento vencimientos
      |SIGUE;
   |SINO;
      |SI #3#0 = 0;
          fectra1 = emision; recibo = 1; #3#2 = 1;
          |HAZ graba;
      |FINSI;
   |FINSI;
   |CIERRA #0;
|FPRC;

|PROCESO importe;
   |SI #3#2 = 1;
      ptasrec1 = 0;
      ptasrec2 = pesetas;
   |SINO;
      ptasrec1 = (pesetas / #3#2) ! EURODB;   || todos vto.menos ultimo
      nume2 = ptasrec1 * (#3#2 - 1);
      ptasrec2 = pesetas - nume2;               || ultimo vto.
   |FINSI;
|FPRC;

|PROCESO graba;
   #0#0 = libro;
   #0#1 = orden;
   #0#2 = serie;
   #0#3 = recibo;
   #0#4 = cuenta;
   #0#5 = digito;
