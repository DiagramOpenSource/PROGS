   |HAZ DiasFijos;
   #0#7 = fectra1; || vto.
   |SI recibo ! #3#2;
      #0#8 = ptasrec1;  || anteriores al ultimo
   |SINO;
      #0#8 = ptasrec2;  || utlimo recibo
   |FINSI;
