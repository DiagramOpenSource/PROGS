         #0#0 = #20#11;
         #0#1 = #20#13;
         |LEE 040#0=;
         |SI FSalida = 0;  |Y #0#5 = nLimite3;
             #20#16 = #0#5;
             |GRABA 020#20a;
             |LIBERA #20;
             enSwRetorno  = 1;
         |FINSI;
         |ACABA;
     |FINSI;

     |SI nEstoy = 2;
