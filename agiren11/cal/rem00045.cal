     |LEE 040#1=;
     |SI FSalida ! 0;  |DDEFECTO #1;  |FINSI;

     #0#0 = #20#11;
     #0#1 = #20#13;
     |LEE 040#0=;
     |SI FSalida ! 0;  |DDEFECTO #0;  |FINSI;

     |SI #1#2 = "N";  |ACABA;  |FINSI;

     |SI #1#4 = #20#15;
         |HAZ PonLimite;
         |ACABA;
     |FINSI;

     |SI #1#5 = #20#15;
         |HAZ PonLimite;
         |ACABA;
     |FINSI;
|FPRC;

