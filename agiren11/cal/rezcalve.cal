     |SI nAnyo = 2001;  nInteres = 6.5;  |FINSI;
     |SI nAnyo = 2002;  nInteres = 5.5;  |FINSI;
     |SI nAnyo = 2003;  nInteres = 5.5;  |FINSI;
     |SI nAnyo = 2004;  nInteres = 4.75; |FINSI;
     |SI nAnyo = 2005;  nInteres = 5;    |FINSI;
     |SI nAnyo = 2006;  nInteres = 5;    |FINSI;
     |SI nAnyo = 2007;  nInteres = 6.25; |FINSI;
     |SI nAnyo = 2008;  nInteres = 7;    |FINSI;
     |SI nAnyo = 2009;  nInteres = 5;    |FINSI;
     |SI nAnyo = 2010;  nInteres = 5;    |FINSI;
     |SI nAnyo = 2011;  nInteres = 5;    |FINSI;
     |SI nAnyo = 2012;  nInteres = 5;    |FINSI;
|FPRC;

|PROCESO Tipo0CIndebidas;
     aAnyoI = #647#18 % -104;
     aAnyoF = #647#19 % -104;
     nAnyo  = aAnyoI;  |HAZ BuscaInteres;

                         nDiasAnyo = 365;
     |SI nAnyo = 1996;   nDiasAnyo = 366;  |FINSI;
     |SI nAnyo = 2000;   nDiasAnyo = 366;  |FINSI;
     |SI nAnyo = 2004;   nDiasAnyo = 366;  |FINSI;
     |SI nAnyo = 2008;   nDiasAnyo = 366;  |FINSI;

     aAlfa  = "31.12." + aAnyoI;
     fFecha = aAlfa;
     nDias  = (fFecha - #647#18);

     |SI nAnyo ! 2009;
         nImporte1 = ((#647Campo % nInteres) * (nDias / nDiasAnyo)) ! 2;
     |SINO;
         |SI #647#18 ] "31.03.2009";
             nImporte1 = ((#647Campo % nInteres) * (nDias / nDiasAnyo)) ! 2;
         |SINO;
             fFecha1   = "31.03.2009";
             fFecha2   = #647#18;
             nDias     = fFecha1 - fFecha2;
             nImporte1 = ((#647Campo % 7) * (nDias / nDiasAnyo)) ! 2;

             fFecha1 = "31.12.2009";
             fFecha2 = "01.04.2009";
             nDias   = fFecha1 - fFecha2;
             |SI nDias > 0;
                 nImporte1 = ((#647Campo % 5) * (nDias / nDiasAnyo)) ! 2;
             |FINSI;
         |FINSI;
     |FINSI;

     nImporte2  = 0;
     nAnyoDesde = nAnyo + 1;
     nAnyoHasta = aAnyoF;
     nAnyoHasta = nAnyoHasta - 1;
     |PARA nAnyo = nAnyoDesde;  |SI nAnyo [  nAnyoHasta;  |HACIENDO nAnyo = nAnyo + 1;
           |HAZ BuscaInteres;
           |SI nAnyo ! 2009;
               nImporte2 = nImporte2 + ((#647Campo % nInteres) ! 2);
           |SINO;
               nImporte2 = nImporte2 + ((((#647Campo % 7) * (89  / 365)) + ((#647Campo % 5) * (274 / 365))) ! 2);
           |FINSI;
     |SIGUE;

     aAlfa  = "01.01." + aAnyoF;
     fFecha = aAlfa;
     nDias  = (#647#19 - fFecha) + 1;
     nAnyo  = aAnyoF;  |HAZ BuscaInteres;

                         nDiasAnyo = 365;
     |SI nAnyo = 1996;   nDiasAnyo = 366;  |FINSI;
     |SI nAnyo = 2000;   nDiasAnyo = 366;  |FINSI;
     |SI nAnyo = 2004;   nDiasAnyo = 366;  |FINSI;
     |SI nAnyo = 2008;   nDiasAnyo = 366;  |FINSI;

     nImporte3 = ((#647Campo % nInteres) * (nDias / nDiasAnyo)) ! 2;

     nCampo     =  Campo + 2;
     #647nCampo = nImporte1 + nImporte2 + nImporte3;
|FPRC;

