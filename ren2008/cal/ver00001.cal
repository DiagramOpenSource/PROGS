|| ************************************************************************
|| PROCESOS DE CAMBIO DE VERSION
|| ************************************************************************

|PROCESO MiraPatrones;
     nSwHay = 0
     eaPrograma  = "";
     eaExtension = "";

     |ABRE #8;
     #8#0 = 9;
     #8#1 = 870;
     |LEE 040#8=;
     |SI FSalida = 0;
         eaExtension = "";
         eaPrograma  = "";

         |CIERRA #8;   |DELINDEX #8;
         |ABRE #500;   |CIERRA #500;   |DELINDEX #500;
         nSwHay = 1;
     |SINO;
         |CIERRA #8;
     |FINSI;

     |ABRE #8;
     #8#0 = 17;
     #8#1 = 911;
     |LEE 040#8=;
     |SI FSalida ! 0;  |O #8#6 = 0;
         eaExtension = "";
         eaPrograma  = "";

         |CIERRA #8;     |DELINDEX #8;
         nSwHay = 1;
     |SINO;
         |CIERRA #8;
     |FINSI;

     |ABRE #401;
     |LEE 040#401p;
     |SI FSalida ! 0;  |O #401#71 ! 11200;  |O #401#1 ! 22100;
         |CIERRA #401;  |DELINDEX #401;
         |ABRE #8;  |CIERRA #8;   |DELINDEX #8;
         eaPrograma  = "";
         eaExtension = "";
         nSwHay     = 1;
     |FINSI;
     |CIERRA #401;

     |ABRE #402;
     #402#0 = 8;
     |LEE 040#402=;
     |SI FSalida ! 0;
         eaExtension = "";
         eaPrograma  = "";

         |CIERRA #402;   |DELINDEX #402;
         nSwHay = 1;
     |SINO;
         |CIERRA #402;
     |FINSI;

     |SI nSwHay = 1;
         |SI eaPrograma = "";
             |MENAV "0000 Han cambiado o no se han instalado los ficheros patrones, por favor instalelos.";
         |FINSI;
         eaRenta = "r08";
         |SUB_EJECUTA_NP ":basica/dspatron";
     |FINSI;

     |ABRE #401;
     |LEE 040#401p;
     |SI FSalida ! 0;  |O #401#71 ! 11200;  |O #401#1 ! 22100;
         enError = 1;
         |CIERRA #401;  |DELINDEX #401;
     |SINO;
         |CIERRA #401;
     |FINSI;

     |ABRE #8;
     #8#0 = 9;
     #8#1 = 870;
     |LEE 040#8=;
     |SI FSalida = 0;
         |CIERRA #8;   |DELINDEX #8;
         enError = 1;
     |SINO;
         |CIERRA #8;
     |FINSI;

     |ABRE #8;
     #8#0 = 17;
     #8#1 = 911;
     |LEE 040#8=;
     |SI FSalida ! 0;  |O #8#6 = 0;
         |CIERRA #8;   |DELINDEX #8;

         enError = 1;
     |SINO;
         |CIERRA #8;
     |FINSI;

     |ABRE #402;
     #402#0 = 8;
     |LEE 040#402=;
     |SI FSalida ! 0;
         |CIERRA #402;   |DELINDEX #402;
         enError = 1;
     |SINO;
         |CIERRA #402;
     |FINSI;
|FPRC;

|PROGRAMA;
     |MENSA "0000Comprobando configuracion patrones.";
     enError = 0;
     |HAZ MiraPatrones;
     |BLIN 05;
|FPRO;
