|FICHEROS;
     rem00999 #1000;
|FIN;

|VARIABLES;
     nCampo = 0;
     nHandle = 0;
     nNume  = 0;
     aAlfa  = "";
     aAlfa1 = "";
     aAlfa2 = "";

     {50}paAlfa = "";
|FIN;

|RUTINA ClaveBaja1000;
     #1000#0 = 0;
     #1000#1 = "   ";
|FRUT;

|RUTINA ClaveAlta1000;
     #1000#0 = 99;
     #1000#1 = "zzz";
|FRUT;

|PROCESO Tipo5;  |TIPO 5;
       |LEE 040#1000p;
       |LINEAS_BI_ATRI #1000#3, S, D, -1;
|FPRC;

|PROCESO Tipo2;  |TIPO 2;
     #1000#3 = "D";
     |SI #1000#1 = "   ";  #1000#3 = "T";  |FINSI;
|FPRC;

|PROCESO Recoge;
     |DBASE aAlfa;   |QBF aAlfa;
     aAlfa = aAlfa + "CogeTxt/";
     aAlfa = aAlfa + "zliquidapat.txt";
     |XABRE "U", aAlfa, nHandle;
     |DDEFECTO #1000;
     |PARA;  |SI  FSalida ] 0;  |HACIENDO;
             |XLEE nHandle, 131, aAlfa1;
             |SI FSalida < 0;  |SAL;  |FINSI;

             aAlfa2 = aAlfa1 % 102;    #1000#0  = aAlfa2;
             aAlfa2 = aAlfa1 % 403;    #1000#1  = aAlfa2;
             aAlfa2 = aAlfa1 % 851;    #1000#2  = aAlfa2;
             aAlfa2 = aAlfa1 % 6001;   #1000#3  = aAlfa2;
             aAlfa2 = aAlfa1 % 6201;   #1000#4  = aAlfa2;
             aAlfa2 = aAlfa1 % 6402;   #1000#5  = aAlfa2;
             aAlfa2 = aAlfa1 % 6708;   #1000#6  = aAlfa2;
             aAlfa2 = aAlfa1 % 7620;   #1000#7  = aAlfa2;
             aAlfa2 = aAlfa1 % 9702;   #1000#8  = aAlfa2;
             aAlfa2 = aAlfa1 % 10008;  #1000#9  = aAlfa2;
             aAlfa2 = aAlfa1 % 10920;  #1000#10 = aAlfa2;

             |GRABA 020#1000n;
             |LIBERA #1000;
             |DDEFECTO #1000;
     |SIGUE;
     |XCIERRA nHandle;
|FPRC;

|PROGRAMA;
     |ABRE #1000;
     |LEE 040#1000p;
     |SI FSalida ! 0;
         |HAZ Recoge;
     |FINSI;
     |CIERRA #1000;

     |ENTLINEAL #1#1000, -1, 2, 22, 1, ClaveBaja1000, ClaveAlta1000;
|FPRO;
