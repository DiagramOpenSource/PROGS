|FICHEROS;
     rem00021 #20;
     rew00550 #550;
|FIN;

|VARIABLES;
     nCampo3    = 0;
     nCampo4    = 0;
     nCampo5    = 0;
     nCampo6    = 0;
     nCampo7    = 0;
     nCampo8    = 0;
     nCampo9    = 0;
     nCampo10   = 0;
     nCampo11   = 0;
     nCampo12   = 0;
     nCampo13   = 0;
     nImputa    = 0;
     nDiasAlq   = 0;
     nDiasUso   = 0;
     nSwHay     = 0;
     nCatastral = 0;
     nValor     = 0;
     nPorce     = 0;
     nTotal1    = 0;
     nTotal2    = 0;

     aFichero   = "";

     &enEmpresa = 0;
|FIN;

|PROCESO GrabaTempo;
     |SI #20#3 = "P";  |ACABA;  |FINSI;

     nCampo3  = 0;
     nCampo4  = 0;
     nCampo5  = 0;
     nCampo6  = 0;
     nCampo7  = 0;
     nCampo8  = 0;
     nCampo9  = 0;
     nCampo10 = 0;
     nCampo11 = 0;
     nCampo12 = 0;
     nCampo13 = 0;
     nImputa  = 0;

     nDiasAlq = #20#31 + #20#34;
     nDiasUso = #20#32 + #20#35;

     nCampo3  = #20#38;
     |SI #20#37 = "A";  nCampo3  = #20#38 * (nDiasAlq / 365);  |FINSI;

     nCampo4  = #20#40;
     |SI #20#39 = "A";  nCampo4  = #20#40 * (nDiasAlq / 365);  |FINSI;

     |SI #20#41 = "A";
         nCampo4  = nCampo4 + (#20#42 * (nDiasAlq / 365));
     |SINO;
         nCampo4  = nCampo4 + #20#42;
     |FINSI;

     |SI #20#43 = "A";
         nCampo4 = nCampo4 + (#20#44 * (nDiasAlq / 365));
     |SINO;
         nCampo4 = nCampo4 + #20#44;
     |FINSI;

     nCampo5  = (nCampo3 + nCampo4);

     nCampo6  = #20#22;
     |SI #20#48 = "A";  nCampo6  = #20#22 * (nDiasAlq / 365);  |FINSI;

     ||SI nCampo6 > nCampo3;  nCampo6 = nCampo3;  |FINSI;

     nCampo7  = #20#60;

     nCampo8  = #20#55;
     |SI #20#54 = "A";  nCampo8  = #20#55 * (nDiasAlq / 365);  |FINSI;

     nCampo9  = #20#21;
     |SI #20#49 = "A";  nCampo9  = #20#21 * (nDiasAlq / 365);  |FINSI;

     |SI #20#50 = "A";
         nCampo9  = nCampo9  + (#20#51 * (nDiasAlq / 365));
     |SINO;
         nCampo9  = nCampo9  + #20#51;
     |FINSI;

     |SI #20#52 = "A";
         nCampo9  = nCampo9  + (#20#53 * (nDiasAlq / 365));
     |SINO;
         nCampo9  = nCampo9  + #20#53;
     |FINSI;

     |SI #20#56 = "A";
         nCampo9  = nCampo9  + (#20#57 * (nDiasAlq / 365));
     |SINO;
         nCampo9  = nCampo9  + #20#57;
     |FINSI;

     nCampo10   = (nCampo6 + nCampo7 + nCampo8 + nCampo9);
     nCampo11   = (nCampo5 - nCampo10);
     nCatastral = 0;

     |SI #20#36 = "S";
         nValor = nCampo3 - (nCampo6 + nCampo7 + nCampo8 + nCampo9);

         nPorce = 2;
         |SI #20#17 = "S";  nPorce = 1.1;  |FINSI;

         |SI #20#15 > 0;
             nCatastral = (#20#15 % nPorce) * (nDiasAlq / 365);
         |SINO;
             nCatastral = #20#27;
             |SI nCatastral < #20#26;  nCatastral = #20#26;  |FINSI;
             nCatastral = ((nCatastral % 50) % 1.1) * (nDiasAlq / 365);
         |FINSI;

         |SI nCatastral > nValor;
             nValor    = nCatastral;
             nCampo3   = nValor;
             nCampo5   = nCampo3 + nCampo4;
             nCampo6   = 0;
             nCampo7   = 0;
             nCampo8   = 0;
             nCampo9   = 0;
             nCampo10  = 0;
             nCampo11  = (nValor + nCampo4);
         |FINSI;
     |FINSI;

     |SI #20#43 = "A";
         nCampo12 = (#20#44 * 0.3) * (nDiasAlq / 365);
     |SINO;
         nCampo12 = #20#44 * 0.3;
     |FINSI;

     nCampo13 = nCampo11 - nCampo12;

     |SI nDiasUso ! 0;
         |SI #20#18 = "U";  |O #20#18 = "L";  |O #20#18 = "O";
             |SI #20#15 ! 0;
                 |SI #20#17 = "S";
                     nImputa = (#20#15 % 1.1) * (nDiasUso / 365);
                 |SINO;
                     nImputa = (#20#15 % 2) * (nDiasUso / 365);
                 |FINSI;
             |SINO;
                 nValor     = #20#26;
                 |SI nValor < #20#27;  nValor = #20#27;  |FINSI;

                 nImputa = ((nValor % 50) % 1.1) * (nDiasUso / 365);
             |FINSI;
         |FINSI;
     |FINSI;

     nSwHay = 1;

     #550#0 = #20#1;
     #550#1 = #20#2;
     #550#2 = #20#13;
     #550#3 = nCampo13;
     #550#4 = nImputa;
     |GRABA 020#550n;
     |LIBERA #550;

     nTotal1 = nTotal1 + #550#3;
     nTotal2 = nTotal2 + #550#4;
|FPRC;

|DEFBUCLE rem00021;
     #20#1;
     ;
     enEmpresa, 01;
     enEmpresa, 99;
     ;
     NULL, GrabaTempo;
|FIN;

|RUTINA ClaveBaja550;
     #550#0 = 1;
|FRUT;

|RUTINA ClaveAlta550;
     #550#0 = 99;
|FRUT;

|PROCESO Rendimientos;
     aFichero = Usuario;  |QBT aFichero;
     aFichero = (aFichero + "zzzzzzzz") % -108;
     |NOME_DAT #550 aFichero;
     |ABRE #550;  |CIERRA #550;  |DELINDEX #550;

     |SALVA_DATOS #20;

     nSwHay  = 0;
     nTotal1 = 0;
     nTotal2 = 0;

     |ABRE #550;
     |BUCLE rem00021;
     |CIERRA #550;

     |SI nSwHay = 1;
         |PINPA #0#550;
         #20#21 = nTotal1;
         #20#22 = nTotal2;

         |ATRI R;
         |PINTA 2256, #20#21;
         |PINTA 2270, #20#22;
         |ATRI r;

         |ENTLINEAL #1#550, -1, 4, 20, 0, ClaveBaja550, ClaveAlta550;
     |FINSI;

     |DELINDEX #550;
     |REPON_DATOS #20;

     |PTEC 802;
     |PTEC 807;
|FPRC;
