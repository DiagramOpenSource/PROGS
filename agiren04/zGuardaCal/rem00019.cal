|FICHEROS;
     rem00019 #19;
     rew99999 #999;
|FIN;

|VARIABLES;
     aFichero      = "";
     nAnyos        = 0;

     &enSwPresenta = 0;
|FIN;

|PROCESO Tipo7Campo2;  |TIPO 7;
     enSwPresenta = 1;
     |HAZ SacaDeclarantes;

     aFichero = (Usuario + "zzzzzzzz") % 108;
     |NOME_DAT #999 aFichero;
     |ABRE #999;
     |SELECT #2#999;
     #999#1 = #19#1;
     |LEE 040#999=;
     |SI FSalida ! 0;  |DDEFECTO #999;  |FINSI;
     |CIERRA #999;

     |C_M #19#2, 1, "S";
     nAnyos = 2003 - #999#6;
     |SI nAnyos [ 65;
         |C_M #19#2, 1, "N";  #19#2 = "N";  |PINTA #19#2;
     |FINSI;
|FPRC;

|PROCESO Tipo0Campo3;  |TIPO 0;
     |C_M #19#4, 1, "S";
     |SI #19#3 = "N";
         |C_M #19#4, 1, "N";  #19#4 = "0";  |PINTA #19#4;
     |FINSI;
|FPRC;

|PROCESO Tipo11m19;  |TIPO 11;
     |SI FSalida ! 10;  |ACABA;  |FINSI;

     |LEE 101#19=;
     |SI FSalida ! 0;  |ACABA;  |FINSI;

     |ENDATOS #1#19;
     |SI FSalida = 0;
         |GRABA 020#19a;
     |FINSI;
     |LIBERA #19;
|FPRC;
