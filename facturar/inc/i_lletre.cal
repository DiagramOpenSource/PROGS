|HAZ en1960;

r2      = 47;
rctraba = rc % 406;
|SI rctraba = "000000"; |HAZ en1960; |FINSI;

|ET en1560;
    rf      = rc % 408;
    rc      = rf;
    fg1     = "UNA ";
    fg2     = "DUES ";
    fg30    = "DUES-CEN-TES ";
    fg31    = "TRES-CEN-TES ";
