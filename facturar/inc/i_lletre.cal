    fg37    = "NOU-CEN-TES ";
    rctraba = rc % 103;
    |SI rctraba > "001"; |HAZ en1760; |FINSI;

    r2      = 39;
    rctraba = rc % 103;
    |SI rctraba ! "000"; |HAZ en1960; |FINSI;

    rf      = rc % 405;
    rc      = rf;
    rctraba = rc % 103;
    |SI rctraba ! "000"; |HAZ en1760; |FINSI;

    r2      = 42;
