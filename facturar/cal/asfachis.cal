|FICHEROS;
    asfachis #0;
    asempres #3;
    asw00050 #900,MANTE;
|FIN;

|VARIABLES;
      aAlfa   = "";

     &d_serie = "";
     &h_serie = "";
     &d_factu = 0;
     &h_factu = 0;
     &d_fecha = @;
     &h_fecha = @;
     &d_clien = 0;
     &h_clien = 0;
     &enDReme = 0;
     &enHReme = 0;
     &enDFPag = 0;
     &enHFPag = 0;
     &empresa = 0;
     &d_viene = 2;
     &cuotas  = "";
     &eaTipoDir = "";
     &eaParrilla = "";
     fFech = @;
     qFech = "";
     dFech = "";
     hFech = "";
|FIN;

|PROCESO tipo_3; |TIPO 3;
    d_serie    = #0#0;
    h_serie    = #0#1;
    d_factu    = #0#2;
    h_factu    = #0#3;
    d_fecha    = #0#4;
    h_fecha    = #0#5;
    d_clien    = #0#8;
    h_clien    = #0#9;
    enDReme    = #0#10;
    enHReme    = #0#11;
    enDFPag    = #0#12;
    enHFPag    = #0#13;
    empresa    = #0#6;
    cuotas     = #0#7;
    eaTipoDir  = #0#14;
    eaParrilla = #0#15;

    |SUB_EJECUTA "asfacges.run";
|FPRC;

|PROCESO Tipo0C6;
    |ABRE #3;
    #3#0 = #0#6;
    |LEE 000#3=;
    |SI FSalida = 0;
        #0#7  = #3#19; |PINTA #0#7;
        #0#14 = #3#15; |PINTA #0#14;
    |FINSI;
    |CIERRA #3;

    |SI #48#3 = "N";
        #0#14 = "F";
        |C_M #0#14, 1, "N";
    |FINSI;
|FPRC;

|PROCESO sacaano; |TIPO 7;
     fFech = ~;
     qFech = fFech % -104;
     dFech = "01.01." + qFech;
     hFech = "31.12." + qFech;
     #0#4  = dFech;
     #0#5  = hFech;
     |PINTA #0#4;
     |PINTA #0#5;
|FPRC;

|PROCESO Baja900;
     #900#0 = 1;
|FPRC;

|PROCESO Alta900;
     #900#0 = 999;
|FPRC;

|PROCESO Tipo7C15;  |TIPO 7;
     |HAZ Tipo0C6;

     |ENTLINEAL #1#900, -1, 7, 22, 0, Baja900, Alta900;
|FPRC;

|PROCESO Tipo0C15;  |TIPO 0;
     aAlfa = ("f5" + Usuario + "zzzzzzzz") % 108; |NOME_DAT #900 aAlfa;

     |SI #0#15 = "P";
         |ENTLINEAL #1#900, -1, 1, 22, 0, Baja900, Alta900;

         |PDEFECTO #0, 0, 14;

         |C_M #0#0,  1, "N";  |PINTA #0#0;
         |C_M #0#1,  1, "N";  |PINTA #0#1;
         |C_M #0#2,  1, "N";  |PINTA #0#2;
         |C_M #0#3,  1, "N";  |PINTA #0#3;
         |C_M #0#4,  1, "N";  |PINTA #0#4;
         |C_M #0#5,  1, "N";  |PINTA #0#5;
         |C_M #0#8,  1, "N";  |PINTA #0#8;
         |C_M #0#9,  1, "N";  |PINTA #0#9;
         |C_M #0#10, 1, "N";  |PINTA #0#10;
         |C_M #0#11, 1, "N";  |PINTA #0#11;
         |C_M #0#12, 1, "N";  |PINTA #0#12;
         |C_M #0#13, 1, "N";  |PINTA #0#13;
     |SINO;
         |ABRE #900;  |CIERRA #900;  |DELINDEX #900

         |ENTLINEAL #1#900, -1, 7, 22, 0, Baja900, Alta900;

         |C_M #0#0,  1, "S";  |PINTA #0#0;
         |C_M #0#1,  1, "S";  |PINTA #0#1;
         |C_M #0#2,  1, "S";  |PINTA #0#2;
         |C_M #0#3,  1, "S";  |PINTA #0#3;
         |C_M #0#4,  1, "S";  |PINTA #0#4;
         |C_M #0#5,  1, "S";  |PINTA #0#5;
         |C_M #0#8,  1, "S";  |PINTA #0#8;
         |C_M #0#9,  1, "S";  |PINTA #0#9;
         |C_M #0#10, 1, "S";  |PINTA #0#10;
         |C_M #0#11, 1, "S";  |PINTA #0#11;
         |C_M #0#12, 1, "S";  |PINTA #0#12;
         |C_M #0#13, 1, "S";  |PINTA #0#13;
     |FINSI;
|FPRC;

|PROCESO Tipo9;  |TIPO 9;
    aAlfa = ("f5" + Usuario + "zzzzzzzz") % 108; |NOME_DAT #900 aAlfa;

    |ABRE #900;  |CIERRA #900;  |DELINDEX #900
|FPRC;
