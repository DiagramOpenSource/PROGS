|FICHEROS;
     ascuolin #1;
     ascuocab #0;
     abom0510 #2;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO GrabaLineas;
     #1#0 = #0#0;
     #1#1 = #2#1;
     #1#2 = #2#2;
     #1#3 = #2#3;
     #1#4 = #2#4;
     #1#5 = #2#5;
     #1#6 = #2#6;
     #1#7 = #2#7;
     |GRABA 020#1n;
     |LIBERA #1;
|FPRC;

|DEFBUCLE abom0510;
     #2#1;
     ;
     #0#3, 001;
     #0#3, 999;
     ;
     NULL, GrabaLineas;
|FIN;

|PROCESO Tipo5;  |TIPO 5;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo ! 1;  |ACABA;  |FINSI;

     |BUCLE abom0510;
|FPRC;


|PROCESO Tipo2B;  |TIPO 2;
     #1#7 = #1#5 * #1#6;
     #0#6 = #0#6 +. #1#7;

     |SI #0#2 = "M"; #0#7 = #0#6 / 12; |FINSI;
     |SI #0#2 = "T"; #0#7 = #0#6 / 4;  |FINSI;
     |SI #0#2 = "S"; #0#7 = #0#6 / 2;  |FINSI;
     |SI #0#2 = "A"; #0#7 = #0#6 / 1;  |FINSI;

     |PINTA #1#7;
     |PINTA #0#6;
     |PINTA #0#7;
|FPRC;
