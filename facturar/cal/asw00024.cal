|FICHEROS;
   asw00024 #0;
|FIN;

|VARIABLES;
   Cont = 0;
|FIN;

|PROCESO MiraEnlace;
   |SI #asw00024#Campo# = "X";
      |PARA Cont = 0; |SI Cont [ 4; |HACIENDO Cont = Cont + 1;
         |SI Campo ! Cont; #asw00024#Cont# = " "; |PINTA #asw00024#Cont#; |FINSI;
      |SIGUE;
   |FINSI;
|FPRC;

|PROCESO MiraCampo;
   |SI #asw00024#Campo# = "X";
      |PARA Cont = 5; |SI Cont [ 8; |HACIENDO Cont = Cont + 1;
         |SI Campo ! Cont; #asw00024#Cont# = " "; |PINTA #asw00024#Cont#; |FINSI;
      |SIGUE;
   |FINSI;
|FPRC;
