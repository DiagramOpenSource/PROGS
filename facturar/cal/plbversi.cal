|PROCESO ashismicVer;
     |SI #ashismic#25 = 0;  |Y #ashismic#27 = 0;
         |ACABA;
     |FINSI;

     #ashisfac#0 = #ashismic#0;
     #ashisfac#1 = #ashismic#1;
     |LEE 000#ashisfac.=;
     |SI FSalida ! 0;  |DDEFECTO #ashisfac;  |FINSI;

     |SI #ashismic#25 ! 0;
         #ashismic#45 = #ashisfac#7 ! 0;

         |SI #ashismic#45 = 0;
             #ashismic#45 = ((#ashismic#25 / #ashismic#24) * 100) ! 0;
         |FINSI;
     |FINSI;

     |SI #ashismic#27 ! 0;
         #ashismic#46 = #ashisfac#9;

         |SI #ashismic#46 = 0;
             #ashismic#46 = ((#ashismic#27 / #ashismic#24) * 100) ! 0;
         |FINSI;
     |FINSI;

     |GRABA 020#ashismic.a;
     |LIBERA #ashismic;
|FPRC;

|DEFBUCLE ashismicVer;
     #ashismic#1;
     ;
     INICIO;
     FINAL;
     be;
     NULL, ashismicVer;
|FIN;

|PROCESO Tipo88Version;  |TIPO 8;
     |ABRE #ashismic;
     |SELECT #6#ashismic;
     |LEE 000#ashismic.u;
     |SI FSalida ! 0;  |DDEFECTO #ashismic;  |FINSI;
     |CIERRA #ashismic;

     |SI #ashismic#45 = 0;
         |PUSHV 0501 2380;
         |BLANCO 0501 2380;
         |CUADRO 0501 2380;

         |PINTA 1005, "Por motivos del cambio de IVA en el ejercicio 2010, vamos a efectuar unos ";
         |PINTA 1105, "ajustes.";
         |PINTA 1305, "Esto puede tardar unos minutos ...";
         |PINTA 1505, "Espere por favor.";

         |ABRE #ashisfac;
         |BUCLE ashismicVer;
         |CIERRA #ashisfac;

         |POPV;
     |FINSI;

     |ABRE #ascontrt;
     |LEE 000#ascontrt.p;
     |SI FSalida = 0;  |CIERRA #ascontrt;  |ACABA;  |FINSI;

