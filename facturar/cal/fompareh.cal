|FICHEROS;
     fompareh #0;
     fomhparc #1;
     fomhparl #2;
|FIN;

|VARIABLES;
     nTotal   = 0;
     nPasada  = 0;

     &vacio   = "                                        ";
|FIN;

|PROCESO fomhparl;
     |SI nPasada = 0;
         nTotal = nTotal + #fomhparl#8;
         |ACABA;
     |FINSI;

     |PRINT;
|FPRC;

|PROCESO cabecera;
     |SI #fompareh#13 > 0;  |O #fompareh#14 < 999999999.99;
         nTotal  = 0;
         nPasada = 0;
         |BUCLELIN 0fomhparl#1;

         |SI nTotal < #fompareh#13;  |O nTotal > #fompareh#14;
             |ACABA;
         |FINSI;
     |FINSI;

     nPasada = 1;
     |BUCLELIN 0fomhparl#1;

     |PIEINF;
|FPRC;

|DEFBUCLE fomhparc;
     #fomhparc#3;
     6, 7, 10;
     #fompareh#0, #fompareh#2, #fompareh#4, #fompareh#7, #fompareh#9,  #fompareh#11;
     #fompareh#1, #fompareh#3, #fompareh#5, #fompareh#8, #fompareh#10, #fompareh#12;
     ;
     NULL, cabecera;
|FIN;

|PROCESO tipo_3; |TIPO 3;
     |IMPRE 0;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |INFOR "fompareh";
     |SI FSalida ! 0; |FINIMP; |ACABA; |FINSI;

     |BUCLE fomhparc;

     |FININF;
     |FINIMP;
|FPRC;
