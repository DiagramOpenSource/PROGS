     asconcep #1;
     ascontft #2;
|FIN;

|VARIABLES;
|FIN;

|PROCESO saldo; |TIPO 0;
     |C_M #0#33, 1, "S";
     |C_M #0#34, 1, "S";
     |SI #0#32 = "N";
         |C_M #0#33, 1, "N";
         |C_M #0#34, 1, "N";
     |FINSI;
|FPRC;

|PROCESO concepto; |TIPO 0;
     |SI #0#33 = "    "; |Y #0#34 = "  "; |ACABA; |FINSI;

     |ABRE #1;
     #1#0 = #0#33;
     #1#1 = #0#34;
     |LEE 040#1=;
     |SI FSalida ! 0;
         |MENAV "     No existe concepto ";
         |CIERRA #1;
         |ERROR;
         |ACABA;
     |FINSI;

     |SI #1#3 ! "A";
         |MENAV "     El concepto no es un A CUENTA ";
         |ERROR;
     |FINSI;
     |CIERRA #1;
|FPRC;

|PROCESO Baja2;
     #2#0 = 0;
|FPRC;

|PROCESO Alta2;
     #2#0 = 99;
|FPRC;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |CABEZA "Control de Facturas";

     |ABRE #0;
     |DDEFECTO #0;
     |LEE 101#0p;
     |SI FSalida ! 0;
         |DDEFECTO #0;
         |GRABA 020#0b;
     |FINSI;

     |PINDA #0#0;

     |ENTLINEAL #1#2, -1, 2, 22, 0, Baja2, Alta2;

     |ENDATOS #1#0;
     |SI FSalida = 0;
         |GRABA 020#0a;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;
