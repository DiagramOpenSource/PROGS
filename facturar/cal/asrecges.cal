          fechv = fechav;

          |SI #712#5 ! 0;
              |SI #712#2 = "D"; em = 1; |SINO; em = 1000; |FINSI;
              j      = #712#5 / em;
              fechav = fech + j;
              fechv  = fechav;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO calcula;
     tdto = (hono % dto) ! EURODB;
     base = hono - tdto;
     tiva = (base % ivabo) ! EURODB;
     tret = 0;
     |SI irpf = "S"; tret = (base % ret) ! EURODB; |FINSI;

     tfac = base + supli + tiva;
     tpagar = (tfac - acue) - tret;

     rlecan = tpagar;
     |HAZ lletres;
|FPRC;

|PROCESO borra;
     Ides = des0 <-;
     Iimp = imp0 <-;
     |PARA i = 0;|SI i < conta_indi;|HACIENDO i = i + 1;
         des = vacio;
         imp = cero;
         Ides = Ides + 1;
         Iimp = Iimp + 1;
     |SIGUE;
|FPRC;

|PROCESO escribe;
