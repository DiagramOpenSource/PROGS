|PROCESO imprelin_n;
     |SI indihono < conta_indi;
         |SI recibo_esp = 0;
             Ides = des1 <-;
             Ides = Ides + indihono;
             des  = #2#6;

             Iimp = imp1 <-;
             Iimp = Iimp + indihono;
             imp  = #2#13;
         |FINSI;

         |SI recibo_esp = 1;
             |SI #2#10 = "H";
                 conta_hono = conta_hono + 1;
                 |SI conta_hono < 15;
                     Ides = des0 <-;
                     Ides = Ides + conta_hono;
                     des = #2#6;

                     Iimp = imp0 <-;
                     Iimp = Iimp + conta_hono;
                     imp = #2#13;
                |FINSI;
             |FINSI;

             |SI #2#10 ! "H";
                 conta_supl = conta_supl + 1;
                 |SI conta_supl < 8;
                     Ides = des0 <-;
                     Ides = Ides + conta_supl;
                     des = #2#6;

                     Iimp = imp0 <-;
                     Iimp = Iimp + conta_supl;
                     imp = #2#13;
                |FINSI;
             |FINSI;
         |FINSI;

         |SI recibo_esp = 2;
             |SI #2#10 = "A";
                 |SI #2#13 ] 0;
                     saldo_n = saldo_n + #2#13;
                 |SINO;
                     saldo_a = saldo_a + (-#2#13);
                 |FINSI;
             |FINSI;

             Ides = des1 <-;
             Ides = Ides + indihono;
             des  = #2#7;

             Iimp = imp1 <-;
             Iimp = Iimp + indihono;
             |SI #2#10 = "A";
                 |SI #2#13 < 0;
                     imp = -#2#13;
                 |SINO;
                     imp = #2#13;
