|FICHEROS;
     abom0512 #0;
|FIN;

|PROCESO MayusM0512;  |TIPO 0;
     #0Campo = #0Campo > #0Campo;
     |PINTA #0Campo;

     |SI #0#3 = "S";
         |C_M #0#2, 1, "N";  #0#2 = "N";  |PINTA #0#2;
         |C_M #0#4, 1, "N";  #0#4 = "N";  |PINTA #0#2;
         |C_M #0#5, 1, "N";  #0#5 = "N";  |PINTA #0#2;
         |C_M #0#6, 1, "N";  #0#6 = "N";  |PINTA #0#2;
     |SINO;
         |C_M #0#2, 1, "S";
         |C_M #0#4, 1, "S";
         |C_M #0#5, 1, "S";
         |C_M #0#6, 1, "S";
     |FINSI;

     |SI #0#4 = "N";
         |C_M #0#5, 1, "N";  #0#5 = "N";  |PINTA #0#2;
         |C_M #0#6, 1, "N";  #0#6 = "N";  |PINTA #0#2;
     |SINO;
         |C_M #0#5, 1, "S";
         |C_M #0#6, 1, "S";
     |FINSI;
|FPRC;
