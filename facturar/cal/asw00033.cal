|FICHEROS;
   asw00033;
   asclient;
|FIN;

|VARIABLES;
   nInkey     = 0;
   aAlfa      = "";
   &eaCta     = "";
   &eaPais    = "";
   &eaIBAN    = "";
   &eaIBANant = "";
|FIN;

|PROCESO TrasPago;
   |ATRI I;
   |SI #asw00033#3 = "R";
      |PINTA 1136, "Recurrente";
   |SINO;
      |PINTA 1136, "Unico     ";
   |FINSI;
   |ATRI N;
|FPRC;

|PROCESO IgualoCampos;
   #asw00033#0 = #asclient#77;
   #asw00033#1 = #asclient#84;
   #asw00033#2 = #asclient#76;
   #asw00033#3 = #asclient#85;
   #asw00033#4 = #asclient#83

   |PINDA #0#asw00033;
|FPRC;

|PROCESO Tipo0C0; |TIPO 0;
     nInkey = FSalida;
     |SI nInkey = 10;
         |HAZ IgualoCampos;
     |FINSI;

     aAlfa = #asw00033#0; |QBF aAlfa;
     |SI aAlfa = ""; |ACABA; |FINSI;

     eaIBANant = #asw00033#0;
     |HAZ CheqIBAN;

     aAlfa = #asw00033#0;  |QBF aAlfa;
     |SI aAlfa ! eaIBAN;
         |CONFI "0000SEl codigo IBAN puede ser erroneo, quiere calcularlo.";
         |SI FSalida = 0;
             #asw00033#0= eaIBAN;
             |PINTA #asw00033#0;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;
