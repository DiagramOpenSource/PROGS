|FICHEROS;
  asexpece #0;
  asexpeca #1;
  asexpeli #2;
|FIN;

|VARIABLES;
 campo = 0;
 campito = 0;
 z = 0;
 x = 0;
 y = 0;
  informe = "asexpece";
  n = "N"
  &conta = 0;
  &expe = 0;
  &clie = 0;
  &concep = "";
  &sw_cierre = 0;
  &fechac = @;
  cliente = "";
|FIN;

|CALCULO existe; |TIPO 0;
campo = Campo
campito = campo - 10;
|SI #0Campo ! "999999";
   |ABRE #1;
   #1#0 = #0campito;
   #1#14 = #0campo;
   |LEE 040#1=;
   |SI FSalida ! 0;
     |AVISO; |AVISO;
     |MENAV "     !!! ATENCION !!!  No existe Expediente.";
   |SINO;
     |SI #1#12 = "S";
        |AVISO; |AVISO;
        |MENAV "     !!! ATENCION !!!  El Expediente ya esta CERRADO";
     |FINSI;
   |FINSI;
|FINSI;
|FCAL;

|CALCULO miralin;
 |SI #2#15 = "N";
     sw_cierre = 1;
 |FINSI;
|FCAL;

|CALCULO lineas;

  |BUCLELIN 0miralin#1;
  clie = #1#1;
  cliente = #1#1;
  |SI sw_cierre = 1;
      concep = "El Expediente : " + expe + " " + conta + " Cliente : " + cliente;
      concep = concep  + "  LE QUEDAN LINEAS POR FACTURAR";
  |SINO;
      concep = "El Expediente : " + expe + " " + conta + " Cliente : " + cliente;
      concep = concep  + "  CERRADO.";
      fechac = #0z;
      #1#12 = "S";
      #1#4 = #0z;
      |GRABA 020#1a;
      |LIBERA #1;
  |FINSI;

  |PRINT;

|FCAL;

|DEFBUCLE asexpece0;
  #1#1;
  12;
  expe, conta, n;
  expe, conta, n;
  be;
  NULL,lineas;
|FIN;

|CALCULO cerrar; |TIPO 3;
    |IMPRE 0;
    |SI FSalida = 0;
       |INFOR informe;
       |SI FSalida = 0;

           |PARA x = 0; |SI x [ 9; |HACIENDO x = x + 1;
              y = x + 10;
              |SI #0y ! "999999";
                  sw_cierre = 0;
                  z = x + 20;
                  expe  = #0x;
                  conta = #0y;
                  |BUCLE asexpece0;
              |FINSI;
           |SIGUE;
           |PIEINF;
           |FININF;
       |SINO;
           |MENAV "     Error en Informe.";
       |FINSI;
       |FINIMP;
    |SINO;
       |MENAV "     Impresion Cancelada.";
    |FINSI;
|FCAL;
