|FICHEROS;
     aboz0008 #0;
     refer@   #100;
|FIN;

|VARIABLES;
     aAlfa   = "";
     aAlfa1  = "";
     nGraba    = 0;
     nCaracter = 0;
     nHandle = 0;
     nPosi   = 0;
     nNumero = 0;
     nNume   = 0;
|FIN;

|PROCESO Tipo3;  |TIPO 3;
|ACABA;
     |DFICE aAlfa;  |QBT aAlfa;

     aAlfa1 = aAlfa + #0#1 + ".ddx";  |FBORRA aAlfa1;
     aAlfa1 = aAlfa + #0#1 + ".ixx";  |FBORRA aAlfa1;

     aAlfa = aAlfa + #0#1 + ".dat";
     |XABRE "BU", aAlfa, nHandle;
     nPosi = #0#0;
     |PARA nPosi = #0#0 - 1; |SI; |HACIENDO nPosi = nPosi + #0#0;
           |XPOSICION nHandle, nPosi, 0;

           nNume     = 0;
           nCaracter = 0;
           |XLEE_NUMERO nHandle, nNume, nCaracter;
           |SI FSalida [ 0;  |SAL;  |FINSI;

           |SI nCaracter = 0;
               |XPOSICION nHandle, nPosi, 0;
               nNume     = 0;
               nCaracter = 10;
               |XGRABA_NUMERO nHandle, nNume, nCaracter;
           |FINSI;
     |SIGUE;
     |XCIERRA nHandle;

     |DBASE aAlfa;
     aAlfa = aAlfa + "def/" + #0#1;
     |CARGA_DEF aAlfa, refer@;
     |ABRE #100;  |CIERRA #100;
     |DESCARGA_DEF #refer@;
|FPRC;
