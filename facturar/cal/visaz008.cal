     |C_M #100#3, 1, "S";
     |C_M #100#4, 1, "S";
     |SI #100#2 = 1;
         |C_M #100#3, 1, "N";
         |C_M #100#4, 1, "N";
     |FINSI;
|FPRC;

|PROCESO Tipo0C3; |TIPO 0;
     |HAZ EsVisados;
     |SI FSalida = 0;
          fDesde = "01.01.2007";
          fSistema = ~;
          aSistema = fSistema;
          aAnyo = aSistema % -104;
          aHasta = "31.12." + aAnyo;
          fHasta = aHasta;

          |SI #visaz008#3 < fDesde; |O #visaz008#3 > fHasta;
               aMensaje = "0000Atencion: La fecha de cobro no puede ser inferior a 2007 ni superior a " + aAnyo;
               |MENAV aMensaje;
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO CuentaTesor;
   nTecla = FSalida;

   |DBASS aAlfa; |QBF aAlfa;
   aAlfa = aAlfa + "contagen/def/cacuenta.mas";
   |CARGA_DEF aAlfa, cacuent@;
   |SI FSalida = 0;
      |DBASS aAlfa; |QBF aAlfa;
      aAlfa = aAlfa + "contagen/def/canempre.mas";
      |CARGA_DEF aAlfa, canempr@;
      |SI FSalida = 0;
         aAlfa = #0Campo; |QBF aAlfa;
         |SI aAlfa ! "";
            |ABRE #asempcon;
            |LEE 000#asempcon.p;
            |SI FSalida ! 0; |ACABA; |FINSI;
            |CIERRA #asempcon;

            |DBASS aAlfa; |QBF aAlfa;
            aAlfa = aAlfa + "contagen/fich/";
            |PATH_DAT #canempr@#aAlfa#;

            aAlfa = aAlfa + ("00000" + #asempcon#1) % -105;
            aAlfa = aAlfa + (("0" + #asempcon#2) % -101) + "/";
            |PATH_DAT #cacuent@#aAlfa#;

            |ABRE #canempr@; |ABRE #cacuent@;

            nMaxDig = 0;
            #canempr@#2 = #asempcon#1;
            #canempr@#3 = #asempcon#2;
            |LEE 000#canempr@.=;
            |SI FSalida = 0;
               |PARA nCont = 14; |SI nCont [ 19; |HACIENDO nCont = nCont + 1;
                  |SI nMaxDig [ #canempr@#nCont#;
                     nMaxDig = #canempr@#nCont#;
                  |FINSI;
               |SIGUE;
            |FINSI;

            aAlfa = #100Campo; |QBF aAlfa;
            aAlfa = aAlfa - ".";
            |PARA; |SI FSalida ! 0; |HACIENDO;
               nCalc1 = ((FSalida / 100) ! 0) + 99; aCad1 = aAlfa % nCalc1;
               nCalc2 = FSalida + 98;               aCad2 = aAlfa % nCalc2;
               aLong = aCad1 % 0; nCalc1 = aLong;
               aLong = aCad2 % 0; nCalc2 = aLong;
               nCalc1 = nMaxDig - (nCalc1 + nCalc2);
               aAlfa = aCad1 + ("0" * nCalc1) + aCad2;

               aAlfa = aAlfa - ".";
            |SIGUE;
            #100Campo = aAlfa; |PINTA #100Campo;

            |SI nTecla = 10;
               #cacuent@#0 = #100Campo;
               |LEE 000#cacuent@.];
               |CONSULTA_CLAVE #1#cacuent@;
               #100Campo = #cacuent@#0; |PINTA #100Campo;
            |FINSI;

            #cacuent@#0 = #100Campo;
            |LEE 000#cacuent@.=;
            |SI FSalida = 0;
               |SI #cacuent@#3 = "N";
                  aMensaje = "0000Cuenta con pase directo = 'N'";
                  |MENAV aMensaje; |ERROR;
               |FINSI;
            |SINO;
               aMensaje = "0000Cuenta inexistente";
               |MENAV aMensaje; |ERROR;
            |FINSI;
         |SINO;
            aMensaje = "0000La cuenta en blanco es incorrecta";
            |MENAV aMensaje; |ERROR;
         |FINSI;
         |CIERRA #canempr@; |DESCARGA_DEF #canempr@;
      |FINSI;
      |CIERRA #cacuent@; |DESCARGA_DEF #cacuent@;
   |FINSI;
|FPRC;
