|PROCESO act_hist;
|SI #0#8 = 0;
    #1#0 = #0#0;
    #1#1 = #0#1;
    |LEE 101#1=;
    |SI FSalida = 0;
        #1#16 = #0#13;
        |GRABA 020#1a;
    |FINSI;
    |LIBERA #1;
    |ACABA;
|FINSI;

#1#0 = #0#0;
#1#1 = #0#1;
|LIBERA #1;
|LEE 140#1=;
|SI FSalida = 0;
    |SI #1#10 = "N"; |O #100#2 = 2;  ||Si no esta cobrado total
        tot = (#1#9 + #0#8) ! EURODB;
        |SI tot > #1#6;
            #1#9 = #1#6; #1#10 = "S";
            #visam017#0 = #0#0;
            #visam017#1 = #0#1;
            |LEE 000#visam017.=;
            |SI FSalida = 0;
               aAlfa = "    La factura " + #0#0 + "/" + (("00000" + #0#1) % -105) + " ya tenia exceso. El exceso se perdera.";
            |SINO;
               |DDEFECTO #visam017;
               #visam017#0 = #0#0;
               #visam017#1 = #0#1;
               #visam017#2 = #0#11;
               #visam017#3 = #0#12;
               #visam017#4 = #1#6;
               #visam017#5 = tot - #1#6;
               #visam017#6 = #0#10;
               |GRABA 020#visam017.n;
            |FINSI;
        |FINSI;
        |SI tot < #1#6; #1#9 = #1#9 + #0#8;       |FINSI;
        |SI tot = #1#6; #1#9 = #1#6; #1#10 = "S"; |FINSI;

        |SI #100#2 = 2;   #1#10 = "N";  |FINSI;
        #1#8  = #0#3;
        #1#16 = #0#13;
        |GRABA 140#1a;

        #90#0 = #1#0;
        #90#1 = #1#1;
        |LIBERA #90;
        |LEE 140#90=;
        |SI FSalida = 0;
            #90#32 = #1#10;
            |GRABA 140#90a;
        |FINSI;
    |FINSI;
