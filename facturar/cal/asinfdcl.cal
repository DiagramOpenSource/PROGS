|FICHEROS;
     asclient #0;                         ||CALCULO DESDE-HASTA PARA INFORMES;
     asinfdcl #1;
|FIN;

|VARIABLES;
     informe = "asinfdcl";
     men1 = "     NO EXISTE INFORME.";
     men2 = "     NO EXISTE IMPRESORA.";
     men3 = "     NO SE PUDIERON SELECCIONAR REGISTROS";
     sw1 = 0;                            ||CAMBIAR VARIABLE informe NOMBRE;
     sw2 = 0;                            ||DE FICHERO Y DE CALCULO.       ;
     &ordena = "           ";
     codigo = "Codigo.";
     razon = "Razon Social";
     nombre = "Nombre";
|FIN;


|CALCULO imprimir;
  |PRINT;
|FCAL;



|DEFBUCLE cainfdcl0;
#0#1;
1,2;
#1#0,#1#2,#1#4;
#1#1,#1#3,#1#5;
e;
NULL,imprimir;
|FIN;


|DEFBUCLE cainfdcl1;
#0#2;
0,2;
#1#2,#1#0,#1#4;
#1#3,#1#1,#1#5;
e;
NULL,imprimir;
|FIN;


|DEFBUCLE cainfdcl2;
#0#3;
0,1;
#1#4,#1#0,#1#2;
#1#5,#1#1,#1#3;
e;
NULL,imprimir;
|FIN;



|CALCULO impre;|TIPO 3;
|IMPRE 0;                                ||Abre impresora;
|SI FSalida ! 0;
    |MENAV men2;
    |VETE termina;
|FINSI;
|INFOR informe;
|SI FSalida ! 0;
    |MENAV men1;
    |FINIMP;
    |VETE termina1;
|FINSI;
|HAZ clave;
    |PIEINF;                      ||condicion salto pagina;
|ET termina1;
|FININF;                          ||cierra informe;
|ET termina;
|FINIMP;
|FCAL;                                   ||termina el calculo;



|CALCULO clave;
|SI #1#6 = 1;
      ordena = codigo;
   |BUCLE cainfdcl0;
|FINSI;
|SI #1#6 = 2;
      ordena = razon;
   |BUCLE cainfdcl1;
|FINSI;
|SI #1#6 = 3;
      ordena = nombre;
   |BUCLE cainfdcl2;
|FINSI;
|FCAL;
