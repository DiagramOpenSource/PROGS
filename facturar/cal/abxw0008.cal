|FICHEROS;
     abxw0008 #908;
|FIN;

|VARIABLES;
     aQueBoton      = "";
     aID            = "";
     nBotonAceptar  = 0;
     nBotonCancelar = 0;

     &enSeguir     = 0;
     &enError      = 0;
|FIN;

|PROCESO Tipo12Fxw8;  |TIPO 12;
     |SI FSalida = 7;
         enError = 1;
         |ACABA;
     |FINSI;

     |SI enSeguir = 1;
         enError = 0;
         |ACABA;
     |FINSI;

     |ERROR;
|FPRC;

|PROCESO Tipo7C0Fxw8;  |TIPO 7;
     aQueBoton     = "|X000";
     aID           = #908#aQueBoton#;
     nBotonAceptar = aID;
     |ESTADO_CONTROL nBotonAceptar, "HIDE";

     |HAZ Tipo0C3Fxw8;
|FPRC;

|PROCESO Tipo0C3Fxw8;  |TIPO 0;
     |SI #908#3 = "S";
          |C_M #908#2, 1, "S";     |PINTA #908#2;
          #908#1 = "P";
     |SINO;
          #908#2 = 1;
          |C_M #908#2, 1, "N";     |PINTA #908#2;
          #908#1 = "T";
     |FINSI;
|FPRC;
