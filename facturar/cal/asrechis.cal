     h_serie     = #0#1;
     d_factu     = #0#2;
     h_factu     = #0#3;
     d_fecha     = #0#4;
     h_fecha     = #0#5;
     d_banco     = #0#6;
     h_banco     = #0#7;
     d_forma     = #0#9;
