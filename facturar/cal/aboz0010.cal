|FICHEROS;
     abom0001;
     abxm0001,MANTE;
|FIN;

|VARIABLES;
     aVersion   = "";
     &enVersion = 0;
|FIN;

|PROGRAMA;
     |VERSIONRTME aVersion;  |QBF aVersion;
     aVersion = ("0" + aVersion) % 102;

     |SI aVersion = "09"
         enVersion = 9;
         |MANTE #abom0001;
     |FINSI;

     |SI aVersion = "01"
         enVersion = 10;
         |MANTE #abxm0001;
     |FINSI;
|FPRO;
