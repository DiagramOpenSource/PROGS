|FICHEROS;
     nomautom #0;
|FIN

|VARIABLES;
     &nEmpAUTO_D = 0;
     &nEmpAUTO_H = 0;
     &nTraAUTO_D = 0;
     &nTraAUTO_H = 0;
     &nMesAUTO = 0;
     &nAnyAUTO = 0;

     &nEmpAUTO = 0;
     &nTraAUTO = 0;
|FIN;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          nEmpAUTO_D = #0#0;
          nEmpAUTO_H = #0#0;
          nTraAUTO_D = #0#1;
          nTraAUTO_H = #0#1;
          nAnyAUTO = #0#4;

          nEmpAUTO = #0#0;
          nTraAUTO = #0#1;
          |PARA nMesAUTO = #0#3; |SI nMesAUTO ] #0#2; |HACIENDO nMesAUTO = nMesAUTO - 1;
               |SUB_EJECUTA "nomdesac;AUTO";
          |SIGUE;
          |PARA nMesAUTO = #0#2; |SI nMesAUTO [ #0#3; |HACIENDO nMesAUTO = nMesAUTO + 1;
               |SUB_EJECUTA "nomcalcu;CALAUTO";
               |SUB_EJECUTA "nomactca;AUTO";
          |SIGUE;
     |FINSI;
|FPRO;
