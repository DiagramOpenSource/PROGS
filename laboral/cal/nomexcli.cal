|FICHEROS;
     nomexcli #0;
     temp0001@ #500;
     nomconli;
|FIN;

|VARIABLES;
     aAlfa = "";
     nModo = 0;
     nPos1 = 0;
     nPos2 = 0;
|FIN;

|PROCESO Comprueba; |TIPO 0;
     |ABRE #nomconli;
     #nomconli#0 = #0#1;
     #nomconli#2 = #0#2;
     |LEE 000#nomconli.=;
     |SI FSalida = 0;
          #0#3 = #nomconli#6;
          |PINTA #0#3;
     |SINO;
          aAlfa = "    ATENCION: Concepto no Definido en convenio. Pulse F5 ";
          aAlfa = aAlfa + "para consultar.";
          |MENAV aAlfa;
          |ERROR;
     |FINSI;
     |CIERRA #nomconli;
|FPRC;

|PROCESO BuscaCon; |TIPO 11;
     |SI FSalida = 13;
          aAlfa = "SELECT nomconli.2,nomconli.6 FROM nomconli INTO tm" + Usuario;
          aAlfa = aAlfa + ".def WHERE 0 == " + #nomexcli#1;
          |SQL aAlfa;
          |SI FSalida ! 0;
               |MENAV "    No hay definidos conceptos en el convenio";
               |ERROR;
               |ACABA;
          |FINSI;
          |DBASE aAlfa; |QBF aAlfa; aAlfa = aAlfa + "def/tm" + Usuario + ".mas";
          |CARGA_DEF aAlfa , temp0001@;
          |SI FSalida = 0;
               aAlfa = "tm" + Usuario; |NOME_DAT #500 aAlfa;
               |ABRE #temp0001@;
               nModo = 2 + 4 + 8 + 16;
               nPos1 = 1250; nPos2 = 2380;
               |C_V #temp0001@#0, 1, "S";
               |C_V #temp0001@#1, 1, "S";
               |LINEAL_SIMPLE #1#temp0001@,nModo, nPos1, nPos2, NULL, NULL, NULL;
               |SI FSalida = 0;
                    #nomexcli#2 = #temp0001@#0;
                    |PINTA #nomexcli#2;
                    #nomexcli#3 = #temp0001@#1;
                    |PINTA #nomexcli#3;
               |SINO;
                    |ERROR;
               |FINSI;
               |CIERRA #temp0001@;
               |DELINDEX #temp0001@;
               |DESCARGA_DEF #temp0001@;
          |FINSI;
          |DBASE aAlfa; |QBF aAlfa; aAlfa = aAlfa + "def/tm" + Usuario + ".mas";
          |FBORRA aAlfa;
          |ACABA;
     |FINSI;
|FPRC;
