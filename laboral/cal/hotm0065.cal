|PROCESO buscaaltaM;
     |ABRE #labtraba;
     #labtraba#0 = #10#1;
     #labtraba#1 = #10#2;
     |LEE 000#labtraba.=;
     |CIERRA #labtraba;

     alfa1 = "";
     fech1 = #labtraba#37;
     alfa1 = fech1;
     |QBF alfa1;
     |SI alfa1 ! ""; |Y alfa1 ! "..........";
          nume1 = fech1;
     |SINO;
          alfa1 = #10#7;
          alfa1 = alfa1 % 402;
          nume1 = alfa1;
          |ABRE #nomvarca;
          #nomvarca#0 = #labtraba#0;
          #nomvarca#1 = #labtraba#1;
          #nomvarca#2 = nume1;
          #nomvarca#3 = 0;
          |LEE 000#nomvarca.=;
          |SI FSalida = 0;
               alfa1 = #nomvarca#16;
               |QBF alfa1;
               |SI alfa1 = "..........";
                    alfa1 = "";
               |FINSI;
          |FINSI;
          fech1 = alfa1;
          |CIERRA #nomvarca;
     |FINSI;

     nume1 = fech1;

     fech2 = #10#7;
     nume2 = fech2;
     nume2 = nume2 - 1;  || aqui me guardo el dia antes del alta

     |SI nume1 = nume2;
          #10#13 = "S";
