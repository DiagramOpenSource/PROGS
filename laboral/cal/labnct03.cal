|FICHEROS;
     labnct03 #0;
     labnct06;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO Bloquea; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo ! 4;
          |MENAV "    Solo disponible el modo consulta";
          |PTEC 806;
     |FINSI;
|FPRC;

|PROCESO Activa16; |TIPO 7;
     |SI #0#10 = "P";
          |CAMPO_MODIFICA #0#16, 1, "S";
     |SINO;
          |CAMPO_MODIFICA #0#16, 1, "N";
          #0#16 = 00;
          |PINTA #0#16;
     |FINSI;
|FPRC;

|PROCESO Tipo14; |TIPO 14;
     |ATRI 0;
