|PROCESO inf_nct06;
     #labnct06#0 = #0#15;
     #labnct06#1 = #0#16
     #labnct06#2 = 01;
|FPRC;

|PROCESO sup_nct06;
     #labnct06#0 = #0#15;
     #labnct06#1 = #0#16;
     #labnct06#2 = 99;
|FPRC;

|PROCESO Tipo20; |TIPO 20;
     |SI FSalida = "OPCION";
          FSalida = "Errores";
     |SINO;
          |PUSHV 14012380;
          |ENTLINEAL #1#labnct06, -3, 4, 22, 1, inf_nct06, sup_nct06;
          |POPV;
     |FINSI;
|FPRC;
