          |CAMPO_VISUAL #0#13, 1, "N";
          |CAMPO_MODIFICA #0#13, 1, "N";
          |CAMPO_VISUAL #0#14, 1, "N";
          |CAMPO_MODIFICA #0#14, 1, "N";
          |PINTA 1741, "                            ";
          |PINTA 1841, "                            ";

          |CAMPO_VISUAL #0#17, 1, "N";
          |CAMPO_MODIFICA #0#17, 1, "N";
          |PINTA 1942, "                            ";
     |SINO;
