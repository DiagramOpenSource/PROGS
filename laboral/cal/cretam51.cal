|FICHEROS;
     cretam51 #0;
     cretam52 #1,MANTE;
     silcon01;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO Blanco;
     |SI #0#2 = "N";
          |BLANCO 11012380;
     |FINSI;
|FPRC;

|PROCESO Tipo13; |TIPO 13;
     |SI #0#2 = "S";
          |ENTLINEAL #1#1, 1, 7, 18, 1, min, max;
     |FINSI;
|FPRC;

|PROCESO min;
     #cretam52#0 = 00001;
|FPRC;

|PROCESO max;
     #cretam52#0 = 99999;
|FPRC;

|PROGRAMA;
     |CLS;
     |CABEZA "Autorizaciones TGSS";
     |PINPA #0#0;
     |ABRE #0;
     |LEE 001#0p;
     |SI FSalida ! 0;
          |DDEFECTO #0;
          |ABRE #silcon01;
          |LEE 000#silcon01.p;
          |SI FSalida = 0;
               #0#1 = #silcon01#0;
          |FINSI;
          |GRABA 020#0n;
          |CIERRA #silcon01;
     |FINSI;
     |LEE 101#0=;
     |PINDA #0#0;

     |ENDATOS #1#0;
     |SI FSalida = 0;
          |GRABA 020#0a;
          |SI #0#2 = "S";
               nModo = (FEntrada / 100) ! 0;
               |SI nModo = 1; |O nModo = 2;
                    |ENTLINEAL #1#1, 1, 2, 18, 1, min, max;
               |FINSI;
          |FINSI;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;
