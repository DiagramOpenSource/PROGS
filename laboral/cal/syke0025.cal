|FICHEROS;
     syke0025;      || cabecera
     syke0021,MANTE;      || lineas
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO borralin;
     |BORRA 020#syke0021.a;
     |LIBERA #syke0021;
|FPRC;

|DEFBUCLE borralin;
     #syke0021#1;
     ;
     #syke0025#0, #syke0025#1, "    ", "    ";
     #syke0025#0, #syke0025#1, "zzzz", "zzzz";
     be;
     NULL, borralin;
|FIN;

|PROCESO borra; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |BUCLE borralin;
     |FINSI;
|FPRC;

|PROCESO inferior;
     #syke0021#4 = #syke0025#0;
     #syke0021#5 = #syke0025#1;
     #syke0021#0 = "    ";
     #syke0021#1 = "    ";
|FPRC;

|PROCESO superior;
     #syke0021#4 = #syke0025#0;
     #syke0021#5 = #syke0025#1;
     #syke0021#0 = "zzzz";
     #syke0021#1 = "zzzz";
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     |ENTLINEAL #1#syke0021, -1, 2, 22, 0, inferior, superior;
|FPRC;

|PROCESO Tipo13; |TIPO 13;
     |ENTLINEAL #1#syke0021, -1, 7, 22, 0, inferior, superior;
|FPRC;
