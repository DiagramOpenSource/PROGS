|FICHEROS;                    || OJO MIGUE. LO QUE CAMBIES AQUI ACUERDATE
     nompasef #0;             || DEL DSPASE00 DE LA BASICA

     copcentr #500;      || centros
     labcentr #501;

     copcontr #510;      || control de la aplicacion contratos
     labcontr #511;

     coplitip #512;      || centros
     lablitip #513;

     nomyrenm #531;
     nomirenm #532;

     labtraba;
     labpadre;
     nomconax;
|FIN;

|VARIABLES;
     aDefOrigen = "";
     aMasOrigen = "";
     aFichOrigen = "";
     aDefDestino = "";
     aMasDestino = "";
     aFichDestino = "";

     aAlfa = "";
     aAlfa1 = "";
     aAlfa2 = "";
     swError = 0;
     nCampo = 0;
     nCampo1 = 0;
     aError = "";
     nCentros = 0;
     aDef = "";
     &eaPathLaboral = "";
     &eaPathAntes = "";
     aParam = "";
|FIN;

|PROCESO nomconax;
     |SI #nomconax#95 = 10;
          #nomconax#95 = 11;
          |GRABA 020#nomconax.a;
          |LIBERA #nomconax;
     |FINSI;
|FPRC;

|DEFBUCLE nomconax;
     #nomconax#1;
     ;
     001;
     999;
     be;
     NULL, nomconax;
|FIN;

|PROCESO labpadre;
     #labtraba#0 = #labpadre#0;
     #labtraba#1 = #labpadre#1;
     |LEE 101#labtraba.=;
     |SI FSalida = 0;
          #labtraba#249 = #labpadre#2;
          #labtraba#250 = #labpadre#3;
          #labtraba#251 = #labpadre#4;
          #labtraba#252 = #labpadre#5;
          |GRABA 020#labtraba.a;
          |LIBERA #labtraba;
     |FINSI;
|FPRC;

|DEFBUCLE labpadre;
     #labpadre#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, labpadre;
|FIN;

|PROCESO DatosPer;
     |ABRE #labtraba;
     |BUCLE labpadre;
     |CIERRA #labtraba;
|FPRC;

|PROCESO AnyadeBarra;
     |QBF aAlfa1;
     aAlfa2 = aAlfa1 % -101;
     |SI aAlfa2 ! "/";
          aAlfa1 = aAlfa1 + "/";
     |FINSI;
|FPRC;

|PROCESO copiafich;
     aFichOrigen = #0#1;
     |QBF aFichOrigen;
     aFichOrigen = aFichOrigen + aDef + ".dat";

     aFichDestino = #0#3;
     |QBF aFichDestino;
     aFichDestino = aFichDestino + "cop" + (aDef % -105) + ".dat";

     |COPIA_FICHERO aFichOrigen, aFichDestino;

     swError = 0;
     |SI FSalida ! 0;
          swError = 1;
     |FINSI;

     |SI swError = 1;
          aError = "    Error en la copia de fichero: " + aDef;
          aError = aError + ". Revise las rutas especificadas.";
          |MENAV aError;
          |ACABA;
     |FINSI;
|FPRC;

|PROCESO resumenes;
     |DELINDEX #531;
     |DELINDEX #532;
|FPRC;

|PROCESO gomcotiz;
     aFichOrigen = #0#3;
     |QBF aFichOrigen;
     aFichOrigen = aFichOrigen + aDef + ".ddd";

     aFichDestino = #0#3;
     |QBF aFichDestino;
     aFichDestino = aFichDestino + aDef + ".dat";

     |COPIA_FICHERO aFichOrigen, aFichDestino;

     aFichOrigen = #0#3;
     |QBF aFichOrigen;
     aFichOrigen = aFichOrigen + aDef + ".xxx";

     aFichDestino = #0#3;
     |QBF aFichDestino;
     aFichDestino = aFichDestino + aDef + ".ddx";

     |COPIA_FICHERO aFichOrigen, aFichDestino;

     aFichOrigen = #0#3;
     |QBF aFichOrigen;
     aFichOrigen = aFichOrigen + aDef + ".iii";

     aFichDestino = #0#3;
     |QBF aFichDestino;
     aFichDestino = aFichDestino + aDef + ".ixx";

     |COPIA_FICHERO aFichOrigen, aFichDestino;
|FPRC;

|PROCESO coplabcontr;
     |PARA nCampo = 0; |SI nCampo [ 86; |HACIENDO nCampo = nCampo + 1;
          #511nCampo = #510nCampo;
     |SIGUE;

     || defecto de ciertos campos
     #labcontr#6 = 0;
     #labcontr#56 = 1;
     #labcontr#60 = "S";
     #labcontr#74 = 00;
     #labcontr#75 = "N";
     #labcontr#76 = "S";
     #labcontr#79 = 1;
     #labcontr#80 = "N";
     #labcontr#83 = "N";
     |SI #labcontr#37 > 1;
          #labcontr#37 = 1;
     |FINSI;

     |GRABA 020#511n;
|FPRC;

|DEFBUCLE copcontr;
     #510#1;
     ;
     1;
     1;
     ;
     NULL, coplabcontr;
|FIN;

|PROCESO labcontr;
     |HAZ copiafich;

     |SI swError ! 0;
          |ACABA;
     |FINSI;

     |DELINDEX #511;

     |ABRE #510;
     |ABRE #511;

     |BUCLE copcontr;

     |CIERRA #511;
     |CIERRA #510;
|FPRC;

|PROCESO coplablitip;
     |PARA nCampo = 0; |SI nCampo [ 11; |HACIENDO nCampo = nCampo + 1;
          #513nCampo = #512nCampo;
     |SIGUE;
     aAlfa = #512#12;
     aAlfa = aAlfa % 101;
     #513#12 = aAlfa;
     |PARA nCampo = 13; |SI nCampo [ 16; |HACIENDO nCampo = nCampo + 1;
          #513nCampo = #512nCampo;
     |SIGUE;
     |GRABA 020#513n;
|FPRC;

|DEFBUCLE lablitip;
     #512#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, coplablitip;
|FIN;

|PROCESO lablitip;
     |HAZ copiafich;

     |SI swError ! 0;
          |ACABA;
     |FINSI;

     |DELINDEX #513;

     |ABRE #512;
     |ABRE #513;

     |BUCLE lablitip;

     |CIERRA #513;
     |CIERRA #512;
|FPRC;

|PROCESO coplabcentr;
     |PARA nCampo = 0; |SI nCampo [ 14; |HACIENDO nCampo = nCampo + 1;
          #501nCampo = #500nCampo;
     |SIGUE;

     #501#22 = #500#15;
     #501#23 = #500#16;

     |PARA nCampo = 24; |SI nCampo [ 33; |HACIENDO nCampo = nCampo + 1;
          #501nCampo = #500nCampo;
     |SIGUE;

     |GRABA 020#501n;
     nCentros = nCentros + 1;
|FPRC;

|DEFBUCLE copcentr;
     #500#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, coplabcentr;
|FIN;

|PROCESO labcentr;
     |HAZ copiafich;

     |SI swError ! 0;
          |ACABA;
     |FINSI;

     |DELINDEX #501;

     |ABRE #500;
     |ABRE #501;

     |BUCLE copcentr;

     |CIERRA #501;
     |CIERRA #500;
|FPRC;

|PROCESO ElProceso;
     aAlfa1 = #0#0;
     |HAZ AnyadeBarra;
     aDefOrigen = aAlfa1;

     aAlfa1 = #0#1;
     |HAZ AnyadeBarra;
     aFichOrigen = aAlfa1;

     aAlfa1 = #0#2;
     |HAZ AnyadeBarra;
     aDefDestino = aAlfa1;

     aAlfa1 = #0#3;
     |HAZ AnyadeBarra;
     aFichDestino = aAlfa1;

     aDef = "labcentr";
     |HAZ labcentr;

     aDef = "lablitip";
     |HAZ lablitip;

     aDef = "labcontr";
     |HAZ labcontr;

     aDef = "gomcotiz";
     |HAZ gomcotiz;

     |HAZ resumenes;

     |HAZ DatosPer;      || traspaso de Datos Complmentarios del labpadre
                         || al labtraba

     |BUCLE nomconax;    || en los % de antiguedad por a¤os, el tipo 10 de
                         || enom, es el tipo 11 de iplabor2, en su momento
                         || se hizo mal
|FPRC;

|PROGRAMA;
     aParam = PARAMETRO;
     |QBF aParam;

     |SI aParam = "";
          |CLS;

          aAlfa = "    ATENCION. Este paso NO debe realizarse si se ha hecho ";
          aAlfa = aAlfa + "el pase desde aginom a PI."
          |MENAV aAlfa;
          aAlfa = "    Si ha sido así el fichero de centros y el resto ya "
          aAlfa = aAlfa + "deben estar actualizados.";
          |MENAV aAlfa;

          |DBASE aDefOrigen;
          #0#0 = aDefOrigen;
          aDefOrigen = aDefOrigen + "def/";
          |DFICE aFichOrigen;
          #0#1 = aFichOrigen;

          |PINPA #0#0;
          |PINDA #0#0;

          |ENDATOS #1#0;

          #0#2 = #0#0;        || Destino = Origen
          #0#3 = #0#1;
     |SINO;
          #0#0 = eaPathAntes + "aginom/";
          #0#1 = eaPathAntes + "aginom/fich/";

          #0#2 = eaPathLaboral;
          #0#3 = eaPathLaboral + "fich/";
          FSalida = 0;
     |FINSI;
     |SI FSalida = 0;
          |HAZ ElProceso;
          ||SI swError ! 0;
          |SI aParam = "";
               aAlfa1 = nCentros;
               |QBF aAlfa1;
               aAlfa1 = "    Realizado con éxito el traspaso de " + aAlfa1 + " centros.";
               |MENAV aAlfa1;
          |FINSI;
          ||FINSI;
          |HAZ DatosPer;
          |HAZ nomconax;
     |FINSI;
|FPRO;
