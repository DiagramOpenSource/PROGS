|PROCESO abre_inforH;
|INFOR informe;
|HAZ desglosa;
    cabeza = "-RELACION DE TRANSFERENCIAS. PERIODO: ";
     deany = deany + 2000;
     aDeany = deany;

     haany = haany + 2000;
     aHaany = haany;
|SI #0#17 = 1;      || separado
        lmes = demes;    ||HAZ lite_mes
        cabeza = cabeza + p_mes + "." + aDeany + "-";
|SINO;              || refundido
    |SI #0#16 = #0#4;|Y #0#15 = #0#3;
            lmes = demes;    ||HAZ lite_mes
            cabeza = cabeza + p_mes + "." + aDeany + "-";
    |SINO;
            lmes = demes;    |HAZ lite_mes;
            cabeza = cabeza + "DE " + p_mes + "." + aDeany + " A ";
            lmes = hames;    |HAZ lite_mes;
            cabeza = cabeza + p_mes + "." + aHaany + "-";
    |FINSI;
|FINSI;

|SI #0#1 = "P";
        cabeza = cabeza + " (PAGAS EXTRAS)";
|FINSI;

    alfa1 = " " * 85;
    cabeza = (cabeza + alfa1) % 185;
|FPRC;



|PROCESO rup_empresH;
|SI #0#11 ! 1;|O #0#18 ! "S";
    |SI #0#11 ! 4;|O #0#19 ! "S";
        |DDEFECTO #1;
            #1#0 = ant_empres;
        |LEE 000#1=;
            alfa1 = #1#2;   |QBF alfa1;
            alfa1 = (alfa1 + " .......................................") % 140;
            des_empres = alfa1;

            swlin = 3;
        |PRINT;
            swlin = 0;

        |HAZ leeempreH;
    |FINSI;
|FINSI;
    ant_empres = #2#0;
    tot_empres = 0;

|PIEINF;
|FININF;
|FPRC;

|PROCESO rup_bancoRealH;
|SI #0#11 = 5;
