     des_bancos = alfa1;

    swlin = 4;
    |PRINT;
    swlin = 0;
|FINSI;
    ant_bancoReal = #2#72 % 104;
    tot_bancoReal = 0;
|FPRC;

|PROCESO rup_bancosH;
|SI #0#11 = 1;
    |HAZ rup_trabaj;

     enCodBanco = ant_bancos;
     |HAZ LeeBancos;

     alfa1 = #709#1;   |QBF alfa1;
     alfa1 = (alfa1 + " .......................................") % 140;
     des_bancos = alfa1;

    swlin = 2;
    |PRINT;
    swlin = 0;
|FINSI;
    ant_bancos = #2#71;
    tot_bancos = 0;
|FPRC;

|PROCESO rup_centroH;
|SI #0#11 = 4;
    |HAZ rup_trabaj;

    |DDEFECTO #5;
        #5#0 = ant_empres;
        #5#1 = ant_centro;
    |LEE 000#5=;
        alfa1 = #5#2;   |QBF alfa1;
        alfa1 = (alfa1 + " .......................................") % 140;
        des_centro = alfa1;

        swlin = 1;
    |PRINT;
        swlin = 0;
|FINSI;
    ant_centro = #2#77;
    tot_centro = 0;
|FPRC;

|PROCESO rup_trabaj;
||SI ant_trabaj ! #2#1;
|SI ant_trabaj ! #2#1; |Y ant_trabaj ! 0;
    |PRINT;
        ant_trabaj = #2#1;
        tot_trabaj = 0;
|FINSI;
|FPRC;

|PROCESO validoH;
    seleccio = 1;

|SI ant_empres = -1;
        ant_empres = #2#0;
        ant_centro = #2#77;
        ant_bancos = #2#71;
        ant_bancoReal = #2#72 % 104;
        ant_trabaj = #2#1;
    |HAZ abre_inforH;
|FINSI;

|SI ant_empres ! #2#0;
    |HAZ rup_trabaj;
    |HAZ rup_bancoRealH;
    |HAZ rup_bancosH;
    |HAZ rup_centroH;
    |HAZ rup_empresH;
    |HAZ abre_inforH;
|SINO;
    |SI ant_centro ! #2#77;
        |HAZ rup_centroH;
        |SI #0#19 = "S";
            |PIEINF;
            |FININF;
            |HAZ abre_inforH;
        |FINSI;
    |FINSI;
    aAlfa = #2#72 % 104;
    |SI ant_bancoReal ! aAlfa;
        |HAZ rup_bancoRealH;
        |SI #0#18 = "S";
            |PIEINF;
            |FININF;
            |HAZ abre_inforH;
        |FINSI;
    |FINSI;
    |SI ant_bancos ! #2#71;
        |HAZ rup_bancosH;
        |SI #0#18 = "S";
            |PIEINF;
            |FININF;
            |HAZ abre_inforH;
        |FINSI;
    |FINSI;
    |SI ant_trabaj ! #2#1;
        |HAZ rup_trabaj;
    |FINSI;
|FINSI;

|SI #0#1 = "M";  |HAZ mensualH;  |FINSI;
|SI #0#1 = "P";  |HAZ pagasH;    |FINSI;
    liquido = devenga - deducir;

    tot_trabaj = tot_trabaj + liquido;
    tot_bancos = tot_bancos + liquido;
    tot_bancoReal = tot_bancoReal + liquido;
    tot_centro = tot_centro + liquido;
    tot_empres = tot_empres + liquido;
