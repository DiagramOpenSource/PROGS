|FICHEROS;
     marin007 #0;
     marin005;
     labempre;
     labtraba;
|FIN;

|VARIABLES;
     aFechaDesde = "";
     aFechaHasta = "";
     fFechaDesde = @;
     fFechaHasta = @;

     aFechaVto = "";
     fFechaVto = @;
     &aFecha = "";
     swPrint = 0;
     &aCursos = "";
     swHayAlgo = 0;
|FIN;

|PROCESO CompFecha;
     aFecha = #0Campo;
     |HAZ CompFecha_plb;
|FPRC;

|PROCESO marin005;
     #labempre#0 = #marin005#0;
     |LEE 000#labempre.=;

     #labtraba#0 = #marin005#0;
     #labtraba#1 = #marin005#1;
     |LEE 000#labtraba.=;

     |SI #0#4 = "X";
          |SI #marin005#8 ! "X";
               |ACABA;
          |FINSI;
     |FINSI;

     |SI #0#5 = "X";
          |SI #marin005#9 ! "X";
               |ACABA;
          |FINSI;
     |FINSI;

     |SI #0#6 = "X";
          |SI #marin005#10 ! "X";
               |ACABA;
          |FINSI;
     |FINSI;

     |SI #0#4 = " "; |Y #0#5 = " "; |Y #0#6 = " ";
          |SI #marin005#8 = " "; |Y #marin005#9 = " "; |Y #marin005#10 = " ";
               |PRINT;
               swHayAlgo = 1;
          |FINSI;
     |SINO;
          |PRINT;
          swHayAlgo = 1;
     |FINSI;
|FPRC;

|DEFBUCLE marin005;
     #marin005#1;
     ;
     #0#0, #0#2;
     #0#1, #0#3;
     ;
     NULL, marin005;
|FIN;

|PROGRAMA;
     |CLS;

     |ABRE #0;
     |LEE 101#0p;
     |SI FSalida ! 0;
         |DDEFECTO #0;
         |GRABA 020#0n;
     |FINSI;

     |PINPA #0#0;
     |LEE 101#0=;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          Impresora = "CrystalReport";
          |IMPRE -1;
          |INFOR "marin007";

          |ABRE #labempre;
          |ABRE #labtraba;
          |ABRE #marin005;

          aFechaDesde = #0#4;
          aFechaHasta = #0#5;
          fFechaDesde = aFechaDesde;
          fFechaHasta = aFechaHasta;
          swHayAlgo = 0;

          |SI #0#4 = "X";
               aCursos = "el curso de 8 horas";
          |FINSI;

          |SI #0#5 = "X";
               |SI #0#4 = "X";
                    aCursos = "el curso de 8 horas y el de 20 horas.";
               |SINO;
                    aCursos = "el curso de 20 horas.";
               |FINSI;
          |FINSI;

          |SI #0#6 = "X";
               |SI #0#4 = "X"; |Y #0#5 = "X";
                    aCursos = "el curso de 8 horas, el de 20 horas y el de 60 horas.";
               |SINO;
                    |SI #0#4 = "X";
                         aCursos = "el curso de 8 horas y el de 60 horas.";
                    |FINSI;
                    |SI #0#5 = "X";
                         aCursos = "el curso de 20 horas y el de 60 horas.";
                    |FINSI;
                    |SI #0#4 = " "; |Y #0#5 = " ";
                         aCursos = "el curso de 60 horas.";
                    |FINSI;
               |FINSI;
          |FINSI;

          |BUCLE marin005;

          |SI swHayAlgo = 0;
               |MENAV "    ATENCION: no se han encontrado trabajadores para la selección realizada";
          |FINSI;
          |PIEINF;
          |FININF;
          |FINIMP;

          |GRABA 020#0a;

          |CIERRA #marin005;
          |CIERRA #labtraba;
          |CIERRA #labempre;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;
