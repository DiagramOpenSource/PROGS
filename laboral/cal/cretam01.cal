|FICHEROS;
     cretam01 #1;
     cretam02 #2,MANTE;
     cretam03 #3,MANTE;
     cretam04 #4,MANTE;

     labtraba;
|FIN;

|VARIABLES;
     fFechaHoy = @;
     nNume = 0;
     nModo = 0;
     &swAlgo = 0;
     aAlfa = "";
     nDia = 0;
     nMes = 0;
     nAny = 0;
     aDia = "";
     aMes = "";
     aAny = "";
     fFecha = @;
     swMes99 = 0;
|FIN;

|PROCESO Defectos; |TIPO 7;
     fFechaHoy = ~;

     aAlfa = fFechaHoy;
     aAlfa = aAlfa % -104;
     nNume = aAlfa;
     #1#1 = nNume;       || el a쨚
     |PINTA #1#1;

     aAlfa = fFechaHoy;
     aAlfa = aAlfa % 402;
     nNume = aAlfa;
     nNume = nNume - 1;
     |SI nNume = 0;
          nNume = 12;
          #1#1 = #1#1 - 1;
          |PINTA #1#1;
     |FINSI;
     #1#2 = nNume;       || el mes
     |PINTA #1#2;
|FPRC;

|PROCESO Borra_04_m01;
     |BORRA 020#4a;
|FPRC;

|DEFBUCLE Borra_04_m01;
     #4#1;
     ;
     #1#0, #1#1, #1#2, #1#3, "               ", "            ", 01;
     #1#0, #1#1, #1#2, #1#3, "zzzzzzzzzzzzzzz", "zzzzzzzzzzzz", 99;
     ;
     NULL, Borra_04_m01;
|FIN;

|PROCESO Borra_03_m01;
     |BORRA 020#3a;
|FPRC;

|DEFBUCLE Borra_03_m01;
     #3#1;
     ;
     #1#0, #1#1, #1#2, #1#3, "               ", "            ";
     #1#0, #1#1, #1#2, #1#3, "zzzzzzzzzzzzzzz", "zzzzzzzzzzzz";
     ;
     NULL, Borra_03_m01;
|FIN;

|PROCESO Borra_02_m01;
     |BORRA 020#2a;
|FPRC;

|DEFBUCLE Borra_02_m01;
     #2#1;
     ;
     #1#0, #1#1, #1#2, #1#3, "               ";
     #1#0, #1#1, #1#2, #1#3, "zzzzzzzzzzzzzzz";
     ;
     NULL, Borra_02_m01;
|FIN;

|PROCESO Tipo2_m01; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |BUCLE Borra_02_m01;
          |BUCLE Borra_03_m01;
          |BUCLE Borra_04_m01;
     |FINSI;
|FPRC;

|PROCESO inferior_02_m01;
     #2#0 = #1#0;
     #2#1 = #1#1;
     #2#2 = #1#2;
     #2#3 = #1#3;
     #2#4 = "               ";
|FPRC;

|PROCESO superior_02_m01;
     #2#0 = #1#0;
     #2#1 = #1#1;
     #2#2 = #1#2;
     #2#3 = #1#3;
     #2#4 = "zzzzzzzzzzzzzzz";
|FPRC;

|PROCESO Muestra_m01; |TIPO 14;
     || muestra siempre el valor de lo que se introduzca en la cabecera

     |ENTLINEAL #1#2, -5, 7, 22, 0, inferior_02_m01, superior_02_m01;

     |SI #1#3 < 5;
          |PINTA 0936, "                                 ";
          |PINTA 1036, "컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴켸";
          |PINTA 1136, "컴컴컴컴컴컴컴컴컴쩡컴컴컴컴컫커";
     |SINO;
          || ATRASOS
          |PINTA 0936, " Periodo:   -        FC:        ";
          |PINTA 0946, #2#17;
          |PINTA 0949, #2#18;
          |PINTA 0952, #2#19;
          |PINTA 0961, #2#20;
          |PINTA 0964, #2#21;
          |PINTA 1136, "컴컴컴컴컴컴컴컴컴쩡컴컴컴컴컫커";
     |FINSI;

     |ATRI I;
|FPRC;

|PROCESO Tipo3_m01; |TIPO 3;
     || llama al lineal que le corresponde

     |ENTLINEAL #1#2, -5, 4, 22, 0, inferior_02_m01, superior_02_m01;
     |PTEC 806;
|FPRC;

|PROCESO Fechas;
     swMes99 = 0;
     nDia = #cretam04#6;
     |SI nDia = 99;
          swMes99 = 1;
          nDia = 01;
     |FINSI;
     aDia = nDia;
     |QBF aDia;
     aDia = ("00" + aDia) % -102;
     nMes = #cretam04#2;
     |SI swMes99 = 1;
          nMes = nMes + 1;
          |SI nMes = 13;
               swMes99 = 2;
               nMes = 01;
          |FINSI;
     |FINSI;
     aMes = nMes;
     |QBF aMes;
     aMes = ("00" + aMes) % -102;
     nAny = #cretam04#1;
     |SI swMes99 = 2;
          nAny = nAny + 1;
     |FINSI;
     aAny = nAny;
     aAlfa = aDia + "." + aMes + "." + aAny;
     #cretam04#8 = aAlfa;
     |PINTA #cretam04#8;

     nDia = #cretam04#21;
     aDia = nDia;
     |QBF aDia;
     aDia = ("00" + aDia) % -102;
     nMes = #cretam04#2;
     |SI swMes99 = 1;
          nMes = nMes + 1;
     |FINSI;
     |SI swMes99 = 2;
          nMes = 01;
     |FINSI;
     aMes = nMes;
     |QBF aMes;
     aMes = ("00" + aMes) % -102;
     nAny = #cretam04#1;
     |SI swMes99 = 2;
          nAny = nAny + 1;
     |FINSI;
     aAny = nAny;
     aAlfa = aDia + "." + aMes + "." + aAny;
     #cretam04#9 = aAlfa;
     |PINTA #cretam04#9;
|FPRC;

|PROCESO OtrosDatos;
     |ABRE #labtraba;

     #labtraba#0 = #cretam04#0;
     #labtraba#1 = #cretam04#7;
     |LEE 000#labtraba.=;

     #cretam04#29 = #labtraba#2;
     |PINTA #cretam04#29;
     #cretam04#73 = #labtraba#45;

     |CIERRA #labtraba;
|FPRC;

|PROCESO MasBases; |TIPO 7;
     |SI #cretam04#40 ! 0; |Y #cretam04#40 ! 0;
          |CAMPO_MODIFICA #cretam04#40, 1, "S";
          |CAMPO_MODIFICA #cretam04#41, 1, "S";
          |CAMPO_VISUAL #cretam04#40, 1, "S";
          |CAMPO_VISUAL #cretam04#41, 1, "S";
          |PINTA #cretam04#40;
          |PINTA #cretam04#41;
          |PINTA 2135, "Base 6:";
     |SINO;
          |CAMPO_MODIFICA #cretam04#40, 1, "N";
          |CAMPO_MODIFICA #cretam04#41, 1, "N";
          |CAMPO_VISUAL #cretam04#40, 1, "N";
          |CAMPO_VISUAL #cretam04#41, 1, "N";
          |PINTA 2135, "                            ";
     |FINSI;
|FPRC;
