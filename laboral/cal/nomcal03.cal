     |SI swError = 2;
          |MENAV "    ATENCION. Error de introducción de datos. Revise el número de minutos";
          |ERROR;
          |ACABA;
     |FINSI;

     aAlfa = aHorasSalida + ":" + aMinutosSalida;
     #1Campo = aAlfa;
     |PINTA #1Campo;
|FPRC;

