|PROCESO Cambia;
     swSigue = 0;
     nModo = (FEntrada / 100) ! 0;
     aAlfa = Contenido;
     |SI #0Campo ! aAlfa; |Y #0Campo ! 0;
          swSigue = 1;
     |FINSI;
     |SI nModo = 1;
          swSigue = 1;
     |FINSI;
     |SI swSigue = 1;
          aAlfa = "     Desea rellenar este  calendario segun los dias no ";
          aAlfa = aAlfa + "laborales del Manteniento de Dias Festivos?";
          |CONFI aAlfa;
          |SI FSalida = 0;
               |HAZ PonFiestas;
          |FINSI;
     |FINSI;
|FPRC;

