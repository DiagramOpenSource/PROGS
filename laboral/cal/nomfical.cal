|PROCESO BuscaPob;
     swExiste = 1;
     |ERROR10;
|FPRC;

|DEFBUCLE BuscaPob;
     #labmunic#3;
     ;
     aCodPobla, "00000";
     aCodPobla, "99999";
     ;
     NULL, BuscaPob;
|FIN;

|PROCESO Poblacion;
     swExiste = 0;
     aCodPobla = #0#159;
     aCodPobla = "00000" + aCodPobla;
     aCodPobla = aCodPobla % -105;

     |BUCLE BuscaPob;
     |SI swExiste  = 1; |Y #0#159 ! 0;
          #0#159 = #labmunic#1;
          |PINTA #0#159;
          || #0#160 = #labmunic#4;
          || PINTA #0#160;
          aAlfa = #labmunic#3; |QBF aAlfa;
          aAlfa1 = #labmunic#2; |QBF aAlfa1;
          #0#161 = aAlfa + " (" + aAlfa1 + ")";
          |PINTA #0#161;
     |SINO;
          |SI #0#159 ! 0;
               aAlfa = "    Municipio no encontrado en el fichero general";
               aAlfa = aAlfa + " de Municipios.";
               |SI swNoMensa = 0;
                    |MENAV aAlfa;
                    |ERROR;
               |FINSI;
          |SINO;
               #0#161 = "No asignado a municipio";
               |PINTA #0#161;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO PintaPob; |TIPO 13;
     || para los que ya lo tuvieran introducidos
     aAlfa = #0#161;
     |QBF aAlfa;
     |SI #0#159 ! 0; |Y aAlfa = "";
          swNoMensa = 1;
          |HAZ Poblacion;
          swNoMensa = 0;
     |FINSI;
|FPRC;

|PROCESO BuscaCodPostal;
     swExiste = 1;
     |ERROR10;
|FPRC;

|DEFBUCLE BuscaCodPostal;
     #labmunic#1;
     ;
     aCodPostal, aDesdeMuni;
     aCodPostal, aHastaMuni;
     ;
     NULL, BuscaCodPostal;
|FIN;

|PROCESO CodigoPostal;
     swExiste = 0;
     aCodPostal = #0#160;
     aCodPostal = "00000" + aCodPostal;
     aCodPostal = aCodPostal % -105;

     aCodMuni = #0#159;
     aCodMuni = "00000" + aCodMuni;
     aCodMuni = aCodMuni % -105;

     |SI #0#159 = 0;
          aDesdeMuni = "00000";
          aHastaMuni = "99999";
     |SINO;
          |SI #0Campo = Contenido;
               aDesdeMuni = aCodMuni;
               aHastaMuni = aCodMuni;
          |SINO;
               aDesdeMuni = "00000";
               aHastaMuni = "99999";
          |FINSI;
     |FINSI;

     |BUCLE BuscaCodPostal;

     |SI swExiste = 1; |Y #0#160 ! 0;
          #0#159 = #labmunic#1;
          |PINTA #0#159;
          #0#160 = #labmunic#4;
          |PINTA #0#160;
          aAlfa = #labmunic#3; |QBF aAlfa;
          aAlfa1 = #labmunic#2; |QBF aAlfa1;
          #0#161 = aAlfa + " (" + aAlfa1 + ")";
          |PINTA #0#161;
     |SINO;
          |SI #0#160 ! 0
               aAlfa = "    No se ha encontrado un municipio para este Cod.";
               aAlfa = aAlfa + " Postal en la tabla de Municipios.";
               |MENAV aAlfa;
               |ERROR;
          |SINO;
               |SI #0#159 = 0
                    #0#161 = "No asignado a municipio";
                    |PINTA #0#161;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO minimo;
     #labmunic#4 = "00000";
     #labmunic#1 = "00000";
|FPRC;

|PROCESO maximo;
     #labmunic#4 = "99999";
     #labmunic#1 = "99999";
|FPRC;

|PROCESO Municipios; |TIPO 66;
     |ABRE #labmunic;
     |CONSULTA_F_CLAVE #1#labmunic, minimo, maximo;
     |SI FSalida = 0;
          #0#159 = #labmunic#1;
          |PINTA #0#159;
          #0#160 = #labmunic#4;
          |PINTA #0#160;
          aAlfa = #labmunic#3; |QBF aAlfa;
          aAlfa1 = #labmunic#2; |QBF aAlfa1;
          #0#161 = aAlfa + " (" + aAlfa1 + ")";
          |PINTA #0#161;
          |ERROR;
     |FINSI;
     |CIERRA #labmunic;
|FPRC;

|PROCESO MunicipiosF5;
     |SI FSalida = 13;
          |HAZ Municipios;
     |FINSI;
|FPRC;

