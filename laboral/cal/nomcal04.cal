|FICHEROS;
     nomcal04;
|FIN;

|VARIABLES;
     nTDMes = 0;
     nMes = 0;
     nAny = 0;
|FIN;

|PROCESO TopeDias;
     nTDMes = 31;
     |SI nMes = 4; |O nMes = 6; |O nMes = 9; |O nMes = 11;
          nTDMes = 30;
     |SINO;
          |SI nMes = 2;
               nTDMes = 28;
               |SI nAny = 1992; |O nAny = 1996; |O nAny = 2000;
               |O nAny = 2004; |O nAny = 2008; |O nAny = 2012;
               |O nAny = 2016; |O nAny = 2020; |O nAny = 2024;
                    nTDMes = 29;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Error31;
     nAny = #nomcal04#2;
     nMes = #nomcal04#3;
     |HAZ TopeDias;
     |SI #nomcal04#4 > nTDMes;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO NoEntres;
     |SI #nomcal04#12 = 1;
          |ERROR;
     |FINSI;
|FPRC;
