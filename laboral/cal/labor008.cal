|FICHEROS;
     labor008;
     usuarios@;
|FIN;

|VARIABLES;
     aMas            = "";

     &eaPathIntegral = "";
     &eaPathBasica   = "";
|FIN;

|PROCESO Tipo7C1Fr8;  |TIPO 7;
     |HAZ BusquedaApli;

     |SI eaPathIntegral ! "";
         aMas = eaPathIntegral + "def/dsam0108.mas";
     |SINO;
         aMas = eaPathBasica + "def/agiusuar.mas";
     |FINSI;
|FPRC;

|PROCESO Tipo66C1Fr8;  |TIPO 66;
     |CARGA_DEF aMas, usuarios@;
     |ABRE #usuarios@;
     #usuarios@#0 = #labor008#1;
     |LEE 000#usuarios@.=;
     |CONSULTA_CLAVE #1#usuarios@;
     |SI FSalida = 0;
         #labor008#1 = #usuarios@#0;
         |PINTA #labor008#1;
     |FINSI;
     |CIERRA #usuarios@;
     |DESCARGA_DEF #usuarios@;

     |ERROR;
|FPRC;

|PROCESO Tipo0C1Fr8;  |TIPO 0;
     |CARGA_DEF aMas, usuarios@;
     |ABRE #usuarios@;
     #usuarios@#0 = #labor008#1;
     |LEE 000#usuarios@.=;
     |SI FSalida ! 0;
         |MENAV "0000El usuario introducido no existe.";
         |ERROR;
     |SINO;
         #labor008#2 = #usuarios@#1;
         |PINTA #labor008#2;
     |FINSI;
     |CIERRA #usuarios@;
     |DESCARGA_DEF #usuarios@;
|FPRC;
