|PROCESO chorradas;
    alfa1 = #1#10;  |QBF alfa1;

    docum1 = alfa1 % 101;
    docum2 = alfa1;
|SI docum1 ] "0";|Y docum1 [ "9";
        docum1 = "";
|SINO;
        docum2 = alfa1 % 2;
|FINSI;

    docum3 = alfa1 % -101;
|SI docum3 ] "0";|Y docum3 [ "9";
        docum3 = "";
|SINO;
        alfa2 = docum2 % 0;   nume1 = alfa2; nume1 = 100 + nume1 - 1;
        docum2 = docum2 % nume1;
|FINSI;
|FPRC;

|PROCESO lee_empresa;
|DDEFECTO #1;
|ABRE #1;
    #1#0 = #0#0;
|LEE 000#1=;
    alfa1 = #1#15;
|QBF alfa1;
|SI alfa1 = "";
        #1#15 = "EL MISMO";
        #1#17 = #1#10;
|FINSI;
|CIERRA #1;
|FPRC;

|PROCESO lee_centro; |TIPO 0;
|ABRE #2;
     #2#0 = #0#0;
     #2#1 = #0#79;
|LEE 000#2=;
|CIERRA #2;
|FPRC;

|PROCESO tipo3; |TIPO 3;
|HAZ r_regis0;   r_emp = #0#0;  |HAZ r_regis1;
|IMPRE 0;
|SI FSalida = 0;
    |HAZ lee_empresa;
    |HAZ chorradas;
    |SI #0#42 = 1;  |INFOR "labinsem";  |FINSI;
    |SI #0#42 = 2;  |INFOR "labinse2";  |FINSI;
    |PRINT;
    |PIEINF;
    |FININF;
|FINSI;
|FINIMP;
|FPRC;
