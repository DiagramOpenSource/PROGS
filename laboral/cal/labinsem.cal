|INCLUYE i_comun;
____________________________________/ CALCULOS DE PANTALLA
|PROCESO modelo; |TIPO 0;
|SI #0#42 = 1; alfa1 = "S"; alfa2 = "N";  |FINSI; || modelo 96
|SI #0#42 = 2; alfa1 = "N"; alfa2 = "S";  |FINSI; || modelo 97

|PARA para1 = 25; |SI para1 [ 40; |HACIENDO para1 = para1 + 1;
    |C_M #0para1, 1, alfa1;        || 96
|SIGUE;
|PARA para1 = 43; |SI para1 [ 76; |HACIENDO para1 = para1 + 1;
    |C_M #0para1, 1, alfa2;        || 97
|SIGUE;
|FPRC;

|PROCESO no77; |TIPO 0;
|SI #0#77 = " ";
