|FICHEROS;
     labtraba #0;
     labprafo;
     nomconca;
|FIN;

|VARIABLES;
     aAlfa = "";
|FIN;

|PROCESO Mensa232_1; |TIPO 7;
     |SI #0#45 = "420"; |O #0#45 = "520";
          aAlfa = "    Porcentaje de salario durante el 1er. a¤o para cont. prácticas (F5-Convenio)";
     |SINO;
          aAlfa = "    Porcentaje de salario durante el 1er. a¤o para contratos de formación";
          |PINTA 1065, "2do./3er.:"
     |FINSI;
     |MENSA aAlfa;
|FPRC;

|PROCESO Mensa233_1; |TIPO 7;
     |SI #0#45 = "420"; |O #0#45 = "520";
          aAlfa = "    Porcentaje de salario durante el 2do. a¤o para contratos en prácticas";
     |SINO;
          aAlfa = "    Porcentaje de salario durante el 2do. y 3er. a¤o para contratos de formación";
     |FINSI;
     |MENSA aAlfa;
|FPRC;

|PROCESO RecConv_1; |TIPO 0;
     |SI #0#45 = "420"; |O #0#45 = "520";
          |SI FSalida = 13;
               |ABRE #nomconca;
               #nomconca#0 = #0#87;
               |LEE 000#nomconca.=;
               |SI FSalida = 0;
                    #labprafo#0 = #nomconca#150;
                    #labprafo#1 = #nomconca#151;
                    |PINTA #labprafo#0;
                    |PINTA #labprafo#1;
                    |ERROR;
               |FINSI;
               |CIERRA #nomconca;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO DefPractica_1; |TIPO 7;
     |SI #labprafo#0 = 0; |Y #labprafo#1 = 0;
          |SI #0#45 = "420"; |O #0#45 = "520";
               |ABRE #nomconca;
               #nomconca#0 = #0#87;
               |LEE 000#nomconca.=;
               |SI FSalida = 0;
                    #labprafo#0 = #nomconca#150;
                    #labprafo#1 = #nomconca#151;
               |FINSI;
               |CIERRA #nomconca;
          |FINSI;
          |SI #0#45 = "421";
               #labprafo#0 = 75;
               #labprafo#1 = 85;
          |FINSI;
          |PINTA #labprafo#0;
          |PINTA #labprafo#1;
     |FINSI;
|FPRC;
