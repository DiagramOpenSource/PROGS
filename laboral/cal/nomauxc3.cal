|FICHEROS;
     nomauxc3 #0;
     nomauxl3;
|FIN;

|VARIABLES;
     nModo = 0;
     nMes = 0;
     nAny = 0;
|FIN;

|PROCESO Copia;
     #nomauxl3#8 = #nomauxc3#3;
     #nomauxl3#9 = #nomauxc3#4;
     |GRABA 020#nomauxl3.n;

     #nomauxc3#2 = #nomauxc3#2 + #nomauxl3#7;
     |PINTA #nomauxc3#2;
|FPRC;

|DEFBUCLE Copia;
     #nomauxl3#1;
     ;
     #nomauxc3#0, #nomauxc3#1, nAny, nMes, 001;
     #nomauxc3#0, #nomauxc3#1, nAny, nMes, 999;
     be;
     NULL, Copia;
|FIN;

|PROCESO CopiaMesAnt;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          nMes = #0#4 - 1;
          |SI nMes = 0;
               nMes = 12;
               nAny = #0#3 - 1;
          |SINO;
               nAny = #0#3;
          |FINSI;

          |BUCLE Copia;
     |FINSI;
|FPRC;
