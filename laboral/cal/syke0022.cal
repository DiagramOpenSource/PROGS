|FICHEROS;
     syke0022;
|FIN;

|VARIABLES;
     nNume = 0;
     nNume1 = 0;
     FEstado = 0;
     nModo = 0;
|FIN;

|PROCESO AltaNo;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |SI FSalida = 0;
               |MENAV "    No se pueden dar de alta conceptos en este lineal";
               |LEE 000#syke0022.u;
               |PONCONTROL 23, "si";
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO inferior;
     #syke0022#0 = 001;
|FPRC;

|PROCESO superior;
     #syke0022#0 = 999;
|FPRC;

|PROGRAMA;
     |CLS;
     |ABRE #syke0022;
     || XXXXXXXXXXXXXXXXXXXXXXX
/*
     |PARA nNume = 20; |SI nNume ] 0; |HACIENDO nNume = nNume - 1;
          nNume1 = nNume + 14;

          #syke0022#0 = nNume1;
          |LEE 000#syke0022.=;
          FEstado = FSalida;

          #syke0022#0 = nNume;
          |LEE 000#syke0022.=;
          #syke0022#0 = nNume1;
          |SI FEstado = 0;
               |GRABA 000#syke0022.a;
          |SINO;
               |GRABA 000#syke0022.n;
          |FINSI;
     |SIGUE;
*/

     |ENTLINEAL #1#syke0022, -1, 2, 22, 1, inferior, superior;
     ||CONSULTA_CLAVE #1#nomboni3;
     |CIERRA #syke0022;
|FPRO;
