|FICHEROS;
     labemett #0;
     nomcnaec;
     sacecotc;
|FIN;

|VARIABLES;
     &eaVarDni = "";
     &enCalcCif = 0;
     aLong = "";
     nLong = 0;
     nPos = 0;
     alfa1 = "";
     alfa2 = "";
     alfa3 = "";
     alfa4 = "";
     aAlfa = "";
     aAlfa1 = "";
     nNume = 0;
     aValor = "";
     nume1 = 0;
     nume2 = 0;
     nume3 = 0;

     swF9 = 0;
     aLon = "";
     nLon = 0;

     nTecla = 0;
     modo = 0;
     aAntLoc = "";
     enPintaLoc = 0;
     aCNAE = "";
     aDesCNAE = "";
     nCampoDes = 0;
     corta = 0;
     jg = 0;
     sitio = 0;

     &nCP1 = 0;
     &nCP2 = 0;
     &eaNomPoblacion = "";
     &eaNomProvincia = "";
|FIN;

|PROCESO letranif_ett; |TIPO 6; || CALCULA LETRA PARA EL NIF CUANDO SE PULSA -F1-
     |SI FSalida = 10;
          eaVarDni  = #0Campo;
          enCalcCif = 1;
          |HAZ CalculoDNI;
          #0Campo = eaVarDni;
          |PINTA #0Campo;
          |ERROR;
     |FINSI;

     eaVarDni  = #0Campo;
     enCalcCif = 0;
     |HAZ CalculoDNI;
     aLong = #0Campo % 0;
     nLong = aLong;
     nPos  = nLong + 100;
     eaVarDni = (eaVarDni + (" " * nLong)) % nPos;
     |SI #0Campo ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #0Campo = eaVarDni;  |PINTA #0Campo;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO comp_reg;
     |SI #0Campo ! 0111; |Y #0Campo ! 0163; |Y #0Campo ! 0000;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO trunca2_ett;
    corta = 0;
    alfa1 = nume2;  alfa2 = alfa1 % 0;  nume3 = alfa2;
|PARA jg = nume3; |SI jg ] 1;|Y corta = 0; |HACIENDO jg = jg - 1;
        sitio = (jg * 100) + 1;
        alfa2 = alfa1 % sitio;
    |SI alfa2 = ".";  corta = jg;  |FINSI;
|SIGUE;
|SI corta ! 0;
        sitio = 100 + corta - 1;
        alfa1 = alfa1 % sitio;
|FINSI;
    nume3 = alfa1;
    nume1 = nume1 - (nume3 * 97);
|FPRC;

|PROCESO trunca3_ett;
    corta = 0;
    alfa1 = nume2;  alfa2 = alfa1 % 0;  nume3 = alfa2;
|PARA jg = nume3; |SI jg ] 1;|Y corta = 0; |HACIENDO jg = jg - 1;
        sitio = (jg * 100) + 1;
        alfa2 = alfa1 % sitio;
    |SI alfa2 = ".";  corta = jg;  |FINSI;
|SIGUE;
|SI corta ! 0;
        sitio = 100 + corta - 1;
        alfa1 = alfa1 % sitio;
|FINSI;
    nume3 = alfa1;
    nume1 = nume1 - (nume3 * 97);
|FPRC;

|PROCESO ValidaNAF; |TIPO 0;
     alfa1 = #0Campo;
     |QBF alfa1;

     aAlfa = alfa1 % 0;            || si mide mas de 10 digitos y me piden
     nNume = aAlfa;                || que calcule el digito de control
     |SI nNume = 12;               || quito el que me hubieran introducido
          alfa1 = alfa1 % 110;
     |FINSI;

     aValor = alfa1;

     alfa2 = alfa1 % 102; alfa3 = alfa1 % -108;
     alfa4 = alfa3 % 101;
     |SI alfa4 = "0";
          alfa3 = alfa3 % 207;
          alfa1 = alfa2 + alfa3;
     |FINSI;
     nume1 = alfa1;
     nume2 = nume1 / 97;
     |HAZ trunca2_ett;
     |SI nume1 ] 0; |Y nume1 [ 99;
          alfa1 = aValor;
          |QBF alfa1;
          alfa2 = nume1;
          alfa2 = "00" + alfa2;
          alfa2 = alfa2 % -102;
          alfa1 = alfa1 + alfa2;
     |FINSI;

     alfa2 = #0Campo;
     |QBF alfa2;

     |SI alfa2 ! alfa1;
          |SI swF9 = 1;
               #0Campo = alfa1;
               |PINTA #0Campo;
          |SINO;
               |MENAV "    ATENCION. Error en la validación. El NAF introducido es incorrecto";
               |ERROR;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO control3_ett; |TIPO 0;
     |SI FSalida = 9;
          aAlfa = #0Campo;
          |QBF aAlfa;
          aLon = aAlfa % 0;
          nLon = aLon;
          |SI nLon = 9; |O nLon = 11;
               |HAZ CompCCC_2;
          |FINSI;
          |SI nLon = 10; |O nLon = 12;
               swF9 = 1;
               |HAZ ValidaNAF;
               swF9 = 0;
               #0#4 = 0000;
               |PINTA #0#4;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO CompCCC_ett; |TIPO 0;
     aAlfa = #0Campo;
     |QBF aAlfa;
     |SI aAlfa = "";
          |ACABA;
     |FINSI;

     aLon = aAlfa % 0;
     nLon = aLon;
     |SI nLon ! 11; |Y nLon ! 12;
          aAlfa = "    Número de caracteres incorrecto. :   ";
          aAlfa = aAlfa + "CCC (11): Prov. (2) + Núm. (7) + DC (2) ";
          aAlfa = aAlfa + "NAF (12): Prov. (2) + Núm. (8) + DC (2)";
          |MENAV aAlfa;
          |ERROR;
     |SINO;
          |SI nLon = 11;
               |HAZ ValidaCCC_ett;
          |FINSI;
          |SI nLon = 12;
               |HAZ ValidaNAF;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO nifletra; |TIPO 6;  || CALCULA LETRA NIF CUANDO SE PULSA -F1-
     nTecla = FSalida;
     |SI FSalida = 10;
         eaVarDni  = #0Campo;
         enCalcCif = 1;
         |HAZ CalculoDNI;
         #0Campo = eaVarDni;
         |PINTA #0Campo;
         |ERROR;
         |ACABA;
     |FINSI;

     eaVarDni  = #0Campo;
     enCalcCif = 0;
     |HAZ CalculoDNI;
     |SI Campo = 26;
          eaVarDni = (eaVarDni + "               ") % 115;
     |SINO;
          eaVarDni = (eaVarDni + "             ") % 112;
     |FINSI;
     |SI #0Campo ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #0Campo = eaVarDni;  |PINTA #0Campo;
             |ERROR;
             |ACABA;
         |FINSI;
     |FINSI;

     modo = (FEntrada / 100) ! 0;
     alfa1 = #0Campo;
     |QBT alfa1;

     |SI alfa1 = ""; |Y modo ! 4;
         ||MENAV "    ATENCION. Falta el DNI del representante";
     |FINSI;
|FPRC;

|PROCESO PintaLoc;
     nCP1 = #0#13;
     nCP2 = #0#14;
     |HAZ LeeLocalidad;
     #0#12 = eaNomPoblacion;
     #0#15 = eaNomProvincia;
     |PINTA #0#12;
     |PINTA #0#15;
|FPRC;

|PROCESO PintaLoc1; |TIPO 7;
     aAntLoc = #0Campo;
|FPRC;

|PROCESO PintaLoc2;
     enPintaLoc = 0;
     aAlfa = #0#6; |QBF aAlfa;
     |SI #0Campo ! aAntLoc; |O aAlfa = "";
          enPintaLoc = 1;
     |FINSI;
|FPRC;

|PROCESO ValidaCCC_ett;
     alfa1 = #0Campo;
     |QBF alfa1;

     aLon = alfa1 % 0;
     nLon = aLon;
     |SI nLon = 11;
          alfa1 = alfa1 % 109;
     |FINSI;

     aValor = alfa1;

     alfa2 = alfa1 % 102; alfa3 = alfa1 % -107;
     alfa4 = alfa3 % 101;
     |SI alfa4 = "0";
          alfa3 = alfa3 % 206;
          alfa1 = alfa2 + alfa3;
     |FINSI;
     nume1 = alfa1;
     nume2 = nume1 / 97;
     |HAZ trunca3_ett;
     |SI nume1 ] 0; |Y nume1 [ 99;
          alfa1 = aValor;
          |QBF alfa1;
          alfa2 = nume1;
          alfa2 = "00" + alfa2;
          alfa2 = alfa2 % -102;
          alfa1 = alfa1 + alfa2;
    |FINSI;

     alfa2 = #0Campo;
     |QBF alfa2;

     |SI alfa2 ! alfa1;
          |MENAV "    ATENCION. Error en la validación. El CCC introducido es incorrecto";
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO CompCCC_2;
     aValor = #0Campo;
     aValor = aValor % 109;
     alfa1 = aValor;
     |QBF alfa1;
     alfa2 = alfa1 % 102; alfa3 = alfa1 % -107;
     alfa4 = alfa3 % 101;
     |SI alfa4 = "0";
          alfa3 = alfa3 % 206;
          alfa1 = alfa2 + alfa3;
     |FINSI;
     nume1 = alfa1;
     nume2 = nume1 / 97;
     |HAZ trunca3_ett;
     |SI nume1 ] 0; |Y nume1 [ 99;
          alfa1 = aValor;
          |QBF alfa1;
          alfa2 = nume1;
          alfa2 = "00" + alfa2;
          alfa2 = alfa2 % -102;
          alfa1 = alfa1 + alfa2;
          #0Campo = alfa1;
          |PINTA #0Campo;
    |FINSI;
|FPRC;
