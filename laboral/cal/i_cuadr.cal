|VARIABLES;
    &c_informe = "";
    &sw_cuadre = 0;

    c_clave = "";
    c_path = "";
    c_alfa1 = "";
    c_fichero1 = "";
    c_fichero2 = "";
|FIN;

|PROCESO c_copia;
|DDEF c_path;

    c_alfa1 =" ************ Configurado cuadre " + c_path + c_informe + c_clave;
|PINTA 2401, c_alfa1;
    c_fichero1 = c_path + c_informe + c_clave;
    c_fichero2 = c_path + c_informe + ".inf";
|COPIA_FICHERO c_fichero1, c_fichero2;
|SI FSalida ! 0;
        c_alfa1 = "    Error [" + FSalida + "] en copia de ficheros ...";
    |MENAV c_alfa1;
    |MENAV "    Proceso abortado ...";
        sw_cuadre = 1;
|FINSI;
|FPRC;

|PROCESO cuadres_si;
|SI c_informe = "nomyytc2";   c_clave = ".A00";    |FINSI;
|SI c_informe = "nomtc21p";   c_clave = ".B00";    |FINSI;
|SI c_informe = "nomyytc1";   c_clave = ".C00";    |FINSI;
|SI c_informe = "nomyreci";   c_clave = ".D00";    |FINSI;
|SI c_informe = "nomyabie";   c_clave = ".E00";    |FINSI;
|SI c_informe = "nomyy110";   c_clave =  ".00";    |FINSI;
|SI c_informe = "nomtalon";   c_clave = ".F00";    |FINSI;
|SI c_informe = "gomyytc2";   c_clave =  ".00";    |FINSI;
|SI c_informe = "gomyytc1";   c_clave =  ".00";    |FINSI;

|HAZ c_copia;
|SI c_clave = ".A00";|O c_clave = ".B00";
    |SI c_clave = ".A00"; c_informe = "nomadjun";  |FINSI;
    |SI c_clave = ".B00"; c_informe = "nomtc21s";  |FINSI;
    |HAZ c_copia;
|FINSI;
|FPRC;

|PROCESO cuadres;
    sw_cuadre = 0;
|INFOR c_informe;
|SI FSalida ! 0;
    ||AVISO;
    ||CONFI "    SCuadre sin configurar. ¨Configurar standar (S/N)?: ";
    ||SI FSalida = 0;
        |HAZ cuadres_si;
    ||SINO;
        ||MENAV "    Proceso abortado ...";
            ||sw_cuadre = 1;
    ||FINSI;
|FINSI;
|FININF;
|FPRC;
