____________________________________/
|PROCESO Copialo;
     aOrigen = "";
     |DDEF aOrigen;
     aOrigen = aOrigen + aCopialo + ".inf";
     aDestin = #0#1 + ":\" + aCopialo + ".inf";
     aDestin = aDestin < aDestin;

     aAlfa1 = "";
     |IP_REMOTO aAlfa1;
     |SI aAlfa1 ! "";
          |ENVIA_FICHERO aOrigen, aDestin;
          |XABRE "EC", aDestin, nHandle;
          |SI FSalida < 0;
               aAlfa1 = "0000No se pudo copiar [" + aOrigen + "]";
               |MENAV aAlfa1;
          |SINO;
               nCuantos = nCuantos + 1;
          |FINSI;
     |SINO;
          |COPIA_FICHERO aOrigen, aDestin;
          |XABRE "E", aDestin, nHandle;
          |SI FSalida < 0;
               aAlfa1 = "0000No se pudo copiar [" + aOrigen + "]";
               |MENAV aAlfa1;
          |SINO;
               nCuantos = nCuantos + 1;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     |SI #0#0 = "SI";|Y #0#1 ! " ";
          aCopialo = "nomyreci";   |HAZ Copialo;
          aCopialo = "nomyabie";   |HAZ Copialo;
          aCopialo = "nomyytc1";   |HAZ Copialo;
          aCopialo = "nomtc21p";   |HAZ Copialo;
          aCopialo = "nomtc21s";   |HAZ Copialo;
          aCopialo = "nomtalon";   |HAZ Copialo;
          aCopialo = "gomyytc1";   |HAZ Copialo;
          aCopialo = "gomyytc2";   |HAZ Copialo;

          |SI nCuantos ! 0;
               aAlfa1 = "0000Copiados [" + nCuantos + "] cuadres.";
               |MENAV aAlfa1;
          |FINSI;
     |FINSI;
|FPRC;
