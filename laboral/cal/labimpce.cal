     #labcerag#103 = aAlfa; |PINTA 1542, #labcerag#103;

     |ATRI 0;
|FPRC;

|PROCESO Marca;
     || Marca/Desmarca
     |SI #labhisce#26 = " ";
          #labhisce#26 = "*";
     |SINO;
          #labhisce#26 = " ";
     |FINSI;
     |PINTA #labhisce#26;
     |GRABA 020#labhisce.a;
     |LIBERA #labhisce;
     |ERROR;
|FPRC;

|PROCESO Impresion;
     || la variable aNetContrata vale algo si estoy desde NetContrata
     aAlfa =         (("00000" + #labhisce#1) % -105);
     aAlfa = aAlfa + (("00000" + #labhisce#2) % -105);
     |SI #labhisce#0 = "G"; aAlfa = "labcerti.tab;" + aNetContrata + aAlfa + "IMP"; |FINSI;
     |SI #labhisce#0 = "A"; aAlfa = "labcerag.tab;" + aNetContrata + aAlfa + "IMP"; |FINSI;

     |MENSA "    Emitiendo certificado";
     |SUB_EJECUTA aAlfa;
     |BLIN 4;
     |HAZ Ayuda;
     |ERROR;
|FPRC;

|PROCESO Edicion;
     || la variable aNetContrata vale algo si estoy desde NetContrata
     aAlfa =         (("00000" + #labhisce#1) % -105);
     aAlfa = aAlfa + (("00000" + #labhisce#2) % -105);
     |SI #labhisce#0 = "G"; aAlfa = "labcerti.tab;" + aNetContrata + aAlfa + "EDI"; |FINSI;
     |SI #labhisce#0 = "A"; aAlfa = "labcerag.tab;" + aNetContrata + aAlfa + "EDI"; |FINSI;
     |MENSA "    Edicion de certificado";
     |SUB_EJECUTA aAlfa;
     |BLIN 4;
     |HAZ Ayuda;
     |ERROR;
|FPRC;

|PROCESO XML_NetContrata;
     aReg = #labhisce#0;
     nEmp = #labhisce#1;
     nTra = #labhisce#2;

     #labhisce#26 = "*";
     |GRABA 020#labhisce.a;
     |LIBERA #labhisce;
     |HAZ XMLMultiple;
     #labhisce#0 = aReg;
     #labhisce#1 = nEmp;
     #labhisce#2 = nTra;
     |LEE 000#labhisce.=;

     |PONCONTROL 23, "si";
|FPRC;

|PROCESO Tipo11; |TIPO 11;
     swNetContrata = 0;
     aNetContrata = "";

     |SI FSalida = 10;        || impresion es el F2
          |HAZ Impresion;
     |FINSI;

     |SI FSalida = 11;        || directo a xml es el F3
          |SI aEmision = "IMP"; |ACABA; |FINSI;        || desde labornet
          |HAZ XML_NetContrata;
     |FINSI;

     |SI FSalida = 12;        || marca/desmarca es el F4
          |SI aEmision = "IMP"; |ACABA; |FINSI;        || desde labornet
          ||SI swNetContrata = 1; |ACABA; |FINSI;       || NetContrata
          |HAZ Marca;
     |FINSI;

     |SI FSalida = 13;        || envio a Netcontrata es el F5
          |SI aEmision = "IMP"; |ACABA; |FINSI;        || desde labornet
          swNetContrata = 1;
          aNetContrata = "netcontr";
          |HAZ XML_NetContrata;
     |FINSI;

     |SI FSalida = 15;        || edicion es el F7
          |SI aEmision = "IMP"; |ACABA; |FINSI;        || desde labornet
          aAlfa = "    S¿Desea modificar los datos del certificado seleccionado?";
          |CONFI aAlfa;
          |SI FSalida = 0;
               |HAZ Edicion;
               |PINTA #labhisce#25;
          |FINSI;
     |FINSI;

     |SI FSalida = 16;        || borrado es el F8
          |SI aEmision = "IMP"; |ACABA; |FINSI;        || desde labornet
          aAlfa = "    SSe borrarán los datos del certificado";
          aAlfa = aAlfa + " seleccionado. ¿Continuar?";
          |CONFI aAlfa;
          |SI FSalida = 0;
               |BORRA 020#labhisce.a;
               |LIBERA #labhisce;
               |PONCONTROL 23, "si";
               |PTEC 806;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo13; |TIPO 13;
   |SI aAntTipo ! #labhisce#0; |FINSI;
   |SI #labhisce#0 = "G"; |HAZ PantaTipoG; |FINSI;
   |SI #labhisce#0 = "A"; |HAZ PantaTipoA; |FINSI;

   aAntTipo = #labhisce#0;
|FPRC;

|PROCESO Ayuda; |TIPO 7;
     |SI aEmision = "IMP";         || desde labornet
          aAlfa = "    Pulse < F2 > para imprimir el certificado de empresa";
          |MENSA aAlfa;
          ||                    1         2         3         4
          ||            234567890123456789012345678901234567890
          |PINTA 0528,                           "ÄÄÄÄÄÄÄÄÄÄÄÄÄ";
          |PINTA 0602, "                                       ";
          |PINTA 0702, "                                       ";
          |PINTA 0802, "                                       ";
          ||            4        5         6         7
          ||            12345678901234567890123456789012345678
          |PINTA 0541, "ÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ";
          |PINTA 0641, "                                      ";
          |PINTA 0741, "                                      ";
          |PINTA 0841, "                                      ";
     |SINO;
          |SI swNetContrata = 1;
          |SINO;
               aAlfa = "    Seleccione una de las opciones con la tecla de función correspondiente";
               |MENSA aAlfa;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO BorraRepes;
     |ABRE #labhisce;
     |LEE 000#labhisce.p;
     |SI FSalida = 0;
          aTipAnt = #labhisce#0;
          nEmpAnt = #labhisce#1;
          nTraAnt = #labhisce#2;
          nLinAnt = #labhisce#3;
/*
          #labhisce#20 = "01.01.0000";
          #labhisce#22 = "01.01.0000";
          |GRABA 020#labhisce.a;
          |LIBERA #labhisce;
*/

          |PARA; |SI; |HACIENDO;
               |LEE 000#labhisce.s;
               |SI FSalida ! 0;
                    |SAL;
               |FINSI;

               |SI aTipAnt = #labhisce#0; |Y nEmpAnt = #labhisce#1; |Y nTraAnt = #labhisce#2;
                    || linea repetida;
                    |SALVA_DATOS #labhisce;

                    #labhisce#0 = aTipAnt;
                    #labhisce#1 = nEmpAnt;
                    #labhisce#2 = nTraAnt;
                    #labhisce#3 = nLinAnt;
                    |LEE 101#labhisce.=;
                    |SI FSalida = 0;
                         |BORRA 020#labhisce.a;
                    |FINSI;
                    |LIBERA #labhisce;

                    |REPON_DATOS #labhisce;

                    |LEE 000#labhisce.s;
               |FINSI;

               aTipAnt = #labhisce#0;
               nEmpAnt = #labhisce#1;
               nTraAnt = #labhisce#2;
               nLinAnt = #labhisce#3;
/*
               #labhisce#20 = "01.01.0000";
               #labhisce#22 = "01.01.0000";
               |GRABA 020#labhisce.a;
               |LIBERA #labhisce;
*/
          |SIGUE;
     |FINSI;
     |CIERRA #labhisce;
|FPRC;

|PROCESO inf;
     #labhisce#0 = aDTipo;
     #labhisce#1 = #0#0;
     #labhisce#2 = #0#2;
     #labhisce#25 = " ";
     #labhisce#20 = "01.06.2010";  || fecha en que se pone en marcha esto
     || #labhisce#20 = "01.01.0000";  || todos los certificados
|FPRC;

|PROCESO sup;
     #labhisce#0 = aHTipo;
     #labhisce#1 = #0#1;
     #labhisce#2 = #0#3;
     |SI #0#6 = "S";
          #labhisce#25 = "z";
     |SINO;
          #labhisce#25 = " ";
     |FINSI;
     #labhisce#20 = "31.12.2999";
|FPRC;

|PROGRAMA;
     aParam = PARAMETRO;
     |QBF aParam;

     aAlfa = aParam % -108;
     |SI aAlfa = "netcontr";
          aParam = aParam - aAlfa;
          swNetContrata = 1;
          aNetContrata = "netcontr";
     |FINSI;

     aRegimen = aParam % 101;
     aEmision = aParam % 203;
     || aEmision = "IMP";          || solo dejo imprimir

     |HAZ BorraRepes;

     aDTipo = aRegimen; |QBF aDTipo;
     |SI aDTipo = "";
        aDTipo = " "; aHTipo = "z";
     |SINO;
        aHTipo = aDTipo; |QBF aHTipo;
     |FINSI;

     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |ABRE #labhisce;
          |ENTLINEAL #2#labhisce, -2, 4, 22, 1, inf, sup;
          nPaso = 1;
          nCuantos = 0;
          |BUCLE Seleccionados;

          |SI nCuantos ! 0;
               |HAZ XMLMultiple;
          |FINSI;
          |CIERRA #labhisce;
     |FINSI;
|FPRO;
