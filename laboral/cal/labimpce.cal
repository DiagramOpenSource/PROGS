          |SI aAlfa1 = "/"; |O aAlfa1 = "\";
               |SI aAlfa2 ! "";
                    |RMKDIR aAlfa2;
               |FINSI;
          |FINSI;
          aAlfa2 = aAlfa2 + aAlfa1;
     |SIGUE;
     |SI aAlfa2 ! "";
          |RMKDIR aAlfa2;
     |FINSI;

