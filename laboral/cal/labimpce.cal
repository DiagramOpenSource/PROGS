               aPath = aPath + "\";
          |FINSI;
          |SI swNetContrata = 1;
               aPath = aPath + "NETCONTRATA\CERTIFICADOS\";
          |SINO;
               aPath = aPath + "xmlcert\";
          |FINSI;
     |FINSI;

     aAlfa1 = aPath % 0;  nNume1 = aAlfa1;
