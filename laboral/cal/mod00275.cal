|PROCESO Buscador;
     || por Codigo
     |SI aCodigo ! "00000";
          |PARA nCampo = 41; |SI nCampo [ 71; |HACIENDO nCampo = nCampo + 1;
               aAlfa1 = #mod00270#nCampo#;
               |QBF aAlfa1;
               aAlfa1 = ("00000" + aAlfa1) % -105;
               aAlfa = aAlfa1 - aCodigo;
               |SI aAlfa ! aAlfa1;
                    aLocalizado = #mod00270#4;
                    |SAL;
               |FINSI;
          |SIGUE;
          |SI aLocalizado ! "";
               |ERROR10;
          |FINSI;
     |FINSI;

     || por Nombre
     aAlfa1 = #mod00270#5;
     |QBF aAlfa1;
     |SI aNombre ! "";
          aAlfa = aAlfa1 - aNombre;
          |SI aAlfa ! aAlfa1;
               aLocalizado = #mod00270#4;
               |ERROR10;
          |FINSI;
     |FINSI;

     || por NIF
     aAlfa1 = #mod00270#4;
     |QBF aAlfa1;
     |SI aNIF ! "";
          aAlfa = aAlfa1 - aNIF;
          |SI aAlfa ! aAlfa1;
               aLocalizado = #mod00270#4;
               |ERROR10;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE Buscador;
