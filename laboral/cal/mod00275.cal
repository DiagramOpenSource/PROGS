|PROCESO BorraCosas;
     |PARA nCampo = 8; |SI nCampo [ 38; |HACIENDO nCampo = nCampo + 1;
          nCampo1 = nCampo + 33;
          nTrabajador = #mod00270#nCampo1#;
          |SI nTrabajador ! 00000;
               |DDEFECTO #gomdetjr;
               #gomdetjr#0 = #mod00270#0;
               #gomdetjr#1 = nTrabajador;
               #gomdetjr#2 = #mod00270#2;
               #gomdetjr#3 = #mod00270#3;
               |LEE 101#gomdetjr.=;
               |SI FSalida = 0;
                    |BORRA 020#gomdetjr.a;
               |FINSI;
               |LIBERA #gomdetjr;

               |DDEFECTO #nomvarli;
               #nomvarli#0 = #mod00270#0;
               #nomvarli#1 = nTrabajador;
               #nomvarli#2 = #mod00270#3;
               #nomvarli#3 = 0;
               #nomvarli#4 = 903;
               |LEE 101#nomvarli.=;
               |SI FSalida = 0;
                    |BORRA 020#nomvarli.a;
               |FINSI;

               #nomvarli#4 = 101;
               |LEE 101#nomvarli.=;
               |SI FSalida = 0;
                    |BORRA 020#nomvarli.a;
               |FINSI;

               #nomvarli#4 = 922;
               |LEE 101#nomvarli.=;
               |SI FSalida = 0;
                    |BORRA 020#nomvarli.a;
               |FINSI;

               |LIBERA #nomvarli;
          |FINSI;
     |SIGUE;
|FPRC;

|PROCESO GrabaNomvarli;
     |SI FSalida = 0;
          aAlfa = #nomvarli#5;
          |QBF aAlfa;
          nNume = aAlfa;
          nNume = nNume + 1
          aAlfa = nNume; |QBF aAlfa; aAlfa = ("          " + aAlfa) % -110;
          #nomvarli#5 = aAlfa;
          |GRABA 020#nomvarli.a;
     |SINO;
          aAlfa = "         1";
          #nomvarli#5 = aAlfa;
          |SI #nomvarli#4 = 903;
               #nomvarli#6 = "Jornadas";
          |FINSI;
          |SI #nomvarli#4 = 922;
               #nomvarli#6 = "Jornadas IT";
          |FINSI;
          |GRABA 020#nomvarli.n;
     |FINSI;
     |LIBERA #nomvarli;
|FPRC;

|PROCESO GrabaJornada;
     |DDEFECTO #nomvarca;
     #nomvarca#0 = #mod00270#0;
     #nomvarca#1 = nTrabajador;
     #nomvarca#2 = #mod00270#3;
     #nomvarca#3 = 0;
     |LEE 101#nomvarca.=;
     |SI FSalida ! 0;
          |GRABA 020#nomvarca.n;
     |FINSI;

     |DDEFECTO #nomvarli;
     #nomvarli#0 = #mod00270#0;
     #nomvarli#1 = nTrabajador;
     #nomvarli#2 = #mod00270#3;
     #nomvarli#3 = 0;

     |SI #mod00270#nCampo# = "X";
          #nomvarli#4 = 101;
          |LEE 101#nomvarli.=;
          |HAZ GrabaNomvarli;

          #nomvarli#4 = 903;
          |LEE 101#nomvarli.=;
          |HAZ GrabaNomvarli;
     |FINSI;

     |SI #mod00270#nCampo# = "I";
          #nomvarli#4 = 922;
          |LEE 101#nomvarli.=;
          |HAZ GrabaNomvarli;
     |FINSI;

     |LIBERA #nomvarca;
|FPRC;

|PROCESO LeeTemporal;
     |HAZ BorraCosas;

     nJornadas = 0;
     nJornadasIT = 0;
     |PARA nCampo = 8; |SI nCampo [ 38; |HACIENDO nCampo = nCampo + 1;
          nCampo1 = nCampo + 33;
          nTrabajador = #mod00270#nCampo1#;

          |SI nTrabajador ! 00000;
               |SI #mod00270#nCampo# = "X";
                    nJornadas = nJornadas + 1;
               |FINSI;
               |SI #mod00270#nCampo# = "I";
                    nJornadasIT = nJornadasIT + 1;
               |FINSI;

               |SI #mod00270#nCampo# = "X"; |O #mod00270#nCampo# = "I";
                    |DDEFECTO #gomdetjr;
                    #gomdetjr#0 = #mod00270#0;
                    #gomdetjr#1 = nTrabajador;
                    #gomdetjr#2 = #mod00270#2;
                    #gomdetjr#3 = #mod00270#3;
                    |LEE 101#gomdetjr.=;
                    FEstado = FSalida;

                    nCampo2 = nCampo - 4;
                    #gomdetjr#nCampo2# = #mod00270#nCampo#;
                    |SI FEstado = 0;
                         |GRABA 020#gomdetjr.a;
                    |SINO;
                         |GRABA 020#gomdetjr.n;
                    |FINSI;
                    |LIBERA #gomdetjr;

                    |HAZ GrabaJornada;
               |FINSI;
          |FINSI;
     |SIGUE;
|FPRC;

