|PROCESO Discap;
     fFecha = ~;
     aAlfa1 = fFecha;
     aAlfa1 = aAlfa1 % -104;

     #nomir003#0 = #labtraba#7;
     #nomir003#40 = aAlfa1;
     #nomir003#41 = #labtraba#0;
     aDiscap = "N";

     |LEE 000#nomir003.=;
     |SI FSalida ! 0;
          |LEE 000#nomir003.];
          |SI FSalida = 0;
               |LEE 000#nomir003.a;
               |SI #nomir003#0 = #labtraba#7; |Y #nomir003#41 = #labtraba#0; |Y FSalida = 0;
                    aDiscap = #nomir003#6;
               |FINSI;
          |SINO;
               |LEE 000#nomir003.u;
               |SI FSalida = 0;
                    |SI #nomir003#0 = #labtraba#7; |Y #nomir003#41 = #labtraba#0;
                         aDiscap = #nomir003#6;
                    |FINSI;
               |FINSI;
          |FINSI;
     |SINO;
          aDiscap = #nomir003#6;
     |FINSI;
|FPRC;

