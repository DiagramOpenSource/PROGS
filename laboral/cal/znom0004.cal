|FICHEROS;
     znom0004 #0;

     labempre;
     labcentr;
|FIN;

|VARIABLES;
     aAlfa = "";
     aAlfa1 = "";
     aAlfa2 = "";
     aAlfa3 = "";
     aAlfa4 = "";
|FIN;

|PROCESO labcentr;
     |SI #labcentr#15 = 000; |O #labcentr#15 = 001;
          #labcentr#15 = #labempre#34;
          |GRABA 020#labcentr.a;
          |LIBERA #labcentr;
     |SINO;
          |SI #labcentr#15 ! #labempre#34;
               aAlfa1 = #labempre#0; |QBF aAlfa1; aAlfa1 = ("00000" + aAlfa1) % -105;
               aAlfa2 = #labempre#34; |QBF aAlfa2; aAlfa2 = ("000" + aAlfa2) % -103;
               aAlfa3 = #labcentr#0; |QBF aAlfa3; aAlfa3 = ("00" + aAlfa3) % -102;
               aAlfa4 = #labcentr#15; |QBF aAlfa4; aAlfa4 = ("000" + aAlfa4) % -103;
               aAlfa = "    SATENCION. Empresa " + aAlfa1 + ",";
               aAlfa = aAlfa + " calend: " + aAlfa2 + ".";
               aAlfa = aAlfa + " Centro " + aAlfa3 + ",";
               aAlfa = aAlfa + " calend: " + aAlfa4 + ".";
               aAlfa = aAlfa + "¿Copiar calendario?"
               |CONFI aAlfa;
               |SI FSalida = 0;
                    #labcentr#15 = #labempre#34;
                    |GRABA 020#labcentr.a;
                    |LIBERA #labcentr;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE labcentr;
     #labcentr#1;
     ;
     #labempre#0, 001;
     #labempre#0, 999;
     be;
     NULL, labcentr;
|FIN;

|PROCESO empresas;
     |SI #labempre#34 ! 000;
          |BUCLE labcentr;
     |FINSI;
|FPRC;

|DEFBUCLE empresas;
     #labempre#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, empresas;
|FIN;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |BUCLE empresas;
     |FINSI;
|FPRO;
