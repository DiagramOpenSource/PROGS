|FICHEROS;
     marin003 #0;
     marin001;
     labempre;
|FIN;

|VARIABLES;
     nCampo = 0;
     nFecha = 0;
|FIN;

|PROCESO UnAnyo;
     nCampo = Campo + 1;
     #0nCampo = #0Campo + 0.012;
     nFecha = #0nCampo
     nFecha = nFecha - 1;
     #0nCampo = nFecha;
     |PINTA #0nCampo;
|FPRC;

|PROCESO impresion;
     #labempre#0 = #marin001#0;
     |LEE 000#labempre.=;
     |PRINT;
|FPRC;

|DEFBUCLE impresion;
     #marin001#1;
     1, 3;
     00001, "S", #marin003#2;
     99999, "S", #marin003#3;
     ;
     NULL, impresion;
|FIN;

|PROCESO Informe; |TIPO 3;
     |ABRE #labempre;
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR "marin003";
          |BUCLE impresion;
          |PIEINF;
          |FININF;
     |FINSI;
     |FINIMP;
     |CIERRA #labempre;
|FPRC;
