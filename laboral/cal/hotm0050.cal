|FICHEROS;
     hotm0050 #0;
     hotm0051 #1;
|FIN;

|VARIABLES;
     nModo=0;
     fFecha=@;
     aCadena="";

     fFechaHoy = @;
     aFechaHoy = "";
     aMes = "";
     aAnyo = "";
|FIN;

|PROCESO defectomes; |TIPO 7;
     fFechaHoy = ~;
     aFechaHoy = fFechaHoy;
     aMes = aFechaHoy % 402;
     #0#2 = aMes;
     |PINTA #0#2;
|FPRC;

|PROCESO defectoanyo; |TIPO 7;
     fFechaHoy = ~;
     aFechaHoy = fFechaHoy;
     aAnyo = aFechaHoy % -104;
     #0#1 = aAnyo;
     |PINTA #0#1;
|FPRC;

