|FICHEROS;
     nomcorin #0;
     nomcorca;
     nomcorli;
|FIN;

|VARIABLES;
     aAlfa = "";
     aParam = "";
|FIN;

|PROCESO nomcorli;
     |SI #0#2 ! "N";
          |PRINT;
     |SINO;
          |SI #nomcorli#9 = "*";
               |PRINT;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE nomcorli;
     #nomcorli#1;
     ;
     #nomcorca#0, 001;
     #nomcorca#0, 999;
     ;
     NULL, nomcorli;
|FIN;

|PROCESO nomcorca;
     |BUCLE nomcorli;
|FPRC;

|DEFBUCLE nomcorca;
     #nomcorca#1;
     ;
     #0#0;
     #0#1;
     ;
     NULL, nomcorca;
|FIN;

|PROGRAMA;
     aParam = PARAMETRO;
     |QBF aParam;
     |SI aParam ! "";
          aAlfa = aParam;
          #0#0 = aAlfa;
          #0#1 = aAlfa;
     |FINSI;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          Impresora = "CrystalReport";
          |IMPRE -1;
          |INFOR "nomcorin";
          |BUCLE nomcorca;
          |PIEINF;
          |FININF;
     |FINSI;
|FPRO;
