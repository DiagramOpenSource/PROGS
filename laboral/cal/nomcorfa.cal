|FICHEROS;
     nomcorfa,MANTE;
     nomlisco;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO Tipo5_nomcorfa; |TIPO 5;
     #nomcorfa#9 = "*";
     |LINEAS_BI_ATRI #nomcorfa#9, 0, I, -1;
|FPRC;

|PROCESO Tipo3Lin_nomcorfa; |TIPO 3;
     |SI FSalida = 0;
          #nomcorfa#9 = "";
     |FINSI;
|FPRC;

|PROCESO Tipo1_nomcorfa; |TIPO 1;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO CompConcRED;
     |SI #nomcorfa#5 ! 9999;
          |ABRE #nomlisco;
          #nomlisco#0 = #nomcorfa#5;
          |LEE 000#nomlisco.=;
          |SI FSalida ! 0;
               |MENAV "    ATENCION: el código del Concepto Retributivo Asociado RED no existe.";
               |ERROR;
          |FINSI;
          |CIERRA #nomlisco;
     |FINSI;
|FPRC;
