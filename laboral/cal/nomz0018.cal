          aAlfa = #0#1 % 402; |QBF aAlfa; nMes = aAlfa;
          aAlfa = #0#1 % -104; |QBF aAlfa; nAny = aAlfa;
/*
          || nada de mes anterior, aqui se busca desde el dia que es

          |SI nDia = 1;
               nMes = nMes - 1;
               |SI nMes = 0;
                    nMes = 12;
                    nAny = nAny - 1;
               |FINSI;
          |FINSI;
*/
          |BUCLE Trabajadores;

          |CIERRA #nomvarca;
          |CIERRA #nomhicca;
          |CIERRA #nomcalca;
          |ET OtraVez;
          |ENTLINEAL #1#nomtmp18, -1, 2, 22, 1, inf04, sup04;
          |SI nNumTrab = 0;
              |CIERRA #20;
              |CIERRA #nomtmp18;
              |DELINDEX #nomtmp18;
              |ACABA;
          |FINSI;

      |ET OtroMenu;
      |MENU Menu;
      nOpcion = FSalida;
      |SI nOpcion = 1; |VETE OtraVez;   |FINSI;
      |SI nOpcion = 2;
           nNumTrab = 0;
           |HAZ Lote;
      |FINSI;
      |SI nOpcion = 3;
         aAlfa = "    S¿Está seguro de que desea salir sin preparar la remesa?";
         |CONFI aAlfa;
         |SI FSalida ! 0; |VETE OtraVez; |FINSI;
      |FINSI;
      |SI nOpcion < 1; |VETE OtroMenu; |FINSI;
      |CIERRA #nomtmp18;   |DELINDEX #nomtmp18;
      |CIERRA #20;
     |FINSI;
     |SI FSalida = 0;
          |GRABA 020#0a;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;
