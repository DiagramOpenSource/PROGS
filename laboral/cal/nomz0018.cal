|PROCESO Lote;
     |ABRE #labtraba;

     |BUCLE el_red;

     |CIERRA #labtraba;

     |SI nNumTrab > 0;
          aAlfa1 = nNumTrab;
          aAlfa = "    Se ha preparado la remesa de comunicación de inicio de ";
          aAlfa = aAlfa + "INACTIVIDAD para " + aAlfa1;
          |SI nNumTrab = 1;
               aAlfa = aAlfa + " trabajador."
          |SINO;
               aAlfa = aAlfa + " trabajadores."
          |FINSI;
          |MENAV aAlfa;

          aAlfa = "    Puede crear el lote desde la opción - Creación Lote ";
          aAlfa = aAlfa + "AFI - para el Sistema RED";
          |MENAV aAlfa;
     |FINSI;
|FPRC;

|PROCESO BuscaERE;
     #nomcalca#0 = #labtraba#0;
     #nomcalca#1 = #labtraba#1;
     #nomcalca#2 = nMes;
     #nomcalca#3 = 0;
     |LEE 000#nomcalca.=;
     |SI FSalida = 0;
          |PARA nCampo = 27; |SI nCampo [ 30; |HACIENDO nCampo = nCampo + 1;
               |SI #nomcalca#nCampo# = "D";
                    swERE = 1;
                    nCampo1 = nCampo + 92;
                    |SI #nomcalca#nCampo1# ! 100;
                         |SI #0#2 = "E"; |O #0#2 = "F";
                              #nomtmp18#5 = "F";
                         |FINSI;
                         |SI #0#2 = "V"; |O #0#2 = "W";
                              #nomtmp18#5 = "W";
                         |FINSI;
                         |SI #0#2 = "R"; |O #0#2 = "S";
                              #nomtmp18#5 = "S";
                         |FINSI;
                         |SI #0#2 = "Y"; |O #0#2 = "U";
                              #nomtmp18#5 = "U";
                         |FINSI;
                         nProp = (100 - #nomcalca#nCampo1#) ! 0;
                    |FINSI;
                    |ACABA;
               |FINSI;
          |SIGUE;
     |FINSI;

     #nomhicca#0 = #labtraba#0;
     #nomhicca#1 = #labtraba#1;
     #nomhicca#66 = nAny - 2000;
     #nomhicca#2 = nMes;
     #nomhicca#3 = 0;
