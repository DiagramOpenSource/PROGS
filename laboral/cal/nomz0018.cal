|PROCESO TiempoParcial;
     swParcial = 0;
     |SI #labtraba#42 = "X"; |O #labtraba#42 = "T";
          |SI 200 [ #labtraba#45; |Y #labtraba#45 [ 299;
               swParcial = 1;
          |FINSI;
          |SI 500 [ #labtraba#45; |Y #labtraba#45 [ 599;
               swParcial = 1;
          |FINSI;
          |SI swParcial = 1;
               nHorasSem = 40;
               |ABRE #nomconca;
               #nomconca#0 = #labtraba#87;
               |LEE 000#nomconca.=;
               |SI FSalida = 0;
                    nHorasSem = #nomconca#162;
               |FINSI;
               nCoefTP = #labtraba#234 / nHorasSem;
               nCoefTP = nCoefTP ! 3;
               #nomtmp18#7 = nCoefTP * 1000;

               |CIERRA #nomconca;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Trabajadores;
     aFechaAlta = #labtraba#36;
     |QBF aFechaAlta;
     |SI aFechaAlta = ".........."; |O aFechaAlta = "";
          |ACABA;
     |FINSI;
     fFechaAlta = aFechaAlta;

     aFechaBaja = #labtraba#37;
     |QBF aFechaBaja;

     |SI aFechaBaja = ".........."; |O aFechaBaja = "";
          aFechaBaja = "31.12.2099";
     |FINSI;
     fFechaBaja = aFechaBaja;

     aFechaLim = #0#1;
     fFechaLim = aFechaLim;

     swSigue = 0;
     |SI fFechaAlta [ fFechaLim; |Y fFechaLim [ fFechaBaja;
          swSigue = 1;
     |FINSI;

     |SI #labtraba#42 = "H";
          swSigue = 0;
     |FINSI;

     #20#0 = #labtraba#0;
     |LEE 000#20=;
     |SI FSalida ! 0;
          swSigue = 0;
     |FINSI;

     |SI swSigue = 1;
          |DDEFECTO #nomtmp18;
          #nomtmp18#0 = #labtraba#0;
          #nomtmp18#1 = #labtraba#1;
          #nomtmp18#2 = #labtraba#2;
          #nomtmp18#3 = 0;
          #nomtmp18#4 = #0#1;
          #nomtmp18#5 = #0#2;

          swERE = 0;
          nProp = 0;
          |HAZ BuscaERE;

          |SI swERE = 1;
               #nomtmp18#6 = "**";
          |FINSI;

          |HAZ TiempoParcial;

          |SI #nomtmp18#5 = "W"; |O #nomtmp18#5 = "F"; |O #nomtmp18#5 = "S";
          |O #nomtmp18#5 = "U";
               |SI nProp ! 0;
                    nProp = nProp * 10;
                    #nomtmp18#8 = nProp;
                    #nomtmp18#3 = nProp;

                    |SI swParcial = 1;
                          #nomtmp18#3 = nProp * nCoefTP;
                    |FINSI;
               |FINSI;
          |FINSI;

          |GRABA 020#nomtmp18.n;
          nNumTrab = nNumTrab + 1;

          |LIBERA #nomtmp18;
     |FINSI;
|FPRC;

