|FICHEROS;
     nominf09 #0;

     nomhicca;

     labempre;
     labcentr;
     labtraba;
|FIN;

|VARIABLES;
     nAnyXX = 0;
     &nMes = 0;
|FIN;

|PROCESO nomhicca;
     #labempre#0 = #nomhicca#0;
     |LEE 000#labempre.=;

     #labtraba#0 = #nomhicca#0;
     #labtraba#1 = #nomhicca#1;
     |LEE 000#labtraba.=;

     #labcentr#0 = #nomhicca#0;
     #labcentr#1 = #labtraba#77;
     |LEE 000#labcentr.=;

     nMes = #nomhicca#2;

     |PRINT;

|FPRC;

|DEFBUCLE nomhicca;
     #nomhicca#1;
     ;
     #0#0, #0#2, nAnyXX, #0#5, 0;
     #0#1, #0#3, nAnyXX, #0#6, 20;
     ;
     NULL, nomhicca;
|FIN;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;

     |ENDATOS #1#0;
     |SI FSalida = 0;
          Impresora = "Crystal Reports";
          |IMPRE 0;
          |SI FSalida = 0;
               |INFOR "nominf09";

               |ABRE #labempre;
               |ABRE #labcentr;
               |ABRE #labtraba;

               nAnyXX = #0#4 - 2000;
               |BUCLE nomhicca;

               |PIEINF;
               |FININF;

               |CIERRA #labtraba;
               |CIERRA #labcentr;
               |CIERRA #labempre;
          |FINSI;
          |FINIMP;
     |FINSI;
|FPRO;
