|FICHEROS;
     pdfinbor #0;
     pdfdocca;
     pdfdocli;
|FIN;

|VARIABLES;
     nLinea = 0;
     aAlfa = "";
     aAlfa1 = "";
|FIN;

|PROCESO NuevosCampos;
     #pdfdocli#0 = #pdfdocca#0;
     #pdfdocli#3 = 000;
     #pdfdocli#4 = "";
     #pdfdocli#5 = "";
     #pdfdocli#6 = "";
     #pdfdocli#7 = "";
     #pdfdocli#8 = "";
     #pdfdocli#9 = "";
     #pdfdocli#10 = "";
     nLinea = nLinea + 1; #pdfdocli#1 = nLinea; #pdfdocli#2 = "pais_emp"; |GRABA 020#pdfdocli.n; |LIBERA #pdfdocli;
     nLinea = nLinea + 1; #pdfdocli#1 = nLinea; #pdfdocli#2 = "pais_cen"; |GRABA 020#pdfdocli.n; |LIBERA #pdfdocli;
     nLinea = nLinea + 1; #pdfdocli#1 = nLinea; #pdfdocli#2 = "pais_tra"; |GRABA 020#pdfdocli.n; |LIBERA #pdfdocli;
     nLinea = nLinea + 1; #pdfdocli#1 = nLinea; #pdfdocli#2 = "municipio_emp"; |GRABA 020#pdfdocli.n; |LIBERA #pdfdocli;
     nLinea = nLinea + 1; #pdfdocli#1 = nLinea; #pdfdocli#2 = "municipio_cen"; |GRABA 020#pdfdocli.n; |LIBERA #pdfdocli;
     nLinea = nLinea + 1; #pdfdocli#1 = nLinea; #pdfdocli#2 = "municipio_tra"; |GRABA 020#pdfdocli.n; |LIBERA #pdfdocli;
     nLinea = nLinea + 1; #pdfdocli#1 = nLinea; #pdfdocli#2 = "nacionalidad"; |GRABA 020#pdfdocli.n; |LIBERA #pdfdocli;
|FPRC;

|PROCESO pdfdocca;
     aAlfa = #pdfdocca#0;
     |QBF aAlfa;
     aAlfa1 = aAlfa % 102;
     |SI aAlfa1 = "pe";
          |SI aAlfa ! "pe-151.pdf";
          |Y aAlfa ! "pe-192.pdf";
          |Y aAlfa ! "pe-197.pdf";
          |Y aAlfa ! "pe-200.pdf";
          |Y aAlfa ! "pe-229.pdf";
          |Y aAlfa ! "pe-230.pdf";
               #pdfdocli#0 = #pdfdocca#0;
               #pdfdocli#1 = 999;
               |LEE 000#pdfdocli.];
               |SI FSalida = 0;
                    |LEE 000#pdfdocli.a;
                    nLinea = #pdfdocli#1;
                    |HAZ NuevosCampos;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE pdfdocca;
     #pdfdocca#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, pdfdocca;
|FIN;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0; |Y #0#0 = "S";
          |ABRE #pdfdocca;
          |ABRE #pdfdocli;
          |BUCLE pdfdocca;
          |CIERRA #pdfdocli;
          |CIERRA #pdfdocca;
     |FINSI;
|FPRO;
