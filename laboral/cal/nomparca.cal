|FICHEROS;
     nomparca;
     nomparli;
|FIN;

|VARIABLES;
     nLineaReal = 0;
     nLineaNueva = 0;
     nInforme = 0;
|FIN;

|PROCESO Renumera; |TIPO 3;
     |ABRE #nomparli;
     nLineaReal = 0;
     nLineaNueva = 1;
     nInforme = #nomparca#0;

     #nomparli#0 = nInforme;
     #nomparli#1 = 1;
     |LEE 101#nomparli.];
     |SI FSalida ! 0; |O #nomparli#0 ! nInforme;
          |ACABA;
     |FINSI;

     |PARA; |SI; |HACIENDO nLineaNueva = nLineaNueva + 1;
          #nomparli#0 = nInforme;
          #nomparli#1 = nLineaNueva;
          |LEE 101#nomparli.];
          |SI FSalida ! 0; |O #nomparli#0 ! nInforme;
               |SAL;
          |SINO;
               |SI #nomparli#1 ! nLineaNueva;
                    nLineaReal = #nomparli#1;
                    |BORRA 020#nomparli.a;

                    #nomparli#1 = nLineaNueva;
                    |GRABA 020#nomparli.n;
                    |LIBERA #nomparli;
               |FINSI;
          |FINSI;
     |SIGUE;
     |CIERRA #nomparli;
|FPRC;
