|FICHEROS;
     bombbs02 #0;
|FIN;

|VARIABLES;
     nModo = 0;
     fFecha = @;
     aAlfa = "";
     nNume = 0;

     &EURODB = 2;
|FIN;

|INCLUYE i_logo;
|INCLUYE i_visado;

/**************** PROCESOS DESDE EL MANTENIMIENTO ***************************/

|PROCESO defectos; |TIPO 7;
     #0#7 = "00.00.2000";
     |PINTA #0#7;
     #0#8 = "00.00.2000";
     |PINTA #0#8;
|FPRC;

|PROCESO desactiva; |TIPO 7;
     |SI #0#6 = "S";
          |CAMPO_MODIFICA #0#7, 1, "S";
     |SINO;
          #0#7 = "00.00.2000";
          |PINTA #0#7;
          |CAMPO_MODIFICA #0#7, 1, "N";
     |FINSI;
|FPRC;

|PROCESO FechaEfecto1;
     fFecha = #0#2;
     fFecha = fFecha + 0.001000;
     aAlfa = fFecha;
     aAlfa = aAlfa % -108;
     aAlfa = "01" + aAlfa;
     fFecha = aAlfa;
     #0#8 = fFecha;
     |PINTA #0#8;
|FPRC;

|PROCESO FechaEfecto2; |TIPO 7;
     |SI #0#6 = "S";
          fFecha = #0#7;
     |SINO;
          fFecha = #0#2;
     |FINSI;
     fFecha = fFecha + 0.001000;
     aAlfa = fFecha;
     aAlfa = aAlfa % -108;
     aAlfa = "01" + aAlfa;
     fFecha = aAlfa;
     #0#8 = fFecha;
     |PINTA #0#8;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     |HAZ Enlace;
|FPRC;

/**************** FINAL PROCESOS DESDE EL MANTENIMIENTO *********************/
