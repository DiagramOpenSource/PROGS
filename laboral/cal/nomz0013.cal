|FICHEROS;
     nomz0013 #0;
|FIN;

|VARIABLES;
     aArchivo = "";
     aBase = "";
     aOrigen = "";
     aDirLocal = "";
     aDestino = "";
|FIN;

|PROGRAMA;
     |CLS;

     |PINPA #0#0;
     |PINDA #0#0;

     |ENDATOS #1#0;

     |SI FSalida = 0;
          aArchivo = #0#0;
          |QBF aArchivo;

          |DBASE aBase;
          |QBF aBase;
          aOrigen = aBase + "documentos/" + aArchivo;

          aDirLocal = "C:\DOCUMENTOS\";
          aDestino = aDirLocal + aArchivo;

          |ENVIA_FICHERO aOrigen, aDestino;
     |FINSI;
|FPRO;
