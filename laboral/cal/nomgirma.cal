               |LEE 000#100u;
               nNume = #100#3 + 1;
          |SINO;
               nNume = 1;
          |FINSI;
     |FINSI;

     #100#0 = #0#0;
     #100#1 = #0#1;
     #100#2 = #0#2;
     #100#3 = nNume;
     aAlfa = Usuario % 0810; |QBF aAlfa;
     #100#4 = aAlfa;

     fFecha = ~;
