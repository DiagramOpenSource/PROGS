|FICHEROS;
     aomentrh;
     aomentr3;
     aomentr4;

     aomparam #1;

     gomantl1@;
     gopre001@;
     gopre002@;
     gopre003@;
     goparame@;
     gopre021@;
     aomtemp4 #50;
     aomtemp5 #51;
     aomentrh@;
|FIN;

|VARIABLES;
     aRepe = "";
     &PRMNT_param = "";
     &PRMNT_capi = 0;
     &enBoton = 0;
     nError = 0;
     &aTrabajador = "";
     nModo = 0;
     &swPaso = 0;
     nTotal = 0;
     aTotal = "";
     aAlfa = "";
     aUnidades = "";
     &nUnidades = 0;
     &nPendientes = 0;
     aPendientes = "";

     aPath = "";
     aPathBas = "";
     aPathDef = "";
     aPathDat = "";
     aDef = "";
     aLon = "";
     nLon1 = 0;
     nLon2 = 0;
     aSer = "";
     nAgrup = 0;
     aDesAgrup = "";
     alfa1 = "";
     aTipo = "";
     &eaSerie = "";
     &enNumero = 0;
     &enEmpresa = 0;
     &enTrabaj = 0;
     &efFecha = @;
     &enLinea = 0;
     nLinea = 0;
     nForma = 0;
|FIN;

|PROCESO Clave; |TIPO 7;
/*
     aSer = #aomentr4#1;
     |QBF aSer;
     aSer = ("00000" + aSer) % -105;
*/

     || #aomentrh#0 = aSer;           || serie expediente
     #aomentrh#0 = #aomentr4#1;    || serie expediente
     #aomentrh#1 = #aomentr4#7;    || nro expediente
     #aomentrh#2 = #aomentr4#2;    || empresa
     #aomentrh#4 = #aomentr4#3;    || trabajador
     #aomentrh#5 = aTrabajador;    || Nombre trabajador
     #aomentrh#6 = #aomentr4#0;    || fecha
     || #aomentrh#7 = #aomentr4#5;    || unidades totales

     nModo = (FEntrada / 100) ! 0;
     |SI #aomentrh#8 ! 1; |Y nModo = 1;         || nro linea
          #aomentrh#8 = #aomentrh#8 + 1;
     |FINSI;
     |SI swPaso = 1; |Y #aomentrh#8 = 1; |Y nModo = 1;        || nro linea
          #aomentrh#8 = #aomentrh#8 + 1;
     |FINSI;

     |PINTA #aomentrh#8;

     |PINTA #aomentrh#4;
     |PINTA #aomentrh#5;
     |PINTA #aomentrh#6;
     || PINTA #aomentrh#7;

|FPRC;

|PROCESO SumaUno;
     swPaso = 1;
|FPRC;

|PROCESO Cuenta;
     nTotal = nTotal + #aomentrh#17;
|FPRC;

|DEFBUCLE Cuenta;
     #aomentrh#1;
     ;
     aAlfa, #aomentr4#7, #aomentr4#2, #aomentr4#3, #aomentr4#0, 001;
     aAlfa, #aomentr4#7, #aomentr4#2, #aomentr4#3, #aomentr4#0, 999;
     ;
     NULL, Cuenta;
|FIN;

|PROCESO UnidAntes; |TIPO 7;
     |SALVA_FICHA #aomentrh;
     nTotal = 0;
     aAlfa = #aomentr4#1;
     |BUCLE Cuenta;
     aUnidades = nUnidades;
     nPendientes = nUnidades - nTotal;
     aPendientes = nPendientes;
     |PINTA 1471, "        ";
     |PINTA 1571, "        ";
     |PINTA 1471, aUnidades;
     |PINTA 1571, aPendientes;
     |REPON_FICHA #aomentrh;
|FPRC;

|PROCESO UnidDespues;
     |HAZ UnidAntes;
|FPRC;

|PROCESO Produccion2;     || por si estamos en produccion hago el cambiazo
     aLon = aPath % 0;
     nLon1 = aLon;
     aAlfa = aPath;
     aAlfa = aAlfa - "dsprofes/";
     aLon = aAlfa % 0;
     nLon2 = aLon;
     |SI nLon1 ! nLon2;
          aPath = aAlfa + "dsempres/";
     |FINSI;
|FPRC;

|PROCESO CargaAgrupPres;
     nLinea = nLinea + 1;
     #51#0 = nLinea;
     #51#1 = #gopre002@#12;
     #51#2 = #gopre002@#13;
     |GRABA 020#aomtemp5.n;
|FPRC;

|DEFBUCLE CargaAgrupPres;
     #gopre002@#7005;
     ;
     #gopre001@#0, #gopre001@#1, nAgrup, 001, "                    ";
     #gopre001@#0, #gopre001@#1, nAgrup, 999, "zzzzzzzzzzzzzzzzzzzz";
     ;
     NULL, CargaAgrupPres;
|FIN;

|PROCESO CargaAgrup;
     #50#0 = #gopre002@#14;
     |LEE 101#50=;
     |SI FSalida ! 0;
          |SI #50#0 = 0;
               #50#0 = 0;
               #50#1 = "Sin Agrupacion";
               |GRABA 020#aomtemp4.n;
          |SINO;
               #gopre021@#0 = #gopre002@#0;
               #gopre021@#1 = #gopre002@#1;
               #gopre021@#2 = #gopre002@#14;
               |LEE 000#gopre021@.=;

               #50#0 = #gopre002@#14;
               #50#1 = #gopre021@#3;
               |GRABA 020#aomtemp4.n;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE CargaAgrup;
     #gopre002@#1003;
     ;
     #gopre001@#0, #gopre001@#1, 001;
     #gopre001@#0, #gopre001@#1, 999;
     ;
     NULL, CargaAgrup;
|FIN;

|PROCESO MinPed;
     || #gomantl1@#0 = #aomentrh#0;
     #gomantl1@#0 = aSer;
     #gomantl1@#1 = #aomentrh#1;
     #gomantl1@#2 = 001;

     #gopre002@#0 = #aomentrh#9;
     #gopre002@#1 = #aomentrh#10;
     #gopre002@#2 = 001;

     |SI #gopre001@#49 = "S";
          #gopre002@#14 = nAgrup;
     |SINO;
          #gopre002@#14 = 000;
     |FINSI;

     #gopre003@#0 = #aomentrh#9;
     #gopre003@#1 = #aomentrh#10;
     #gopre003@#2 = #aomentrh#12;
     #gopre003@#3 = 001;
|FPRC;

|PROCESO MaxPed;
     || #gomantl1@#0 = #aomentrh#0;
     #gomantl1@#0 = aSer;
     #gomantl1@#1 = #aomentrh#1;
     #gomantl1@#2 = 999;

     #gopre002@#0 = #aomentrh#9;
     #gopre002@#1 = #aomentrh#10;
     #gopre002@#2 = 999;

     |SI #gopre001@#49 = "S";
          #gopre002@#14 = nAgrup;
     |SINO;
          #gopre002@#14 = 999;
     |FINSI;

     #gopre003@#0 = #aomentrh#9;
     #gopre003@#1 = #aomentrh#10;
     #gopre003@#2 = #aomentrh#12;
     #gopre003@#3 = 999;
|FPRC;

|PROCESO PidePres3; |TIPO 7;
     aTipo = "";
     |ABRE #1; |LEE 000#1p; |CIERRA #1;
     |SI #1#4 = "E"; |ACABA; |FINSI;

     |DBASS aPath;
     |HAZ Produccion2;
     aPathBas = aPath + #1#1; |QBF aPathBas;
     aPathDef = aPathBas + "def/";
     aPathDat = aPathBas + "fich/" + (("00000" + #aomentr4#2) % -105) + "/";

     aDef = aPathDef + "gomantl1"; |CARGA_DEF aDef, gomantl1@;
     |SI FSalida ! 0; |ACABA; |FINSI;
     aDef = aPathDef + "gopre001"; |CARGA_DEF aDef, gopre001@;
     aDef = aPathDef + "gopre002"; |CARGA_DEF aDef, gopre002@;
     aDef = aPathDef + "gopre003"; |CARGA_DEF aDef, gopre003@;
     aDef = aPathDef + "goparame"; |CARGA_DEF aDef, goparame@;
     aDef = aPathDef + "gopre021"; |CARGA_DEF aDef, gopre021@;

     |PATH_DAT #gomantl1@#aPathDat#;
     |PATH_DAT #gopre001@#aPathDat#;
     |PATH_DAT #gopre002@#aPathDat#;
     |PATH_DAT #gopre003@#aPathDat#;
     |PATH_DAT #goparame@#aPathDat#;
     |PATH_DAT #gopre021@#aPathDat#;

     |SI #1#4 = "C"; |O #1#4 = "P";
          aSer = #aomentrh#0;
          |QBF aSer;
          aSer = ("00000" + aSer) % -105;
          |ABRE #gomantl1@;
          |SELECT #2#gomantl1@;
          #gomantl1@#0 = aSer;
          #gomantl1@#1 = #aomentrh#1;
          #gomantl1@#3 = #aomentrh#9;
          #gomantl1@#4 = #aomentrh#10;
          |LEE 000#gomantl1@.];
          |SI FSalida = 0; |Y #gomantl1@#0 = aSer; |Y #gomantl1@#1 = #aomentrh#1;
               |CONSULTA_F_CLAVE #1#gomantl1@, MinPed, MaxPed;
               |SI FSalida ! 0;
                    |ERROR;
               |SINO;
                    #aomentrh#9 = #gomantl1@#3;
                    #aomentrh#10 = #gomantl1@#4;

                    |PINTA #aomentrh#9;
                    |PINTA #aomentrh#10;

                    |ABRE #gopre001@;
                    #gopre001@#0 = #aomentrh#9;
                    #gopre001@#1 = #aomentrh#10;
                    |LEE 020#gopre001@.=;
                    |CIERRA #gopre001@;
                    aTipo = #gopre001@#18;

                    || AGRUPACIONES DE CAPITULOS

                    nAgrup = 0;
                    aDesAgrup = "";
                    |SI #gopre001@#49 = "S";
                         |ABRE #gopre021@;

                         alfa1 = Usuario; |QBT alfa1; alfa1 = ("t" + alfa1) % 108;
                         |NOME_DAT #50 alfa1;
                         |DELINDEX #50;
                         |ABRE #50;
                         |BUCLE CargaAgrup;
                         |CONSULTA_CLAVE #1#aomtemp4;
                         |SI FSalida = 0;
                              nAgrup = #aomtemp4#0;
                              aDesAgrup = #aomtemp4#1;

                              #aomentrh#13 = nAgrup;
                              #aomentrh#14 = aDesAgrup;

                              |PINTA #aomentrh#13;
                              |PINTA #aomentrh#14;
                         |FINSI;
                         |CIERRA #50;
                         |CIERRA #gopre021@;

                         alfa1 = Usuario; |QBT alfa1; alfa1 = ("u" + alfa1) % 108;
                         |NOME_DAT #51 alfa1;
                         |DELINDEX #51;
                         |ABRE #51;
                         nLinea = 0;
                         |BUCLE CargaAgrupPres;

                         ||CONSULTA_F_CLAVE #1#gopre002@, MinPed, MaxPed;
                         |CONSULTA_CLAVE #1#aomtemp5;
                         |SI FSalida ! 0;
                              |ERROR;
                         |SINO;
                              #aomentrh#11 = #aomtemp5#0;
                              #aomentrh#12 = #aomtemp5#1;
                              #aomentrh#13 = nAgrup;

                              |PINTA #aomentrh#11;
                              |PINTA #aomentrh#12;
                              |PINTA #aomentrh#13;
                         |FINSI;
                         |CIERRA #51;
                    |SINO;
                         |ABRE #gopre002@;
                         |SELECT #6#gopre002@;
                         #gopre002@#0 = #aomentrh#9;
                         #gopre002@#1 = #aomentrh#10;
                         #gopre002@#12 = #aomentrh#12;
                         |LEE 000#gopre002@.];

                         ||CONSULTA_F_CLAVE #1#gopre002@, MinPed, MaxPed;
                         |CONSULTA_F_CLAVE #7#gopre002@, MinPed, MaxPed;
                         |SI FSalida ! 0;
                              |ERROR;
                         |SINO;
                              #aomentrh#11 = #gopre002@#2;
                              #aomentrh#12 = #gopre002@#12;
                              #aomentrh#13 = #gopre002@#14;

                              |PINTA #aomentrh#11;
                              |PINTA #aomentrh#12;
                              |PINTA #aomentrh#13;
                         |FINSI;
                         |CIERRA #gopre002@;
                    |FINSI;

                    |SI #1#4 = "P";
                         |ABRE #gopre003@;
                         |SELECT #2#gopre003@;
                         #gopre003@#0 = #aomentrh#9;
                         #gopre003@#1 = #aomentrh#10;
                         #gopre003@#2 = #aomentrh#12;
                         #gopre003@#4 = #aomentrh#15;
                         |LEE 000#gopre003@.];
                         |CONSULTA_F_CLAVE #1#gopre003@, MinPed, MaxPed;
                         |SI FSalida ! 0;
                              |ERROR;
                         |SINO;
                              #aomentrh#15 = #gopre003@#4;
                              #aomentrh#16 = #gopre003@#5;
                              |PINTA #aomentrh#15;
                              |PINTA #aomentrh#16;
                         |FINSI;
                         |CIERRA #gopre003@;
                    |FINSI;
               |FINSI;
          |FINSI;
     |SINO;
          |MENAV "0000No existe lineas de expediente...";
          |ERROR;
     |FINSI;

     |DESCARGA_DEF #gopre021@;
     |DESCARGA_DEF #goparame@;
     |DESCARGA_DEF #gopre003@;
     |DESCARGA_DEF #gopre002@;
     |DESCARGA_DEF #gopre001@;
     |DESCARGA_DEF #gomantl1@;
|FPRC;

|PROCESO Observ; |TIPO 0;
     nError = 0;
     |SI FSalida = 10;
          nError = 1;
          |ERROR;
     |FINSI;
     |SI FSalida > 11;
          nError = 1;
          |ERROR;
     |FINSI;
     |SI FSalida = 11;
          |SI aTipo = "A";
               eaSerie = #aomentrh#9;
               enNumero = #aomentrh#10;
               efFecha = #aomentrh#6;
               enEmpresa = #aomentrh#2;
               enTrabaj = #aomentrh#4;
               |SUB_EJECUTA_NP "aomcomen";
               |ERROR;
               nError = 1;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO MensaLin; |TIPO 7;
     |SI aTipo = "A";
          |MENSA "    Introduzca la unidades a adjudicar o <F3> para editar observaciones del parte";
     |SINO;
          |MENSA "    Introduzca la unidades a adjudicar";
     |FINSI;
|FPRC;

|PROCESO Albaran;
     |SI nError ! 0; |ACABA; |FINSI;

     |ABRE #1; |LEE 000#1p; |CIERRA #1;

     |DBASS aPath;
     |HAZ Produccion2;
     aPathBas = aPath + #1#1; |QBF aPathBas;
     aPathDef = aPathBas + "def/";
     aPathDat = aPathBas + "fich/" + (("00000" + #aomentr4#2) % -105) + "/";

     aDef = aPathDef + "gopre001"; |CARGA_DEF aDef, gopre001@;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |PATH_DAT #gopre001@#aPathDat#;
     |ABRE #gopre001@;
     #gopre001@#0 = #aomentrh#9;
     #gopre001@#1 = #aomentrh#10;
     |LEE 020#gopre001@.=;
     |SI #gopre001@#18 = "A";
          |CONFI "0000NDesea Introducir Materiales?";
          |SI FSalida = 0;
               aAlfa = #aomentrh#9 + (("000000" + #aomentrh#10) % -106);
               aAlfa = aAlfa + (("000000" + #aomentr3#0) % -105);
               aAlfa = aAlfa + (("000000" + #aomentr3#1) % -106);
               aAlfa = aAlfa + #aomentrh#6;
               PRMNT_param = aAlfa;
               aAlfa = ":dscomer9/vipez006.run agif0041 ";
               aAlfa = aAlfa + (("00000" + #aomentr4#2) % -105);
               |SUB_EJECUTA_NP aAlfa;
          |FINSI;
     |FINSI;
     |CIERRA #gopre001@;
     |PATH_DAT #gopre001@#aPathDat#;
     |DESCARGA_DEF #gopre001@;
|FPRC;

|PROCESO Uni7; |TIPO 7;
     |ESTADO_CONTROL enBoton, "ENABLE";
|FPRC;

|PROCESO Uni0; |TIPO 0;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |HAZ NoRepe;
          |SI aRepe = "S"; |ERROR; |ACABA; |FINSI;
     |FINSI;

     |ESTADO_CONTROL enBoton, "DISABLE";
     |SI FSalida ! 13; |ACABA; |FINSI;

     |ABRE #1; |LEE 000#1p; |CIERRA #1;

     |DBASS aPath;
     |HAZ Produccion2;
     aPathBas = aPath + #1#1; |QBF aPathBas;
     aPathDef = aPathBas + "def/";
     aPathDat = aPathBas + "fich/" + (("00000" + #aomentr4#2) % -105) + "/";

     aDef = aPathDef + "gopre001"; |CARGA_DEF aDef, gopre001@;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |PATH_DAT #gopre001@#aPathDat#;
     |ABRE #gopre001@;
     #gopre001@#0 = #aomentrh#9;
     #gopre001@#1 = #aomentrh#10;
     |LEE 020#gopre001@.=;
     |SI #gopre001@#18 = "A";
          aAlfa = #aomentrh#9 + (("000000" + #aomentrh#10) % -106);
          aAlfa = aAlfa + (("000000" + #aomentr3#0) % -105);
          aAlfa = aAlfa + (("000000" + #aomentr3#1) % -106);
          aAlfa = aAlfa + #aomentrh#6;
          PRMNT_param = aAlfa;
          PRMNT_capi = #aomentrh#11;
          aAlfa = ":dscomer9/vipez007.run agif0041 ";
          aAlfa = aAlfa + (("00000" + #aomentr4#2) % -105);
          |SUB_EJECUTA_NP aAlfa;
          |ERROR; |PTEC 807;
     |SINO;
          |MENAV "0000El presupuesto no es de administracion.";
     |FINSI;
     |CIERRA #gopre001@;
     |PATH_DAT #gopre001@#aPathDat#;
     |DESCARGA_DEF #gopre001@;
|FPRC;

|PROCESO NoRepe;
     aRepe = "N";
     |DDEF aAlfa;
     aAlfa = aAlfa + "aomentrh";
     |CARGA_DEF aAlfa, aomentrh@;
     |SI FSalida = 0;
          |ABRE #aomentrh@;
          |SELECT #2#aomentrh@;
          #aomentrh@#0 = #aomentrh#0;
          #aomentrh@#1 = #aomentrh#1;
          #aomentrh@#2 = #aomentrh#2;
          #aomentrh@#4 = #aomentrh#4;
          #aomentrh@#6 = #aomentrh#6;
          #aomentrh@#9 = #aomentrh#9;
          #aomentrh@#10 = #aomentrh#10;
          #aomentrh@#12 = #aomentrh#12;
          #aomentrh@#15 = #aomentrh#15;
          |LEE 000#aomentrh@.=;
          |SI FSalida = 0;
               aRepe = "S";
               |MENAV "0000No se puede repetir presupuesto, capitulo, partida para el mismo dia...";
          |FINSI;
          |DESCARGA_DEF #aomentrh@;
     |FINSI;
|FPRC;
