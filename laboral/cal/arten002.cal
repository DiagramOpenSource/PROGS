|FICHEROS;
     arten002 #0;
|FIN;

|VARIABLES;
     aAlfa = "";
     aUltimo = "";
|FIN;

|PROCESO Limpia;
     |SI #0#3 = "A";
          |CAMPO_MODIFICA #0#2, 1, "N";
          #0#2 = "";
          |PINTA #0#2;
     |SINO;
          |CAMPO_MODIFICA #0#2, 1, "S";
     |FINSI;
|FPRC;

|PROCESO Barra;
     aAlfa = #0#1;
     |QBF aAlfa;

     aUltimo = aAlfa % -101;
     |SI aUltimo ! "/"; |Y aUltimo ! "\";
          aAlfa = aAlfa + "/";
          #0#1 = aAlfa;
     |FINSI;
|FPRC;

|PROGRAMA;
     |CLS;
     |CABEZA "Control Aplicacion";
     |PINPA #0#0;
     |ABRE #0;
     |LEE 001#0p;
     |SI FSalida ! 0;
          |DDEFECTO #0;
          |GRABA 020#0n;
     |FINSI;
     |LEE 101#0=;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |GRABA 020#0a;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;
