     nCampo2             = 0;
     FEstado             = 0;
     nMes                = 0;
     nAny                = 0;
     nNume               = 0;
     nTramos             = 0;
     nImpresion          = 0;
     nMesDesde           = 0;
     nMesHasta           = 0;
     nAnyDesde           = 0;
     nAnyHasta           = 0;
     nPasada             = 0;
     nOk                 = 0;
     nTp                 = 0;
     nDTp                = 0;
     nHTp                = 0;
     nOpcion             = 0;

     aReferencia         = "";
     aFecha              = "";
     aAlfa               = "";
     aAntCCC             = "";
     aEmp                = "";
     aNomEmp             = "";
     aFic                = "";

     fFecha              = @;

 {-1}aMenu               = "";
     aMenu1              = "2400";
     aMenu2              = "1";
     aMenu3              = "Archivo documental activado. Elija opci¢n: ";
     aMenu4              = "IAB";
     aMenu5              = "Imprimir";
     aMenu6              = "Archivar";
     aMenu7              = "Ambos";

     &nError             = 0;
     &eaVengoDe          = "";
