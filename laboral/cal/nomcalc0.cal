|FICHEROS;
     nomconax #14;
|FIN;

|VARIABLES;
     &nHospi_Acc = 0;                || SIN hospitalizacion
     &nz_D = 0;
     &nz_H = 0;
     &ny_D = 0;
     &ny_H = 0;
|FIN;

|PROCESO simula;
     || vamos a generar:    DESDE    HASTA
     || 1ra linea acc:       nz_D     nz_H
     || 2da linea acc:      #14#47   #14#49
     || 3ra linea acc:      #14#48   #14#50
     || 4a  linea acc:       ny_D     ny_H

     || y a calcular los dias de desglose de tramos en funcion a eso
     || asi sacaremos dacci_Z (para el segundo tramo) y dacci_Y para el tercero

     |SI nHospi_Acc = 0;                || SIN hospitalizacion
          |SI #14#47 ! 1;
               nz_D = 1;
               nz_H = #14#47 - 1;
          |SINO;
               nz_D = 0;
               nz_H = 0;
          |FINSI;
          |SI #14#48 = 0; |Y #14#49 ! 999;
               #14#48 = #14#49 + 1;
               #14#50 = 999;
          |FINSI;
          |SI #14#50 ! 0; |Y #14#50 ! 999;
               ny_D = #14#50 + 1;
               ny_H = 999;
          |SINO;
               ny_D = 0;
               ny_H = 0;
          |FINSI;
     |SINO;
          |SI #14#168 ! 1;
               nz_D = 1;
               nz_H = #14#168 - 1;
          |SINO;
               nz_D = 0;
               nz_H = 0;
          |FINSI;
          |SI #14#169 = 0; |Y #14#170 ! 999;
               #14#169 = #14#170 + 1;
               #14#171 = 999;
          |FINSI;
          |SI #14#171 ! 0; |Y #14#171 ! 999;
               ny_D = #14#171 + 1;
               ny_H = 999;
          |SINO;
               ny_D = 0;
               ny_H = 0;
          |FINSI;
     |FINSI;
|FPRC;
