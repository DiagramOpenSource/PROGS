     nomcnaco #0;
     nomcnama #1;
     nomocuma;
     nomtmpfe #500;
|FIN;

|VARIABLES;
     aAlfa = "";
     nModo = 0;
     nPos1 = 0;
     nPos2 = 0;
|FIN;

|PROCESO nomcnama;
     #500#0 = #nomcnama#6;
     |LEE 101#500=;
     |SI FSalida ! 0;
          #500#0 = #nomcnama#6;
          |GRABA 020#500n;
          |LIBERA #500;
     |FINSI;
|FPRC;

|DEFBUCLE nomcnama;
     #nomcnama#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, nomcnama;
|FIN;

|PROCESO lee_fechas; |TIPO 66;
          aAlfa = Usuario; |QBT aAlfa;    aAlfa = ("f" + aAlfa) % 108;
          |NOME_DAT #500 aAlfa;
          |DELINDEX #500;

          |ABRE #500;
          |BUCLE nomcnama;

          nModo = 2 + 4 + 8 + 16;
          nPos1 = 0860; nPos2 = 1480;
          |LINEAL_SIMPLE #1#500, nModo, nPos1, nPos2, NULL, NULL, NULL;
          |SI FSalida = 0;
               #nomcnaco#0 = #500#0;
               |PINTA #nomcnaco#0;
          |FINSI;

          |CIERRA #500;
          |ERROR;
          |ACABA;
|FPRC;

|PROCESO superior;
     #nomcnama#6 = #nomcnaco#0;
     #nomcnama#0 = "zzzzzzzzzz";
|FPRC;

|PROCESO inferior;
     #nomcnama#6 = #nomcnaco#0;
     #nomcnama#0 = "          ";
|FPRC;
