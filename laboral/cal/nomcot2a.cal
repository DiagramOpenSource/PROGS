|FICHEROS;
     nomcot2a;
     nomcot2b,MANTE;
     nomcalca;
|FIN;

|VARIABLES;
     aAlfa1 = "";
|FIN;

|PROCESO NombreCampo;
     aAlfa1 = #nomcot2a#0;
     |QBF aAlfa1;
     aAlfa1 = ("000" + aAlfa1) % -103;
     aAlfa1 = "|C" + aAlfa1 + "NOMBRE";
     #nomcot2a#1 = #nomcalca#aAlfa1#;
     |PINTA #nomcot2a#1;
|FPRC;

|PROCESO TipoImporte;
     |SI #nomcot2b#4 = 1; #nomcot2b#5 = "Base "; |FINSI;
     |SI #nomcot2b#4 = 2; #nomcot2b#5 = "C.Tra"; |FINSI;
     |SI #nomcot2b#4 = 3; #nomcot2b#5 = "C.Emp"; |FINSI;
     |SI #nomcot2b#4 = 4; #nomcot2b#5 = "C.Tot"; |FINSI;
     |SI #nomcot2b#4 = 5; #nomcot2b#5 = "Horas"; |FINSI;
     |SI #nomcot2b#4 = 6; #nomcot2b#5 = "Cod. "; |FINSI;
     |PINTA #nomcot2b#5;
|FPRC;
