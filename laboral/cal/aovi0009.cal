|FICHEROS;
     aovi0009 #0;
     aovi0008;

     labempre;
     labtraba;
|FIN;

|VARIABLES;
     fFecha = @;
     aFecha = "";
     aAny = "";
     aMes = "";
     nAny = 0;
     nMes = 0;
|FIN;

|PROCESO Defectos; |TIPO 7;
     fFecha = ~;
     aFecha = fFecha;
     aAny = aFecha % -104;
     aMes = aFecha % 402;
     nAny = aAny;
     nMes = aMes;
     #0#4 = nAny;
     #0#5 = nMes;
     |PINTA #0#4;
     |PINTA #0#5;
|FPRC;

|PROCESO Impresion;
     #labempre#0 = #aovi0008#0;
     |LEE 000#labempre.=;

     #labtraba#0 = #aovi0008#0;
     #labtraba#1 = #aovi0008#3;
     |LEE 000#labtraba.=;

     |PRINT;
     |PIEINF;
|FPRC;

|DEFBUCLE Impresion;
     #aovi0008#1;
     ;
     #0#0, #0#4, #0#5, #0#2;
     #0#1, #0#4, #0#5, #0#3;
     ;
     NULL, Impresion;
|FIN;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |IMPRE 0;
          |SI FSalida = 0;
               |ABRE #labempre;
               |ABRE #labtraba;

               |INFOR "aovi0009";
               |BUCLE Impresion;
               |FININF;

               |CIERRA #labtraba;
               |CIERRA #labempre;
          |FINSI;
          |FINIMP;
          || AQUI LA IMPRESION
     |FINSI;
|FPRO;
