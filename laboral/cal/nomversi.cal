|FICHEROS;
     nomversi;
|FIN;

|VARIABLES;
     aBase = "";
     aAlfa = "";
     aMenu = "";
     aArchivo = "";
     nCanal = 0;
     aLinea = "";
     aVersion = "";
|FIN;

|PROGRAMA;
     |CLS;

     |DBASS aBase;
     |QBF aBase;
     aMenu = aBase + "laboral/laboral.men";
     aArchivo = aMenu;

     |XABRE "U", aArchivo, nCanal;
     |SI FSalida ] 0;
          |XLEE nCanal, 100, aLinea;
     |FINSI;
     |XCIERRA nCanal;

     aLinea = aLinea - "#:PI Nominas y S.Social ";
     aVersion = aLinea;

     aAlfa = "    Se encuentra instalada la versión " + aVersion;
     aAlfa = aAlfa + " de la aplicación laboral";
     |MENAV aAlfa;
|FPRO;
