|PROCESO Impresion;
     |ABRE #labcontr; |LEE 000#labcontr.p; |CIERRA #labcontr;
     |SI #labcontr#84 ! "S";
          |ACABA;
     |FINSI;

     eaQueEs = "C";

     aAlfa = "*laborn10;X";
     aAlfa = aAlfa + "18";
     aAlfa = aAlfa + #laborn09#0;
     aAlfa = aAlfa + #laborn09#1;
     aAlfa1 = #laborn09#3;
     aAlfa = aAlfa + aAlfa1;
     aAlfa = aAlfa + #laborn09#4;
     aAlfa = aAlfa + "IMPRE";

     |MENSA "    Imprimiendo solicitud de corrección";
     |SUB_EJECUTA aAlfa;
|FPRC;

|PROCESO Contador;
     |ABRE #laborn90;

     #laborn90#0 = #laborn09#5;
     #laborn90#1 = 99999;

     |LEE 000#laborn90.];
     |SI FSalida = 0;
          |SI #laborn90#0 ! #laborn09#5;
               |LEE 000#laborn90.a;
          |FINSI;
     |SINO;
          |LEE 000#laborn90.u;
     |FINSI;

     |SI FSalida = 0;
     |Y #laborn90#0 = #laborn09#5;
          nNume = #laborn90#1;

          |DDEFECTO #laborn90;
          #laborn90#0 = #laborn09#5;
          #laborn90#1 = nNume + 1;

     |SINO;
          || es el primero de esta empresa !!!
          |DDEFECTO #laborn90;
          #laborn90#0 = #laborn09#5;
          #laborn90#1 = 1;
     |FINSI;

     #laborn90#2 = "E";
     #laborn90#3 = "Embargo";
     #laborn90#4 = #laborn09#1;
     #laborn90#5 = #laborn09#2;
     #laborn90#6 = #laborn09#3;
     #laborn90#7 = #laborn09#4;

     |GRABA 020#laborn90.n;

     #laborn09#21 = #laborn90#1;

     |LIBERA #laborn90;
     |CIERRA #laborn90;
|FPRC;

|PROGRAMA;
     |CLS;

     |DFICE aAlfa1;
     |DFICB aAlfa2;

     |SI aAlfa1 ! aAlfa2;
          |PATH_DAT #labempre#aAlfa1#;
     |FINSI;

     aParam = PARAMETRO;
     |SI aParam = "MODIF"; |O aParam = "CONSUL";
          |ABRE #laborn09;
          |SI aParam = "MODIF";
               #laborn09#0 = "P";         || estado: pendiente, consulta/modificacion
          |FINSI;
          |SI aParam = "CONSUL";
               #laborn09#0 = "A";         || estado: actualizado, solo consulta
          |FINSI;
          #laborn09#1 = eaUsuario;
          #laborn09#2 = "";
          #laborn09#3 = eaFecha;
          #laborn09#4 = eaHora;
          |LEE 101#laborn09.=;

          |PARA nCampo = 0; |SI nCampo [ 28; |HACIENDO nCampo = nCampo + 1;
               #laborn19#nCampo# = #laborn09#nCampo#;
          |SIGUE;
          |PINPA #0#laborn19;
          |PINDA #0#laborn19;
          |HAZ Activa28;

          |ABRE #labtraba;
          #labtraba#0 = #laborn19#5;
          #labtraba#1 = #laborn19#6;
          |LEE 000#labtraba.=;
          |CIERRA #labtraba;

          |SI aParam = "MODIF";
               |ENDATOS #1#laborn19;
          |FINSI;
          |SI aParam = "CONSUL";
               |PARA nCampo = 0; |SI nCampo [ 28; |HACIENDO nCampo = nCampo + 1;
                    #laborn19#nCampo# = #laborn09#nCampo#;
               |SIGUE;
               |PINDA #0#laborn19;
               |HAZ Activa28;
               |PAUSA;
          |FINSI;
     |SINO;
          ||HAZ Proceso;
          |CLS;
          |PINPA #0#laborn19;
          |PINDA #0#laborn19;
          |HAZ Activa28;
          |ENDATOS #1#laborn19;
     |FINSI;

     |SI FSalida = 0; |Y aParam ! "CONSUL";
          |SI aParam = "MODIF";
               |PARA nCampo = 0; |SI nCampo [ 28; |HACIENDO nCampo = nCampo + 1;
                    #laborn09#nCampo# = #laborn19#nCampo#;
               |SIGUE;
               |GRABA 020#laborn09.a;
          |SINO;
               |ABRE #laborn09;
               #laborn09#0 = "P";    || Estado: Pendiente
               #laborn09#1 = Usuario % 0810;     || Usuario
               #laborn09#2 = "";     || Perfiles, en principio no lo uso
               #laborn09#3 = ~;      || Fecha
               |HORA aHora;
               #laborn09#4 = aHora;  || Hora
               |PARA nCampo = 5; |SI nCampo [ 28; |HACIENDO nCampo = nCampo + 1;
                    #laborn09#nCampo# = #laborn19#nCampo#;
               |SIGUE;
               |HAZ Contador;
               |GRABA 020#laborn09.n;

               |SI FSalida = 0;
                    |MENAV "    Se ha grabado correctamente la solicitud de embargo";
               |FINSI;
               ||HAZ Impresion;
               |CIERRA #laborn09;
/*
               |ABRE #nompmail; |LEE 000#nompmail.p; |CIERRA #nompmail;
               |SI #nompmail#11 = "S";
                    |HAZ Email;
               |FINSI;
*/
          |FINSI;

          |CIERRA #laborn09;
     |FINSI;

     |DFICE aAlfa1;
     |DFICB aAlfa2;

     |SI aAlfa1 ! aAlfa2;
