|PROCESO Email;
     aAlfa = #laborn19#5; |QBF aAlfa;
     eaEmpresa = ("00000" + aAlfa) % -105;
     eaEmpresa = eaEmpresa + " - " + #labempre#2;

     aAlfa = #laborn19#6; |QBF aAlfa;
     eaTrabajador = ("00000" + aAlfa) % -105;
     eaTrabajador = eaTrabajador + " - " + #labtraba#2;

     eaAccion = "embargo";

     |HAZ ConsEmail;
|FPRC;

