|PROCESO Empresas19; |TIPO 7;
     |ABRE #labempre;
     |BUCLE labempre19;
     |CIERRA #labempre;

     |SI nEmpresas = 1;
          || tengo una unica empresa que elegir

          #laborn19#5 = nCodEmp;
          |CAMPO_MODIFICA #laborn19#5, 1, "N";
          |PINTA #laborn19#5;
          #laborn19#8 = aNomEmp;
          |PINTA #laborn19#8;
     |FINSI;
|FPRC;

