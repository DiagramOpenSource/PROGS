|FICHEROS;
     nomlicoc #0;
     nomlicol #1,MANTE;
     nomhicca;
     labtraba;
|FIN;

|VARIABLES;
     fFecha = @;
     aFecha = "";
|FIN;

|PROCESO Defecto; |TIPO 7;
     fFecha = ~;
     aFecha = fFecha;
     aFecha = aFecha % -104;
     #0#1 = aFecha;
     #0#1 = #0#1 - 1;
     |PINTA #0#1;
|FPRC;

|PROCESO Totales; |TIPO 7;
     |SI #labtraba#247 = 911;      || Regimen del Carbon
          #1#8 = #1#5 * #1#6;
          #1#9 = #1#5 * #1#7;
          |PINTA #1#8;
          |PINTA #1#9;
     |SINO;
          |SI #1#10 ! 0;
               #1#8 = #1#10 * #1#6;
               #1#9 = #1#10 * #1#7;
          |SINO;
               #1#8 = #1#5 * #1#6;
               #1#9 = #1#5 * #1#7;
          |FINSI;
          |PINTA #1#8;
          |PINTA #1#9;
     |FINSI;
|FPRC;

|PROCESO Dias; |TIPO 7;
     |ABRE #nomhicca;
     #nomhicca#0 = #1#0;
     #nomhicca#1 = #1#3;
     #nomhicca#66 = #1#1 - 2000;
     #nomhicca#2 = #1#2;
     #nomhicca#3 = 0;
     |LEE 000#nomhicca.=;
     |SI FSalida = 0;
          |SI #labtraba#247 = 911;      || Regimen del Carbon
               #1#5 = #nomhicca#8;
               #1#10 = #nomhicca#55;     || dias trabajados
               #1#11 = #nomhicca#132;   || dias regimen carbon IT (laborables IT)
          |SINO;
               #1#5 = #nomhicca#8;
               #1#10 = #nomhicca#133;     || dias trabajados
               #1#11 = #nomhicca#31 + #nomhicca#32 + #nomhicca#65;  || dias IT
          |FINSI;
          |PINTA #1#5;
          |PINTA #1#10;
          |PINTA #1#11;
     |FINSI;
     |CIERRA #nomhicca;
|FPRC;
