|FICHEROS;
     marin001 #0;
|FIN;

|VARIABLES;
     nCampo = 0;
     nFecha = 0;
|FIN;

|PROCESO UnAnyo;
     nCampo = Campo + 1;
     #0nCampo = #0Campo + 0.012;
     nFecha = #0nCampo
     nFecha = nFecha - 1;
     #0nCampo = nFecha;
     |PINTA #0nCampo;
|FPRC;

|PROCESO SinPrevencion;
     |SI #0Campo = "N";
          |CAMPO_MODIFICA #0#2, 1, "N";
          |CAMPO_MODIFICA #0#3, 1, "N";
          |CAMPO_MODIFICA #0#4, 1, "N";
          #0#2 = "00.00.0000";
          #0#3 = "00.00.0000";
          |PINTA #0#2;
          |PINTA #0#3;
     |SINO;
          |CAMPO_MODIFICA #0#2, 1, "S";
          |CAMPO_MODIFICA #0#3, 1, "S";
          |CAMPO_MODIFICA #0#4, 1, "S";
          #0#2 = ~;
          #0#3 = ~;
          |PINTA #0#2;
          |PINTA #0#3;
     |FINSI;
|FPRC;

|PROCESO SinMedicina;
     |SI #0Campo = "N";
          |CAMPO_MODIFICA #0#6, 1, "N";
          |CAMPO_MODIFICA #0#7, 1, "N";
          #0#6 = "00.00.0000"
          #0#7 = "00.00.0000"
          |PINTA #0#6;
          |PINTA #0#7;
     |SINO;
          |CAMPO_MODIFICA #0#6, 1, "S";
          |CAMPO_MODIFICA #0#7, 1, "S";
          #0#6 = ~;
          #0#7 = ~;
          |PINTA #0#6;
          |PINTA #0#7;
     |FINSI;
|FPRC;
