    alfa1 = "";
    alfa2 = "";
    alfa3 = "";
    alfa4 = "";
    nume0 = 0;
    nume1 = 0;
    nume2 = 0;
    nume3 = 0;
    nume4 = 0;
    nume5 = 0;
    fech1 = @;
    fech2 = @;
    fech3 = @;
    fech4 = @;
    fecsys = @;
    fecsis = @;
    fecbaj = "";
    fecalf = "";
    topemes = 0;
    bisiesto = 0;
    dia = 0;
    any = 0;
    mes = 0;
