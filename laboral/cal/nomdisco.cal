     |FINSI;
     |SI aMonDisco = "E";
          alfa4 = #13#6;  alfa4 = "000000000000" + alfa4;  alfa4 = alfa4 % -112;
     |FINSI;

     |HAZ total;

     #0#4 = 2;
     |SI #0#4 = 1;
         alfa5 = (#13#5 % 104) + (#13#5 % 504) + (#13#5 % 910);  || cuenta cte.
         alfa6 = #13#5 % 1902;     || digito control
     |SINO;
