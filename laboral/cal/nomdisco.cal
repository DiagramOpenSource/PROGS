    |SI aTipo = "EMB"; alfa8 = ""; |FINSI;
    traba = "012" + alfa8;  |HAZ lineas;          || domicilio+numero+piso

    alfa9 = alfa8 % 0;  nume1 = alfa9;  nume1 = nume1 - 36;
|SI nume1 > 0;
        nume1 = (37 * 100) + nume1;
        traba = alfa8 % nume1;
        traba = "013" + traba;  |HAZ lineas;          || resto domicilio
