    alfa8 = #2#8;  alfa8 = "00"  + alfa8;  alfa8 = alfa8 % -102;
    alfa9 = #2#9;  alfa9 = "000" + alfa9;  alfa9 = alfa9 % -103;
    traba = "014" + alfa8 + alfa9 + " " + #2#6;       || cod.postal+pob.
    |SI aTipo = "EMB"; traba = "014"; |FINSI;
|HAZ lineas;

