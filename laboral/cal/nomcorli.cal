|FICHEROS;
     nomcorli;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO Tipo5; |TIPO 5;
     #nomcorli#9 = "*";
     |LINEAS_BI_ATRI #nomcorli#9, 0, I, -1;
|FPRC;

|PROCESO Tipo3Lin; |TIPO 3;
     |SI FSalida = 0;
          #nomcorli#9 = "";
     |FINSI;
|FPRC;

|PROCESO Tipo1; |TIPO 1;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |ERROR;
     |FINSI;
|FPRC;
