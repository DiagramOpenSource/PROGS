|FICHEROS;
    labcopho #0;
    labcentr #1;
    labcalen #2;
    labempre #4;
|FIN;

|VARIABLES;
    nume1 = 0;
|FIN;

|PROCESO copia; |TIPO 3;
|ABRE #2;
    #2#00 = #0#0;
    #2#49 = #0#1;
|LEE 000#2=;
|SI FSalida = 0;
        #2#00 = #0#2;
        #2#49 = #0#3;
    |GRABA 000#2n;
    |SI FSalida ! 0; |Y #0#4 = "S";
        |BORRA 020#2c;
        |GRABA 020#2n;
    |FINSI;
|SINO;
    |MENAV "    ATENCION !!! Codigo origen inexistente ...";
|FINSI;
|CIERRA #2;
|FPRC;

|PROCESO pinta1; |TIPO 0;
|SI #0Campo ! 0;
    |ABRE #4;
        #4#0 = #0Campo;
    |LEE 000#4=;
    |SI FSalida ! 0;
        |DDEFECTO #4;
            #4#2 = "INEXISTENTE !!!";
    |FINSI;
    |SI Campo = 0;  nume1 = 11;  |SINO;  nume1 = 14;  |FINSI;
        nume1 = (nume1 * 100) + 32;
    |ATRI I;
    |PINTA nume1, #4#2;
    |ATRI 0;
    |CIERRA #4;
|FINSI;
|FPRC;

|PROCESO pinta2; |TIPO 0;
|SI #0Campo ! 0;
    |ABRE #1;
        nume1 = Campo - 1;
        #1#0 = #0nume1;
        #1#1 = #0Campo;
    |LEE 040#1=;
    |SI FSalida ! 0;
        |DDEFECTO #1;
            #1#3 = "INEXISTENTE !!!";
    |FINSI;
    |SI Campo = 1;  nume1 = 12;  |SINO;  nume1 = 15;  |FINSI;
        nume1 = (nume1 * 100) + 32;
    |ATRI I;
    |PINTA nume1, #1#3;
    |ATRI 0;
    |CIERRA #1;
|FINSI;
|FPRC;
