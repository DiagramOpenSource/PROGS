     |SI fech2 < fech1; |Y FSalida ! 2; |Y FSalida ! 4;
          aAlfa = "    ATENCION: La fecha de inicio de contrato es anterior ";
          aAlfa = aAlfa + "a la fecha de nacimiento del trabajador.";
          |MENAV aAlfa;
