|FICHEROS;
     nomocu00 #0;
     nomocuhi #1,MANTE;
|FIN;

|VARIABLES;
     aAlfa = "";
     nModo = 0;
     fFecha = @;
     &nElAny = 0;
     &nElMes = 0;
|FIN;

|PROCESO superior;
     #1#0 = #0#0;
     #1#4 = 99999;
     #1#2 = #0#1;
|FPRC;

|PROCESO inferior;
     #1#0 = #0#0;
     #1#4 = 00001;
     #1#2 = #0#1;
|FPRC;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |HAZ ITIMS00;
          |ENTLINEAL #1#nomocuhi, 1, 3, 22, 0, inferior, superior;
     |FINSI;
|FPRO;

|PROCESO ITIMS00;
     nElAny = #0#1;
     nElMes = 01;
     |HAZ ValoresITIMS;
|FPRC;
