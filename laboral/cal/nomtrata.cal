|FICHEROS;
     nomtrato #0;
     nomtrata;
     labtraba;
     labcon00;
     coptrata@;
|FIN;

|VARIABLES;
     nModo = 0;
     nContrato = 0;
     aContrato = "";
     aPassword = "";
|FIN;

|PROCESO labtraba;
     nContrato = #labtraba#45;

     #nomtrato#0 = #0#0;
     #nomtrato#1 = nContrato;
     |LEE 101#nomtrato.=
     |SI FSalida ! 0
          aContrato = nContrato;
          aContrato = "000" + aContrato;
          aContrato = aContrato % -103;
          #labcon00#0 = aContrato;
          |LEE 000#labcon00.=;
          |SI FSalida = 0;
               #nomtrato#2 = #labcon00#1;
          |FINSI;
          |GRABA 020#nomtrato.n;
     |FINSI;
|FPRC;

|DEFBUCLE labtraba;
     #labtraba#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, labtraba;
|FIN;

|PROCESO pase;
     nModo = (FEntrada/100) ! 0;
     |SI nModo ! 1; |Y nModo ! 2;
          |ACABA;
     |FINSI;
     |SI FSalida = 13;
          |ABRE #labcon00;
          |ABRE #nomtrato;
          |BUCLE labtraba;
          |CIERRA #nomtrato;
          |CIERRA #labcon00;
     |FINSI;
|FPRC;

|PROCESO Tipo88; |TIPO 88;
     |CLS;
     |CUADRO 0501 2380;
     |BLANCO 0602 2279;
     |PINTA 1320, "Introduzca Password : ";
     E_inf = "";
     E_sup = "20";
     aPassword = 1342 ? -1;
     |SI aPassword ! "<MYPASWD>           ";
      |Y aPassword ! "<MYPASWD>               ";
         |MENAV "     Permiso denegado.";
         |PTEC 806;
     |FINSI;
|FPRC;

