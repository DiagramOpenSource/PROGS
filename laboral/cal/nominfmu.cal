|FICHEROS;
     nominfmu #0;
     labempre;
     labcentr;
|FIN;

|VARIABLES;
     nMutua = 0;
|FIN;

|PROCESO labcentr;
     nMutua = 0;
     #labempre#0 = #labcentr#0;
     |LEE 000#labempre.=;

     |SI #labcentr#13 ! 0;
          nMutua = #labcentr#13;
     |SINO;
          |SI #labempre#19 ! 0;
               nMutua = #labempre#19;
          |FINSI;
     |FINSI;

     |SI #0#2 [ nMutua; |Y nMutua [ #0#3;
          |PRINT;
     |FINSI;
|FPRC;

|DEFBUCLE labcentr;
     #labcentr#1;
     ;
     #0#0, 000;
     #0#1, 999;
     ;
     NULL, labcentr;
|FIN;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          Impresora = "CrystalReport";
          |IMPRE -1;
          |INFOR "nominfmu";
          |ABRE #labempre;
          |BUCLE labcentr;
          |CIERRA #labempre;
          |PIEINF;
          |FININF;
          |FINIMP;
     |FINSI;
|FPRO;
