|FICHEROS;
     nomz0012 #0;
     labtraba;
|FIN;

|VARIABLES;
     aAlfa = "";
     aCaracter = "";
     nCampo = 0;
     nLista = 0;
|FIN;

|PROCESO CambiaBarras;
     aAlfa = #labtraba#nCampo#;
     |QBF aAlfa;
     |SI aAlfa ! "";
          aCaracter = aAlfa % 301;
          |SI aCaracter = "/";
               aAlfa = (aAlfa % 102) + "." + (aAlfa % 402) + "." + (aAlfa % -104);
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO labtraba;
     |PARA nLista = 0; |SI nLista [ 19; |HACIENDO nLista = nLista + 1;
          nCampo = #0nLista;
          |SI nCampo ! -1;
               |HAZ CambiaBarras;
               #labtraba#nCampo# = aAlfa;
          |FINSI;
     |SIGUE;

     |GRABA 020#labtraba.a;
     |LIBERA #labtraba;
|FPRC;

|DEFBUCLE labtraba;
     #labtraba#1;
     ;
     INICIO;
     FINAL;
     be;
     NULL, labtraba;
|FIN;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |ABRE #labtraba;
          |BUCLE labtraba;
          |CIERRA #labtraba;
     |FINSI;
|FPRO;
