|FICHEROS;
     znom0005 #0;

     labempre;
     labcentr;
     nomconca;
|FIN;

|VARIABLES;
     aAlfa = "";
     aAlfa1 = "";
     aAlfa2 = "";
     aAlfa3 = "";
     aAlfa4 = "";
|FIN;

|PROCESO labcentr;
     |SI #labcentr#16 = 000; |O #labcentr#16 = 001;
          #labcentr#16 = #labempre#1;

          |DDEFECTO #nomconca;
          #nomconca#0 = #labempre#1;
          |LEE 000#nomconca.=;
          |SI FSalida ! 0;
               #labcentr#19 = #nomconca#1;
          |FINSI;

          |GRABA 020#labcentr.a;
          |LIBERA #labcentr;
     |SINO;
          |SI #labcentr#16 ! #labempre#1;
               aAlfa1 = #labempre#0; |QBF aAlfa1; aAlfa1 = ("00000" + aAlfa1) % -105;
               aAlfa2 = #labempre#1; |QBF aAlfa2; aAlfa2 = ("000" + aAlfa2) % -103;
               aAlfa3 = #labcentr#0; |QBF aAlfa3; aAlfa3 = ("00" + aAlfa3) % -102;
               aAlfa4 = #labcentr#16; |QBF aAlfa4; aAlfa4 = ("000" + aAlfa4) % -103;
               aAlfa = "    SATENCION. Empresa " + aAlfa1 + ",";
               aAlfa = aAlfa + " Conv.: " + aAlfa2 + ".";
               aAlfa = aAlfa + " Centro " + aAlfa3 + ",";
               aAlfa = aAlfa + " Conv.: " + aAlfa4 + ".";
               aAlfa = aAlfa + "¿Copiar Convenio?"
               |CONFI aAlfa;
               |SI FSalida = 0;
                    #labcentr#16 = #labempre#1;

                    |DDEFECTO #nomconca;
                    #nomconca#0 = #labempre#1;
                    |LEE 000#nomconca.=;
                    |SI FSalida ! 0;
                         #labcentr#19 = #nomconca#1;
                    |FINSI;
                    |GRABA 020#labcentr.a;
                    |LIBERA #labcentr;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE labcentr;
     #labcentr#1;
     ;
     #labempre#0, 001;
     #labempre#0, 999;
     be;
     NULL, labcentr;
|FIN;

|PROCESO empresas;
     |BUCLE labcentr;
|FPRC;

|DEFBUCLE empresas;
     #labempre#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, empresas;
|FIN;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |ABRE #nomconca;
          |BUCLE empresas;
          |CIERRA #nomconca;
     |FINSI;
|FPRO;
