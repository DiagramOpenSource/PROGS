|FICHEROS;
    nomxcate #0;
    nomcateg #1;
    nomcatca #2;
    nomcatli #3;
|FIN;

|VARIABLES;
    swprint = 0;
    mensa0 = "    Proceso ABORTADO !!!";
   &auxili = 0;
   &ambos = 0;
|FIN;

|PROCESO printa;
    ambos = 0;
    auxili = 1;
|PRINT;
|FPRC;

|PROCESO lineas;
|BUCLELIN 0printa#2;
|FPRC;

|DEFBUCLE nomxcate1;   || De cabecera de categorias
#2#1;
;
#1#0,#1#1;
#1#0,#1#1;
e;
NULL,lineas;
|FIN;

|PROCESO cabecera;
    ambos = 1;
    auxili = 0;
|PRINT;
    swprint = 1;
|SI #0#5 = "S";
    |BUCLE nomxcate1;
|FINSI;
|FPRC;

|DEFBUCLE nomxcate0;   || De categorias
#1#1;
;
#0#0,#0#2;
#0#1,#0#3;
e;
NULL,cabecera;
|FIN;

|PROCESO imprime; |TIPO 3;
|IMPRE 0;
|SI FSalida = 0;
    |INFOR "nomxcate";
    |BUCLE nomxcate0;
    |SI swprint = 1;
        |PIEINF;
    |FINSI;
    |FININF;
|SINO;
    |MENAV mensa0;
|FINSI;
|FINIMP;
|FPRC;
