         |SI #3#11 ] 251; |Y #3#11  [ 500; |Y los_87 ] 20;  plan_error = 1;  |FINSI;
         |SI #3#11 ] 501;                  |Y los_87 ] 30;  plan_error = 1;  |FINSI;
         |SI plan_error ! 0;
             |ATRI P;
             |MENAV "    AVISO!!! Plantilla de aprendices supera a la maxima establecida ...";
             |ATRI 0;
         |SINO;
             |DDEFECTO #2;
             |ABRE #2;
                 #2#0 = #0#1;
                 #2#1 = #0#2;
             |LEE 000#2=;
             |CIERRA #2;
         |FINSI;

     #0#55 = los_87 + 1;
     |HAZ EsMinusvalido;
     |SI aMinusvalido="M";
          #0#56 = los_97+1;
     |SINO;
          #0#56 = los_97;
     |FINSI;
     |PINTA #0#55;
     |PINTA #0#56;
|FPRC;


