         |SI #3#11 ]  26; |Y #3#11  [  40; |Y los_87 ]  4;  plan_error = 1;  |FINSI;
         |SI #3#11 ]  41; |Y #3#11  [  50; |Y los_87 ]  5;  plan_error = 1;  |FINSI;
         |SI #3#11 ]  51; |Y #3#11  [ 100; |Y los_87 ]  8;  plan_error = 1;  |FINSI;
