|PROCESO ControlaHora; |TIPO 0;
     |SI #0#50>#0#32; |O #0#50>#0#58;
          |MENAV "0000No puedes poner mas horas teoricas que reales!";
          |ERROR;
     |SINO;
          aux=#0#58 % 15;
          |SI #0#50<aux;
               |MENAV "    AVISO!!! Es inferior al 15% de las horas jornada ...";
          |FINSI;
          #0#31=#0#50*100/#0#58;
          |PINTA #0#31;
     |FINSI;
|FPRC;

|PROCESO lee_871;
|SI #2#45 = "085";|O #2#45 = "087"; |O #2#45 = "421"; || todos menos minusvalidos
    |SI #2#1 ! #0#2;
            los_87 = los_87 + 1;
    |FINSI;
|FINSI;
|SI #2#45 = "097";                  || solo los minusvalidos
    |SI #2#1 ! #0#2;
            los_97 = los_97 + 1;
            los_87 = los_87 + 1;
    |FINSI;
|FINSI;
|FPRC;

|DEFBUCLE lee_421;
