|FICHEROS;
     labrecon  #0;
     labregis  #1;
     labtraba  #2;
     labempre  #3;
     labcontr  #4;
     labretmp  #5,MANTE;
|FIN;

|VARIABLES;
     fDefecto = @;
     aDefecto = "";
     fFichDesde = @;
     fFichHasta = @;
     fFich = @;
     fAltaDesde = @;
     fAltaHasta = @;
     fAlta = @;
     swImprime = 0;
     swHayAlgo = 0;
     dia1 = "";
     mes1 = "";
     &any1 = "";
     &dia = 0;
     mes = 0;
     any = 0;
     tope = 0;
     mod = 0;
     sw = 0;
     i = 0;
     men1 = "0000ATENCION Fecha erronea";
     fec = "";
     fec1 = "";
     fec2 = 0;
     meslet = "";
     blanco = "          ";
     puntos = "..........";
     &aMes = "";
     &aPC = "";
     &aFechaIni = "";
     &aLinea1 = "";
     &aLinea2 = "";
     aEmpresa = "";
     aDomicilio = "";
     aLocalidad = "";
     &aFecha = "";

     {-1}menu = "";
     menu1 = "2400";
     menu2 = "2";
     menu3 = "Elegir Opcion:";
     menu4 = "IRC";
     menu5 = " Imprimir ";
     menu6 = " Revisar ";
     menu7 = " Cancelar ";
     aAlfa1 = "";
     nOpcion = 0;
     inkey = 0;
     nLen = 0;
|FIN;

|PROCESO defecto; |TIPO 7;
     fDefecto = ~;
     aDefecto = fDefecto;
     #0Campo = aDefecto;
     |PINTA #0Campo;
|FPRC;

|PROCESO fechacon; |TIPO 0;
     fec = #0Campo; sw = 0;
     |SI fec ! blanco; |Y fec ! puntos; |Y fec ! Aindefinido;
          dia1 = fec % A102;
          mes1 = fec % A402;
          any1 = fec % A704;
          dia = dia1; mes = mes1; any = any1;
          |SI dia ! 0; |Y mes ! 0; |Y any ! 0;
               |SI mes [ 12; |Y mes ] 1;
                    |SI mes = 1; |O mes = 3; |O mes = 5; |O mes = 7; |O mes = 8; |O mes = 10; |O mes = 12;
                         tope = 31;
                    |SINO;
                    |SI mes = 2;
                         mod = any $ 4;
                         |SI mod = 0;
                              tope = 29;
                         |SINO;
                              tope = 28;
                         |FINSI;
                    |SINO;
                         tope = 30;
                    |FINSI;
               |FINSI;
               |SI dia [ tope;
                    sw = 1;
               |FINSI;
          |FINSI;
     |FINSI;
     |SINO;
          sw = 1;
     |FINSI;
     |SI sw = 0;
          |MENAV men1;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO meseslet;               || RUTINA GENERAL DE TRANSFORMACION
    dia1 = fec1 % 102;           || DE FECHAS A LETRAS
    any1 = fec1 % 704;
    fec1 = fec1 % 402;
    fec2 = fec1;
|SI fec2 = 1;  meslet = "Enero     ";   |FINSI;
|SI fec2 = 2;  meslet = "Febrero   ";   |FINSI;
|SI fec2 = 3;  meslet = "Marzo     ";   |FINSI;
|SI fec2 = 4;  meslet = "Abril     ";   |FINSI;
|SI fec2 = 5;  meslet = "Mayo      ";   |FINSI;
|SI fec2 = 6;  meslet = "Junio     ";   |FINSI;
|SI fec2 = 7;  meslet = "Julio     ";   |FINSI;
|SI fec2 = 8;  meslet = "Agosto    ";   |FINSI;
|SI fec2 = 9;  meslet = "Septiembre";   |FINSI;
|SI fec2 = 10; meslet = "Octubre   ";   |FINSI;
|SI fec2 = 11; meslet = "Noviembre ";   |FINSI;
|SI fec2 = 12; meslet = "Diciembre ";   |FINSI;
|FPRC;

|PROCESO carga;
     fFichDesde = #labrecon#5;
     fFichHasta = #labrecon#6;
     fAltaDesde = #labrecon#7;
     fAltaHasta = #labrecon#8;
     fec1 = #labrecon#15;
     aEmpresa = #labrecon#9;
     aDomicilio = #labrecon#10;
     aLocalidad = #labrecon#12;
     |QBF aEmpresa;
     |QBF aDomicilio;
     |QBF aLocalidad;
     aLinea1 = aEmpresa + ", con domicilio en " + aLocalidad + "; ";
     aAlfa1 = aLinea1 % A0;
     nLen = aAlfa1;
     |SI nLen > 83;
          aLinea1 = aEmpresa + ", con domicilio en ";
          aLinea2 = aLocalidad + "; " + aDomicilio + ", y tel‚fono: " + #0#11;
     |SINO;
          aLinea2 = aDomicilio + ", y tel‚fono: " + #0#11;
     |FINSI;
     |HAZ meseslet;
     aMes = meslet;

     aFecha = aLocalidad + ", a " + dia1 + " de " + meslet + " de " + any1;
|FPRC;

|PROCESO localidad; |TIPO 7;
     |ABRE #4;
     |LEE 000#4p;
     |CIERRA #4;

     #0Campo = #4#33;
     |PINTA #0Campo;
|FPRC;

|PROCESO seleccion;
     swImprime = 0;
     fFich = #labregis#21;
     |SI #labrecon#0 = "C";
          fAlta = #labregis#7;
          aPC = "contratos";
          aFechaIni = #labregis#7;
     |SINO;
          fAlta = #labregis#19;
          aPC = "pr¢rrogas";
          aFechaIni = #labregis#19;
     |FINSI;

     |SI fFich ] fFichDesde; |Y fFich [ fFichHasta;
          swImprime = swImprime + 1;
     |FINSI;

     |SI fAlta ] fAltaDesde; |Y fAlta [ fAltaHasta;
          swImprime = swImprime + 1;
     |FINSI;

     |SI swImprime = 2;
          swHayAlgo = 1;

          |DDEFECTO #labtraba;
          #labtraba#0 = #labregis#1;
          #labtraba#1 = #labregis#2;
          |LEE 000#labtraba.=;
          |SI #labtraba#44 = "B"; |Y #labrecon#19 = "N";
               |ACABA;
          |FINSI;

          |DDEFECTO #labempre;
          #labempre#0 = #labregis#1;
          |LEE 000#labempre.=;

          |PRINT;
     |FINSI;
|FPRC;

|DEFBUCLE impresion;
     #labregis#2;
     ;
     #labrecon#1, #labrecon#3;
     #labrecon#2, #labrecon#4;
     ;
     NULL, seleccion;
|FIN;

|PROCESO manual;
     swHayAlgo = 1;
     #labregis#0 = #labretmp#0;
     |LEE 000#labregis.=;

     |SI #labrecon#0 = "C";
          fAlta = #labregis#7;
          aPC = "contratos";
          aFechaIni = #labregis#7;
     |SINO;
          fAlta = #labregis#19;
          aPC = "pr¢rrogas";
          aFechaIni = #labregis#19;
     |FINSI;

     |DDEFECTO #labtraba;
     #labtraba#0 = #labregis#1;
     #labtraba#1 = #labregis#2;
     |LEE 000#labtraba.=;

     |DDEFECTO #labempre;
     #labempre#0 = #labregis#1;
     |LEE 000#labempre.=;

     |PRINT;
|FPRC;

|DEFBUCLE manual;
     #labretmp#1;
     ;
     INICIO;
     FINAL;
     ;
     NULL, manual;
|FIN;

|PROCESO seleccion2;
     |SI #labrecon#18 = "M";
          |CAMPO_MODIFICA #labrecon#1,1,"N";
          |CAMPO_MODIFICA #labrecon#2,1,"N";
          |CAMPO_MODIFICA #labrecon#3,1,"N";
          |CAMPO_MODIFICA #labrecon#4,1,"N";
          |CAMPO_MODIFICA #labrecon#5,1,"N";
          |CAMPO_MODIFICA #labrecon#6,1,"N";
          |CAMPO_MODIFICA #labrecon#7,1,"N";
          |CAMPO_MODIFICA #labrecon#8,1,"N";
          ||MANTE #labretmp;
          ||SUB_EJECUTA "labretmp";
          ||HAZ SelManual;
     |SINO;
          |CAMPO_MODIFICA #labrecon#1,1,"S";
          |CAMPO_MODIFICA #labrecon#2,1,"S";
          |CAMPO_MODIFICA #labrecon#3,1,"S";
          |CAMPO_MODIFICA #labrecon#4,1,"S";
          |CAMPO_MODIFICA #labrecon#5,1,"S";
          |CAMPO_MODIFICA #labrecon#6,1,"S";
          |CAMPO_MODIFICA #labrecon#7,1,"S";
          |CAMPO_MODIFICA #labrecon#8,1,"S";
     |FINSI;
|FPRC;

|PROCESO inicio;
     |ABRE #labtraba;
     |ABRE #labempre;
     |HAZ carga;
     |SI #labrecon#18 = "A";
          |IMPRE 0;
          |SI FSalida = 0;
               |PARA i = 1; |SI i [ #labrecon#16; |HACIENDO i = i + 1;
                    swHayAlgo = 0;
                    |INFOR "labrecon";
                    |BUCLE impresion;
                    |SI swHayAlgo = 1;
                         |PIEINF;
                    |FINSI;
                    |FININF;
               |SIGUE;
               |FINIMP;
          |SINO;
               |MENAV "0000Proceso abortado.";
          |FINSI;
          |SI swHayAlgo ! 1;
               |MENAV "    Seleccion vacia.";
          |FINSI;
     |FINSI;
     |SI #labrecon#18 = "M";
          |HAZ SelManual;
          |SI nOpcion ! 3;
               |IMPRE 0;
               |SI FSalida = 0;
                    |PARA i = 1; |SI i [ #labrecon#16; |HACIENDO i = i + 1;
                         |INFOR "labrecon";
                         |ABRE #labregis;
                         |ABRE #labtraba;
                         |BUCLE manual;
                         |CIERRA #labregis;
                         |CIERRA #labtraba;
                         |SI swHayAlgo = 1;
                              |PIEINF;
                         |FINSI;
                         |FININF;
                    |SIGUE;
                    |FINIMP;
               |SINO;
                    |MENAV "0000Proceso abortado.";
               |FINSI;
          |FINSI;
     |FINSI;
     |CIERRA #labtraba;
     |CIERRA #labempre;
|FPRC;

|PROGRAMA;
     |CLS;
     |PINPA #0#0;
     |ABRE #0;
     |DDEFECTO #0;
     |LEE 000#0p;
     |SI FSalida = 0;  |BORRA 020#0a;  |FINSI;
     |CIERRA #0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |HAZ inicio;
          |ABRE #0; |GRABA 000#0n; |CIERRA #0;
     |SINO;
          |ABRE #0; |GRABA 000#0n; |CIERRA #0;
     |FINSI;
|FPRO;

