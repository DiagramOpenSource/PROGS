|PROCESO min;
     #labretmp#0 = 000001;
|FPRC;

|PROCESO max;
     #labretmp#0 = 999999;
|FPRC;

|PROCESO SelManual;
     aAlfa1 = Usuario; |QBT aAlfa1;    aAlfa1 = ("t" + aAlfa1) % 108;
     |NOME_DAT #5 aAlfa1;
     |DELINDEX #5;
     |PUSHV 0501 2380;
     |PARA; |SI; |HACIENDO;
          |ENTLINEAL #1#5, 1, 1, 22, 1, min, max;
          |MENU menu;
          nOpcion = FSalida;
          |SI nOpcion = 1;     |SAL;                      |FINSI;
          |SI nOpcion = 3;     |SAL;             |FINSI;
     |SIGUE;
     |POPV;
|FPRC;
