|FICHEROS;
     motibare #3;
|FIN;

|VARIABLES;
     nModo = 0;
     nPos1 = 0;
     nPos2 = 0;
|FIN;

|PROCESO inferior;
     #motibare#0 = 01;
|FPRC;

|PROCESO superior;
     #motibare#0 = 99;
|FPRC;

|PROGRAMA;
     |CLS;
     |ABRE #motibare;
     |SI PARAMETRO = "MODIF";
          |ENTLINEAL #1#motibare, -1, 2, 22, 1, inferior, superior;
     |SINO;
          |CONSULTA_CLAVE #1#motibare;
     |FINSI;
     |CIERRA #motibare;
|FPRO;
