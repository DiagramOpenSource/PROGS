|PROCESO imprime;
     |IMPRE 0;
     |SI FSalida = 0;
          |INFOR "nombon59";
          |BUCLE impresion;
          |PIEINF;
          |FININF;
     |SINO;
          |MENAV "0000Proceso abortado.";
     |FINSI;
     |FINIMP;
|FPRC;

|PROCESO Tipo3; |TIPO 3;
     aAlfa1 = Usuario;  |QBF aAlfa1;    aAlfa1 = "b" + aAlfa1;
     |NOME_DAT #100 aAlfa1;
     |DELINDEX #100;
     |ABRE #100;
     nNume = #0#4 + #0#5 + #0#6 + #0#7 + #0#8 + #0#9 + #0#10 + #0#11;
     |SI nNume = 0;      || todos los convenios;
          nDesdeConv = 001;
          nHastaConv = 999;
          |BUCLE labtraba;
     |SINO;
          |PARA nNume = 4; |SI nNume [ 11; |HACIENDO nNume = nNume + 1;
               nDesdeConv = #0nNume;
               nHastaConv = #0nNume;
               |BUCLE labtraba;
          |SIGUE;
     |FINSI;
     |CIERRA #100;

     |PARA; |SI; |HACIENDO;
