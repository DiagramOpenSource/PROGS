|FICHEROS;
     nomnorco #0;
|FIN;

|VARIABLES;
     nCampo = 0;
|FIN;

|PROCESO ActivaNorma; |TIPO 7;
     nCampo = Campo - 1;
     |SI #0nCampo = "S";
          |CAMPO_MODIFICA #0Campo, 1, "S";
     |SINO;
          |CAMPO_MODIFICA #0Campo, 1, "N";
          #0Campo = 0;
          |PINTA #0Campo;
     |FINSI;
|FPRC;
