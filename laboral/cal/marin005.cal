|FICHEROS;
     marin005 #0;
|FIN;

|VARIABLES;
     nCampo = 0;
     nFecha = 0;
     fFecha = @;
     aAlfa = "";
     nPos = 0;

     &aFecha = "";
     &aError = "";

{-1}menu = "";
    menu1 = "2310";
    menu2 = "1";
    menu3 = "Informes: ";
    menu4 = "RCV";
    menu5 = "Vto. Reconocimiento";
    menu6 = "Cursos de formaci¢n";
    menu7 = "   Volver  ";
     nOpcion = 0;
|FIN;

|PROCESO Tipo20; |TIPO 20;
     |SI FSalida = "OPCION";
          FSalida = "Informes";
     |SINO;
          |MENU menu;
          nOpcion = FSalida;
          |SI nOpcion ! 1; |Y nOpcion ! 2;
          |SINO;
               |SI nOpcion = 1;
                    |SUB_EJECUTA "marin006";
               |FINSI;
               |SI nOpcion = 2;
                    |SUB_EJECUTA "marin007";
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Curso; |TIPO 14;
     |SI #0Campo = "X";
          aAlfa = "Realizado    ";
     |SINO;
          aAlfa = "No realizado ";
     |FINSI;

     |SI Campo = 8; nPos = 1632; |FINSI;
     |SI Campo = 9; nPos = 1732; |FINSI;
     |SI Campo = 10; nPos = 1832; |FINSI;

     |ATRI S;
     |PINTA nPos, aAlfa;
     |ATRI 0;
|FPRC;

|PROCESO CompFecha;
     aFecha = #0Campo;
     |HAZ CompFecha_plb;
|FPRC;

|PROCESO UnAnyo;
     aFecha = #0Campo;
     fFecha = aFecha;
     fFecha = fFecha + 0.012;
     nFecha = fFecha;
     nFecha = nFecha - 1;
     fFecha = nFecha;
     aFecha = fFecha;
     nCampo = Campo + 1;
     #0nCampo = aFecha;
     |PINTA #0nCampo;
|FPRC;

|PROCESO SinPrevencion;
     |SI #0Campo = "N";
          |CAMPO_MODIFICA #0#2, 1, "N";
          |CAMPO_MODIFICA #0#3, 1, "N";
          |CAMPO_MODIFICA #0#4, 1, "N";
          #0#2 = "00.00.0000";
          #0#3 = "00.00.0000";
          |PINTA #0#2;
          |PINTA #0#3;
     |SINO;
          |CAMPO_MODIFICA #0#2, 1, "S";
          |CAMPO_MODIFICA #0#3, 1, "S";
          |CAMPO_MODIFICA #0#4, 1, "S";
          #0#2 = ~;
          #0#3 = ~;
          |PINTA #0#2;
          |PINTA #0#3;
     |FINSI;
|FPRC;

|PROCESO SinMedicina;
     |SI #0Campo = "N";
          |CAMPO_MODIFICA #0#6, 1, "N";
          |CAMPO_MODIFICA #0#7, 1, "N";
          #0#6 = "00.00.0000"
          #0#7 = "00.00.0000"
          |PINTA #0#6;
          |PINTA #0#7;
     |SINO;
          |CAMPO_MODIFICA #0#6, 1, "S";
          |CAMPO_MODIFICA #0#7, 1, "S";
          #0#6 = ~;
          #0#7 = ~;
          |PINTA #0#6;
          |PINTA #0#7;
     |FINSI;
|FPRC;
