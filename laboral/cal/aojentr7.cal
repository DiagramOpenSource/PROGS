|PROCESO Clave; |TIPO 7;
     #aojentr7#0 = #aojentr6#1;    || serie expediente
     #aojentr7#1 = #aojentr6#2;    || nro expediente
     #aojentr7#2 = #aojentr6#0;    || empresa
     #aojentr7#4 = #aojentr6#5;    || trabajador
     #aojentr7#5 = aTrabajador;    || Nombre trabajador
     #aojentr7#6 = #aojentr6#4;    || fecha
     |PINTA #aojentr7#8;

     |PINTA #aojentr7#4;
     |PINTA #aojentr7#5;
     |PINTA #aojentr7#6;
|FPRC;

|PROCESO SumaUno;
     swPaso = 1;
|FPRC;

|PROCESO Cuenta;
     |SI #aojentr7#19 = 0;
          nTotal = nTotal + #aojentr7#17;
     |FINSI;
|FPRC;

|DEFBUCLE Cuenta;
     #aojentr7#1;
     ;
     #aojentr6#0, #aojentr6#1, #aojentr6#2, #aojentr6#3, #aojentr6#4, #aojentr6#5, 001;
     #aojentr6#0, #aojentr6#1, #aojentr6#2, #aojentr6#3, #aojentr6#4, #aojentr6#5, 999;
     ;
     NULL, Cuenta;
|FIN;

|PROCESO UnidAntes; |TIPO 7;
     |SALVA_FICHA #aojentr7;
     nTotal = 0;
     |BUCLE Cuenta;
     aUnidades = nUnidades;
     nPendientes = nUnidades - nTotal;
     aPendientes = nPendientes;
     |PINTA 1471, "        ";
     |PINTA 1571, "        ";
     |PINTA 1471, aUnidades;
     |PINTA 1571, aPendientes;
     |REPON_FICHA #aojentr7;
|FPRC;

|PROCESO UnidDespues;
     |HAZ UnidAntes;
|FPRC;



|PROCESO Produccion2;     || por si estamos en produccion hago el cambiazo
     aLon = aPath % 0;
     nLon1 = aLon;
     aAlfa = aPath;
     aAlfa = aAlfa - "dsprofes/";
     aLon = aAlfa % 0;
     nLon2 = aLon;
     |SI nLon1 ! nLon2;
          aPath = aAlfa + "dsempres/";
     |FINSI;
|FPRC;

|PROCESO CargaAgrupPres;
     nLinea = nLinea + 1;
     #51#0 = nLinea;
     #51#1 = #gopre002@#12;
     #51#2 = #gopre002@#13;
     |GRABA 020#aomtemp5.n;
|FPRC;

|DEFBUCLE CargaAgrupPres;
     #gopre002@#7005;
     ;
     #gopre001@#0, #gopre001@#1, nAgrup, 001, "                    ";
     #gopre001@#0, #gopre001@#1, nAgrup, 999, "zzzzzzzzzzzzzzzzzzzz";
     ;
     NULL, CargaAgrupPres;
|FIN;

|PROCESO CargaAgrup;
     #50#0 = #gopre002@#14;
     |LEE 101#50=;
     |SI FSalida ! 0;
          |SI #50#0 = 0;
               #50#0 = 0;
               #50#1 = "Sin Agrupacion";
               |GRABA 020#aomtemp4.n;
          |SINO;
               #gopre021@#0 = #gopre002@#0;
               #gopre021@#1 = #gopre002@#1;
               #gopre021@#2 = #gopre002@#14;
               |LEE 000#gopre021@.=;

               #50#0 = #gopre002@#14;
               #50#1 = #gopre021@#3;
               |GRABA 020#aomtemp4.n;
          |FINSI;
     |FINSI;
|FPRC;

|DEFBUCLE CargaAgrup;
     #gopre002@#1003;
     ;
     #gopre001@#0, #gopre001@#1, 001;
     #gopre001@#0, #gopre001@#1, 999;
     ;
     NULL, CargaAgrup;
|FIN;

|PROCESO MinPed;
     #gomantl1@#0 = #aojentr7#0;
     || #gomantl1@#0 = aSer;
     #gomantl1@#1 = #aojentr7#1;
     #gomantl1@#2 = 001;

     #gopre002@#0 = #aojentr7#9;
     #gopre002@#1 = #aojentr7#10;
     #gopre002@#2 = 001;

     |SI #gopre001@#49 = "S";
          #gopre002@#14 = nAgrup;
     |SINO;
          #gopre002@#14 = 000;
     |FINSI;

     #gopre003@#0 = #aojentr7#9;
     #gopre003@#1 = #aojentr7#10;
     #gopre003@#2 = #aojentr7#12;
     #gopre003@#3 = 001;
|FPRC;

|PROCESO MaxPed;
     #gomantl1@#0 = #aojentr7#0;
     || #gomantl1@#0 = aSer;
     #gomantl1@#1 = #aojentr7#1;
     #gomantl1@#2 = 999;

     #gopre002@#0 = #aojentr7#9;
     #gopre002@#1 = #aojentr7#10;
     #gopre002@#2 = 999;

     |SI #gopre001@#49 = "S";
          #gopre002@#14 = nAgrup;
     |SINO;
          #gopre002@#14 = 999;
     |FINSI;

     #gopre003@#0 = #aojentr7#9;
     #gopre003@#1 = #aojentr7#10;
     #gopre003@#2 = #aojentr7#12;
     #gopre003@#3 = 999;
|FPRC;

|PROCESO PidePres3; |TIPO 7;
     aTipo = "";
     |ABRE #1; |LEE 000#1p; |CIERRA #1;

     |DBASS aPath;
     |HAZ Produccion2;
     aPathBas = aPath + #1#1; |QBF aPathBas;
     aPathDef = aPathBas + "def/";
     aPathDat = aPathBas + "fich/" + (("00000" + #aojentr6#0) % -105) + "/";

     aDef = aPathDef + "gomantl1"; |CARGA_DEF aDef, gomantl1@;
     |SI FSalida ! 0; |ACABA; |FINSI;
     aDef = aPathDef + "gopre001"; |CARGA_DEF aDef, gopre001@;
     aDef = aPathDef + "gopre002"; |CARGA_DEF aDef, gopre002@;
     aDef = aPathDef + "gopre003"; |CARGA_DEF aDef, gopre003@;
     aDef = aPathDef + "goparame"; |CARGA_DEF aDef, goparame@;
     aDef = aPathDef + "gopre021"; |CARGA_DEF aDef, gopre021@;

     |PATH_DAT #gomantl1@#aPathDat#;
     |PATH_DAT #gopre001@#aPathDat#;
     |PATH_DAT #gopre002@#aPathDat#;
     |PATH_DAT #gopre003@#aPathDat#;
     |PATH_DAT #goparame@#aPathDat#;
     |PATH_DAT #gopre021@#aPathDat#;

     |SI #1#4 = "E";
          |ABRE #gomantl1@;
          #gomantl1@#0 = #aojentr7#0;
          #gomantl1@#1 = #aojentr7#1;
          #gomantl1@#2 = 1;
          |LEE 000#gomantl1@.];
          |SI FSalida = 0; |Y #gomantl1@#0 = #aojentr7#0; |Y #gomantl1@#1 = #aojentr7#1;
               #aojentr7#9 = #gomantl1@#3; |PINTA #aojentr7#9;
               #aojentr7#10 = #gomantl1@#4; |PINTA #aojentr7#10;
          |SINO;
               |MENAV "0000Expediente sin presupuestos...";
               |ERROR;
          |FINSI;
          |CIERRA #gomantl1@;
     |FINSI;

     |SI #1#4 = "C"; |O #1#4 = "P";
          /*
          aSer = #aojentr7#0;
          |QBF aSer;
          aSer = ("00000" + aSer) % -105;
          */
          |ABRE #gomantl1@;
          |SELECT #2#gomantl1@;
          #gomantl1@#0 = #aojentr7#0;
          #gomantl1@#1 = #aojentr7#1;
          #gomantl1@#3 = #aojentr7#9;
          #gomantl1@#4 = #aojentr7#10;
          |LEE 000#gomantl1@.];
          |SI FSalida = 0; |Y #gomantl1@#0 = #aojentr7#0; |Y #gomantl1@#1 = #aojentr7#1;
               |CONSULTA_F_CLAVE #1#gomantl1@, MinPed, MaxPed;
               |SI FSalida ! 0;
                    |ERROR;
               |SINO;
                    #aojentr7#9 = #gomantl1@#3;
                    #aojentr7#10 = #gomantl1@#4;

                    |PINTA #aojentr7#9;
                    |PINTA #aojentr7#10;

                    |ABRE #gopre001@;
                    #gopre001@#0 = #aojentr7#9;
                    #gopre001@#1 = #aojentr7#10;
                    |LEE 020#gopre001@.=;
                    |CIERRA #gopre001@;
                    aTipo = #gopre001@#18;

                    || AGRUPACIONES DE CAPITULOS

                    nAgrup = 0;
                    aDesAgrup = "";
                    |SI #gopre001@#49 = "S";
                         |ABRE #gopre021@;

                         alfa1 = Usuario; |QBT alfa1; alfa1 = ("t" + alfa1) % 108;
                         |NOME_DAT #50 alfa1;
                         |DELINDEX #50;
                         |ABRE #50;
                         |BUCLE CargaAgrup;
                         |CONSULTA_CLAVE #1#aomtemp4;
                         |SI FSalida = 0;
                              nAgrup = #aomtemp4#0;
                              aDesAgrup = #aomtemp4#1;

                              #aojentr7#13 = nAgrup;
                              #aojentr7#14 = aDesAgrup;

                              |PINTA #aojentr7#13;
                              |PINTA #aojentr7#14;
                         |FINSI;
                         |CIERRA #50;
                         |CIERRA #gopre021@;

                         alfa1 = Usuario; |QBT alfa1; alfa1 = ("u" + alfa1) % 108;
                         |NOME_DAT #51 alfa1;
                         |DELINDEX #51;
                         |ABRE #51;
                         nLinea = 0;
                         |BUCLE CargaAgrupPres;

                         ||CONSULTA_F_CLAVE #1#gopre002@, MinPed, MaxPed;
                         |CONSULTA_CLAVE #1#aomtemp5;
                         |SI FSalida ! 0;
                              |ERROR;
                         |SINO;
                              #aojentr7#11 = #aomtemp5#0;
                              #aojentr7#12 = #aomtemp5#1;
                              #aojentr7#13 = nAgrup;

                              |PINTA #aojentr7#11;
                              |PINTA #aojentr7#12;
                              |PINTA #aojentr7#13;
                         |FINSI;
                         |CIERRA #51;
                    |SINO;
                         |ABRE #gopre002@;
                         |SELECT #6#gopre002@;
                         #gopre002@#0 = #aojentr7#9;
                         #gopre002@#1 = #aojentr7#10;
                         #gopre002@#12 = #aojentr7#12;
                         |LEE 000#gopre002@.];

                         ||CONSULTA_F_CLAVE #1#gopre002@, MinPed, MaxPed;
                         |CONSULTA_F_CLAVE #7#gopre002@, MinPed, MaxPed;
                         |SI FSalida ! 0;
                              |ERROR;
                         |SINO;
                              #aojentr7#11 = #gopre002@#2;
                              #aojentr7#12 = #gopre002@#12;
                              #aojentr7#13 = #gopre002@#14;

                              |PINTA #aojentr7#11;
                              |PINTA #aojentr7#12;
                              |PINTA #aojentr7#13;
                         |FINSI;
                         |CIERRA #gopre002@;
                    |FINSI;

                    |SI #1#4 = "P";
                         |ABRE #gopre003@;
                         |SELECT #2#gopre003@;
                         #gopre003@#0 = #aojentr7#9;
                         #gopre003@#1 = #aojentr7#10;
                         #gopre003@#2 = #aojentr7#12;
                         #gopre003@#4 = #aojentr7#15;
                         |LEE 000#gopre003@.];
                         |CONSULTA_F_CLAVE #1#gopre003@, MinPed, MaxPed;
                         |SI FSalida ! 0;
                              |ERROR;
                         |SINO;
                              #aojentr7#15 = #gopre003@#4;
                              #aojentr7#16 = #gopre003@#5;
                              |PINTA #aojentr7#15;
                              |PINTA #aojentr7#16;
                         |FINSI;
                         |CIERRA #gopre003@;
                    |FINSI;
               |FINSI;
          |SINO;
               |MENAV "0000No existe lineas de expediente...";
               |ERROR;
          |FINSI;
     |FINSI;

     |DESCARGA_DEF #gopre021@;
     |DESCARGA_DEF #goparame@;
     |DESCARGA_DEF #gopre003@;
     |DESCARGA_DEF #gopre002@;
     |DESCARGA_DEF #gopre001@;
     |DESCARGA_DEF #gomantl1@;
|FPRC;

|PROCESO Observ; |TIPO 0;
     nError = 0;
     |SI FSalida = 10;
          nError = 1;
          |ERROR;
     |FINSI;
     |SI FSalida > 11;
          nError = 1;
          |ERROR;
     |FINSI;
     |SI FSalida = 11;
          |SI aTipo = "A";
               eaSerie = #aojentr7#9;
               enNumero = #aojentr7#10;
               efFecha = #aojentr7#6;
               enEmpresa = #aojentr7#2;
               enTrabaj = #aojentr7#4;
               |SUB_EJECUTA_NP "aomcomen";
               |ERROR;
               nError = 1;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO MensaLin; |TIPO 7;
     |SI aTipo = "A";
          |MENSA "    Introduzca la unidades a adjudicar o <F3> para editar observaciones del parte";
     |SINO;
          |MENSA "    Introduzca la unidades a adjudicar";
     |FINSI;
|FPRC;

|PROCESO Albaran;
     |SI nError ! 0; |ACABA; |FINSI;

     |ABRE #1; |LEE 000#1p; |CIERRA #1;

     |DBASS aPath;
     |HAZ Produccion2;
     aPathBas = aPath + #1#1; |QBF aPathBas;
     aPathDef = aPathBas + "def/";
     aPathDat = aPathBas + "fich/" + (("00000" + #aojentr6#0) % -105) + "/";

     aDef = aPathDef + "gopre001"; |CARGA_DEF aDef, gopre001@;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |PATH_DAT #gopre001@#aPathDat#;
     |ABRE #gopre001@;
     #gopre001@#0 = #aojentr7#9;
     #gopre001@#1 = #aojentr7#10;
     |LEE 020#gopre001@.=;
     |SI #gopre001@#18 = "A";
          |CONFI "0000NDesea Introducir Materiales?";
          |SI FSalida = 0;
               aAlfa = #aojentr7#9 + (("000000" + #aojentr7#10) % -106);
               aAlfa = aAlfa + (("000000" + #aojentr5#0) % -105);
               aAlfa = aAlfa + (("000000" + #aojentr5#1) % -106);
               aAlfa = aAlfa + #aojentr7#6;
               PRMNT_param = aAlfa;
               aAlfa = ":dscomer9/vipez006.run agif0041 ";
               aAlfa = aAlfa + (("00000" + #aojentr6#0) % -105);
               |SUB_EJECUTA_NP aAlfa;
          |FINSI;
     |FINSI;
     |CIERRA #gopre001@;
     |PATH_DAT #gopre001@#aPathDat#;
     |DESCARGA_DEF #gopre001@;
|FPRC;

|PROCESO Uni7; |TIPO 7;
     |SI #1#4 ! "E";
          |ESTADO_CONTROL enBoton, "ENABLE";
     |FINSI;
|FPRC;

|PROCESO Uni0; |TIPO 0;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |HAZ NoRepe;
          |SI aRepe = "S"; |ERROR; |ACABA; |FINSI;
     |FINSI;

     |ESTADO_CONTROL enBoton, "DISABLE";
     |SI FSalida ! 13; |ACABA; |FINSI;

     |ABRE #1; |LEE 000#1p; |CIERRA #1;

     |DBASS aPath;
     |HAZ Produccion2;
     aPathBas = aPath + #1#1; |QBF aPathBas;
     aPathDef = aPathBas + "def/";
     aPathDat = aPathBas + "fich/" + (("00000" + #aojentr6#0) % -105) + "/";

     aDef = aPathDef + "gopre001"; |CARGA_DEF aDef, gopre001@;
     |SI FSalida ! 0; |ACABA; |FINSI;

     |PATH_DAT #gopre001@#aPathDat#;
     |ABRE #gopre001@;
     #gopre001@#0 = #aojentr7#9;
     #gopre001@#1 = #aojentr7#10;
     |LEE 020#gopre001@.=;
     |SI #gopre001@#18 = "A";
          aAlfa = #aojentr7#9 + (("000000" + #aojentr7#10) % -106);
          aAlfa = aAlfa + (("000000" + #aojentr5#0) % -105);
          aAlfa = aAlfa + (("000000" + #aojentr5#1) % -106);
          aAlfa = aAlfa + #aojentr7#6;
          PRMNT_param = aAlfa;
          PRMNT_capi = #aojentr7#11;
          aAlfa = ":dscomer9/vipez007.run agif0041 ";
          aAlfa = aAlfa + (("00000" + #aojentr6#0) % -105);
          |SUB_EJECUTA_NP aAlfa;
          |ERROR; |PTEC 807;
     |SINO;
          |MENAV "0000El presupuesto no es de administracion.";
     |FINSI;
     |CIERRA #gopre001@;
     |PATH_DAT #gopre001@#aPathDat#;
     |DESCARGA_DEF #gopre001@;
|FPRC;

|PROCESO NoRepe;
     aRepe = "N";
     |DDEF aAlfa;
     aAlfa = aAlfa + "aojentr7";
     |CARGA_DEF aAlfa, aojentr7@;
     |SI FSalida = 0;
          |ABRE #aojentr7@;
          |SELECT #2#aojentr7@;
          #aojentr7@#0 = #aojentr7#0;
          #aojentr7@#1 = #aojentr7#1;
          #aojentr7@#2 = #aojentr7#2;
          #aojentr7@#4 = #aojentr7#4;
          #aojentr7@#6 = #aojentr7#6;
          #aojentr7@#9 = #aojentr7#9;
          #aojentr7@#10 = #aojentr7#10;
          #aojentr7@#12 = #aojentr7#12;
          #aojentr7@#15 = #aojentr7#15;
          #aojentr7@#19 = #aojentr7#19;
          |LEE 000#aojentr7@.=;
          |SI FSalida = 0;
               aRepe = "S";
               |MENAV "0000No se puede repetir presupuesto, capitulo, partida, dieta para el mismo dia";
          |FINSI;
          |DESCARGA_DEF #aojentr7@;
     |FINSI;
|FPRC;
