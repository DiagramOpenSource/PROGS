     &PRMNT_capi = 0;
     &enBoton = 0;
     &enBotonD = 0;
     nError = 0;
     &aTrabajador = "";
     nModo = 0;
     &swPaso = 0;
     nTotal = 0;
     aTotal = "";
     aAlfa = "";
     aUnidades = "";
     &nUnidades = 0;
     &nPendientes = 0;
