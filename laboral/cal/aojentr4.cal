|PROCESO DefectoCero; |TIPO 7;   || proceso para grabar el tipo de horas = tipo 00
     |ABRE #aomhoras;            || para vipei que NO usa el tipo de hora
     #aomhoras#0 = 00;
     |LEE 101#aomhoras.=;
     |SI FSalida ! 0;
          #aomhoras#0 = 00;
          #aomhoras#2 = "No se utiliza Tipo de Hora";
          |GRABA 020#aomhoras.n;
     |FINSI;
     |CIERRA #aomhoras;
|FPRC;

|PROCESO inferior;
     || #aojentrh#0 = aSer;           || serie expediente
     #aojentrh#0 = #aojentr4#1;    || serie expediente
     #aojentrh#1 = #aojentr4#7;    || nro expediente
     #aojentrh#2 = #aojentr4#2;    || empresa
     #aojentrh#4 = #aojentr4#3;    || trabajador
     #aojentrh#6 = #aojentr4#0;    || fecha
     #aojentrh#8 = 001;            || linea
|FPRC;

|PROCESO superior;
     || #aojentrh#0 = aSer;           || serie expediente
     #aojentrh#0 = #aojentr4#1;    || serie expediente
     #aojentrh#1 = #aojentr4#7;    || nro expediente
     #aojentrh#2 = #aojentr4#2;    || empresa
     #aojentrh#4 = #aojentr4#3;    || trabajador
     #aojentrh#6 = #aojentr4#0;    || fecha
     #aojentrh#8 = 999;            || linea
|FPRC;

