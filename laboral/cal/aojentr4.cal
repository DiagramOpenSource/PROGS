|PROCESO UniDefecto;
     |HAZ BuscaPresu;

     |ABRE #4;
     #4#0 = #0#2;
     #4#1 = #0#3;
     |LEE 020#4=;
     |CIERRA #4;
     |ABRE #3;
     #3#0 = #0#1;
     #3#1 = #0#7;
     #3#2 = #0#2;
     #3#4 = #0#3;
     #3#6 = #0#0;
     #3#8 = 1;
     |LEE 000#3];
     |SI FSalida ! 0; |O #3#0 ! #0#1; |O #3#1 ! #0#7; |O #3#2 ! #0#2;
               |O #3#4 ! #0#3; |O #3#6 ! #0#0;
          |DDEFECTO #3;
          #3#0 = #0#1;
          #3#1 = #0#7;
          #3#2 = #0#2;
          #3#4 = #0#3;
          #3#6 = #0#0;
          #3#5 = #4#2;
          #3#8 = 1;
          #3#17 = #0#5;
          #3#9 = eaSerPre;
          #3#10 = enNumPre;
          |GRABA 020#3n;
     |FINSI;
     |CIERRA #3;
|FPRC;

