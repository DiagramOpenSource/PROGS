|FICHEROS;
     nomw0021;
|FIN;

|VARIABLES;
     aAlfa       = "";

     &eaEntidad  = "";
     &eaSucursal = "";
     &eaCuenta   = "";
     &eaDC       = "";
     &eaIBAN     = "";
     &eaCta      = "";
     &eaPais     = "";
|FIN;

|PROCESO Tipo12Fw21;  |TIPO 12;
     |SI #nomw0021#4 ! "ES ";
          |ACABA;
     |FINSI;

     eaEntidad  = #nomw0021#0; |QBF eaEntidad;
     eaSucursal = #nomw0021#1; |QBF eaSucursal;
     eaCuenta   = #nomw0021#2; |QBF eaCuenta;
     eaDC       = "";

     |HAZ CalculoDC;

     |SI #nomw0021#3 ! eaDC;
         aAlfa = "     El Dígito de Control debería ser " + eaDC;
         |MENAV aAlfa;
     |FINSI;

     eaCta  = #nomw0021#0 + #nomw0021#1 + #nomw0021#3 + #nomw0021#2;
     eaPais = #nomw0021#4;
     |HAZ CheqIBAN;

     aAlfa = #nomw0021#5;    |QBF aAlfa;
     |SI aAlfa ! eaIBAN;
         |MENAV "0000El código IBAN introducido es incorrecto";
         |SI #nomw0021#4 = "ES ";
              || dejo pasar resto de paises
              |ERROR;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO DC;
     |SI #nomw0021#4 ! "ES ";
          |ACABA;
     |FINSI;
     eaEntidad  = #nomw0021#0; |QBF eaEntidad;
     eaSucursal = #nomw0021#1; |QBF eaSucursal;
     eaCuenta   = #nomw0021#2; |QBF eaCuenta;
     eaDC       = "";

     |HAZ CalculoDC;

     |SI #nomw0021#3 ! eaDC;
         aAlfa = "     El Dígito de Control debería ser " + eaDC;
         |MENAV aAlfa;
     |FINSI;
|FPRC;

|PROCESO Tipo7IBAN_Fw21;  |TIPO 7;
     aAlfa = #nomw0021#5;   |QBF aAlfa;

     |SI aAlfa ! "";  |ACABA;  |FINSI;

     eaCta  = #nomw0021#0 + #nomw0021#1 + #nomw0021#3 + #nomw0021#2;
     eaPais = #nomw0021#4;
     |HAZ CalcuIBAN;

     #nomw0021#5 = eaIBAN;
     |PINTA #nomw0021#5;
|FPRC;

|PROCESO Tipo0IBAN_Fw21;  |TIPO 0;
     |SI #nomw0021#4 ! "ES ";
          |ACABA;
     |FINSI;

     eaCta  = #nomw0021#0 + #nomw0021#1 + #nomw0021#3 + #nomw0021#2;
     eaPais = #nomw0021#4;
     |HAZ CheqIBAN;

     aAlfa = #nomw0021#5;    |QBF aAlfa;
     |SI aAlfa ! eaIBAN;
         |CONFI "0000SEl codigo IBAN puede ser erroneo, quiere calcularlo.";
         |SI FSalida = 0;
             #nomw0021#5 = eaIBAN;
             |PINTA #nomw0021#5;
             |ERROR;
         |FINSI;
     |FINSI;
|FPRC;

|PROCESO Pais;
     |SI #nomw0021#4 ! "ES ";
          #nomw0021#0 = "";
          #nomw0021#1 = "";
          #nomw0021#2 = "";
          #nomw0021#3 = "";
          |PINTA #nomw0021#0;
          |PINTA #nomw0021#1;
          |PINTA #nomw0021#2;
          |PINTA #nomw0021#3;
          |CAMPO_MODIFICA #nomw0021#0, 1, "N";
          |CAMPO_MODIFICA #nomw0021#1, 1, "N";
          |CAMPO_MODIFICA #nomw0021#2, 1, "N";
          |CAMPO_MODIFICA #nomw0021#3, 1, "N";
     |SINO;
          |CAMPO_MODIFICA #nomw0021#0, 1, "S";
          |CAMPO_MODIFICA #nomw0021#1, 1, "S";
          |CAMPO_MODIFICA #nomw0021#2, 1, "S";
          |CAMPO_MODIFICA #nomw0021#3, 1, "S";
     |FINSI;
|FPRC;
