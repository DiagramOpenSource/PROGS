||Enviamos el valor del registro a guardar.  (TODOS LOS CAMPOS QUE QUERAMOS)
          fFecha = ~;
          aFecha = fFecha;

          enNumCampoArchi = 4;          ||Fecha Creacion
          eaValorArchi = aFecha;
          |HAZ MeteCampoDSArchi;

          |HORA aHora;
          |QBF aHora;
          aHora = aHora % 105;

          enNumCampoArchi = 5;          ||Hora Creacion
          eaValorArchi = aHora;
          |HAZ MeteCampoDSArchi;


          aAlfa = Usuario % 810;
          |QBF aAlfa;
          enNumCampoArchi = 6;          ||Usuario
          eaValorArchi = aAlfa;
          |HAZ MeteCampoDSArchi;

          ||Ejemplo
          aAlfa = "Documento Almacenado directamente desde Compaq";
          |QBF aAlfa;
          enNumCampoArchi = 13;          ||Observaciones 3
          eaValorArchi = aAlfa;
          |HAZ MeteCampoDSArchi;
          || .....

          ||Rellenamos el resto de campos del Regitro de Documentos
          ||(dsarm005) del dsarchi.
     |FINSI;

