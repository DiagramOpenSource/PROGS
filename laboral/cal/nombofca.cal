|PROCESO defany; |TIPO 7;
     fFecha = ~;
     aFecha = fFecha;
     aAny = aFecha % -104;
     nAny = aAny;
     #0#1 = nAny;
     |PINTA #0#1;
|FPRC;

|PROCESO ElInforme;
     aAlfa1 = #0#0;
     |QBF aAlfa1;
     aAlfa1 = ("00000" + aAlfa1) % -105;

     aAlfa2 = #0#1;
     |QBF aAlfa2;
     aAlfa2 = ("0000" + aAlfa2) % -104;

     aAlfa = "nominf05;" + aAlfa1 + aAlfa2 + aInformeSN;
     |SUB_EJECUTA_NP aAlfa;
|FPRC;

