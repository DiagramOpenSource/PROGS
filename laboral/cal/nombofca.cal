/** PROCESOS PARA CARGAR LOS CENTROS NORMALES Y DE FORMACION DE LA EMPRESA **/
/****************************************************************************/

|PROCESO Comp99;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1; |Y #nombofli#4 = 99;
          aAlfa = "    ATENCION. Es necesario especificar el CENTRO de trabajo ";
          aAlfa = aAlfa + "donde se va a declarar la bonificación.";
          |MENAV aAlfa;
          aAlfa = "    Pulse F11 para consultar la lista de centros.";
          |MENAV aAlfa;
          |ERROR;
          swError = 1;
     |FINSI;
|FPRC;

|PROCESO Comp99_2; |TIPO 7;
     swError = 0;
     |HAZ Comp99;
     |SI swError = 1;
          |PTEC 808;
     |FINSI;
|FPRC;

|PROCESO ConsultaCentros; |TIPO 66;
     nModo = 2 + 4 + 8 + 16;
     nPos1 = 1251; nPos2 = 2280;
     |LINEAL_SIMPLE #1#100, nModo, nPos1, nPos2, NULL, NULL, NULL;
     |SI FSalida = 0;
          #nombofli#4 = #100#0;
          #nombofli#6 = #100#1;
          |PINTA #nombofli#4;
          |PINTA #nombofli#6;
     |FINSI;
     |ERROR;
|FPRC;

|PROCESO nomcenes;
     #100#0 = #nomcenes#13;
     #100#1 = "F";
     #100#2 = #nomcenes#2;
     |GRABA 020#100n;
     |LIBERA #100;
|FPRC;

|DEFBUCLE nomcenes;
     #nomcenes#1;
     ;
     #0#0, 001, "87";
     #0#0, 999, "87";
     ;
     NULL, nomcenes;
|FIN;

|PROCESO labcentr;
     #100#0 = #labcentr#1;
     #100#1 = " ";
     #100#2 = #labcentr#2;
     |GRABA 020#100n;
     |LIBERA #100;
|FPRC;

|DEFBUCLE labcentr;
     #labcentr#1;
     ;
     #0#0, 001;
     #0#0, 999;
     ;
