|PROCESO DefectoTipo; |TIPO 7;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1;
          |SI #nombofli#3 = 0;     || aun no he metido importe
               #nombofli#6 = " ";
          |FINSI;
