|PROCESO BorraLineas;
     |BORRA 020#nombofli.a;
|FPRC;

|DEFBUCLE BorraLineas;
     #nombofli#1;
     ;
     #0#0, #0#1, 01, 001, " ", "B";
     #0#0, #0#1, 12, 999, "F", "P";
     ;
     NULL, BorraLineas;
|FIN;

|PROCESO Tipo20; |TIPO 20;
     aAntSalida = FSalida;

     |SI swLinAlta = 1; |Y swAltaConfirmada = 0;
          |LEE 000#nombofca.=;
          |SI FSalida ! 0;
               |MENAV "    ATENCION. Alta CANCELADA. Se borrarán las líneas introducidas para esta empresa/año";
               |BUCLE BorraLineas;
               |ENTLINEAL #1#nombofli, 3, 7, 22, 0, inferior, superior;
          |FINSI;
     |FINSI;

     FSalida = aAntSalida;

     |SI FSalida = "OPCION";
          FSalida = "Informe";
     |SINO;
          |SI #0#1 < 2011;
               |MENAV "    Opción disponible para el año 2011 en adelante";
          |SINO;
               aInformeSN = "S";
               |HAZ ElInforme;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Borrado; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |BUCLE BorraLineas;
     |FINSI;
|FPRC;

|PROCESO Lineas;
     nCreditoManual = nCreditoManual + #nombofli#3;
|FPRC;

|DEFBUCLE Lineas;
     #nombofli#1;
     ;
     #0#0, #0#1, 01, 001, " ", "B";
     #0#0, #0#1, 12, 999, "F", "B";
     ;
     NULL, Lineas;
|FIN;

|PROCESO SumaLineas;
||TIPO 7;
     |SI #0#1 < 2011;
          |BLANCO 1251 2371;
          |ACABA;
     |FINSI;

     ||SALVA_FICHA #nombofli;

     nCreditoManual = 0;
     |BUCLE Lineas;
     |ATRI R;
     |PINTA 1652, " Cr‚dito consumido ";
     |ATRI 0;
     aAlfa = nCreditoManual;
     |QBF aAlfa;
     aAlfa = aAlfa + " euros       ";
     aAlfa = aAlfa % 114;
     |PINTA 1755, aAlfa;

     ||REPON_FICHA #nombofli;

     #nombofca#2 = nCreditoManual;

     |SI nCreditoManual > nCredito;
          |AVISO;
          |CUADRO 2051 2371;
          |ATRI S;
          |PINTA 2152, " CREDITO EXCEDIDO  ";
          |ATRI 0;
     |SINO;
          |BLANCO 2051 2371;
     |FINSI;
|FPRC;

|PROCESO PintaCredito; |TIPO 13;
     |ENTLINEAL #1#nombofli, 3, 7, 22, 0, inferior, superior;

     |SI #0#1 < 2011;
          |BLANCO 1251 2371;
          nModo = FEntrada;
          |SI nModo = 303;    || modificacion tras consulta
               |HAZ CargaCentros;

               |ENTLINEAL #1#nombofli, 3, 2, 22, 0, inferior, superior;

               |DELINDEX #100;
               |CIERRA #100;
          |FINSI;
          |ACABA;
     |FINSI;

     aInformeSN = "N";
     |HAZ ElInforme;
     |CUADRO 1251 1971;
     |ATRI R;
     |PINTA 1352, " Cr‚dito formaci¢n ";
     |ATRI 0;
     aAlfa = nCredito;
     |QBF aAlfa;
     aAlfa = aAlfa + " euros       ";
     aAlfa = aAlfa % 114;
     |PINTA 1455, aAlfa;

     |HAZ SumaLineas;

     nModo = FEntrada;
     |SI nModo = 303;    || modificacion tras consulta
          |HAZ CargaCentros;

          |ENTLINEAL #1#nombofli, 3, 2, 22, 0, inferior, superior;

          |DELINDEX #100;
          |CIERRA #100;
     |FINSI;
|FPRC;

|PROCESO TipoResta; TIPO 2;
     |SI #nombofli#5 ! "P";
          nModo = (FEntrada / 100) ! 0;
          |SI nModo = 2; |O nModo = 3;
                #nombofca#2 = #nombofca#2 - #nombofli#3;
          |FINSI;
          ||PINTA #nombofca#2;

          aAlfa = #nombofca#2;
          |QBF aAlfa;
          aAlfa = aAlfa + " euros       ";
          aAlfa = aAlfa % 114;
          |PINTA 1755, aAlfa;
     |FINSI;
|FPRC;

|PROCESO TipoSuma;
     |SI #nombofli#5 = "P";
          |ACABA;
     |FINSI;

     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1; |O nModo = 2;
          #nombofca#2 = #nombofca#2 + #nombofli#3;
     |FINSI;
     ||PINTA #nombofca#2;

     aAlfa = #nombofca#2;
     |QBF aAlfa;
     aAlfa = aAlfa + " euros       ";
     aAlfa = aAlfa % 114;
     |PINTA 1755, aAlfa;
|FPRC;

