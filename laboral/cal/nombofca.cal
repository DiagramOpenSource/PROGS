|FPRC;

|PROCESO inferior;
     #nombofli#0 = #0#0;
     #nombofli#1 = #0#1;
     #nombofli#2 = 01;
     #nombofli#4 = 01;
     #nombofli#6 = " ";
     #nombofli#5 = "B";
|FPRC;

|PROCESO superior;
     #nombofli#0 = #0#0;
     #nombofli#1 = #0#1;
     #nombofli#2 = 12;
     #nombofli#4 = 99;
     #nombofli#6 = "F";
     #nombofli#5 = "P";
|FPRC;

|PROCESO Tipo3; TIPO 3;
     |SI #0#1 < 2011;
          |BLANCO 1251 2371;
     |FINSI;
     |HAZ PintaCredito;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 1; |O nModo = 2;
          |HAZ CargaCentros;

          |ENTLINEAL #1#nombofli, 3, 2, 22, 0, inferior, superior;

          |DELINDEX #100;
          |CIERRA #100;
     |SINO;
          |HAZ CargaCentros;

          |ENTLINEAL #1#nombofli, 3, 7, 22, 0, inferior, superior;

          |DELINDEX #100;
          |CIERRA #100;
     |FINSI;

     swLinAlta = 0;
     swAltaConfirmada = 0;

     |SI nModo = 1;
          swLinAlta = 1;
     |FINSI;
|FPRC;
