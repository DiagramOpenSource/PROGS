|FICHEROS;
     mod00257 #0;
     mod00258 #1,MANTE;
     labtraba;
|FIN;

|PROCESO labtraba;
     |SI #labtraba#42 ! "E";
          |ACABA;
     |FINSI;

     #mod00258#0 = #labtraba#0;
     #mod00258#1 = #labtraba#1;
     |LEE 000#mod00258.=;
     |SI FSalida ! 0;
          #mod00258#2 = #labtraba#2;
          #mod00258#3 = "M";
          |GRABA 020#mod00258.n;
     |FINSI;
|FPRC;

|DEFBUCLE labtraba;
     #labtraba#8;
     ;
     "A", #0#0, 00001;
     "A", #0#0, 99999;
     ;
     NULL, labtraba;
|FIN;

|PROCESO CargaTrab;
     |ABRE #mod00258;
     |BUCLE labtraba;
     |CIERRA #mod00258;
|FPRC;

|PROCESO Cambia; |TIPO 11;
     |SI FSalida = 13;
          |SI #1#3 = "Q";
               #1#3 = "M";
          |SINO;
               #1#3 = "Q";
          |FINSI;
          |GRABA 020#1a;
          |PINTA #1#3;
          |ERROR;
     |FINSI;
|FPRC;

|PROCESO NoModif; |TIPO 0;
     |ERROR;
|FPRC;
