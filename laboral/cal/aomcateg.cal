|PROCESO tipo8; |TIPO 8;
|DDEFECTO #1;
|ABRE #1;
    #1#0 = 0;
|GRABA 000#1n;
|CIERRA #1;

|DDEFECTO #2;
|ABRE #2;
    #2#0 = 0;
|GRABA 000#2n;
|CIERRA #2;
|FPRC;

|PROCESO concepto; |TIPO 0;
|SI #0Campo = 0;    |ACABA;   |FINSI;

|DDEFECTO #1;
|ABRE #1;
    #1#0 = #0Campo;
|LEE 000#1=;
|SI FSalida = 0;
        sw_no = 0;
    |SI Campo = 4;|Y #1#3 ! "N";   sw_no = 1;     |FINSI;
    |SI Campo = 5;|Y #1#3 ! "E";   sw_no = 1;     |FINSI;
    |SI Campo = 6;|Y #1#3 ! "F";   sw_no = 1;     |FINSI;
    |SI Campo = 7;|Y #1#3 ! "D";   sw_no = 1;     |FINSI;
    |SI Campo = 8;|Y #1#3 ! "T";   sw_no = 1;     |FINSI;
    |SI Campo = 9;|Y #1#3 ! "L";   sw_no = 1;     |FINSI;
    |SI sw_no ! 0;
            alfa1 = "    El concepto [" + #1#0 + "] es de tipo [" + #1#3 + "] ...";
        |MENAV alfa1;
        |ERROR;
    |FINSI;
|SINO;
    |MENAV "    Concepto inexistente ...";
    |ERROR;
|FINSI;
|CIERRA #1;
|FPRC;
