                    aPatron = aAlfa + "XDCR\DCL\";
               |FINSI;
               |SI #cretat05#2 = "L";
                    aPatron = aAlfa + "XDCR\RLC\";
               |FINSI;
          |SINO;
               aPatron = aAlfa + "SVA\Msjrec\";
          |FINSI;
          aFichero = aPatron + #cretat05#1;

          aCadena = aFichero;
          swMsDos = 0;
          swBasico = 1;
          |HAZ Tildes;
          aFichero = aCadenaSal;

          |DBASE aDestino;
