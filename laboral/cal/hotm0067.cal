     |DDEFECTO #8;
     |SI #0#45 = "N";         || agrupar empresas S/N
          |SI #0#43 = "M";
               #8#0 = #1#0;       || empresa (proc.mensual)
               #8#1 = #1#1;       || trabajador
          |SINO;
               #8#0 = #70#0;      || empresa (proc.historico)
               #8#1 = #70#1;      || trabajador
          |FINSI;
     |SINO;
          #8#0 = 1;                || siempre la misma empresa
          |SI #0#43 = "M";
               #8#1 = (#1#0 * 100000) + #1#1;       || trabajador
          |SINO;
               #8#1 = (#70#0 * 100000) + #70#1;     || trabajador
          |FINSI;
     |FINSI;

     #labempre#0 = #8#0;
     |LEE 020#labempre.=;

     #labtraba#0 = #8#0;
     #labtraba#1 = #8#1;
     |LEE 020#labtraba.=;

     #8#15 = centro;      || centro
     #8#2 = area;         || area
     #8#3 = secc;         || seccion
     |LEE 101#8=;
