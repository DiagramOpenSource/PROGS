|FICHEROS;
    nomw0015 #0;
    nomw0016 #6;
    labtraba #2;
|FIN;

|VARIABLES;
     nTrabajador = 0;
     nForma      = 0;
     nModo       = 0;

     &enEmpresa  = 0;
     &enModo     = 0;
|FIN;

|RUTINA ClaveBaja6;
      #6#0 = #0#0;
      #6#1 = #0#1;
      #6#2 = 001;
|FRUT;

|RUTINA ClaveAlta6;
      #6#0 = #0#0;
      #6#1 = #0#1;
      #6#2 = 999;
|FRUT;

|PROCESO Tipo7;  |TIPO 7;
     |ABRE #2;
     #2#0 = #0#0;
     #2#1 = #0#1;
     |LEE 040#2=;
     |SI FSalida ! 0;  |DDEFECTO #2;  |FINSI;
     |CIERRA #2;

     |ATRI I;
     |PINTA 0928, #2#2;
     |ATRI i;

     |ENTLINEAL #1#6, -3, 7, 22, 0, ClaveBaja6, ClaveAlta6;
|FPRC;

|PROCESO Borra06;
     |BORRA 020#6a;
     |LIBERA #6;
|FPRC;

|DEFBUCLE nomw0016;
     #6#1;
     ;
     #0#0, #0#1, 001;
     #0#0, #0#1, 999;
     be;
     NULL, Borra06;
|FIN;

|PROCESO Tipo2;  |TIPO 2;
     nForma = 0 +. 1;
     nModo  = (FEntrada / 100) ! 0;

     |SI nModo = 3;
         |BUCLE nomw0016;
         |ACABA;
     |FINSI;

     |SI nForma = 1;
         |ENTLINEAL #1#6, -3, 2, 22, 0, ClaveBaja6, ClaveAlta6;
     |FINSI;
|FPRC;

|RUTINA ClaveBaja0;
     #0#0 = enEmpresa;
     #0#1 = 00001;
|FRUT;

|RUTINA ClaveAlta0;
     #0#0 = enEmpresa;
     #0#1 = 99999;
|FRUT;

|PROGRAMA;
     |ENTLINEAL #1#0, -2, enModo, 22, 1, ClaveBaja0, ClaveAlta0;
|FPRO;
