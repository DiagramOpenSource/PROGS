|FICHEROS;
     cretat53 #0;
|FIN;

|VARIABLES;
     aAlfa1 = "";
     aIPF = "";
|FIN;

|PROCESO ipf;
     aAlfa1 = #0#6;
     aAlfa1 = aAlfa1 % 101;
     |SI aAlfa1 ] "0";|Y aAlfa1 [ "9";
          aIPF = "1";            || tipo identificacion DNI
     |SINO;
          |SI aAlfa1 ! "X"; |Y aAlfa1 ! "Y"; |Y aAlfa1 ! "Z";
               aIPF = "9";        || tipo identificacion CIF
          |SINO;
               aIPF = "6";        || tipo identificacion NIE
          |FINSI;
     |FINSI;
     #0#5 = aIPF;
     |PINTA #0#5;
|FPRC;
