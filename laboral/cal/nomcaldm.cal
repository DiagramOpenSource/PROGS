|FICHEROS;
     nomcaldm;
|FIN;

|VARIABLES;
     aAlfa = "";
|FIN;

|PROCESO Borra;
     |BORRA 000#nomcaldm.a;
|FPRC;

|DEFBUCLE Borra;
     #nomcaldm#1;
     ;
     99999
     999999;
     ;
     NULL, Borra;
|FIN;

|PROCESO Renumera;
/*
     aAlfa = #nomcaldm#0;
     aAlfa = ("00000" + aAlfa) % -105;
     aAlfa = (aAlfa % 102) + "0" + (aAlfa % -103);
*/
     #nomcaldm#0 = #nomcaldm#0 / 10;
     |GRABA 000#nomcaldm.n;
|FPRC;

|DEFBUCLE Renumera;
     #nomcaldm#1;
     ;
     000000;
     999999;
     ;
     NULL, Renumera;
|FIN;

|PROCESO inferiorBo;
     #nomcaldm#0 = 000001;
|FPRC;

|PROCESO superiorBo;
     #nomcaldm#0 = 999999;
|FPRC;

|PROGRAMA;
     |CLS;
     |ABRE #nomcaldm;
     ||BUCLE Renumera;
     ||BUCLE Borra;
     |ENTLINEAL #1#nomcaldm, -1, 2, 22, 1, inferiorBo, superiorBo;
     |CONSULTA_CLAVE #1#nomcaldm;
     |CIERRA #nomcaldm;
|FPRO;
