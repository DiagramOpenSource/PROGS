|FICHEROS;
     nomgirtm #100;
|FIN

|VARIABLES;

|FIN;

|PROCESO Tipo5; |TIPO 5;
     #100#9 = "N";
     |LINEAS_BI_ATRI #100#9, S, I, -1;
|FPRC;

|PROCESO Ayuda; |TIPO 7;
     |MENSA "    Pulse < F5 > para forzar la emision/actualizacion del trabajador con incid.";
|FPRC;

|PROCESO Cambia;
     |SI FSalida = 13;
          |SI #100#9 = "N";
               #100#9 = "F";
               |PINTA #100#9;
          |SINO;
               |SI #100#9 = "F";
                    #100#9 = "N";
                    |PINTA #100#9;
               |FINSI;
          |FINSI;

          ||PONCONTROL 23, "si";
          |GRABA 020#100.a;
          |LIBERA #100;
     |FINSI;
|FPRC;
