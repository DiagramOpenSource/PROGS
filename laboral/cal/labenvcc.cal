|FICHEROS;
   labenvcc #0;
   labenvcl #1;

   labregis #10;
|FIN;

|VARIABLES;
   aAlfa = "";

   nModo = 0;
|FIN;

|PROCESO PintaTodo; |TIPO 13;
   |SI #labenvcc#5 = "C"; aAlfa = " Contratos       "; |FINSI;
   |SI #labenvcc#5 = "P"; aAlfa = " Prorrogas       "; |FINSI;
   |SI #labenvcc#5 = "O"; aAlfa = " Copias basicas  "; |FINSI;
   |SI #labenvcc#5 = "T"; aAlfa = " Transformaciones"; |FINSI;
   |ATRI R; |PINTA 0659, aAlfa; |ATRI N;
|FPRC;

|PROCESO MarcaPdts;
   #labregis#0 = #labenvcl#9;
   |LEE 000#labregis.=;
   |SI FSalida = 0;
      #labregis#27 = "P"; |GRABA 020#labregis.a;
   |FINSI;
|FPRC;

|DEFBUCLE MarcaPdts;
#labenvcl#1;
;
#labenvcc#0, #labenvcc#6, 001;
#labenvcc#0, #labenvcc#6, 999;
;
NULL,MarcaPdts;
|FIN;

|PROCESO Tipo1C; |TIPO 1;
   nModo = (FEntrada / 100) ! 0;
   |SI nModo = 3;
      |ABRE #labregis;
      |BUCLE MarcaPdts;
      |CIERRA #labregis;
   |FINSI;
|FPRC;

|PROCESO Tipo2L; |TIPO 2;
   nModo = (FEntrada / 100) ! 0;
   |SI nModo = 3;
      |ABRE #labregis;
      #labregis#0 = #labenvcl#9;
      |LEE 000#labregis.=;
      |SI FSalida ! 0;
         |CONFI "    NRegistro del contrato no encontrado. No se volvera a dejar pendiente. Continuar ? ";
         |SI FSalida ! 0;
            |ERROR;
         |FINSI;
      |SINO;
         #labregis#27 = "P"; |GRABA 020#labregis.a;
      |FINSI;
      |CIERRA #labregis;
   |FINSI;
|FPRC;

|PROCESO Tipo7; |TIPO 7;
   nModo = (FEntrada / 100) ! 0;
   |SI nModo ! 2; |Y nModo ! 3;
      #labenvcc#6 = 1; |PINTA #labenvcc#6;
   |FINSI;
|FPRC;
