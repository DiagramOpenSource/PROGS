               |SINO;
                    |SI #labtraba#42 = "D"; |Y #labtraba#64 = "M";
                         swSigue = swSigue + 1;
                    |FINSI;
               |FINSI;
          |FINSI;
          |SI #labtraba#45 ! "421"; |Y #labtraba#45 ! "422";
