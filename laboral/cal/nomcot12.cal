                         || ERE, si que cotizo como los mensuales
                         swSigue = swSigue + 1;
                    |SINO;
                         || si no, si es por otra causa de IT, cotizo como
                         || diario, por 31 dias
                    |FINSI;
               |SINO;
                    swSigue = swSigue + 1;
               |FINSI;
          |SINO;
               swSigue = swSigue + 1;
          |FINSI;
          |SI #nomcot02#27 = "HUE"; |O #nomcot02#27 = "ASR";
               || nunca en huelga ni ASR
               swSigue = swSigue - 1;
          |FINSI;
          |SI swSigue = 5;
