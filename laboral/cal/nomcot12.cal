          |FINSI;
     |FINSI;

     |SI aSituacion = "HTP";
          #nomcot02#87 = #nomcot01#21;
          |SI #labtraba#42 = "X"; |O #labtraba#42 = "T";
          |O #labtraba#42 = "P";
          ||O #labtraba#42 = "I";
               nNume = #labtraba#47;
               |SI nNume = 0;
