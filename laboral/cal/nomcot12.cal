               nNume = #labtraba#45;
               |SI nNume ] 300; |Y nNume [ 399;
                    swFijoDisc = 1;
               |FINSI;
          |FINSI;

          || hacemos otra cosa mas en esta pasada: el indicador 51M
          swSigue = 0;
