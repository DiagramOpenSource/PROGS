          |FINSI;
          |SI nTopemes ! 30; |Y nDiasAlta = nTopemes;
               swSigue = swSigue + 1;
          |FINSI;
          |SI #labtraba#42 = "X"; |O swFijoDisc = 1;
               |SI nDiasIT ! 0;
                    |SI nDiasERE ! 0;
