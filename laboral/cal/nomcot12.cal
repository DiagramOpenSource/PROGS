|PROCESO HorasTP;
     |SI #labtraba#42 = "X"; |O #labtraba#42 = "T";
     |O #labtraba#42 = "P";
     ||O #labtraba#42 = "I";  || los "I" NO
          nNume = #labtraba#47;
          |SI nNume = 0;
               nNume = (#labtraba#234 / 7) ! 2;
          |FINSI;
     |SINO;
          nNume = (#nomconca#162 / 7);
     |FINSI;
     |SI #labtraba#42 = "C"; |O #labtraba#42 = "F";
          |SI #labtraba#47 ! 0;
               nNume = #labtraba#47;
          |FINSI;
     |FINSI;
