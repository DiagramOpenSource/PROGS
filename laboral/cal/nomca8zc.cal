     |XABRE "C", aAlfa, nHandle;
     |SI FSalida ] 0;
          |XLEE nHandle, 250, aAlfa; aOrigen = aAlfa;
          |XLEE nHandle, 250, aAlfa; aDestino = aAlfa;
          |XCIERRA nHandle;
