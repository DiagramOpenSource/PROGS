          |PINTA 1121, "   Estado: descargando instalador.   ";
*/
          |PINPA #0#0;
          |PTEC 802;
          |PAUSA;
          aIP = ""; |IP_REMOTO aIP;
          |SI aIP = "";
               |COPIA_FICHERO aOrigen, aDestino;
          |SINO;
               |ENVIA_FICHERO aOrigen, aDestino;
          |FINSI;
          nResultado = FSalida;
     |SINO;
          nResultado = -1;
     |FINSI;

     aAlfa = PARAMETRO;
     |XABRE "CW", aAlfa, nHandle;
     |SI FSalida ] 0;
          aAlfa = nResultado; |XGRABA nHandle, aAlfa;
          |XCIERRA nHandle;
     |FINSI;
|FPRO;
