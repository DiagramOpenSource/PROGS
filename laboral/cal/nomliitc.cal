|FICHEROS;
     nomliitc;
     nomliitl;
     nomlinom;
|FIN;

|VARIABLES;
     nNume = 0;
     nModo = 0;
|FIN;

|PROCESO Tipo80; |TIPO 80;
     |CAMPO_ANCHO #nomlinom#1, 1, 35;
|FPRC;

|PROCESO nomliitl;
     #nomliitl#0 = #nomliitc#0;
     |GRABA 020#nomliitl.n;
     |LIBERA #nomliitl;
|FPRC;

|DEFBUCLE nomliitl;
     #nomliitl#1;
     ;
     "CAS", "  ";
     "CAS", "zz";
     ;
     NULL, nomliitl;
|FIN;

|PROCESO copia;
     |BUCLE nomliitl;
|FPRC;

|PROCESO CopiaESP; |TIPO 6;
     |ABRE #nomlinom;
     #nomlinom#0 = #nomliitc#0;
     |LEE 000#nomlinom.=;
     |SI FSalida ! 0;
          |MENAV "    ATENCION. El idioma ha de existir en el mantenimiento de idiomas para el recibo";
          |CIERRA #nomlinom;
          |ERROR;
          |ACABA;
     |FINSI;

     nModo = FEntrada;
     nModo = (nModo / 100) ! 0;
     |SI nModo = 1;
          |CONFI "    SCopiar del CAS?";
          |SI FSalida = 0;
               |HAZ copia;
          |FINSI;
     |FINSI;
|FPRC;
