|FICHEROS;
     nominf10 #0;

     labempre;
     labcentr;

     nombofca;
     nombofli;
|FIN;

|VARIABLES;
     swHay = 0;
|FIN;

|PROCESO nombofli;
     |SI swHay = 0;
          Impresora = "CrystalReport";
          |IMPRE -1;
          |INFOR "nominf10";
          swHay = 1;
     |FINSI;

     #labempre#0 = #nombofli#0;
     |LEE 000#labempre.=;

     #labcentr#0 = #nombofli#0;
     #labcentr#1 = #nombofli#4;
     |LEE 000#labcentr.=;

     |PRINT;
|FPRC;

|DEFBUCLE nombofli;
     #nombofli#1;
     ;
     #nombofca#0, #nombofca#1, #0#4, 001, " ", " ";
     #nombofca#0, #nombofca#1, #0#5, 999, "z", "z";
     ;
     NULL, nombofli;
|FIN;

|PROCESO nombofca;
     |BUCLE nombofli;
|FPRC;

|DEFBUCLE nombofca;
     #nombofca#1;
     ;
     #0#0, #0#2;
     #0#1, #0#3;
     ;
     NULL, nombofca;
|FIN;

|PROGRAMA;
     |CLS;
     |PINPA #0#0

     |ABRE #0;
     |DDEFECTO #0;
     |LEE 000#0p;
     |SI FSalida = 0;  |BORRA 020#0a;  |FINSI;
     |PINDA #0#0;
     |ENDATOS #1#0;

     |SI FSalida = 0;
          |ABRE #labempre;
          |ABRE #labcentr;

          |BUCLE nombofca;

          |SI swHay = 1;
               |PIEINF;
               |FININF;
               |FINIMP;
          |SINO;
               |MENAV "    No se han encontrado bonificaciones en el periodo solicitado";
          |FINSI;

          |CIERRA #labcentr;
          |CIERRA #labempre;
     |FINSI;

     |GRABA 000#0n;
     |CIERRA #0;
|FPRO;
