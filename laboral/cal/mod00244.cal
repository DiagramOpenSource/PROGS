          nPrecio = #mod00242#6;
     |SINO;
          nPrecio = #mod00242#2;
     |FINSI;

     |SI #mod00249#6 ! 0;
           nPrecio = #mod00249#6;
     |SINO;
           |SI #mod00249#3 ! 0;
                nPrecio = #mod00249#3;
           |FINSI;
     |FINSI;

     #mod00244#6 = nPrecio;
