|FICHEROS;
     silcon23 #0;
     cretam3e;
|FIN;

|VARIABLES;
     aValor = "";
     aAlfa = "";
     nCampo = 0;

     nCol = 0;
     aCol = "";
     nColLetra = 0;
     aLetraCol = "";
     nNumeCol = 0;
     nLinea = 0;

     aUno = "";
     aDos = "";
     nUno = 0;
     nDos = 0;
|FIN;

|PROCESO ColumnaExcel;
     |SI nCol [ 26;
          nColLetra = nCol + 64;
          aLetraCol = &nColLetra;
     |SINO;
          |SI nCol [ 52;
               nNumeCol = (nCol - 26) + 64;
               aLetraCol = "A" + &nNumeCol;
          |SINO;
               |SI nCol [ 78;
                    nNumeCol = (nCol - 52) + 64;
                    aLetraCol = "B" + &nNumeCol;
               |SINO;
                    nNumeCol = (nCol - 78) + 64;
                    aLetraCol = "C" + &nNumeCol;
               |FINSI;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO Cambia;
     aValor = "";
     aCol = #0#10;
     |QBF aCol;
     aCol = ("  " + aCol) % -102;
     aUno = aCol % 101;
     aDos = aCol % 201;

     nDos = &aDos;
     nDos = nDos + 1;
     |SI nDos = 91;
          aDos = "A";
          |SI aUno = " ";
               aUno = "A";
          |SINO;
               nUno = &aUno;
               nUno = nUno + 1;
               aUno = &nUno;
          |FINSI;
     |SINO;
          aDos = &nDos;
     |FINSI;
     aValor = aUno + aDos;
     |QBF aValor;

     #0#10 = aValor;
     |GRABA 020#0.a;
     |LIBERA #0;
|FPRC;

|DEFBUCLE Cambia;
     #silcon23#1;
     ;
     "FIE", nLinea;
     "FIE", 99999;
     ;
     NULL, Cambia;
|FIN;

|PROCESO inserta_col; |TIPO 11;
     |SI FSalida = 13;
          |MENAV "    Efecinco!!! ";
          |CONFI "    NContinuar??? ";
          |SI FSalida = 0;
               nLinea = #0#1;
               |BUCLE Cambia;
          |FINSI;
     |FINSI;
|FPRC;

|PROCESO descripcion;
     |SI #0Campo ! Contenido;
          nCampo = #0#8;
          aAlfa = nCampo;
          |QBF aAlfa;
          aAlfa = ("000" + aAlfa) % -103;
          aAlfa = "|C" + aAlfa + "NOMBRE";
          #0#9 = #cretam3e#aAlfa#;
          |PINTA #0#9;
     |FINSI;
|FPRC;

|PROCESO inferior;
     #silcon23#0 = "FIE";
     #silcon23#1 = 10;
|FPRC;

|PROCESO superior;
     #silcon23#0 = "FIE";
     #silcon23#1 = 99999;
|FPRC;

|PROGRAMA;
     |CLS;
     |ENTLINEAL #1#silcon23, -1, 2, 22, 1, inferior, superior;
|FPRO;
