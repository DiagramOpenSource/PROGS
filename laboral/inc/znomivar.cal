      |SIGUE;
      aAlfa = aAlfa2;              |QBF aAlfa; aAlfa = aAlfa + ":";
      aAlfa = aAlfa + #nompmail#7; |QBF aAlfa; aAlfa = aAlfa + ":";
      aDirOrigen = aAlfa + aDirOrigen;
   |FINSI;

