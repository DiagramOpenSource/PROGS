     #nomocupa#1 = #nomocuma#1;
     #nomocupa#2 = #nomocuma#2;
     #nomocupa#3 = #nomocuma#3;
     #nomocupa#4 = #nomocuma#4;
     #nomocupa#5 = #nomocuma#5;

     |SI FEstado = 0;
          |GRABA 020#nomocupa.a;
