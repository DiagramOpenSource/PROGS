||****************************************************************************
||  PROCESO PARA GRABAR EN EL CONTROL DE NOMINAS PAGADAS
||****************************************************************************

|PROCESO GrabaNompag;
     |ABRE #nomcontr; |LEE 000#nomcontr.p; |CIERRA #nomcontr;
     |SI #nomcontr#42 = "N"; |ACABA; |FINSI;
     |ABRE #nompagca;
     |ABRE #nompagli;
     |SI nTip = 10;
          nTip = 0;
          aNorPag = "P";
     |SINO;
          aNorPag = "N";
     |FINSI;
     #nompagca#0 = #labempre#0;
     #nompagca#1 = nAny;
     #nompagca#2 = nMes;
     #nompagca#3 = nTip;
     #nompagca#4 = aNorPag;
     |LEE 101#nompagca.=;
     |SI FSalida ! 0;
          #nompagca#0 = #labempre#0;
          #nompagca#1 = nAny;
          #nompagca#2 = nMes;
          #nompagca#3 = nTip;
          #nompagca#4 = aNorPag;
          |GRABA 020#nompagca.n;
          |LIBERA #nompagca;
     |FINSI;

     |SI aNorPag = "N";  nTipoCuenta = 1;  |FINSI;
