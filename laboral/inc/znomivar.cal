     |LEE 000#nomcnama.];
     |SI FSalida = 0;
          |SI #nomcnama#0 = "          "; |Y #nomcnama#6 = fFecha;
               |HAZ Actualizador;
          |SINO;
               |LEE 000#nomcnama.a;
               fFecha = #nomcnama#6;
               |HAZ Actualizador;
          |FINSI;
     |SINO;
          |LEE 000#nomcnama.u;
          fFecha = #nomcnama#6;
          |HAZ Actualizador;
     |FINSI;

     |CIERRA #nomcnama;
|FPRC;

