          |ABRE #nomconl2;
          #nomconl2#0 = #0#0;     || cod.convenio
          #nomconl2#1 = #0#2;     || cod.cto.
          |LEE 101#nomconl2.=;
          |SI FSalida = 0;
               |BORRA 020#nomconl2.a;
