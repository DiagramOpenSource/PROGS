|PROCESO clavep; |TIPO 0;
    inkey = FSalida;
|SI #0Campo = "P";
    |SI #0#2 < 201; |O #0#2 > 208;
        |ERROR;
    |FINSI;
|FINSI;
|SI #0Campo = "A";|Y inkey = 0;
    |SI #0#2 < 201;|O #0#2 > 208;
        |PUSHV 0501 2380;
        |DDEFECTO #2;
        |ABRE #2;
            #2#0 = #0#0;     || cod.convenio
            #2#1 = #0#2;     || cod.cto.
        |LEE 101#2=;
            FEstado = FSalida;
        |PINPA #0#2;
        |PINDA #0#2;
        |ENDATOS #1#2;
        |SI FSalida = 0;
            |SI FEstado = 0; |GRABA 020#2a; |FINSI;
            |SI FEstado ! 0; |GRABA 020#2n; |FINSI;
        |FINSI;
        |LIBERA #2;
        |CIERRA #2;
        |POPV;
    |SINO;
        |ERROR;
    |FINSI;
|FINSI;
|FPRC;

