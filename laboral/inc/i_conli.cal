     |SI #0#3 = "E";
          |PUSHV 0501 2380;
          |DDEFECTO #nomconl2;
          |ABRE #nomconl2;
          #nomconl2#0 = #0#0;     || cod.convenio
          #nomconl2#1 = #0#2;     || cod.cto.
          |LEE 101#nomconl2.=;
          FEstado = FSalida;
          |PINPA #0#nomconl2;
          |PINDA #0#nomconl2;
          |ENDATOS #1#nomconl2;
          |SI FSalida = 0;
               |SI FEstado = 0; |GRABA 020#nomconl2.a; |FINSI;
               |SI FEstado ! 0; |GRABA 020#nomconl2.n; |FINSI;
          |FINSI;
          |LIBERA #nomconl2;
