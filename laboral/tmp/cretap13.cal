               |PINTA 1424, aCadenaSal;

               aAlfa = " Recogiendo respuestas ";
               aCadena = aAlfa; nEspacio = 33; |HAZ Centra;
               |PINTA 1524, aCadenaSal;
          |SINO;
               swNoMensaje = 1;
          |FINSI;
     |SINO;
          |SI aParam = "A";
               |SI #cretam00#6 = 1; |O #cretam00#6 = 2;
                    swNoMensaje = 1;
               |FINSI;
          |FINSI;
     |FINSI;

