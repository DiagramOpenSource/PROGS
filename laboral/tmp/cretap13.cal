          |PARA; |SI FLinea ] 0; |HACIENDO;
               |XLEE nHandle, 250, aLinea;
               FLinea = FSalida;

               |QBF aLinea;

               aAlfa = aLinea;
               aAlfa1 = aLinea - "xml";
               |SI aAlfa = aAlfa1;
                    aAlfa1 = aLinea - "t01";
               |FINSI;
               aFichero = "";
               |SI aAlfa ! aAlfa1;
                    aFecha = aLinea % 110;
                    aFecha = (aFecha % 102) + "." + (aFecha % 402) + "."  + (aFecha % -104);
                    fFecha = aFecha;
                    aHora = aLinea % 1305;
                    aHora = (aHora % 102) + (aHora % 402);
                    nHora = aHora;

                    swAcaba = 0;
                    |SI fFecha < fFechaRef;
                         swAcaba = 1;
                    |SINO;
                         |SI fFecha = fFechaRef;
                              |SI nHora [ nHoraRef;
                                   swAcaba = 1;
                              |FINSI;
                         |FINSI;
                    |FINSI;
/*
                    fFechaRefPruebas = "17.12.2014";
