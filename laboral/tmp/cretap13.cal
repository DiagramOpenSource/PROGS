     |DELINDEX #cretat07;
|FPRC;

|PROGRAMA;
     |CLS;
/*
     |ABRE #cretam40;
     |LEE 000#cretam40.p;
     FEstado = FSalida;
     #cretam40#1 = "01.01.2015";
     #cretam40#2 = "0000";
     |SI FEstado ! 0;
          |GRABA 020#cretam40.n;
     |SINO;
          |GRABA 020#cretam40.a;
     |FINSI;
     |CIERRA #cretam40;
*/
     |HAZ Ejecutables;

     |ABRE #cretam00;
     |LEE 000#cretam00.p;
     |SI FSalida ! 0;
          swPrimeraVez = 1;
     |FINSI;
     |CIERRA #cretam00;

     aParam = PARAMETRO;
     |QBF aParam;
     |SI aParam = "M";
                         || con menu
          |PINPA #0#0;
          |PINDA #0#0;
          |ENDATOS #1#0;
     |SINO;
          |SI aParam = "0 laboral fich NULL creta";
               aParam = "A";       || automatico, al entrar
          |FINSI;

          |SI aParam = "A"; |Y #cretam00#6 = 3;
               #0#0 = "C";
          |SINO;
               |HAZ Cabecera;
               #0#0 = "T";
          |FINSI;
     |FINSI;

     |SI #0#0 = "S"; |O #0#0 = "T";
          aAlfa = Usuario;  |QBF aAlfa; aAlfa = "c1" + aAlfa;
          |NOME_DAT #cretat05#aAlfa#;
          |DELINDEX #cretat05;
          |ABRE #cretat05;
          |ABRE #cretae15;

          |HAZ Recogida;

          || esto realmente ya no hace falta, lo tenia para que en el lineal
          || las respuestas salieran ordenadas por fecha

          nContador = 0;
          |BUCLE Renumera;

          |SI nContador ! 0;
               |SI #0#0 = "S";
                    || muestro el lineal;
                    |HAZ Muestra;
               |FINSI;
               |SI #0#0 = "T";
                    |HAZ Respuestas;
               |FINSI;

               |SI #0#0 ! "C"; |Y nOpcion ! 3;
                    aAlfa = nTotalFicheros;
                    |QBF aAlfa;
                    |HAZ Resultado;
                    || aAlfa = "    Se han recogido y registrado " + aAlfa + " ficheros de respuesta";
                    || MENAV aAlfa;
               |FINSI;
          |SINO;
               |SI swNoMensaje = 0;
                    |MENAV "    No se han encontrado nuevas respuestas pendientes de leer. ";
               |FINSI;
          |FINSI;

          |CIERRA #cretae15;
          |CIERRA #cretat05;
     |FINSI;
|FPRO;
