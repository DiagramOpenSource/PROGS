|FPRC;

|PROCESO sup2;
   #2#0 = "zzzzzzzzzzzz";
   #2#2 = enPlan;
|FPRC;

|PROCESO leocuen; |TIPO 0;
  |SI FSalida = 10;
      |ABRE #2;
      #2#0 = #0Campo;
      #2#2 = enPlan;
      |CONSULTA_F_CLAVE #1#2, inf2, sup2;
      #0Campo = #2#0;
      |PINTA #0Campo;
      |CIERRA #2;
  |FINSI;
  |ABRE #2;
  #2#0 = #0Campo;
  #2#2 = enPlan;
  |LEE 001#2=;
  |SI FSalida = 0;
      #0#4 = #2#1; |PINTA #0#4;
  |FINSI;
  |CIERRA #2;

|FPRC;
