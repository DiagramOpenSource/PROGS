 cuenta = #0#3 % 101;
 |SI cuenta = "6"; |O cuenta = "7";
     #0#5 = 0;
 |SINO;
     #0#5 = #0#9;
 |FINSI;
 |PINTA #0#5;
|FPRC;

|PROCESO saldo; |TIPO 0;
 #0#8 = #0#5 + #0#6 - #0#7;
 |PINTA #0#8;
