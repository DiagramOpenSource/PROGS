|FICHEROS;
   vadirect #99;
|FIN;

|VARIABLES;
   dirfich1 = "";
   longi = "";
   num   = 0 ;
|FIN;

|INCLUYE dscnex_v;

|PROCESO vbase; |TIPO 8;
 |ABRE #99;
 |DDEFECTO #99;
 #99#0 = 1;
 |LEE 000#99=;
 |SI FSalida ! 0;
      dirfich0 = "*";
 |SINO;
      dirfich0 = #99#1;
 |FINSI;
 |QBF dirfich0;
 |SI dirfich0 ! "*";
     longi = dirfich0 % 0;
     num   = longi;
     num   = 100 + (num - 5);
     longi = num;
     dirfich1  = (dirfich0 % longi);
 |FINSI;
|FPRC;


|PROCESO SinBarra;  |TIPO 5;
     FSalida = "SINBARRA";
|FPRC;
