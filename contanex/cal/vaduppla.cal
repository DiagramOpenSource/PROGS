|FICHEROS;
    vaduppla #0;
    vaplanca #1;
    vaplanli #2;
    va8planc #11;
    va8planl #12;
|FIN;

|PROCESO estaono; |TIPO 0;
    |SI #0#4 = 2;
        |ABRE #1;
        #1#0 = #0#0;
        |LEE 040#1=;
        |SI FSalida ! 0;
            |MENAV "      No existe estructura ORIGEN";
            |ERROR;
        |SINO;
            #0#1 = #1#1;
            |PINTA #0#1;
        |FINSI;
        |CIERRA #1;
     |SINO;
        |ABRE #11;
        #11#0 = #0#0;
        |LEE 040#11=;
        |SI FSalida ! 0;
            |MENAV "      No existe estructura ORIGEN";
            |ERROR;
        |SINO;
            #0#1 = #11#1;
            |PINTA #0#1;
        |FINSI;
        |CIERRA #11;
    |FINSI;
|FPRC;

|PROCESO noexist; |TIPO 0;
  |SI #0#4 = 2;
     |ABRE #1;
     #1#0 = #0#2;
     |LEE 040#1=;
     |SI FSalida = 0;
         |MENAV "      Ya existe estructura DESTINO";
         |ERROR;
     |FINSI;
     |CIERRA #1;
  |SINO;
     |ABRE #11;
     #11#0 = #0#2;
     |LEE 040#11=;
     |SI FSalida = 0;
         |MENAV "      Ya existe estructura DESTINO";
         |ERROR;
     |FINSI;
     |CIERRA #11;
  |FINSI;
|FPRC;

|PROCESO lineapl8;
  #12#0 = #0#2;
  |GRABA 040#12n;
  |LIBERA #12;
|FPRC;

|PROCESO lineapla;
  #2#0 = #0#2;
  |GRABA 040#2n;
  |LIBERA #2;
|FPRC;

|PROCESO pasalo; |TIPO 3;
  |SI #0#4 = 2;
      |ABRE #1;
      #1#0 = #0#2;
      #1#1 = #0#3;
      |GRABA 040#1n;
      |LIBERA #1;
      #1#0 = #0#0;
      |LEE 040#1=;
      |SI FSalida = 0;
          |BUCLELIN 0lineapla#1;
      |FINSI;
      |CIERRA #1;
  |SINO;
      |ABRE #11;
      #11#0 = #0#2;
      #11#1 = #0#3;
      |GRABA 040#11n;
      |LIBERA #11;
      #11#0 = #0#0;
      |LEE 040#11=;
      |SI FSalida = 0;
          |BUCLELIN 0lineapl8#11;
      |FINSI;
      |CIERRA #11;
  |FINSI;
|FPRC;
