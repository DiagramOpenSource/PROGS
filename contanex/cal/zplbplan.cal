|FICHEROS;
     vaticuen;
     va8planc;
     va8planl;
|FIN;

|VARIABLES;
     &eaPrograma  = "";
     &eaExtension = "";
     &eaPlanillaX = "";
     &enEjercicio = 0;
     &enPlan     = 0;
|FIN;

|PROCESO vaticuenPlan;
     |ABRE #vaticuen;
     #vaticuen#2  = enPlan;
     #vaticuen#0  = "";
     |LEE 000#vaticuen.];
     |SI FSalida ! 0;  |O #vaticuen#2 ! enPlan;
         |CIERRA #vaticuen;

         eaPrograma  = "vaticuen";
         eaExtension = "nex";
         |SUB_EJECUTA_NP ":basica/dspatron";

         |ABRE #vaticuen;
     |FINSI;

     #vaticuen#2 = enPlan;
     #vaticuen#0  = "";
     |LEE 000#vaticuen.];
     |SI FSalida ! 0;  |O #vaticuen#2 ! enPlan;
         |MENAV "0000No existe el Plan Contable para el 2008";
         |MENAV "0000Consulte con su administrador";
     |FINSI;
     |CIERRA #vaticuen;
|FPRC;

|PROCESO va8plancPlanX;
     |ABRE #va8planc;
     #va8planc#0  = 0;
     |LEE 000#va8planc.];
     |SI FSalida ! 0;
         |CIERRA #va8planc;

         eaPrograma  = "va8planc";
         eaExtension = "nex";
         |SUB_EJECUTA_NP ":basica/dspatron";

         eaPrograma  = "va8planl";
         eaExtension = "nex";
         |SUB_EJECUTA_NP ":basica/dspatron";

         |ABRE #va8planc;
     |FINSI;

     #va8planc#0  = 0;
     |LEE 000#va8planc.];
     |SI FSalida ! 0;
         |MENAV "0000No existe la planilla de Balances para el Plan del 2008";
         |MENAV "0000Consulte con su administrador";
     |FINSI;
     |CIERRA #va8planc;
|FPRC;

|PROCESO PlanDePlanillasX;
     |SI enEjercicio ! 0;
         |SI enEjercicio [ 2007;  enPlan = 1990;  |FINSI;
         |SI enEjercicio ] 2008;  enPlan = 2008;  |FINSI;
     |FINSI;

     |SI enPlan ! 2008;  |ACABA;  |FINSI;

     |SI eaPlanillaX = "vaticuen";  |HAZ vaticuenPlan;  |FINSI;
     |SI eaPlanillaX = "va8planc";  |HAZ va8plancPlanX;  |FINSI;
|FPRC;
