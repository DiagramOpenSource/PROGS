         |SINO;
            |SI #eosactiv@#46 ! "S";
               #eosm3130@#0 = #recappla#0;
               #eosm3130@#1 = #recappla#1;
            |SINO;
               #eosm3130@#0 = #recappla#5;
               #eosm3130@#1 = #recappla#6;
            |FINSI;
         |FINSI;
         #eosm3130@#2 = Cont;
         #eosm3130@#3 = nAnyoRenta;
         |LEE 010#eosm3130@.=;
         |SI FSalida = 0;
            |SI #eosm3130@#4 ! "          ";
