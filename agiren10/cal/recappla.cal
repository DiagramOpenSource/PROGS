          #rem00031#1  = Linea1;
          |LEE 000#11=;
          |SI FSalida ! 0;  |SAL;  |FINSI;
          Linea1 = Linea1 + 1;
   |SIGUE;

   || Se crea de nuevo el registro en vacio

   |DDEFECTO #rem00031;
   #rem00031#0  = #recappla#2;
   #rem00031#1  = Linea1;
   |SI #eosactiv@#22 ! "A";
      |SI #eosactiv@#42 = "N";
         #rem00031#16 = 9;
      |SINO;
         |SI #eosactiv@#46 ! "S";
            #rem00031#16 = #eosactiv@#1;
         |SINO;
            #rem00031#16 = #eosactiv@#6;
         |FINSI;
      |FINSI;
   |SINO;
      #rem00031#16 = 0;
   |FINSI;
   |SI #recappla#3 = "T";
       #rem00031#2 = "TITULAR";
   |SINO;
       #rem00031#2 = "CONYUGE";
   |FINSI;

   #rem00031#15 = #eosplan1@#79 % nPorce;

   |GRABA 020#rem00031.n;
   |LIBERA #rem00031;

   enEmpresa    = #recappla#2;
   eaPrograma   = "rem00031";
   eaPunto      = "  ";
   |HAZ ProgramaActivo;
|FPRC;

|PROCESO DesglosaBien;
      Comodin1 = Comodin + "eoscomul.mas";
      |CARGA_DEF Comodin1,eoscomul@;

      |PATH_DAT #7 Path;

      |ABRE #eoscomul@;
      |SELECT #2#eoscomul@;
      #eoscomul@#3 = #0#0;
      #eoscomul@#5 = #0#1;
      #eoscomul@#7 = nAnyoRenta;
      |LEE 010#eoscomul@.=;
      |SI FSalida = 0;
          #0#0 = #eoscomul@#0;
          #0#1 = #eoscomul@#1;
          #0#5 = #eoscomul@#3;
          #0#6 = #eoscomul@#5;
      |FINSI;
      |CIERRA #eoscomul@;
      |DESCARGA_DEF #eoscomul@;
|FPRC;

|PROGRAMA;
   |ABRE #401;
   |LEE 040#401p;
   nPatrones = FSalida;
   |CIERRA #401;

   |SI nPatrones ! 0;
       |SUB_EJECUTA_NP "rez00001;INSTALAR";
   |FINSI;

   |HAZ QueVersion;

   nAnyoRenta = 10;

   || Revisar Paths de Agicli

   |DBASS Comodin; |QBF Comodin;
   Comodin1 = Comodin + "agicli/def/agicontr.mas";
   |CARGA_DEF Comodin1,agicontr@;
   |PATH_DAT #10 Comodin;

   |ABRE #agicontr@;
   |LEE 010#agicontr@.p;
   |SI FSalida = 0;
      Path = #agicontr@#3;
      |QBF Path;
      |SI Path = "*"; |Y Path = "";
         |DBASS Comodin; |QBF Comodin;
         Path = Comodin + "agieos/fich/";
      |FINSI;
   |SINO;
      |DBASS Comodin; |QBF Comodin;
      Path = Comodin + "agieos/fich/";
   |FINSI;
   |CIERRA #agicontr@;
   |DESCARGA_DEF #agicontr@;

   |PATH_DAT #999 Path;
   |ABRE #999;
   |LEE 040#999p;
   |SI FSalida ! 0;  |O #999#1 ! "E";
       |CIERRA #999;
       |MENAV "    Para poder hacer el pase la REO debe estar en EUROS.";
       |ACABA;
   |FINSI;
   |CIERRA #999;

   Comodin = Comodin + "agieos/def/";

   |SI enEmpresa ! 0;
      #0#0 = enEmprEos;
      #0#1 = enActiEos;
      #0#2 = enEmprRen;
      #0#4 = eaCaracter;
      #0#3 = eaTitCony;
      |HAZ DesglosaBien;
      FSalida = 0;
   |SINO;
      |CLS;
      |CABEZA "Captura datos de planillas de REO a renta"
      |PINPA #0#0;
      |PINDA #0#0;

      Comodin1 = Comodin + "eosactiv.mas";
      |CARGA_DEF Comodin1,eosactiv@;
      |PATH_DAT #2  Path;
      |ABRE #eosactiv@;

      |ENDATOS #1#0;
      FEstado = FSalida;

      |CIERRA #eosactiv@;
      |DESCARGA_DEF #eosactiv@;
      FSalida = FEstado;
   |FINSI;

   |SI FSalida = 0;
       Comodin1 = Comodin + "eosplan1.mas";  |CARGA_DEF Comodin1,eosplan1@;
       Comodin1 = Comodin + "eosclien.mas";  |CARGA_DEF Comodin1,eosclien@;
       Comodin1 = Comodin + "eosactiv.mas";  |CARGA_DEF Comodin1,eosactiv@;
       Comodin1 = Comodin + "eosm0020.mas";  |CARGA_DEF Comodin1,eosm0020@;
       Comodin1 = Comodin + "eosm3130.mas";  |CARGA_DEF Comodin1,eosm3130@;
       Comodin1 = Comodin + "eoscomul.mas";  |CARGA_DEF Comodin1,eoscomul@;

      |PATH_DAT #2  Path;
      |PATH_DAT #3  Path;
      |PATH_DAT #4  Path;
      |PATH_DAT #5  Path;
      |PATH_DAT #6  Path;
      |PATH_DAT #7  Path;

      |ABRE #rem00031;
      |ABRE #rem00025;
      |ABRE #rem00026;
      |ABRE #reeospla;
      |ABRE #eosactiv@;
      |ABRE #eosclien@;
      |ABRE #eosm3130@;
      |ABRE #eosm0020@;
      |ABRE #eosplan1@;
      |ABRE #eoscomul@;
      |SELECT #2#eoscomul@;

      #eosactiv@#0 = #recappla#0;
      #eosactiv@#1 = #recappla#1;
      #eosactiv@#2 = nAnyoRenta;
      |LEE 010#eosactiv@.=;
      |SI FSalida = 0;
         |LEE 010#reeospla.p;
         |SI FSalida = 0;
            nPagos = 0;
            |HAZ PagosACuenta;
            |HAZ PasaPlanilla;
         |SINO;
            |MENAV "    No se ha definido el control de planillas";
         |FINSI;
      |SINO;
         |MENAV "    No se han encontrado datos de dicha actividad";
      |FINSI;

      |SELECT #1#eoscomul@;
      |CIERRA #eoscomul@;
      |CIERRA #rem00031;
      |CIERRA #rem00025;
      |CIERRA #rem00026;
      |CIERRA #reeospla;
      |CIERRA #eosplan1@;
      |CIERRA #eosm3130@;
      |CIERRA #eosm0020@;
      |CIERRA #eosactiv@;
      |CIERRA #eosclien@;

      |DESCARGA_DEF #eoscomul@;
      |DESCARGA_DEF #eosm3130@;
      |DESCARGA_DEF #eosm0020@;
      |DESCARGA_DEF #eosactiv@;
      |DESCARGA_DEF #eosclien@;
      |DESCARGA_DEF #eosplan1@;
   |FINSI;
|FPRO;
