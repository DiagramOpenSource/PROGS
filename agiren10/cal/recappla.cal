   |SIGUE;

   || Se crea de nuevo el registro en vacio

   |DDEFECTO #rem00031;
   #rem00031#0  = #recappla#2;
   #rem00031#1  = Linea1;
   |SI #eosactiv@#22 ! "A";
      |SI #eosactiv@#42 = "N";
