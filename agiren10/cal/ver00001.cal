|FICHEROS;
     ver00001 #100;
     retabla1 #401;
     retabla2 #402;
     rem00008 #8;
     rem00500 #500;
|FIN;

|VARIABLES;
     &enAgricola = 0;
     &enError    = 0;
     &eaRenta    = "";

      nSwHay     = 0;
      nLimite    = 0;
      nPorce     = 0;
      nCanti1    = 0;
      nCanti2    = 0;
      nCanti3    = 0;
      nCanti4    = 0;
      nDias      = 0;
      nValor     = 0;
      nRecalculo = 0;

      aAlfa      = "";
      aAlfa1     = "";
      aAlfa2     = "";
|FIN;

|| ************************************************************************
|| PROCESOS DE CAMBIO DE VERSION
|| ************************************************************************

|PROCESO MiraPatrones;
     nSwHay     = 0;
     enAgricola = 0;

     |ABRE #401;
     |LEE 040#401p;
     |SI FSalida ! 0;
         |CIERRA #401;  |DELINDEX #401;
         nSwHay = 1;
     |SINO;
         |CIERRA #401;
     |FINSI;

     |ABRE #402;
     #402#0 = 0;
     |LEE 040#402=;
     |SI FSalida ! 0;  |O #402#45 = 0;
         |CIERRA #402;  |DELINDEX #402;
         nSwHay      = 1;
     |SINO;
         |CIERRA #402;
     |FINSI;

     aAlfa1 = "CARNE DE AVES.";

     |ABRE #500;
     #500#0 = 72;
     |LEE 040#500=;
     |SI FSalida ! 0;  |DDEFECTO #500;  |FINSI;
     |CIERRA #500;

     aAlfa2 = #500#1;  |QBF aAlfa2;
     |SI aAlfa1 ! aAlfa2;
         |DELINDEX #500;
         nSwHay      = 1;
         enAgricola  = 1;
     |FINSI;

     |SI nSwHay = 1;
         |MENAV "0000 Han cambiado o no se han instalado los ficheros patrones, por favor instalelos.";
         |SUB_EJECUTA_NP "*:agiren10/rez00001";
     |FINSI;

     |SI nSwHay = 2;
         |SUB_EJECUTA_NP "*:agiren10/rez00001;AUTOMATICO";
     |FINSI;

     |ABRE #401;
     |LEE 040#401p;
     |SI FSalida ! 0;
         enError = 1;
         |CIERRA #401;  |DELINDEX #401;
     |SINO;
         |CIERRA #401;
     |FINSI;

     |ABRE #402;
     #402#0 = 0;
     |LEE 040#402=;
     |SI FSalida ! 0;  |O #402#45 = 0;
         enError = 1;
         |CIERRA #402;  |DELINDEX #402;
     |SINO;
         |CIERRA #402;
     |FINSI;

     aAlfa1 = "CARNE DE AVES.";

     |ABRE #500;
     #500#0 = 72;
     |LEE 040#500=;
     |SI FSalida ! 0;  |DDEFECTO #500;  |FINSI;
     |CIERRA #500;

     aAlfa2 = #500#1;  |QBF aAlfa2;
     |SI aAlfa1 ! aAlfa2;
         enError = 1;
         |DELINDEX #500;
     |FINSI;
|FPRC;

|PROGRAMA;
     |MENSA "0000Comprobando configuracion patrones.";
     enError = 0;
     |HAZ MiraPatrones;
     |BLIN 05;
|FPRO;
