              Primero = 1;
           |SINO;
              |SAL;
           |FINSI;
        |FINSI;
        LongAlfa = Comodin % 0;
        Long = LongAlfa;
        Long = Long - 1;
        Calc = 100 + Long;
        Comodin = Comodin % Calc;
     |SIGUE;


