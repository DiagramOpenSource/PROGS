|PROCESO MiraPatrones;
     nSwHay = 0;

     |ABRE #0;
     |LEE 040#0p;
     |SI FSalida ! 0;  |O #0#4 < 920;
         |CIERRA #0;  |DELINDEX #0;
         nSwHay = 1;
     |FINSI;
     |CIERRA #0;

     |ABRE #1;
     #1#0 = 30;
     #1#1 = "759";
     |LEE 040#1=;
     |SI FSalida ! 0;
         |CIERRA #1;
         |DELINDEX #1;
         nSwHay = 1;
     |SINO;
         |CIERRA #1;
     |FINSI;

     |ABRE #8;
     #8#0 = 17;
     #8#1 = 899;
     |LEE 040#8=;
     |SI FSalida ! 0;  |O #8#11 ! 31110;
         |ABRE #7;  |CIERRA #7;  |DELINDEX #7;
         |CIERRA #8;
         |DELINDEX #8;
         nSwHay = 1;
     |SINO;
         |CIERRA #8;
     |FINSI;

     |SI nSwHay = 1;
         |MENAV "0000 Han cambiado ficheros patrones, por favor instalelos.";
         |SUB_EJECUTA_NP "rez00001";
     |FINSI;

     |ABRE #0;
     |LEE 040#0p;
     |SI FSalida ! 0;  |O #0#4 < 920;
         |CIERRA #0;  |DELINDEX #0;
         enError = 1;
     |FINSI;
     |CIERRA #0;

     |ABRE #1;
     #1#0 = 30;
     #1#1 = "759";
     |LEE 040#1=;
     |SI FSalida ! 0;
         |CIERRA #1;  |DELINDEX #1;
         enError = 1;
     |SINO;
         |CIERRA #1;
     |FINSI;

     |ABRE #8;
     #8#0 = 17;
     #8#1 = 899;
     |LEE 040#8=;
     |SI FSalida ! 0;  |O #8#11 ! 31110;
         |ABRE #7;  |CIERRA #7;  |DELINDEX #7;
                    |CIERRA #8;   |DELINDEX #8;
         enError = 1;
     |SINO;
         |CIERRA #8;
     |FINSI;
|FPRC;

|PROGRAMA;
     nRecalculo = 0;

     |MENSA "0000Comprobando configuracion patrones.";
     enError = 0;
     |HAZ MiraPatrones;
     |SI enError = 1;
         |ABRE #100;
         |LEE 101#100p;
         |SI FSalida = 0;
             |BORRA 020#100a;
             |LIBERA #100;
         |FINSI;
         |CIERRA #100;

         |ACABA;
     |FINSI;

     |BLIN 5;
|FPRO;
