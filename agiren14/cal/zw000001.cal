|FICHEROS;
     zw000001 #101;

     reacceso #0;
     rem00100 #2;
     rem00101 #3;

     :/agicli/def/agifigen #716;
|FIN;

|VARIABLES;
     aBase          = "";
     aAlfa          = "";

     nHandle        = 0;

     &eaVarDni      = "";
