|FIN;

|PROCESO Tipo12;  |TIPO 12;
     |SI FSalida = 7;   |ACABA;  |FINSI;

     aAlfa = "";
     |ABRE #2;
     |SELECT #2#2;
     #2#1 = #101#2;
     |LEE 000#2=;
     |SI FSalida = 0;  |Y #2#0 ! #101#0;
         aAlfa = "0000NEl NIF se esta usando ya en el codigo " + #2#0 + " como Titular. Desea Continuar";
     |FINSI;

     |SELECT #3#2;
     #2#35 = #101#2;
     |LEE 000#2=;
     |SI FSalida = 0;  |Y #2#0 ! #101#0;
         aAlfa = "0000 El NIF se esta usando ya en el codigo " + #2#0 + " como Conyuge. Desea Continuar";
     |FINSI;
     |CIERRA #2;

     |ABRE #3;
     |SELECT #2#3;
     #3#2 = #101#2;
     |LEE 000#3=;
     |SI FSalida = 0;  |Y #3#0 ! #101#0;
         aAlfa = "0000NEl NIF se esta usando ya en el codigo " + #3#0 + " como Descendiente o Ascendiente. Desea Continuar";
     |FINSI;
     |CIERRA #3;

     |SELECT #1#2;
     |SELECT #1#3;

     |SI aAlfa ! "";
         |CONFI aAlfa;
         |SI FSalida ! 0;  |ERROR;  |FINSI;
     |FINSI;
|FPRC;

|PROCESO Tipo66C0;  |TIPO 66;
     |ABRE #716;

     #716#0 = #101#0;
     |LEE 000#716=;

     |CONSULTA_CLAVE #1#716;
     |SI FSalida = 0;
        #101#0 = #716#0;
     |FINSI;
     |CIERRA #716;

     |ERROR;
|FPRC;

|PROCESO Tipo0C0;   |TIPO 0;
     |C_M #101#0, 0, aAlfa;
     |SI aAlfa = "N";  |ACABA;  |FINSI;


     #0#0 = #101#0;
     |LEE 000#0=;
     |SI FSalida = 0;
         |MENAV "0000El c¢digo introducido ya existe en la renta.";
         |ERROR;
         |CIERRA #0;
         |ACABA;
     |FINSI;

     |ABRE #716;
     #716#0 = #101#0;
     |LEE 000#716=;
     |SI FSalida ! 0;  |DDEFECTO #716;  |FINSI;
     |CIERRA #716;

     #101#1 = #716#2;   |PINTA #101#1;
     #101#2 = #716#13;  |PINTA #101#2;
|FPRC;

|PROCESO Tipo6C2;  |TIPO 6;
     |SI FSalida = 10;
         eaVarDni  = #101#2;
         enCalcCif = 1;
         |HAZ CalculoDNI;
         #101#2 = eaVarDni;
         |PINTA #101#2;
         |ERROR;
         |ACABA;
     |FINSI;

     eaVarDni  = #101#2;
     enCalcCif = 0;
     |HAZ CalculoDNI;
     eaVarDni = (eaVarDni + "            ") % 112;
     |SI #101#2 ! eaVarDni;
         |CONFI "0000SEl Nif puede ser Erroneo. Desea Calcularlo.";
         |SI FSalida = 0;
             #101#2 = eaVarDni;  |PINTA #101#2;
             |ERROR;
             |ACABA;
         |FINSI;
     |FINSI;
|FPRC;
