|FICHEROS;
     ver00001;
     retabla2;
     retabla4;
     rem00500;
     rem00008;
|FIN;

|VARIABLES;
     aDef         = "";
     aMas         = "";
     aBase        = "";
     aPerceptor   = "";
     aAlfa        = "";
     aAlfa1       = "";
     aAlfa2       = "";

     nSwHay       = 0;
     nLimite      = 0;
     nPorce       = 0;
     nCanti1      = 0;
     nCanti2      = 0;
     nCanti3      = 0;
     nCanti4      = 0;
     nDias        = 0;
     nValor       = 0;
     nRecalculo   = 0;

     &enError     = 0;
     &eaRenta     = "";
|FIN;

|| ************************************************************************
|| PROCESOS DE CAMBIO DE VERSION
|| ************************************************************************

|PROCESO MiraPatrones;
     nSwHay     = 0;

     |ABRE #retabla4;
     |LEE 040#retabla4.p;
     |SI FSalida ! 0;
         |CIERRA #retabla4;  |DELINDEX #retabla4;

         |SUB_EJECUTA_NP "rez00001;AUTOMATICO";
     |SINO;
         |CIERRA #retabla4;
     |FINSI;

     |ABRE #rem00008;
     #rem00008#0 = 17;
     #rem00008#1 = 145;
     |LEE 000#rem00008.=;
     |SI FSalida ! 0;
          |CIERRA #rem00008;  |DELINDEX #rem00008;

          |SUB_EJECUTA_NP "rez00001;AUTOMATICO";
     |FINSI;
     |CIERRA #rem00008;

     |ABRE #rem00008;
     #rem00008#0 = 17;
     #rem00008#1 = 947;
     |LEE 000#rem00008.=;
     |SI FSalida ! 0;  |O #rem00008#11 ! 25000;
          |CIERRA #rem00008;  |DELINDEX #rem00008;

          |SUB_EJECUTA_NP "rez00001;AUTOMATICO";
     |FINSI;
     |CIERRA #rem00008;

     |ABRE #retabla2;
     #retabla2#0 = 11;
     |LEE 000#retabla2.=;
     |SI FSalida ! 0;  |O #retabla2#5 = 11.5;
          |CIERRA #retabla2;  |DELINDEX #retabla2;

          |SUB_EJECUTA_NP "rez00001;AUTOMATICO";
     |FINSI;
     |CIERRA #retabla2;

     |ABRE #rem00500;
     #rem00500#0 = 223;
     |LEE 000#rem00500.=;
     |SI FSalida ! 0;  |O #rem00500#5 ! 0.27;
          |CIERRA #rem00500;  |DELINDEX #rem00500;

          |SUB_EJECUTA_NP "rez00001;AUTOMATICO";
     |FINSI;
     |CIERRA #rem00500;

     |ABRE #retabla4;
     |LEE 040#retabla4.p;
     |SI FSalida ! 0;
         enError = 1;
         |CIERRA #retabla4;  |DELINDEX #retabla4;
     |SINO;
         |CIERRA #retabla4;
     |FINSI;

     |ABRE #retabla2;
     #retabla2#0 = 11;
     |LEE 000#retabla2.=;
     |SI FSalida ! 0;  |O #retabla2#5 = 11.5;
           enError = 1;
           |CIERRA #retabla2;  |DELINDEX #retabla2;
     |SINO;
           |CIERRA #retabla2;
     |FINSI;

     |ABRE #rem00500;
     #rem00500#0 = 223;
     |LEE 000#rem00500.=;
     |SI FSalida ! 0;  |O #rem00500#5 ! 0.27;
          |CIERRA #rem00500;  |DELINDEX #rem00500;

          enError = 1;
     |SINO;
          |CIERRA #rem00500;
     |FINSI;

     |ABRE #rem00008;
     #rem00008#0 = 17;
     #rem00008#1 = 145;
     |LEE 000#rem00008.=;
     |SI FSalida ! 0;
          |CIERRA #rem00008;  |DELINDEX #rem00008;

          enError = 1;
     |SINO;
         |CIERRA #rem00008;
     |FINSI;

     |ABRE #rem00008;
     #rem00008#0 = 17;
     #rem00008#1 = 947;
     |LEE 000#rem00008.=;
     |SI FSalida ! 0;  |O #rem00008#11 ! 25000;
          |CIERRA #rem00008;  |DELINDEX #rem00008;

          enError = 1;
     |SINO;
         |CIERRA #rem00008;
     |FINSI;
|FPRC;

|PROGRAMA;
     |MENSA "0000Comprobando configuracion patrones.";
     enError = 0;
     |HAZ MiraPatrones;
|FPRO;
