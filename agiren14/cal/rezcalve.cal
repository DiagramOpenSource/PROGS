|PROCESO MiraImpre;
     |SI #1001#5 = "   ";  |ACABA;  |FINSI;

     |SI #1001#5 ! "714";
         |SI #100#14 = "INACTIVO  ";
             #100#14 = "NO IMPRESO";
         |FINSI;

         |SI #1001#6 = "S";  |Y #100#14 = "NO IMPRESO";
             #100#14 = "IMPRESO";
         |FINSI;

         |SI #1001#6 = "N";  |Y #100#14 = "IMPRESO   ";
              #100#14 = "PARCIAL";
         |FINSI;
     |FINSI;

     |SI #1001#5 = "714";
