|PROCESO LeePension;
     |ABRE #calcul05@;
     #calcul05@#0 = #calcul03@#0;
     #calcul05@#1 = #calcul03@#1;
     #calcul05@#2 = #calcul03@#2;
     |LEE 000#calcul05@.=;
     |SI FSalida ! 0;  |DDEFECTO #calcul05@;  |FINSI;
     |CIERRA #calcul05@;

     |ABRE #calcul06@;
     #calcul06@#0 = #calcul03@#0;
     #calcul06@#1 = #calcul03@#1;
     #calcul06@#2 = #calcul03@#2;
     |LEE 000#calcul06@.=;
     |SI FSalida ! 0;  |DDEFECTO #calcul06@;  |FINSI;
     |CIERRA #calcul06@;

     |SI  #calcul05@#83 ! #calcul06@#83;
      |O  #calcul05@#85 ! #calcul06@#85;
          #1001#0  = #3#0;
          #1001#1  = #3#1;
          #1001#2  = #3#2;
          #1001#5  = #3#5;
          #1001#7  = #3#7;
          #1001#37 = #3#37;
          #1002#5  = "497";
          #1002#6  = "Exceso no reducido sistema de prevision social";
          #1002#7  = #calcul06@#83 + #calcul06@#85;
          enAnt    = #calcul05@#83 + #calcul05@#85;
          |PRINT;
          nHay    = 1;
          nSwPrint = 1;
      |FINSI;

      |SI  #calcul05@#133 ! #calcul06@#133;
       |O  #calcul05@#134 ! #calcul06@#134;
