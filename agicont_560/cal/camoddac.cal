|PROCESO actualiza;
  ini = dig1;
  fecalf = #1#3;
  mes = fecalf % 402;
  mesfec = mes;
  |SI ini = 1;
      mesac = mesfec;
  |SINO;
      mesac = (mesfec - ini) + 1;
      |SI mesac [ 0;
          mesac = (13 - ini) + mesfec;
      |FINSI;
  |FINSI;
  a = (mesac * 3) + 9;
  b = a + 1;
  c = a + 2;
  |SI #1#4 = "D";
      #2a = #2a + #1#5;
      #2#51 = #2#51 + #1#5;
  |SINO;
