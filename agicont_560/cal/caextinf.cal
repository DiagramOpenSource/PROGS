|FICHEROS;
    caextinf #0;
    caextras #1;
    caexttip #2;
    caextral #3;
|FIN;

|VARIABLES;
    &entrada = 1;
    seleccio = 0;
    &sw_2 = 0;
    &sw_3 = 0;
    &sw_4 = 0;
    &sw_5 = 0;
    &sw_6 = 0;
    alfa1 = "";
    &swprim = 0;
|FIN;

|INCLUYE acceso_i;

|PROCESO fechas; |TIPO 7;
    #0#2 = fec1;
    #0#3 = fec2;
|PINTA #0#2;
|PINTA #0#3;
|FPRC;

|PROCESO switches;
    sw_2 = 0;
    sw_3 = 0;
    sw_4 = 0;
    sw_5 = 0;
    sw_6 = 0;
    alfa1 = #1#5;  |QBF alfa1;  |SI alfa1 ! "";  sw_2 = 1;  |FINSI;
    alfa1 = #1#6;  |QBF alfa1;  |SI alfa1 ! "";  sw_3 = 1;  |FINSI;
    alfa1 = #1#7;  |QBF alfa1;  |SI alfa1 ! "";  sw_4 = 1;  |FINSI;
    alfa1 = #1#8;  |QBF alfa1;  |SI alfa1 ! "";  sw_5 = 1;  |FINSI;
    alfa1 = #1#9;  |QBF alfa1;  |SI alfa1 ! "";  sw_6 = 1;  |FINSI;
|FPRC;

|PROCESO lee_tipos;
 |ABRE #2;
 #2#0 = #1#3;
 |LEE 001#2=;
 |SI FSalida ! 0; |DDEFECTO #2; |FINSI;
 |CIERRA #2;
|FPRC;

|PROCESO imprlin;
 |PRINT;
 swprim = 1;
|FPRC;

|PROCESO imprime;
 |HAZ lee_tipos;
 |HAZ switches;
 swprim = 0;
 |BUCLELIN 0imprlin#1;
 seleccio = 1;
|FPRC;

|DEFBUCLE lectura1;      || por diario/fecha/docum.
 #1#1;
 3;
 #0#0, #0#2, #0#4, #0#7;
 #0#1, #0#3, #0#5, #0#8;
 e;
 NULL, imprime;
|FIN;

|DEFBUCLE lectura2;      || por diario/docum./fecha
 #1#3;
 3;
 #0#0, #0#4, #0#2, #0#7;
 #0#1, #0#5, #0#3, #0#8;
 e;
 NULL, imprime;
|FIN;

|DEFBUCLE lectura3;      || por tipos
 #1#2;
 ;
 #0#7, #0#0, #0#2, #0#4;
 #0#8, #0#1, #0#3, #0#5;
 e;
 NULL, imprime;
|FIN;

|PROCESO impresion; |TIPO 3;
  seleccio = 0;
  |IMPRE 0;
  |SI FSalida = 0;
      |INFOR "caextinf";
      |SI #0#6 = 1;  |BUCLE lectura1;  |FINSI;
      |SI #0#6 = 2;  |BUCLE lectura2;  |FINSI;
      |SI #0#6 = 3;  |BUCLE lectura3;  |FINSI;
      |SI seleccio = 0;
          |MENAV "    ATENCION!!! Seleccion Vacia ...";
      |SINO;
          |PIEINF;
      |FINSI;
      |FININF;
  |SINO;
    |MENAV "    Proceso Abortado ...";
  |FINSI;
  |FINIMP;
|FPRC;
