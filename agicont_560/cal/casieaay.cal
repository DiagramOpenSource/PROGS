____________________________________/ CALCULOS DE SALTO DE BLOQUES <F2>
|PROCESO salta1; |TIPO 0;
  |SI FSalida = 10;
      mod = "N"
      |HAZ acumula;
      mod = "S";
      |HAZ operac;
      |HAZ ivas;
  |SINO;
      mod = "S"
      |HAZ acumula;
  |FINSI;
|FPRC;

|PROCESO salta2; |TIPO 0;
  inkey = FSalida;
  |SI inkey = 10;
      mod = "N";
      |HAZ operac;
      mod = "S";
      |HAZ acumula;
      |HAZ ivas;
  |SINO;
      mod = "S";
      |HAZ operac;
  |FINSI;
|FPRC;

|PROCESO salta3; |TIPO 0;
  ||  #0Campo = #0Campo > " ";
  |PINTA #0Campo;
  |SI FSalida = 10;
      mod = "N";
      |HAZ ivas;
      mod = "S";
      |HAZ acumula;
      |HAZ operac;
  |SINO;
      mod = "S";
      |HAZ ivas;
  |FINSI;
|FPRC;

|PROCESO acumula;
  |PARA x = 0;  |SI x [ 15;  |HACIENDO x = x + 1;
        |CAMPO_MODIFICA #0x, 1, mod;
  |SIGUE;
|FPRC;

|PROCESO operac;
  |PARA x = 16;  |SI x [ 47;  |HACIENDO x = x + 1;
        |CAMPO_MODIFICA #0x, 1, mod;
  |SIGUE;
|FPRC;

|PROCESO ivas;
  |CAMPO_MODIFICA #0#48, 1, mod;
  |CAMPO_MODIFICA #0#49, 1, mod;
  |CAMPO_MODIFICA #0#51, 1, mod;
|FPRC;

|PROCESO nocomen; |TIPO 7;
  |SI tipo = "M";
      |CAMPO_MODIFICA #0#50, 1, "S";
  |SINO;
      |CAMPO_MODIFICA #0#50, 1, "N";
  |FINSI;
|FPRC;

|PROCESO modiono;  |TIPO 7;
  |SI #0#48 = "I";  |O #0#48 = "i";  |O #0#48 = "R";  |O #0#48 = "r";
      |CAMPO_MODIFICA #0#51, 1, "S";
  |SINO;
      |CAMPO_MODIFICA #0#51, 1, "N";
  |FINSI;
|FPRC;

|PROCESO noescero;
  |SI #0#51 ! 0;  |ACABA;  |FINSI;
  |SI #0#48 = "I";  |O #0#48 = "i";  |O #0#48 = "R";  |O #0#48 = "r";
      |MENAV "     Debe Introducir el Tipo de IVA";
      |ERROR;
  |FINSI;
|FPRC;
