|FICHEROS;
   camingau #0;
|FIN;

|VARIABLES;
   &entrada = 1;
   &contador = 0;
   &fichero  = "";
   inkey = 0;
   &quepant  = 0;
   &noremesa = 0;
|FIN;

|RUTINA inferior;
 #0#0 = 1;
|FRUT;

|RUTINA superior;
 #0#0 = contador;
|FRUT;

|PROGRAMA;
 |SI quepant = 0;
     |PINPA #0#0;
 |SINO;
     |PINPA #1#0;
     |C_P #0#10,1,0720; |C_P #0#13, 1,0733;
     |C_V #0#16,1,"S";
     |C_P #0#0, 1,1008; |C_P #0#1, 1,1015;
     |C_P #0#8, 1,1018; |C_P #0#2, 1,1021;
     |C_P #0#3, 1,1028; |C_P #0#9, 1,1032;
     |C_P #0#4, 1,1043; |C_P #0#5, 1,1058;
     |C_P #0#14,1,1070; |C_P #0#7, 1,1072;
 |FINSI;
 noremesa = 0;
 |NOME_DAT #0fichero;
 |ENTLINEAL #1#0, 1, 4, 21, 0, inferior, superior;
 |SI quepant = 1;
     |CONFI "2400SDesea crear el Confirme : ";
     |SI FSalida ! 0; noremesa = 1;  |FINSI;
 |FINSI;
|FPRO;

|PROCESO pidetecl; |TIPO 11;
    inkey = FSalida;

    |SI inkey < 9; |O inkey > 11;
        |ACABA;
    |FINSI;

    |SI inkey ! 10;
        |SI quepant = 0;
            |SI #0#7 = "*";
                #0#7 = " ";
            |SINO;
                #0#7 = "*";
            |FINSI;
        |SINO;
            |SI #0#7 = "S";
                #0#7 = "N";
            |SINO;
                #0#7 = "S";
            |FINSI;
        |FINSI;
    |FINSI;

    |SI inkey ! 9;
        |SI #0#14 = "T";
            #0#14 = "C";
        |SINO;
            #0#14 = "T";
        |FINSI;
    |FINSI;

    |PINTA #0#7;
    |PINTA #0#14;
    |GRABA 020#0a;
    |LIBERA #0;
    |PTEC 802;
|FPRC;
