        |SI x ] perdesde;|Y x [ perhasta;
                #3#10 = #3#10 + (#1x * -1);
                habep2 = habep2 + (#1x * -1);
        |FINSI;
    |SIGUE;
        sw_h2 = 1;
|FINSI;

|SI #3#0 = "H"; |Y #3#1 = 3;
    |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8   = #3#8   + (#1x * -1);
            haber3 = haber3 + (#1x * -1);
        |SI x ] perdesde;|Y x [ perhasta;
