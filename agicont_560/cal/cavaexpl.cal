        |PRINT;
    |FINSI;
|FINSI;
|DDEFECTO #1;
    #1#0 = nue_cuent;
    #1#1 = nue_nivel;          || situa de nuevo el puntero
|LEE 000#1=;

    cantidad = guarda_can;
    per_can = guarda_per;
