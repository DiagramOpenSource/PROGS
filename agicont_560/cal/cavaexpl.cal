|PARA; |SI FSalida = 0; |HACIENDO;
    |LEE 000#1s;
        alfa2 = #1#0 % nume2;
    |SI alfa1 = alfa2;|Y FSalida = 0;
        |SI #3#0 = #1#5;|Y #3#1 = #1#4;|Y #3#2 = #1#6;
                        |Y #3#3 = #1#7;|Y #3#4 = #1#8;|Y #1#3 = "S";
            |HAZ recuenta;
        |FINSI;
    |SINO;
        |DDEFECTO #1;
            #1#0 = act_cuent;
            #1#1 = act_nivel;          || lee la cuenta manual
        |LEE 020#1=;
            FSalida = 111;
    |FINSI;
|SIGUE;
|FPRC;

|PROCESO nivel_manual2;
    guarda_can = cantidad;
    guarda_per = per_can;
    cantidad = 0;
    per_can = 0;

|DDEFECTO #1;
    #1#0 = act_cuent;
    #1#1 = act_nivel;          || lee la cuenta manual
|LEE 000#1=;
|SI FSalida = 0;
