|PROCESO resulta;    || Calculamos cada cuenta
|SI #3#0 = "D"; |Y #3#1 = 1;
    |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8 = #3#8   + #1x;
            debe1 = debe1 + #1x;
        |SI x ] perdesde;|Y x [ perhasta;
                #3#10 = #3#10 + #1x;
                debp1 = debp1 + #1x;
        |FINSI;
    |SIGUE;
        sw_d1 = 1;
|FINSI;

|SI #3#0 = "D"; |Y #3#1 = 2;
    |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8 = #3#8   + #1x;
            debe2 = debe2 + #1x;
        |SI x ] perdesde;|Y x [ perhasta;
                #3#10 = #3#10 + #1x;
                debp2 = debp2 + #1x;
        |FINSI;
    |SIGUE;
        sw_d2 = 1;
|FINSI;

|SI #3#0 = "D"; |Y #3#1 = 3;
    |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8 = #3#8   + #1x;
            debe3 = debe3 + #1x;
        |SI x ] perdesde;|Y x [ perhasta;
                #3#10 = #3#10 + #1x;
                debp3 = debp3 + #1x;
        |FINSI;
    |SIGUE;
        sw_d3 = 1;
|FINSI;

|SI #3#0 = "D"; |Y #3#1 = 4;
    |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8 = #3#8   + #1x;
            debe4 = debe4 + #1x;
        |SI x ] perdesde;|Y x [ perhasta;
                #3#10 = #3#10 + #1x;
                debp4 = debp4 + #1x;
        |FINSI;
    |SIGUE;
        sw_d4 = 0;
|FINSI;

|SI #3#0 = "H"; |Y #3#1 = 1;
    |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8   = #3#8   + (#1x * -1);
            haber1 = haber1 + (#1x * -1);
        |SI x ] perdesde;|Y x [ perhasta;
                #3#10 = #3#10 + (#1x * -1);
                habep1 = habep1 + (#1x * -1);
        |FINSI;
    |SIGUE;
        sw_h1 = 1;
|FINSI;

|SI #3#0 = "H"; |Y #3#1 = 2;
    |PARA x = eldesde; |SI x [ elhasta; |HACIENDO x = x + 3;
            #3#8   = #3#8   + (#1x * -1);
