|VARIABLES;
    &entrada = 0;
    cuenta = 0;
    digito = 0;
    cta = "";
    long1 = "";
    long = 0;
    cont = 0;
    sw = 0;
    num = 0;
    men2  = "     Longitud de Cuenta Fuera de Nivel !";
