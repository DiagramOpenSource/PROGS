|INCLUYE acceso_i;
|INCLUYE puntos_i;

|PROCESO cuenta; |TIPO 6;
  entrada = 1;
  salidas = FSalida;
  cuenta = Campo;
  digito = Campo + 1;
  p_alfa1 = #0cuenta;
  |HAZ punto;
  |SI p_error ! 0;  |ERROR;  |ACABA;  |FINSI;
  cta  = #0cuenta;
  |QBF cta;
  long1 = cta % A0;
  long = long1;
  cont = 0;
  sw = 0;
  |PARA num = 14; |SI num [ 19; |HACIENDO num = num + 1;
        cont = cont + 1;
        |SI long = #30num;
            sw = 1;
            |SAL;
        |FINSI;
  |SIGUE;
  |SI sw = 0;
      |MENAV men2;
      |ERROR;
      |ACABA;
  |FINSI;
  #0digito = cont;
|FPRC;

|PROCESO c_departa; |TIPO 7;
|SI #30#24 = "N";
    |CAMPO_MODIFICA #0#6, 1, "N";
|SINO;
    |CAMPO_MODIFICA #0#6, 1, "S";
|FINSI;
|FPRC;

