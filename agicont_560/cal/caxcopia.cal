    caxcopia #0;
    caxcontr #1;
|FIN;

|VARIABLES;
    alfa1 = "";
    alfa2 = "";
    alfa3 = "";
    alfa4 = "";
    nume1 = 0;
    nume2 = 0;
    nume3 = 0;
    nume4 = 0;
    para1 = 0;
    para2 = 0;
    para3 = 0;
    para4 = 0;
    fech1 = @;
    fech2 = @;
    fech3 = @;
    fech4 = @;
    FEstado = 0;
    seleccio = 0;

    direc = "";
    fiche = "";
    unida = "";
    dire = "";
    sistem = "";
|FIN;

|PROCESO mayus;
  #0Campo = #0Campo > " ";
  |PINTA #0Campo;
|FPRC;

____________________________________/ CALCULOS DE PANTALLA
|PROCESO copiando; |TIPO 3;
|SI #0#0 = "SI";
    |ABRE #1;
    |LEE 001#1p;
    |SI FSalida = 0;
        |HAZ ejecuta;
    |SINO;
        |MENAV "    Debera definir el Control de Exportaciones/Importaciones ...";
    |FINSI;
    |CIERRA #1;
|FINSI;
|FPRC;

|PROCESO ejecuta;
|ATRI P;
|PINTA 2305, "Ejecutando proceso de copia ...";
|ATRI 0;

    dire = "";
|DIRECTORIO dire;
|DFICE direc;            || directorio
|QUE_SISTEMA sistem;     || s.o.
    unida = #1#8;        || unidad

    fiche = "exmaasil.asc";   |HAZ ejecuta2;
    fiche = "excuenta.asc";   |HAZ ejecuta2;
    fiche = "exivarep.asc";   |HAZ ejecuta2;
    fiche = "exivasop.asc";   |HAZ ejecuta2;
    fiche = "exmancob.asc";   |HAZ ejecuta2;
    fiche = "exmanpag.asc";   |HAZ ejecuta2;

|BLIN 23;
|FPRC;

|PROCESO ejecuta2;
|SI #1#1 = "E";     |HAZ exporta;  |FINSI;
|SI #1#1 = "I";     |HAZ importa;  |FINSI;
|FPRC;

|PROCESO exporta;
|SI sistem ! "ESDOS";
        fiche = dire + "bin/agcopiaa " + fiche + " " + unida + ": " + direc;
|SINO;
        fiche = dire + "bin/agicp " + direc + fiche + " " + unida + ":" + fiche;
|FINSI;
|SYSTEM fiche;
|FPRC;

|PROCESO importa;
    fiche = dire + "bin/agcopiaa " + fiche + " " + direc + fiche + " " + unida + ":";
|SYSTEM fiche;
|FPRC;
