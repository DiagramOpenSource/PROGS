|FICHEROS;
    cacambiy #0;
|FIN;

|VARIABLES;
    alfa1 = "";
    alfa2 = "";
    alfa3 = "";
    alfa4 = "";
    nume1 = 0;
    nume2 = 0;
    nume3 = 0;
    nume4 = 0;
    &dig3 = 0;
    &dig4 = 0;
    &dig5 = 0;
    &dig6 = 0;
    &dig7 = 0;
    &dig8 = 0;
    &dig9 = 0;
    salidas = 0;
|FIN;

|INCLUYE punto2_i;

|PROCESO nivel; |TIPO 0;
    salidas = FSalida;
    p_alfa1 = #0Campo;
|HAZ punto2;
|SI p_error ! 0;  |ERROR;  |ACABA;  |FINSI;
|FPRC;

|PROCESO mayus; |TIPO 0;
    #0Campo = #0Campo > #0Campo;
|PINTA #0Campo;
|FPRC;

|PROCESO no_copia; |TIPO 0;
    alfa1 = #0#2;
|CAMPO_MODIFICA #0#3, 1, alfa1;
|CAMPO_MODIFICA #0#4, 1, alfa1;
|CAMPO_MODIFICA #0#5, 1, alfa1;
|FPRC;

|PROCESO no_rellena; |TIPO 0;
    alfa1 = #0#6;
|CAMPO_MODIFICA #0#7, 1, alfa1;
|CAMPO_MODIFICA #0#8, 1, alfa1;
|FPRC;

|PROCESO def_5; |TIPO 7;
|SI #0#2 = "S";
    |SI dig3 = 1;  nume1 = dig4;  |FINSI;
    |SI dig3 = 2;  nume1 = dig5;  |FINSI;
    |SI dig3 = 3;  nume1 = dig6;  |FINSI;
    |SI dig3 = 4;  nume1 = dig7;  |FINSI;
    |SI dig3 = 5;  nume1 = dig8;  |FINSI;
    |SI dig3 = 6;  nume1 = dig9;  |FINSI;
        #0#5 = nume1 - #0#4 + 1;
    |PINTA #0#5;
|FINSI;
|FPRC;

|PROCESO top_5; |TIPO 0;
|SI #0#2 = "S";
    |SI dig3 = 1;  nume1 = dig4;  |FINSI;
    |SI dig3 = 2;  nume1 = dig5;  |FINSI;
    |SI dig3 = 3;  nume1 = dig6;  |FINSI;
    |SI dig3 = 4;  nume1 = dig7;  |FINSI;
    |SI dig3 = 5;  nume1 = dig8;  |FINSI;
    |SI dig3 = 6;  nume1 = dig9;  |FINSI;
        nume2 = nume1 - #0#4 + 1;
    |SI #0#5 > nume2;
        |MENAV "    Error de Entrada ...";
        |ERROR;
    |FINSI;
|FINSI;
|FPRC;

|PROCESO top_8; |TIPO 0;
|SI #0#6 = "S";
    |SI dig3 = 1;  nume1 = dig4;  |FINSI;
    |SI dig3 = 2;  nume1 = dig5;  |FINSI;
    |SI dig3 = 3;  nume1 = dig6;  |FINSI;
    |SI dig3 = 4;  nume1 = dig7;  |FINSI;
    |SI dig3 = 5;  nume1 = dig8;  |FINSI;
    |SI dig3 = 6;  nume1 = dig9;  |FINSI;
        nume2 = nume1 - #0#7 + 1;
    |SI #0#8 > nume2;
        |MENAV "    Error de Entrada ...";
        |ERROR;
    |FINSI;
|FINSI;
|FPRC;
