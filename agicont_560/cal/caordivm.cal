|PROCESO ordenar;
 |NOPARA;
  |HAZ nom_usu;
  alfa1 = "S" + nom_tmp;
  |NOME_DAT #11 alfa1;
  |ABRE #11;
  |CIERRA #11;
  |DELINDEX #11;

  |SI tipo = "S";
    |HAZ nom_usu;
    alfa1 = "P" + nom_tmp;
    |NOME_DAT #13 alfa1;
    |ABRE #13;
    |CIERRA #13;
    |DELINDEX #13;
    |ABRE #13;
  |FINSI;
  |SI tipo = "R";
    |HAZ nom_usu;
    alfa1 = "C" + nom_tmp;
    |NOME_DAT #12 alfa1;
    |ABRE #12;
    |CIERRA #12;
    |DELINDEX #12;
    |ABRE #12;
  |FINSI;

  |ABRE #11;
  |CUADRO 2041 2272;
  |ATRI P;  |PINTA 2142, "    <PREPARANDO TEMPORALES>   ";  |ATRI 0;

  sw     = 0;
  numero = 0;
  feini  = #0#2;
  fefin  = fec2;
  |SI tipo = "S"; |BUCLE gra_tempo_S; |CIERRA #13; |FINSI;
  |SI tipo = "R"; |BUCLE gra_tempo_R; |CIERRA #12; |FINSI;
  |CIERRA #11;
  |PINTA 2142, "                              ";

  |SI sw = 0;
      |MENAV "    ATENCION!!!.  No existen Facturas para Ordenar. ";
  |SINO;
       fech1  = "01.01.1900";
       fech2  = "31.12.2090";
       sw     = 0;
       numero = 0;
       feini  = #0#2;
       fefin  = fec2;
       ultfec = "";
       ordena = #0#6;
       |SI tipo = "S"; |ABRE #16; |ABRE #3; |FINSI;
       |SI tipo = "R"; |ABRE #15; |ABRE #2; |FINSI;
       |BUCLE lee_tempo;
       |SI tipo = "S"; |CIERRA #16; |CIERRA #3; |FINSI;
       |SI tipo = "R"; |CIERRA #15; |CIERRA #2; |FINSI;
  |FINSI;

  |DELINDEX #11;
  |SI tipo = "S"; |DELINDEX #13; |FINSI;
  |SI tipo = "R"; |DELINDEX #12; |FINSI;
 |SIPARA;
|FPRC;
