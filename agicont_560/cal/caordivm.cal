      #0#3 = ultfec;
      |SI #0#3 < fec2; #0#2 = ultfec + 1; |FINSI;
      #0#4 = numero;
      #0#6 = #0#4 + #0#5;
      #0#7 = "NO";
      |SI estado = 0; |GRABA 011#0a; |FINSI;
      |SI estado ! 0; |GRABA 011#0n; |FINSI;
  |FINSI;

  |LIBERA #0;
  |CIERRA #0;
