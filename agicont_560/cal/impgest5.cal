                    |VETE pp;      ||le busco otro hasta que no lo encuentre
               |FINSI;

               #2#0 = dia;
               #2#1 = fech;
               #2#2 = docu;
               #2#4 = 1;
               |LEE 101#2];
               |PARA ; |SI FSalida = 0;
                       |Y #2#0 = dia;
