               |SI #4#8 = "D";     ||Es cuenta de debe
                    #4#4 = #2#5;   ||Cuenta
                    #4#5 = #2#7;   ||Digito
                    #4#16 = #2#5;  ||cuenta DEBE
                    #4#17 = #2#7;  ||Digito DEBE
                    #4#10 = "";
                    #4#11 = 0;
                    asiedebe = asiedebe + #2#12;
                    cuenta = #4#16;
                    nivel = #4#17;
                    |SI d_conh=0; c_conh=#4#4; d_conh=#4#5; |FINSI;
               |SINO;              ||Es cuenta de haber
