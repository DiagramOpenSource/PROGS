                |SIGUE;
                #2#0 = dia;
                #2#1 = fech;
                #2#2 = docu;
                #2#4 = 0;
                |LEE 101#2=;
                |SI FSalida = 0;
                    #2#47 = #20#1;
                    |GRABA 001#2a;
                    |LIBERA #2;
                |FINSI;
                #2#4 = 1;
                |LEE 001#2=;
                |VETE asie1;  ||Cuando ya tiene un nuevo asiento, lo adjudico
          |FINSI;
