          |DDEFECTO #3;
          #3#0 = #2#0;        ||diario
          #3#1 = #2#1;        ||fecha
          #3#2 = #2#47;        ||documento
          |LEE 001#3=;
          |SI FSalida!0;
               #4#0 = #2#0;   ||diario
               #4#1 = #2#1;   ||fecha
               #4#2 = #2#47;  ||documento
               #4#3 = #2#4;   ||linea
