                   opera = "A";
               |HAZ altatra;       || fichero de movimientos
               asiento = #2#47;
               fecha = #2#1;
               fecha1 = #2#1;
               diario = #2#0;
               |HAZ cuentas;       ||En el proceso cuentas, averiguo si la
