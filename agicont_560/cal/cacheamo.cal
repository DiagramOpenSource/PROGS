|FICHEROS;
   cacheamo #0;
   cacheaml #1;
   camocabe #2;
   cacuenta #3;
|FIN;

|INCLUYE acceso_i;
|INCLUYE puntos_i;

|VARIABLES;
   &entrada = 1;
   Linea = 1;

   Comodin = "";
   Long = 0;
   LongAlfa = "";
   Nivel = 0;
   sw = 0;

|FIN;

|PROCESO Amortiz;
   |DDEFECTO #cacheaml;
   #cacheaml#0 = #cacheamo#0;
   #cacheaml#1 = Linea;
   #cacheaml#2 = #camocabe#15;
   #cacheaml#3 = #camocabe#0;
   #cacheaml#4 = #camocabe#2;
   #cacheaml#5 = #camocabe#6;
   #cacheaml#6 = #camocabe#16;
   #cacheaml#7 = #camocabe#17;
   #cacheaml#8 = #camocabe#7;
   |GRABA 020#cacheaml.n;
   Linea = Linea + 1;
   #cacheamo#2 = #cacheamo#2 + #camocabe#7;
|FPRC;

|DEFBUCLE Amortiz;
#camocabe#4;
;
#cacheamo#0,0;
#cacheamo#0,9;
;
NULL,Amortiz;
|FIN;

|RUTINA inf;
   #1#0 = #0#0;
   #1#1 = 1;
|FRUT;

|RUTINA sup;
   #1#0 = #0#0;
   #1#1 = 999;
|FRUT;

|PROCESO BusquedaCuenta;|TIPO 6;
   salidas = FSalida;
   |HAZ punto;
   |SI p_error ! 0; |ERROR; |ACABA; |FINSI;
|FPRC;


|PROCESO PonCuenta;
   #0#0 = #cacuenta#0;
   |PINTA #0#0;
   #0#1 = #cacuenta#53;
   |PINTA #0#1;
|FPRC;

|PROGRAMA;
   |HAZ inicio;

   |CLS;
   |CABEZA "Chequeo de cuentas de amortizacion";
   |PINPA #0#0;
   |PINDA #0#0;

   |ENDATOS #1#0;
   |SI FSalida ! 0; |ACABA; |FINSI;

   Linea = 1;
   |DELINDEX #cacheaml;
   |ABRE #cacheaml;
   |BUCLE Amortiz;
   |CIERRA #cacheaml;

   |PINTA #0#2;

   |ENTLINEAL #1#1,1,4,20,1,inf,sup;
|FPRO;
