  |SI #9#35 [ 10; x = #9#35 + 6;  |FINSI;
  |SI #9#35 > 10; x = #9#35 + 14; |FINSI;
  #0#14 = #9#35;
  #0#15 = #9x;
  |PINTA #0#14;
  |PINTA #0#15;
  swpasa = 1;
|FPRC;

