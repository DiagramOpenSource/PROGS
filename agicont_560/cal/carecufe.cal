|FICHEROS;
  carecufe #0;
|FIN;

|VARIABLES;
  &entrada = 1;
|FIN;

|INCLUYE acceso_i;
|INCLUYE recupe_i;

|PROCESO fecha;
  |SI #0Campo < fec1; |O #0Campo > fec2;
      |MENAV "    ERROR ESTA FECHA NO CORRESPONDE AL EJERCICIO";
      |ERROR;
      |ACABA;
  |FINSI;
  |SI Campo = 1;
      |SI #0#0 > #0#1; |ERROR; |FINSI;
  |FINSI;
|FPRC;

|PROCESO concepto;
  |SI #0#3 > #0#4; |ERROR; |FINSI;
|FPRC;


|PROCESO mayus;
  #0Campo = #0Campo > " ";
  |PINTA #0Campo;
|FPRC;

|PROCESO recupera; |TIPO 3;
  |HAZ inicio;
  conforme = #0#2;
  fini = #0#0;
  ffin = #0#1;
  cini = #0#3;
  cfin = #0#4;
  |HAZ recuperacion;
|FPRC;
