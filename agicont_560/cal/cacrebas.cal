|FICHEROS;
    cacrebas #0;
|FIN;

|VARIABLES;
    alfa1 = "";
    alfa2 = "";
    alfa3 = "";
    alfa4 = "";
    nume1 = 0;
    nume2 = 0;
    nume3 = 0;
    nume4 = 0;
    fech1 = @;
    fech2 = @;
    fech3 = @;
    fech4 = @;
    FEstado = 0;
    ficher = "";
    basico = "";
    dir_fich = "";
    dir_empr = "";
    temporal1 = "";
    temporal2 = "";
|FIN;

|PROCESO mayor;
  #0Campo = #0Campo > " ";
  |PINTA #0Campo;
|FPRC;

|PROCESO creacion; |TIPO 3;
  |DBASE dir_fich;
  dir_fich = dir_fich + "fich/";
  |DFICE dir_empr;
  |SI #0#0 = "S";
      ficher = dir_empr + "cacuenta";       || cuentas
      basico = dir_fich + "plancont";
      |HAZ copia;
  |FINSI;
  |SI #0#1 = "S";
      ficher = dir_empr + "casieaca";
      basico = dir_fich + "predecab";       || predef. cabs.
      |HAZ copia;
      ficher = dir_empr + "casieali";       || predef. lins.
      basico = dir_fich + "predelin";
      |HAZ copia;
      ficher = dir_empr + "casiegru";       || grupos
      basico = dir_fich + "predegru";
      |HAZ copia;
      ficher = dir_empr + "casiesub";       || subgrupos
      basico = dir_fich + "predesub";
      |HAZ copia;
  |FINSI;
  |SI #0#2 = "S";
      ficher = dir_empr + "caconcep";
      basico = dir_fich + "preconce";
      |HAZ copia;
  |FINSI;
|FPRC;

|PROCESO  copia;
  alfa2 = " Copiando ... " + ficher;
  |ATRI R;
  |PINTA 2305, alfa2;
  |ATRI 0;
  temporal1 = ficher + ".ixx";
  temporal2 = basico + ".ixx";
  |COPIA_FICHERO temporal1,temporal2;
  |SI FSalida ! 0;  |ACABA;  |FINSI;
  temporal1 = ficher + ".dat";
  temporal2 = basico + ".dat";
  |COPIA_FICHERO temporal1,temporal2;
  |SI FSalida ! 0;
      temporal2 = basico + ".ixx";
      |FBORRA temporal2;
      |ACABA;
  |FINSI;
  temporal1 = ficher + ".ddx";
  temporal2 = basico + ".ddx";
  |COPIA_FICHERO temporal1,temporal2;
|FPRC;
