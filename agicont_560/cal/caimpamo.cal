|PROCESO pasarla; || Crea las Cabeceras

 #2#0 = #4#0;
 #2#1 = #4#1;
 |LEE 101#2=;
 nEstado = FSalida;
 |SI nEstado = 0; |Y #0#9 = "S";
     |CIERRA #6;
     |BUCLE BorraLineas;
     |ABRE #6;
     |BORRA 020#2a;
     |LIBERA #2;
     #2#0 = #4#0;
     #2#1 = #4#1;
     |LEE 101#2=;
     nEstado = FSalida;
 |FINSI;

 |SI nEstado ! 0;
     |DDEFECTO #2;
     |PARA para1 = 0; |SI para1 [ 23; |HACIENDO para1 = para1 + 1;
           #2para1 = #4para1;
     |SIGUE;
     grupoact = #2#15;
     |HAZ vergrupo;
     #2#15 = grupoact;
     |GRABA 020#2b;
 |FINSI;

 |BUCLE laslineas;

 |LIBERA #2;
 |LIBERA #4;
|FPRC;

|DEFBUCLE pasar_a;
 #4#1;
 ;
 #0#0, 00;
 #0#1, 99;
 be;
 NULL, pasarla;
|FIN;

