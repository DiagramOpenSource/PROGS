 |DDEFECTO #6;
 #6#0 = #3#0;
 #6#1 = #3#1;
 #6#2 = #3#2;
 #6#3 = #3#3;
 #6#4 = #3#4;
 #6#5 = #3#5;
 #6#6 = #3#6;
 #6#7 = #3#7;
 #6#8 = #3#8;
 #6#9 = #3#9;
 |SI aMonedaC ! aMonedaA;   ||Cambio de Moneda
       impMoneda = #6#5; |HAZ ElCambioMoneda; #6#5 = impMoneda;
       impMoneda = #6#9; |HAZ ElCambioMoneda; #6#9 = impMoneda;
       #6#10 = #6#9 - #6#5;
