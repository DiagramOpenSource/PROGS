  dia  = #0#1 % 102;
  anyo = #0#1 % 704; any = anyo;
  mes  = #0#1 % 402;
  mes1 = mes;
  |SI mes1 = 1;  mes = "ENERO";      |FINSI;
  |SI mes1 = 2;  mes = "FEBRERO";    |FINSI;
