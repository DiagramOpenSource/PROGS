|PROCESO impre;|TIPO 3;
  |HAZ fecha;
  |ABRE #7; |ABRE #8;
  #7#0 = #30#2;
  #7#1 = #0#0;
  |LEE 101#7=;
  |SI FSalida ! 0;
      |MENAV "0000SELECCION VACIA";
      |CIERRA #7; |CIERRA #8;
      |ACABA;
  |FINSI;
  #8#0 = #30#2;
  #8#1 = #0#0;
  |LEE 101#8=;
  |SI FSalida ! 0;
      |MENAV "0000SELECCION VACIA";
      |CIERRA #7; |CIERRA #8;
      |ACABA;
  |FINSI;

  |HAZ MiraMoneda;

  |ABRE #2;
  #2#0 = #30#2;
  #2#1 = #30#26;
  |LEE 001#2=;
  |SI FSalida ! 0;
      |MENAV "0000No se encuentra el fichero de Portadas Modelos";
      |CIERRA #2;
      |CIERRA #8;
      |CIERRA #7;
      |ACABA;
  |FINSI;

  |SI #0#2 = "L";
      |HAZ Laser;
  |SINO;
      |IMPRE 0;
      |SI FSalida = 0;
          |SI #0#2 = "O"; |HAZ impre95;   |FINSI;
          |SI #0#2 = "B"; |HAZ impblanco; |FINSI;
      |FINSI;
  |FINSI;

  |CIERRA #8;
  |CIERRA #7;
  |CIERRA #2;
|FPRC;

