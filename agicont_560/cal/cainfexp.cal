|FPRC;

|PROCESO porimpre;
    agrupacion = 0;
    epigrafe   = 0;
|SI sw = 0;
        wagrupacion = #1#2;
        wepigrafe   = #1#4;
        agrupacion = 1;
    |SI #1#4 ! 0; epigrafe   = 1; |FINSI;
    |PRINT;
        sw = 1;
|FINSI;

|SI wagrupacion ! #1#2;
        linea      = 1;
        agrupacion = 1;
        epigrafe   = 1;
    |PRINT;
        wagrupacion = #1#2;
        wepigrafe   = #1#4;
        linea       = 0;
|SINO;
    |SI wepigrafe ! #1#4;
            linea    = 1;
            epigrafe = 1;
        |PRINT;
            wepigrafe = #1#4;
            linea     = 0;
        |SI #1#6 ! 0;
                epigrafe = 0;
        |FINSI;
    |FINSI;
|FINSI;

|SI epigrafe = 0; |Y agrupacion = 0;
    |PRINT;
|FINSI;
|FPRC;

|PROCESO porpanta;
    agrupacion = 0;
    epigrafe   = 0;
|SI sw = 0;
        wagrupacion = #1#2;
        wepigrafe   = #1#4;
        agrupacion = 1;
    |SI #1#4 ! 0;   epigrafe = 1;   |FINSI;
    |HAZ pintapan;
        sw = 1;
|FINSI;

|SI wagrupacion ! #1#2;
        linea      = 1;
        agrupacion = 1;
        epigrafe   = 1;
    |HAZ pintapan;
        wagrupacion = #1#2;
        wepigrafe   = #1#4;
        linea       = 0;
|SINO;
    |SI wepigrafe ! #1#4;
            linea      = 1;
            epigrafe   = 1;
        |HAZ pintapan;
            wepigrafe = #1#4;
            linea       = 0;
    |FINSI;
|FINSI;

|SI epigrafe = 0; |Y agrupacion = 0;
    |HAZ pintapan;
|FINSI;
|FPRC;

|PROCESO pintapan;
    agru = #1#2;
    agru = ("00" + agru) % -102;
    epig = #1#4;
    epig = ("00" + epig) % -102;
    part = #1#6;
    part = ("00" + part) % -102;

|SI linea = 1;
        #2#3 = "";
        #2#4 = "";
    |HAZ enpanta;
|FINSI;

|SI agrupacion = 1;
        #2#3 = #1#0 + #1#1 + agru + "0000";
        #2#4 = #1#3;
    |HAZ enpanta;
|FINSI;

|SI epigrafe = 1;
        #2#3 = #1#0 + #1#1 + agru + epig + "00";
        #2#4 = "     " + #1#5;
    |HAZ enpanta;
    |SI #1#6 ! 0;
            #2#3 = #1#0 + #1#1 + agru + epig + part;
            #2#4 = "          " + #1#7;
        |HAZ enpanta;
    |FINSI;
|FINSI;

|SI agrupacion = 0; |Y epigrafe = 0;
        #2#3 = #1#0 + #1#1 + agru + epig + part;
        #2#4 = "          " + #1#7;
    |HAZ enpanta;
|FINSI;
|FPRC;

|PROCESO enpanta;
|PINTA #2#0;
|PINTA #2#1;
|PINTA #2#2;

    pepe = #2#4;
|QBF pepe;
|SI pepe = "";  #2#3 = "";  |FINSI;

    Pos = lin;
|PINDA #0#2;
    Pos = -1;
    lin = lin + 100;
|SI lin > 1200;  |HAZ cambiapa;  |FINSI;
|FPRC;

|PROCESO cambiapa;
    error = 0;
    lin = 100;
|PAUSA;
|SI FSalida = 1; |O FSalida = 7;
        error = 1;
    |ERROR10;
|SINO;
    |PINPA #0#2;
|FINSI;
|FPRC;

