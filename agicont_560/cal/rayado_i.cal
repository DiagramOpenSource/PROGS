|FICHEROS;
|FIN;

|VARIABLES;
  &lon_imp = 0;               || caracteres impreso
  &esc_imp = 0;               || lineas escritas
  &pie_imp = 0;               || lineas del pie
  lin_imp = 0;                || lineas del impresora
  fal_imp = 0;                || lineas que quedan
  barra_imp = 0;              || numero de barras a escribir
  subra_imp = 0;              || cuenta_caracteres
  &print_imp = "";            || string a imprimir
  xx_imp = 0;                 || variable del para
|FIN;

|PROCESO rayado;
  |CIMPR 11;
  lin_imp = FSalida;
  fal_imp = lin_imp - pie_imp - esc_imp;
  |SI fal_imp [ 3;  |ACABA;  |FINSI;
  barra_imp = fal_imp - 4;
  subra_imp = ((lon_imp - barra_imp) / 2) ! 0;
  print_imp = "_" * subra_imp;
  |PRINT;
  |PARA xx_imp = 1;  |SI xx_imp [ barra_imp;  |HACIENDO xx_imp = xx_imp + 1;
        print_imp = " " * subra_imp;
        print_imp = print_imp + "\";
        subra_imp = subra_imp + 1;
        |PRINT;
  |SIGUE;
  print_imp = " " * subra_imp;
  subra_imp = lon_imp - subra_imp;
  print_imp = "_" * subra_imp;
  |PRINT;
|FPRC;
