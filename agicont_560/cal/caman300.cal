|PROCESO mayor; |TIPO 0;
   alfa1 = #2Campo > " ";
   #2Campo = alfa1;
   |PINTA #2Campo;
|FPRC;

|PROCESO i_archiv; |TIPO 7;
  |SI #2#4 ! "S";  |ACABA;  |FINSI;
  |AVISO;
  |CONFI "2400NEste Modelo ya esta Impreso, Desea Modificarlo : ";
  |SI FSalida = 0;
      |AVISO;
      |CONFI "2400NEsta Seguro? : ";
      |SI FSalida ! 0;
          |PTEC 807;
      |SINO;
          #2#4 = "N"; |PINTA #2#4;
      |FINSI;
  |SINO;
      |PTEC 807;
  |FINSI;
|FPRC;

|PROCESO empresa; |TIPO 0;
  cont = FEntrada / 100;
  cont = cont ! 0;
  |SI cont ! 1;  |ACABA;  |FINSI;
  |ABRE #1;
  |LEE 000#1p;
  |SI FSalida = 0;
      #2#6  = #1#1; |PINTA #2#6;
      #2#9  = #1#2; |PINTA #2#9;
      #2#12 = #1#3; |PINTA #2#12;
      #2#15 = #1#4; |PINTA #2#15;
      #2#18 = #1#5; |PINTA #2#18;
      #2#47 = #1#6; |PINTA #2#47;
  |SINO;
      |MENAV "    No esta definido el control del IVA ...";
      |ERROR;
  |FINSI;
  |CIERRA #1;
|FPRC;

|PROCESO lee_port; |TIPO 7
      |DDEFECTO #8;
      |ABRE #8;
      #8#0 = #30#2;
      #8#1 = #30#26;
      |LEE 000#8=;
      |SI FSalida ! 0;
          |MENAV "    No encontrado Fichero Portadas Modelos";
      |SINO;
          #2#45 = #8#93;
          |PINTA #2#45;
      |FINSI;
      |CIERRA #8;
|FPRC;

|PROCESO periodo; |TIPO 0;
|HAZ empresa;
|SI #2#45 = "T"; |Y #2#1 > 4;
    |MENAV "     El 300 esta preparado como Trimestral";
    |ERROR;
|FINSI;
|SI #2#45 = "A"; |Y #2#1 > 1;
    |MENAV "     El 300 esta preparado como Anual";
    |ERROR;
|FINSI;
|FPRC;

|PROCESO anyopas; |TIPO 7;
  |ACABA;                           || no utiliza este campo en Contabilidad
  cont = (FEntrada / 100) ! 0;
  |SI cont ! 1;  |ACABA;  |FINSI;
  |SI #2#1 = 1;
      trime   = #2#1;
      anyo    = #2#2;
      activi  = #2#45;
      |DDEFECTO #2;
      #2#1    = trime - 1;
      #2#2    = anyo;
      #2#45   = activi;
      compen  = 0;
      |LEE 040#2=;
      |SI FSalida = 0;
          |SI #2#55 < 0; compen = -#2#55; |FINSI;
      |FINSI;
      |DDEFECTO #2;
      #2#1    = trime;
      #2#2    = anyo;
      #2#45   = activi;
      |LEE 040#2=;
      |SI FSalida ! 0;
          |DDEFECTO #2;
          #2#1 = trime;
          #2#2 = anyo;
          #2#45 = activi;
      |FINSI;
      #2#29   = compen;
      |PINTA #2#2;
      |PINTA #2#1;
      |PINTA #2#45;
      |PINTA #2#2;
  |FINSI;
|FPRC;


|PROCESO sumacuot; |TIPO 0;
     #2#20 = #2#7 + #2#10 + #2#13 + #2#16 + #2#19 + #2#48 + #2#50;
     |PINTA #2#20;
     |HAZ especifi;
     |HAZ atribuir;
|FPRC;

|PROCESO cuota2; |TIPO 0;
     #2#25 = #2#21 + #2#22 + #2#23 + #2#24 + #2#43 + #2#44 + #2#51 + #2#58;
     |PINTA #2#25;
     |HAZ atribuir;
|FPRC;

|PROCESO atribuir; |TIPO 0;
    #2#26 = #2#20 - #2#25;
    |PINTA #2#26;
    #2#28 = #2#26 % #2#27;
    |PINTA #2#28;
    |HAZ total;
|FPRC;

|PROCESO total; |TIPO 0;
total = #2#28 - #2#29 - #2#30;
|SI total = 0;
    #2#33 = 0;
    #2#32 = 0;
    #2#31 = 0;
|FINSI;
|SI total > 0;
    #2#33 = total;
    #2#31 = 0;
    #2#32 = 0;
    |PINTA #2#31;
    |PINTA #2#32;
    |PINTA #2#33;
|SINO;
    total = total * (-1);
    #2#33 = 0;
    #2#32 = total;
    #2#31 = 0;
    |SI #8#71 = "D";
        |SI #2#45 = "T"; |Y #2#1 = 4;
            #2#32 = 0;
            #2#31 = total;
        |FINSI;
        |SI #2#45 = "M"; |Y #2#1 = 12;
            #2#32 = 0;
            #2#31 = total;
        |FINSI;
        |SI #2#45 = "A";
            #2#32 = 0;
            #2#31 = total;
        |FINSI;
    |FINSI;
    |PINTA #2#31;
    |PINTA #2#32;
    |PINTA #2#33;
|FINSI;
|HAZ especifi;
|FPRC;

|PROCESO especifi; |TIPO 0;
#2#35 = #2#54;
#2#52 = #2#49;
#2#42 = #2#5  + #2#8  + #2#11;
#2#42 = #2#42 + #2#34 + #2#35 + #2#36 + #2#37 + #2#38 + #2#39
#2#42 = #2#42 - #2#40 - #2#41;
|PINTA #2#35;
|PINTA #2#52;
|PINTA #2#42;
|FPRC;

|PROCESO totales; |TIPO 3;
