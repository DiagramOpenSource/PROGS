  raf_cue = #0#0;
  raf_dig = #0#1;
  raf_des = #0#2;
  raf_sw = 0;
  |SI digito2 = "57";  |Y  #30#23 = "S";
      |SUB_EJECUTA "camanba1.tab";
      |VETE salir;
  |FINSI;
  |SI digito2 = "43";  |O digito2 = "44";
      |SI #30#21 ! "N";
          tipoalta = "C";
          |SUB_EJECUTA "caclipr1.tab";
      |FINSI;
  |SINO;
      |SI digito2 = "40";  |O digito2 = "41";  |O digito2 = "52";
          |SI #30#22 = "S";
              tipoalta = "P";
              |SUB_EJECUTA "caclipr1.tab";
          |FINSI;
      |SINO;
           tipoalta = "X";
           |SUB_EJECUTA "caclipr1.tab";
      |FINSI;
  |FINSI;
  |ET salir;
  |SI raf_sw = 1;
      #0#2 = raf_des;
      |PINTA #0#2;
