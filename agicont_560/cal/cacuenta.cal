 #21#11 = #0#1;
 |LEE 101#21=;
 |SI FSalida = 0;
     |BORRA 020#21a;
 |SINO;
     #21#23 = "P";
     #21#10 = #0#0;
     #21#11 = #0#1;
     |LEE 101#21=;
     |SI FSalida = 0;
         |BORRA 020#21a;
     |FINSI;
 |FINSI;
 |LIBERA #21;
 |CIERRA #21;
|FPRC;

