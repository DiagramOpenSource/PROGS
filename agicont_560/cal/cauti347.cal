|FICHEROS;
  cauti347 #0;
  agipobla #1;
  agiprovi #2;
  caprovin #3;
|FIN;

|VARIABLES;
   &swmodi      = 0;
   &swentra     = 0;
   &elcampo0    = "";
   &elcampo1    = "";
   &elcampo2    = "";
   &elcampo3    = "";
   &elcampo4    = "";
   &elcampo5    = 0;
   &elcampo6    = 0;
   &elcampo7    = "";
   &elcampo8    = "";
   &aste = "";
   &agi1 = "";
|FIN;

|PROGRAMA;
  |HAZ tipo_8;
  |CAMPO_MODIFICA #0#1, 1, "S";
  |CAMPO_MODIFICA #0#2, 1, "S";
  |CAMPO_MODIFICA #0#3, 1, "S";
  |CAMPO_MODIFICA #0#4, 1, "S";
  |CAMPO_MODIFICA #0#5, 1, "S";
  |CAMPO_MODIFICA #0#6, 1, "S";
  |CAMPO_MODIFICA #0#8, 1, "S";
  |SI swmodi = 1;
      |CAMPO_MODIFICA #0#1, 1, "N";
      |CAMPO_MODIFICA #0#2, 1, "N";
      |CAMPO_MODIFICA #0#3, 1, "N";
      |CAMPO_MODIFICA #0#4, 1, "N";
      |CAMPO_MODIFICA #0#5, 1, "N";
      |CAMPO_MODIFICA #0#6, 1, "N";
      |CAMPO_MODIFICA #0#8, 1, "N";
  |FINSI;
  swentra = 0;
  |HAZ cargavar;
  |PINPA #0#0;
  |PINDA #0#0;
  |SI swmodi = 1; |PAUSA; |ACABA; |FINSI;

  |ENDATOS #1#0;
  |SI FSalida = 0;
      swentra = 1;
      |HAZ descarga;
  |FINSI;
|FPRO;

|PROCESO cargavar;
  #0#0  = elcampo0;
  #0#1  = elcampo1;
  #0#2  = elcampo2;
  #0#3  = elcampo3;
  #0#4  = elcampo4;
  #0#5  = elcampo5;
  #0#6  = elcampo6;
  #0#7  = elcampo7;
  #0#8  = elcampo8;
|FPRC;

|PROCESO descarga;
  elcampo0  = #0#0;
  elcampo1  = #0#1;
  elcampo2  = #0#2;
  elcampo3  = #0#3;
  elcampo4  = #0#4;
  elcampo5  = #0#5;
  elcampo6  = #0#6;
  elcampo7  = #0#7;
  elcampo8  = #0#8;
|FPRC;

|PROCESO provincia; |TIPO 0;
  |SI aste = "*";
      |ABRE #3;
      #3#0 = #0#5;
      |LEE 000#3=;
      |SI FSalida = 0;
          #0#7 = #3#1;
          |PINTA #0#7;
      |FINSI;
      |CIERRA #3;
  |SINO;
      |ABRE #2;
      #2#0 = #0#5;
      |LEE 000#2=;
      |SI FSalida = 0;
          #0#7 = #2#1;
          |PINTA #0#7;
      |FINSI;
      |CIERRA #2;
  |FINSI;
|FPRC;

|PROCESO poblacion; |TIPO 0;
  |SI aste ! "*";
      |ABRE #1;
      #1#0 = #0#5;
      #1#1 = #0#6;
      |LEE 000#1=;
      |SI FSalida = 0;
          #0#8 = #1#2;
          |PINTA #0#8;
      |FINSI;
      |CIERRA #1;
  |FINSI;
|FPRC;

|PROCESO tipo_8;
  |SI aste ! "*";
      |PATH_DAT #1 agi1;
      |PATH_DAT #2 agi1;
  |SINO;
      |CAMPO_MODIFICA #0#7, 1, "S";
      |CAMPO_MODIFICA #0#8, 1, "S";
  |FINSI;
|FPRC;
