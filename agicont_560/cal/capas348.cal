        #1#2 = 0;
        #1#3 = #3#51;
        #1#4 = tipo1;
        #1#5 = 0;
        #1#6 = 0;
        #1#7 = #3#50;
        |GRABA 020#1b;
   |FINSI;
   #1tipo2 = #1tipo2 + #3#26;
   |GRABA 020#1a;
   |LIBERA #1;
   |SI tipo1 = 1; totalvent = totalvent + #3#26; |FINSI;
