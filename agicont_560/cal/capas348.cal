|PROCESO sumacomp;
  tipo1 = 0; || (2) compras
  tipo2 = 0; || (2) normal, (5) arrendamientos
  sien = 0;
|| .................................... Seleccion de Compras
  |SI #0#4 = "S";
      |SI #2#6 ] #0#5;  |Y #2#6 [ #0#6;
          sien = 1;
          tipo1 = 2;
          tipo2 = 2;
      |FINSI;
  |SINO;
      sien = 0;
      |PARA desde = 7;  |SI desde [ 38;  |HACIENDO desde = desde + 1;
            |SI #0desde = 0;  |SAL;  |FINSI;
            |SI #2#6 = #0desde;
                sien = 1;
                tipo1 = 2;
                tipo2 = 2;
                |SAL;
            |FINSI;
      |SIGUE;
  |FINSI;
|| .................................... Seleccion de Arrendamientos
  |SI #0#118 = "S";
      |SI #0#119 ! 0; |O #0#120 ! 0; |Y #0#121 ! 0;
          swgr = 0; concepto = #2#6;
          |SI #0#119 ! 0; grupo = #0#119; |HAZ vergrupo; |FINSI;
          |SI #0#120 ! 0; |Y swgr = 0; grupo = #0#120; |HAZ vergrupo; |FINSI;
          |SI #0#121 ! 0; |Y swgr = 0; grupo = #0#121; |HAZ vergrupo; |FINSI;
          |SI swgr = 1;
              sien = 1;
              tipo1 = 2;
              tipo2 = 5;
          |FINSI;
      |SINO;
          |PARA desde = 122;  |SI desde [ 126;  |HACIENDO desde = desde + 1;
                |SI #0desde = 0;  |SAL;  |FINSI;
                |SI #2#6 = #0desde;
                    sien = 1;
                    tipo1 = 2;
                    tipo2 = 5;
                    |SAL;
                |FINSI;
          |SIGUE;
      |FINSI;
  |FINSI;
|| .................................../ Fin compras.

|SI sien = 0; || No es de Compras.
    concepto = #2#6;
    |HAZ verpagos;
|FINSI;

|SI sien = 1; || Es concepto elegido.
    #1#3 = #2#51;
    #1#4 = tipo1;
    |LIBERA #1;
    |LEE 101#1=;
    |SI FSalida ! 0;
        #1#0 = #2#4;
        #1#1 = #2#5;
        #1#2 = 0;
        #1#3 = #2#51;
        #1#4 = tipo1;
        #1#5 = 0;
        #1#6 = 0;
        #1#7 = #2#50;
        |GRABA 020#1b;
   |FINSI;
   #1tipo2 = #1tipo2 + #2#26;
   |GRABA 020#1a;
   |LIBERA #1;
   |SI tipo1 = 2; totalcomp = totalcomp + #2#26; |FINSI;
|FINSI;
|FPRC;

|DEFBUCLE compras;
#2#1;
;
00,000001, "  ";
99,999999, "zz";
e;
NULL,sumacomp;
|FIN;

|PROCESO sumavent;
