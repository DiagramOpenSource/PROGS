|FICHEROS;
     zconcili #100,MANTE;
     camaasil #1;
     caconccu #0;
|FIN;

|VARIABLES;
    &entrada = 1;
    &ext1     = "";
    &ext2     = "";
    &contador = 0;
    &fichero  = "";
    &cuenta   = "";
    &digito   = 0;
    &saldoant = 0;
    &nombre   = "";
    &asien1   = 0;
    &asien2   = 0;
    &docum    = 0;
    &fecha    = @;
    &SalActual = 0;
    &TipoCuadre = "";
    sw        = 0;
|FIN;

|PROCESO Tipo11; |TIPO 11;
     |SI FSalida < 9; |O FSalida > 13;
        |ACABA;
     |FINSI;

     |SI FSalida = 9;
         |LIBERA #100;
         |LEE 110#100=;
         |SI #100#8 = " ";
             #100#8 = "*";
             #0#7 = #0#7 + #100#6;
             #0#8 = #0#8 + #100#7;
         |SINO;
             #100#8 = " ";
             #0#7 = #0#7 - #100#6;
             #0#8 = #0#8 - #100#7;
         |FINSI;
         #0#3 = #0#7 - #0#8;

         |PINTA #0#7;
         |PINTA #0#8;
         |PINTA #0#3;
         |PINTA #100#8;

         |GRABA 020#100a;
         #0#3 = #0#3 + (#100#6 - #100#7);
         |PTEC 809;
         |ACABA;
     |FINSI;

     |SI FSalida = 10;
         ext1 = #0#0;
         ext2 = #0#1;
         |SUB_EJECUTA "caextrac.run";
         |ERROR;
         |ACABA;
     |FINSI;

     |SI FSalida = 11;
         ext1   = #100#1;
         ext2   = #100#1;
         asien1 = 0;
         asien2 = 999999;
         docum  = #100#3;
         fecha  = #100#2;
         |SUB_EJECUTA "caditrac.run";
         |ERROR;
         |ACABA;
     |FINSI;

     |SI FSalida = 12;
         ext1 = #0#0;
         ext2 = #0#1;
         |SUB_EJECUTA "caextrai.run";
         |ERROR;
         |ACABA;
     |FINSI;

     |SI FSalida = 13;
         ext1 = #0#0;
         ext2 = #0#1;
         |SUB_EJECUTA "caconci1.run";
         |ERROR;
         |ACABA;
     |FINSI;
|FPRC;

|RUTINA inferior;
    #100#0 = 1;
|FRUT;

|RUTINA superior;
    #100#0 = contador;
|FRUT;

|PROCESO Concilia;
     |NOME_DAT #100fichero;
     |ENTLINEAL #1#100, 1, 4, 21, 0, inferior, superior;
|FPRC;
