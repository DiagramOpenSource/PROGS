|FICHEROS;
     siiw0100,MANTE;
|FIN;

|VARIABLES;
|FIN;

|PROCESO T12w100;   |TIPO 12;
     |SI FSalida = 7;  |ACABA;  |FINSI;

     |SI FSalida ! 1;
         |POSICIONATE #siiw0100#0;
         |ERROR;
         |ACABA;
     |FINSI;
|FPRC;

|PROCESO T0C0w100;   |TIPO 0;
     |C_M #siiw0100#1, 1, "S";
     |C_M #siiw0100#2, 1, "S";
     |C_M #siiw0100#3, 1, "S";

     |SI #siiw0100#0 = "I";
         |C_M #siiw0100#1, 1, "N";  #siiw0100#1 = 0;
         |C_M #siiw0100#2, 1, "N";  #siiw0100#2 = 0;
         |C_M #siiw0100#3, 1, "N";  #siiw0100#3 = 0;
     |FINSI;

     |PINTA #siiw0100#1;
     |PINTA #siiw0100#2;
     |PINTA #siiw0100#3;
|FPRC;
