|FICHEROS;
     siim1000;

     siiw0010,MANTE;
|FIN;

|VARIABLES;
     n0Vent              = 0;
     nID                 = 0;

     aAlfa               = "";

     {-1}aVent           = "";
         aVent1          = 0;
         aVent2          = 0;
         aVent3          = 0;
         aVent4          = 0;
         aVent5          = "";

     &enID               = 0;
|FIN;

|PROCESO T12;  |TIPO 12;
     |SI FSalida = 7;  |ACABA;  |FINSI;

     |CONFI "0000SConforme con lo introducido";
     |SI FSalida ! 0;
         |ERROR;
     |FINSI;
|FPRC;

|PROCESO Alta;
     enID = 0;

     |VENTANA 0501, 1285, -1, 16, "Nueva descripci¢n de la operaci¢n";
     n0Vent = FSalida;

     |PARA nID = 1;  |SI nID [ 9999;  |HACIENDO nID = nID + 1;
           #siim1000#0 = nID;
           |LEE 000#siim1000.=;
           |SI FSalida ! 0;
               |SAL;
           |FINSI;
     |SIGUE;

     |DDEFECTO #siim1000;

     #siim1000#0 = nID;
     |GRABA 020#siim1000.b;

     #siiw0010#0 = #siim1000#1 % 180;
     #siiw0010#1 = #siim1000#1 % 8180;
     #siiw0010#2 = #siim1000#1 % 16180;

     |PINPA #0#siiw0010;
     |PINDA #0#siiw0010;
     |ENDATOS #1#siiw0010;
     |SI FSalida = 0;
         #siim1000#1 = #siiw0010#0 + #siiw0010#1 + #siiw0010#2;

         |GRABA 020#siim1000.a;

         enID = #siim1000#0;
     |SINO;
         |BORRA 020#siim1000.a;
     |FINSI;
     |LIBERA #siim1000;

     |FINVENTANA n0Vent;
|FPRC;

|PROCESO Baja;
     |CONSULTA_CLAVE #2#siim1000;
     |SI FSalida = 0;
         |CONFI "0000NSeguro que quiere eliminar el registro";
         |SI FSalida = 0;
             |LEE 101#siim1000.=;
             |SI FSalida = 0;
                 |BORRA 020#siim1000.a;
                 |LIBERA #siim1000;
             |FINSI;
         |FINSI;
     |FINSI;
|FPRC;

|PROGRAMA;
     |SI PARAMETRO = "MAS";
         |HAZ Alta;
     |FINSI;

     |SI PARAMETRO = "MENOS";
         |HAZ Baja;
     |FINSI;
|FPRO;

|PROCESO T80;  |TIPO 80;
     |ABRET #siim1000;
|FPRC;

|PROCESO T90;  |TIPO 90;
     |CIERRAT #siim1000;
|FPRC;
