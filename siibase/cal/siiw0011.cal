|FICHEROS;
     siiw0011;
|FIN;

|VARIABLES;
     nCmpGet             = 0;
     nID                 = 0;

     aAlfa               = "";
     aID                 = "";
     aMes                = "";

     fFecha              = @;


 {20}aPopup              = "";

 {-1}aGet                = 0;
     aGet1               = 0;
     aGet2               = 0;

|FIN;

|PROCESO T12w11;  |TIPO 12;
     |SI FSalida = 7;  |ACABA;  |FINSI;

     |REFERENCIA_DEF aGet1, #siiw0011;
     |PARA nCmpGet = 0;  |SI nCmpGet [ 11;  |HACIENDO nCmpGet = nCmpGet + 1;
           aGet2 = nCmpGet;
           IaGet = aGet1<-;
           |ESPECIFICA "GETDATACONTROL", aGet;
           #siiw0011#nCmpGet# = FSalida;
     |SIGUE;
|FPRC;

|PROCESO T0C7w11;  |TIPO 0;
     |SI #siiw0011#6 = "N";
         |ACABA;
     |FINSI;

                              aMes = "";
     |SI #siiw0011#7 = "01";  aMes = "ENERO";       |FINSI;
     |SI #siiw0011#7 = "02";  aMes = "FEBRERO";     |FINSI;
     |SI #siiw0011#7 = "03";  aMes = "MARZO";       |FINSI;
     |SI #siiw0011#7 = "04";  aMes = "ABRIL";       |FINSI;
     |SI #siiw0011#7 = "05";  aMes = "MAYO";        |FINSI;
     |SI #siiw0011#7 = "06";  aMes = "JUNIO";       |FINSI;
     |SI #siiw0011#7 = "07";  aMes = "JULIO";       |FINSI;
     |SI #siiw0011#7 = "08";  aMes = "AGOSTO";      |FINSI;
     |SI #siiw0011#7 = "09";  aMes = "SEPTIEMBRE";  |FINSI;
     |SI #siiw0011#7 = "10";  aMes = "OCTUBRE";     |FINSI;
     |SI #siiw0011#7 = "11";  aMes = "NOVIEMBRE";   |FINSI;
     |SI #siiw0011#7 = "12";  aMes = "DICIEMBRE";   |FINSI;
     |SI #siiw0011#7 = "1T";  aMes = "1 TRIMSTRE";  |FINSI;
     |SI #siiw0011#7 = "2T";  aMes = "2 TRIMSTRE";  |FINSI;
     |SI #siiw0011#7 = "3T";  aMes = "3 TRIMSTRE";  |FINSI;
     |SI #siiw0011#7 = "4T";  aMes = "4 TRIMSTRE";  |FINSI;

     |SI aMes = "";
         |MENAV "0000Periodo incorrecto. ";
         |ERROR;
         |ACABA;
     |FINSI;
|FPRC;

|PROCESO T66C7w11;  |TIPO 66;
     aAlfa = "|C007WID";
     aID   = #siiw0011#aAlfa#;
     nID   = aID;

     aPopup1  = nID;
     aPopup2  = 16;
     aPopup3  = "01-ENERO";
     aPopup4  = "02-FEBRERO";
     aPopup5  = "03-MARZO";
     aPopup6  = "04-ABRIL";
     aPopup7  = "05-MAYO";
     aPopup8  = "06-JUNIO";
     aPopup9  = "07-JULIO";
     aPopup10 = "08-AGOSTO";
     aPopup11 = "09-SEPTIEMBRE";
     aPopup12 = "10-OCTUBRE";
     aPopup13 = "11-NOVIEMBRE";
     aPopup14 = "12-DICIEMBRE";
     aPopup15 = "1T-1 TRIMESTRE";
     aPopup16 = "2T-2 TRIMESTRE";
     aPopup17 = "3T-3 TRIMESTRE";
     aPopup18 = "4T-4 TRIMESTRE";

     IaPopup  = aPopup1  <-;
     |ESPECIFICA "TRACKPOPUP", aPopup;
     |SI FSalida ! 0;
         IaPopup  = aPopup3  <-;
         IaPopup  = IaPopup + (FSalida - 1);

         aAlfa = aPopup % 102;  #siiw0011#7 = aAlfa;
     |FINSI;

     |PINTA #siiw0011#7;
|FPRC;

|PROCESO T7C7w11;  |TIPO 7;
     |C_M #siiw0011#7, 1, "S";
     |SI #siiw0011#6 = "N";
         |C_M #siiw0011#7, 1, "N";
         #siiw0011#7 = "";
     |SINO;
         |SI #siiw0011#7 = "  ";
             fFecha      = ~;
             #siiw0011#7 = fFecha % 402;
         |FINSI;
     |FINSI;

     |PINTA #siiw0011#7;
|FPRC;

|PROCESO T7C5w11;  |TIPO 7;
     |C_M #siiw0011#5, 1, "S";
     |SI #siiw0011#4 = "N";
         |C_M #siiw0011#5, 1, "N";  #siiw0011#5 = 0;
     |SINO;
         |SI #siiw0011#5 = 0;
             fFecha = ~;
             aAlfa  = fFecha % -104;
             #siiw0011#5 = aAlfa;
         |FINSI;
     |FINSI;

     |PINTA #siiw0011#5;
|FPRC;
