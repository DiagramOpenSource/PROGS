|FICHEROS;
     siim0000;
|FIN;

|VARIABLES;
     nOk                 = 0;
     nApl                = 0;

     aMas                = "";
     aBaseAplis          = "";

|FIN;

|PROCESO T7C4;   |TIPO 7;
     |DBASS aBaseAplis;  |QBF aBaseAplis;

     nApl = 0;
     aMas = aBaseAplis + "agicli/def/agifigen.mas";
     |XABRE "E", aMas, 0;
     |SI FSalida ] 0;
         nApl = nApl + 1;
     |FINSI;

     aMas = aBaseAplis + "basica/def/agifigen.mas";
     |XABRE "E", aMas, 0;
     |SI FSalida ] 0;
         nApl = nApl + 1;
     |FINSI;

     |C_M #siim0000#4, 1, "S";
     |SI nApl [ 1;
         |C_M #siim0000#4, 1, "N";  #siim0000#4 = "";
     |FINSI;

     |PINTA #siim0000#4;
|FPRC;

|PROCESO T0C4;   |TIPO 0;
     |SI #siim0000#4 = "        ";
         |ACABA;
     |FINSI;

     nOk = 0;
     |SI #siim0000#4 = "CONTAGEN";  nOk = 1;  |FINSI;
     |SI #siim0000#4 = "STARIII ";  nOk = 1;  |FINSI;

     |SI nOk = 0;
         |MENAV "0000Introduzca CONTAGEN o STARIII";
         |ERROR;
     |FINSI;
|FPRC;

|PROGRAMA;
      |LEE 101#siim0000.p;
      |SI FSalida ! 0;
          |DDEFECTO #siim0000;
          |GRABA 020#siim0000.b;
      |FINSI;

      |PINPA #0#siim0000;
      |PINDA #0#siim0000;
      |ENDATOS #1#siim0000;
      |SI FSalida = 0;
          |GRABA 020#siim0000.a;
      |FINSI;
      |LIBERA 020#siim0000;
|FPRO;

|PROCESO T80;  |TIPO 80;
     |ABRET #siim0000;

     FSalida = 2499;
|FPRC;

|PROCESO T90;  |TIPO 90;
     |CIERRAT #siim0000;
|FPRC;
