|FICHEROS;
     dschintm #0;
|FIN;

|VARIABLES;
     aDefApl = "";
     aDefAcc = "";
|FIN;

|PROCESO EnBlanco;
     |SI #0#2 = "        ";
          |CAMPO_MODIFICA #0#3, 1, "N";
          |CAMPO_MODIFICA #0#4, 1, "N";
     |SINO;
          |CAMPO_MODIFICA #0#3, 1, "S";
          |CAMPO_MODIFICA #0#4, 1, "S";
     |FINSI;
|FPRC;

|PROCESO DefEmp; |TIPO 7;
     |SI #0#2 ! "        "; |Y #0#3 ! "      ";
          #0#4 = #0#3;
          |PINTA #0#4;
     |FINSI;
|FPRC;

|PROCESO DefApl1; |TIPO 7;
     |SI aDefApl ! "";
          #0#1 = aDefApl;
          |PINTA #0#1;
     |FINSI;
|FPRC;

|PROCESO DefApl2;
     |SI aDefApl = "";
          aDefApl = #0#1;
     |FINSI;
|FPRC;

|PROCESO DefAcc1; |TIPO 7;
     |SI aDefAcc ! "";
          #0#2 = aDefAcc;
          |PINTA #0#2;
     |FINSI;
|FPRC;

|PROCESO DefAcc2;
     |SI aDefAcc = "";
          aDefAcc = #0#2;
     |FINSI;
|FPRC;
