|FICHEROS;
     dsctdm01;

     tabladat@;
|FIN;

|VARIABLES;
     nRango     = 0;

     aAlfa      = "";
     aBase      = "";
     aDef       = "";
     aQueQuiero = "";
     aDescri    = "";
|FIN;

|PROCESO Tipo7C3;  |TIPO 7;
     aAlfa = #dsctdm01#3;  |QBF aAlfa;
     |SI aAlfa ! "";  |ACABA;  |FINSI;

     #dsctdm01#3 = aDescri;
     |PINTA  #dsctdm01#3;
|FPRC;

|PROCESO Tipo0C2;  |TIPO 0;
     |C_M #dsctdm01#3, 1, "S";
     |SI #dsctdm01#2 = 0;
         |C_M #dsctdm01#3, 1, "N";
         #dsctdm01#3 = -1;
         |PINTA #dsctdm01#3;
     |FINSI;
|FPRC;

|PROCESO Tipo0C1;  |TIPO 0;
     |DBASS aBase;   |QBT aBase;
     aDef = aBase + #dsctdm01#0 + "/def/" + #dsctdm01#1;  |QBT aDef;
     |CARGA_DEF aDef, tabladat@;
     |SI FSalida ! 0;
         aAlfa = "     No existe el Def " + aDef + " en esta aplicaci¢n";
         |MENAV aAlfa;
         |ACABA;
     |FINSI;

     aQueQuiero = "|TITULO";
     aDescri    = #tabladat@#aQueQuiero#;

     |DESCARGA_DEF #tabladat@;
|FPRC;


|PROCESO Evnt;
     |SI FSalida = 1;  |O FSalida = 2;
         |LEE 000#dsctdm01.=;
     |FINSI;
|FPRC;

|PROCESO Salir;
     |PTEC 806;
|FPRC;

|PROGRAMA;
     |CABEZA "Tablas a contar";

     |CONTROL_SIMPLE 0, "BOTON,{{001}}Salir", 3485, 3495, Salir;

     nRango = 1 + 4 + 8 + 16 + 128;

     |LEE 040#dsctdm01.p;

     |LINEAL_SIMPLE #1#dsctdm01, nRango, 0602, 3398, NULL, NULL, Evnt;

     |PULSATECLA;
|FPRO;

|PROCESO Tipo80;  |TIPO 80;
     |ABRET #dsctdm01;

     FSalida = 3599;
|FPRC;

|PROCESO Tipo90;  |TIPO 90;
     |CIERRAT #dsctdm01;
|FPRC;
