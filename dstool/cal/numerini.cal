|FICHEROS;
     numerini #0;
|FIN;

|VARIABLES;
     &eaNumIni = "";

     nIni = 0;
     aIni = "";
     {-1}aSerie = "";
     aMensaje = "";
|FIN;

|PROGRAMA;
     aIni = "";
     |HAZ AveriguaINI;
     eaNumIni = aIni;
|FPRO;

|PROCESO AveriguaINI;
     aSerie = "";
     |ESPECIFICA "SERIALIZACION", aSerie;
     aSerie = aSerie % 106;                  ||Numero INI
     nIni = aSerie;
     aIni = ("000000" + nIni) % -106;
|FPRC;
