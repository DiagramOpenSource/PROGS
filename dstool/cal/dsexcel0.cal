|FICHEROS;
     dsexcel0 #0;
     dsexcel2 #2;
     dsexcel3 #3;
|FIN;

|VARIABLES;
     nModo = 0;
|FIN;

|PROCESO dsexcel2;
     |BORRA 020#2a;
|FPRC;

|PROCESO dsexcel3;
     |BORRA 020#3a;
|FPRC;

|DEFBUCLE dsexcel2;
     #2#1;
     ;
     #0#0;
     #0#0;
     be;
     NULL, dsexcel2;
|FIN;

|DEFBUCLE dsexcel3;
     #3#1;
     ;
     #0#0, "        ", 000;
     #0#0, "zzzzzzzz", 999;
     be;
     NULL, dsexcel3;
|FIN;

|PROCESO Tipo2; |TIPO 2;
     nModo = (FEntrada / 100) ! 0;
     |SI nModo = 3;
          |BUCLE dsexcel2;
          |BUCLE dsexcel3;
     |FINSI;
|FPRC;
