|FICHEROS;
     cocorreo #0;
|FIN;

|VARIABLES;
     aIP = "";
|FIN;

|PROCESO Autenti; |TIPO 0;
     |SI #0#9 = "S";
          |C_M #0#6, 1, "S";
          |C_M #0#7, 1, "S";
     |SINO;
          |C_M #0#6, 1, "N";
          |C_M #0#7, 1, "N";
     |FINSI;
|FPRC;

|PROGRAMA;
     |CLS;
     |ABRE #0;
     |LEE 101#0p;
     |SI FSalida ! 0; |GRABA 020#0b; |FINSI;
     |PINPA #0#0;
     |PINDA #0#0;
     |ENDATOS #1#0;
     |SI FSalida = 0;
          |GRABA 020#0a;
     |FINSI;
     |LIBERA #0;
     |CIERRA #0;
|FPRO;

|PROCESO Tipo0CampoIP; |TIPO 0;
     |SI #cocorreo#1 = 0; |Y #cocorreo#2 = 0; |Y #cocorreo#3 = 0; |Y #cocorreo#4 = 0;
          |C_M #cocorreo#10, 1, "S";
     |SINO;
          |C_M #cocorreo#10, 1, "N";
          aIP = #cocorreo#1 + "." + #cocorreo#2 + "." + #cocorreo#3 + "." + #cocorreo#4;
          #cocorreo#10 = aIP;
     |FINSI;
     |PINTA #cocorreo#10;
|FPRC;
