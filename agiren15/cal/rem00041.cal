     |SI FSalida = 0;
         |LEE 000#20a;
     |SINO;
         |LEE 000#20u;
     |FINSI;

     |DDEFECTO #641;
     #641#0 = enEmpresa;
     #641#1 = 1;
     |SI FSalida = 0;  |Y  #20#0 = enEmpresa;
         #641#1 = #20#1 + 1;
     |FINSI;

     |DDEFECTO #20;

     #20#0 = #641#0;
     #20#1 = #641#1;

     enModoReg = 1;
     |HAZ CargaFantasma;

     |C_M #641#1, 1, "S";
     |ENDATOS #1#641;
     |SI FSalida = 0;
