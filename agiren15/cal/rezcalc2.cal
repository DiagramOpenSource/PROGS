         nCoefi = 50;  eaContri = "CONYUGE   ";   |HAZ Liquida2;
         nCoefi = 100; eaContri = "CONJUNTA  ";   |HAZ Liquida2;

         |SI eaMuerto ! "";
             nCoefi = 50;  eaContri = "CONJUNTAE ";   |HAZ Liquida2;
         |FINSI;

         |ACABA;
     |FINSI;

     |SI #datfic0@#2 ! "VARIOS    ";                || SI es TITULAR O CONYUGE;
         nCoefi = 100;  eaContri = #datfic0@#2;          |HAZ Liquida2;
         nCoefi = 100;  eaContri = "CONJUNTA  ";  |HAZ Liquida2;

         |SI eaMuerto = "T";  |Y #datfic0@#2 ! "TITULAR   ";
             nCoefi = 100;  eaContri = "CONJUNTAE ";   |HAZ Liquida2;
         |FINSI;

         |SI eaMuerto = "C";  |Y #datfic0@#2 ! "CONYUGE   ";
             nCoefi = 100;  eaContri = "CONJUNTAE ";   |HAZ Liquida2;
         |FINSI;
         |ACABA;
